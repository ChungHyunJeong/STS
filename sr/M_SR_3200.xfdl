	//[검색]NULL 체크
	var arrArgs = ["this.div_Search.form.edt_CusmCd^"+ this.div_Search.form.sta_CusmCd.text +"|NULL"
				  ]; 
	
	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs) ) {
		return false;
	}
		
