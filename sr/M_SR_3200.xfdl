/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {	
		
		//관리번호가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PRCH_NO"))) {
		
			//내용 Web컴포넌트에 값 세팅
			var oWeb = this.div_DetailInput.form.tab_Detail.tpg_01.form.web_ContNm.getProperty("window");
			var sContNm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WORK_NM");//본문내용
			oWeb.callMethod("fn_setData", this.gfn_ReplaceWebEditorData(sContNm));	
			
			//this.fn_CnthNoFormat(this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo);
			
			//이미지-첨부파일 정보 조회
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.PICH_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PRCH_NO");//관리번호-발행번호
			this.fvImgFileDiv.fn_AtchFileSearch(oFileInfo);			
			
			//첨부파일 정보 조회
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
			oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PRCH_NO");//PK번호2-프로젝트번호
			this.fvFileDiv.fn_AtchFileSearch(oFileInfo);
			
			//Master 계약번호 포맷설정
			if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO").length > 8) {
				sFormat = "@@@@-@-@@@@";
			} else {
				sFormat = "@@@-@@@@";
			}
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_format(sFormat);

			
			this.fvDtlSearchYn = true;//상세조회여부
			this.fn_JuchaList();
 		}
	} else {
		this.div_DetailInput.form.tab_Detail.set_enable(false);
		this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master10] Row 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무	
	
	//데이터 변경여부 체크 
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	

	return true;
};  

