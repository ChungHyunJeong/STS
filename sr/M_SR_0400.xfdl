/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master] 데이터 Row 변경 이벤트 - ds_Master10
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{	
	if (e.newrow > -1) {		
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD"))) {				
		
			var strPartCd = ""	
			
			var sPartCd =  this.ds_Master10.getColumn(this.ds_Master10.rowposition,"PART_CD");
			
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PartCd.set_value("");
			this.gfn_SetMultiComboCode(this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PartCd, "");
			

			if(!this.gfn_IsNull(sPartCd)){
				var arrCode   = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PART_CD").split("ⓐ");				
				
				if (arrCode.length > 0) {
					if (this.gfn_IsNull(arrCode[0])) {
						strPartCd = "";
					} else {
						strPartCd =  arrCode.toString();
					}
				} else {
					strPartCd = "";
				}
				
				var strPartCd = nexacro.replaceAll(strPartCd, "ⓐ", ",");				
				this.gfn_SetMultiComboCode(this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PartCd, strPartCd);
			}
			
				//사업자번호, 주민등록번호 포멧 셋팅
				var sCutyCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUTY_CD");
			
			if (sCutyCd == '0030') {
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.set_format("######-#######");
				this.div_MasterInput.form.tab_Master.tpg_01.form.sta_SaumNo.set_text("주민등록번호");
			} else {
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.set_format("###-##-#####");
				this.div_MasterInput.form.tab_Master.tpg_01.form.sta_SaumNo.set_text("사업자번호");
			}
		
			//조회된 정보가 있으면 - 상세조회
			if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD"))) {
				//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
				this.ds_Master20.clearData();		//고객정보
				this.ds_Detail50.clearData();		//고객처정보 - 계좌정보
				this.ds_Detail60.clearData();		//고객처정보 - 경영정보
				this.ds_Detail70.clearData();		//고객처정보 - 공급품목
				this.ds_Detail80.clearData();		//고객처정보 - 주요매출처
				this.ds_Detail90.clearData();		//고객처정보 - 인증현황
				this.ds_Detail100.clearData();		//고객처정보 - 신용평가
				
				this.ds_Input.clearData();
				
				this.ds_Input.addRow();
				this.ds_Input.setColumn(0, "CUSM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD"));
						
				//[공통]서비스정보 초기화
				this.gfn_InitComService(this);
				
				//[공통]서비스정보 추가
				var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
				svcAddObj.sSelectSql = "sr:M_SR_0400_D_SL50";	//계좌정보
				this.gfn_AddComSearchService(this, svcAddObj);
				
				svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
				svcAddObj.sSelectSql = "sr:M_SR_0400_D_SL60";	//경영정보
				this.gfn_AddComSearchService(this, svcAddObj);
				
				svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
				svcAddObj.sSelectSql = "sr:M_SR_0400_D_SL70";	//공급품목
				this.gfn_AddComSearchService(this, svcAddObj);
				
				svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
				svcAddObj.sSelectSql = "sr:M_SR_0400_D_SL80";	//주요매출처
				this.gfn_AddComSearchService(this, svcAddObj);
				
				svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
				svcAddObj.sSelectSql = "sr:M_SR_0400_D_SL90";	//인증현황
				this.gfn_AddComSearchService(this, svcAddObj);

				svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
				svcAddObj.sSelectSql = "sr:M_SR_0400_D_SL100";	//신용평가
				this.gfn_AddComSearchService(this, svcAddObj);	

				svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
				svcAddObj.sSelectSql = "sr:M_SR_0400_M_SL20";	//고객정보
				this.gfn_AddComSearchService(this, svcAddObj);

				svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
				svcAddObj.sSelectSql = "sr:M_SR_0400_M_SL13";	//영업담당/품질담당
				this.gfn_AddComSearchService(this, svcAddObj);

				//[공통]서비스 호출 변수
				var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
				svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
				svcCallObj.sActionName = this.G_SV_SR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
				svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
				svcCallObj.sOutDatset = "ds_Detail50=ds_Detail50 ds_Detail60=ds_Detail60 ds_Detail70=ds_Detail70 ds_Detail80=ds_Detail80 ds_Detail90=ds_Detail90 ds_Detail100=ds_Detail100 ds_Master20=ds_Master20 ds_SacpCd=ds_Master13";//출력 데이터셋
				svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
				svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

				//[공통]서비스 호출
				this.gfn_CallComService(this, svcCallObj);		
			}
		}
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master] Row 변경전 이벤트 - ds_Master10
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10) || this.gfn_DsIsUpdated(this.ds_Detail60) || this.gfn_DsIsUpdated(this.ds_Detail70) || this.gfn_DsIsUpdated(this.ds_Detail80) || this.gfn_DsIsUpdated(this.ds_Detail90) || this.gfn_DsIsUpdated(this.ds_Detail100)) {
		this.gfn_Confirm("SM003", "", "CONF.MAST10_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	

	return true;
};  

/*---------------------------------------------------------------------------------------------
 *    03. [데이터셋-ds_Master] 데이터 Row 변경 이벤트 - ds_Master20
  ---------------------------------------------------------------------------------------------*/
this.ds_Master20_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{

	if (e.newrow > -1 && !this.fvFirstFileSearch) {		
		if (!this.gfn_IsNull(this.ds_Master20.getColumn(this.ds_Master20.rowposition, "CSPM_CD"))) {						
			//첨부파일 정보 조회
			var oFileInfo2 = new Object();//[공통] 첨부파일정보
			oFileInfo2.PRGM_ID = this.fvCusmFileId;//프로그램ID
			oFileInfo2.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
			oFileInfo2.PK01_NO = this.ds_Master20.getColumn(this.ds_Master20.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo2.PK02_NO = this.ds_Master20.getColumn(this.ds_Master20.rowposition, "CSPM_CD");//PK번호2-고객번호
			
			this.fvFileDiv.fn_AtchFileSearch(oFileInfo2);
		}
	}
	this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CscmId.set_readonly(true);
	//SRM_ID 값이 없을때..
	if(this.gfn_IsNull(this.ds_Master20.getColumn(e.newrow,"CSCM_ID"))){
		this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CscmId.set_readonly(false);
	}
};


