		svcCallObj.isSync = true;//동기식으로 설정할지 여부

		//[공통]서비스 호출
		this.gfn_CallComService(this, svcCallObj);


		if(this.ds_Exists.rowcount > 0){
			var arrMsgArg = ["사업자번호"];
			
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.setFocus();
