		//-------------------------------------------------------------------------------------------------------------
	
	
		this.gfn_GridDelProcess(this, this.ds_Master20);//crud_flag 변경-삭제(D)		
	}else if(iTabIndex == 2){
		this.gfn_GridDelProcess(this, this.ds_Master30);//crud_flag 변경-삭제(D)		
	}else if(iTabIndex == 3){
		this.gfn_GridDelProcess(this, this.ds_Master40);//crud_flag 변경-삭제(D)		
	}
};

