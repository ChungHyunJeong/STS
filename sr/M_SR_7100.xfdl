		if(this.ds_Exists.rowcount > 0){
			this.ds_Master20.setColumn(this.ds_Master20.rowposition, "s_chk", "0");
			var arrMsgArg = ["파트너사 정기평가가 존재하여 삭제할 수"];		
			this.gfn_Alert("MM046", arrMsgArg); //	
			return; 
