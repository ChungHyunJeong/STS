this.fn_DetailGrd01DeleteRow = function()
{
	var iTabIndex = this.div_MasterInput.form.tab_Master.tabindex ;	 
	
	if(iTabIndex == 0){
		//---- 해당년도에 평가자료가 있으면 삭제 안되게		
		this.ds_Input.clearData();
		this.ds_Input.addRow();
		this.ds_Exists.clearData();		
		
		this.ds_Input.setColumn(0, "JGPH_YY", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JGPM_YY"));
		this.ds_Input.setColumn(0, "CUSM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD"));
		
		//[공통]서비스정보 초기화
		this.gfn_InitComService(this);
		
		//[공통]서비스정보 추가
		var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
