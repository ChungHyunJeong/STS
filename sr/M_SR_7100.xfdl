this.fn_ComBtnSave = function()
{ 

	//변경여부 체크
	var iTabIndex = this.div_MasterInput.form.tab_Master.tabindex;	
	
	if(iTabIndex==0){
	 	if (!this.gfn_DsIsUpdated(this.ds_Master10)) {
			this.gfn_Alert("SM036");//변경된 내용이 없습니다.
			return;
		}		
		//저장시 재조회 Key설정
		this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JGPM_YY");//조회Key01
		this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JGPM_GU");//조회Key02
		this.fvSchKey03 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD");//조회Key03  	

	}else if(iTabIndex==1){
	 	if (!this.gfn_DsIsUpdated(this.ds_Master20)) {
			this.gfn_Alert("SM036");//변경된 내용이 없습니다.
			return;
		}		
		//저장시 재조회 Key설정
		this.fvSchKey01 = this.ds_Master20.getColumn(this.ds_Master20.rowposition, "JGPM_YY");//조회Key01
		this.fvSchKey02 = this.ds_Master20.getColumn(this.ds_Master20.rowposition, "SYST_ID");//조회Key02		
		
	}else if(iTabIndex==2){
	 	if (!this.gfn_DsIsUpdated(this.ds_Master30)) {
			this.gfn_Alert("SM036");//변경된 내용이 없습니다.
			return;
		}
		//저장시 재조회 Key설정
		this.fvSchKey01 = this.ds_Master30.getColumn(this.ds_Master30.rowposition, "JGPM_YY");//조회Key01
		this.fvSchKey02 = this.ds_Master30.getColumn(this.ds_Master30.rowposition, "JGPM_GU");//조회Key02		
		this.fvSchKey03 = this.ds_Master30.getColumn(this.ds_Master30.rowposition, "JGPE_CD");//조회Key02		
	}else if(iTabIndex==3){
	 	if (!this.gfn_DsIsUpdated(this.ds_Master40)) {
			this.gfn_Alert("SM036");//변경된 내용이 없습니다.
			return;
		}
		//저장시 재조회 Key설정
		this.fvSchKey01 = this.ds_Master40.getColumn(this.ds_Master40.rowposition, "JGPM_YY");//조회Key01
		this.fvSchKey02 = this.ds_Master40.getColumn(this.ds_Master40.rowposition, "JGPF_CD");//조회Key02		
	}	
  	
  	//Master 데이터 유효성 체크
  	if (!this.fn_VaildMasterSave()) return;
  	

  	//Master 입력/수정/삭제 상태 체크
  	//var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
  	


  	//[공통]서비스 정보 초기화	
  	this.gfn_InitComService(this);
  	
  	//[공통]서비스정보 추가
  	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
  	
  	//[공통]서비스 호출 변수
  	var sInputDaset = "" //입력 데이터셋
  	var sOutDatset = "";//출력 데이터셋
  	var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2

  	//Master 변경건이 있으면 Master 저장
	if(iTabIndex==0){
		if (this.gfn_DsIsUpdated(this.ds_Master10)) {  		
			//[공통]서비스 정보 추가
			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sInsertSql = "sr:M_SR_7100_M_IN10";//저장Sql
			svcAddObj.sUpdateSql = "sr:M_SR_7100_M_UP10";//수정Sql		
			svcAddObj.sDeleteSql = "sr:M_SR_7100_M_DL10";//삭제Sql
			svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
									 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.  
			this.gfn_AddComSaveService(this, svcAddObj);  		
			sInputDaset = "ds_Master10=ds_Master10:U" //입력 데이터셋  
		}	
	}else if(iTabIndex==1){
		if (this.gfn_DsIsUpdated(this.ds_Master20)) {  		
			//[공통]서비스 정보 추가
			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sInsertSql = "sr:M_SR_7100_M_IN20";//저장Sql
			svcAddObj.sUpdateSql = "sr:M_SR_7100_M_UP20";//수정Sql		
