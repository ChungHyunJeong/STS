this.fn_Confirm = function()
{
	//부적합통보확인 유효성 체크
	if (!this.fn_VaildConfirm()) return;	
	
	this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PTCP_CD", this.gfn_GetSessionValue("SYST_ID"));
