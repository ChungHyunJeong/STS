/*---------------------------------------------------------------------------------------------
 *    [Master-Button] 부적합통보확인 버튼 이벤트
  ---------------------------------------------------------------------------------------------*/
this.div_MasterInput_btn_Confirm_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var sRSPA_NM = ""
	var sACPA_NM = ""
	var sACPA_DD = ""
	
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {

		sRSPA_NM = this.ds_Detail10.getColumn(iLoop, "RSPA_NM"); //부적합원인
		sACPA_NM = this.ds_Detail10.getColumn(iLoop, "ACPA_NM"); //조치방안
		sACPA_DD = this.ds_Detail10.getColumn(iLoop, "ACPA_DD"); //조치일정
		
		if (this.gfn_IsNull(sRSPA_NM) || sRSPA_NM == "" || this.gfn_IsNull(sACPA_NM) || sACPA_NM == "" || this.gfn_IsNull(sACPA_DD) || sACPA_DD == "") {		
			//탭, 위치 이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);	
			
			if (this.gfn_IsNull(sRSPA_NM) || sRSPA_NM == "") {
				alertMsg = "부적합원인";
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(11,iLoop);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();
			}
			
			if (this.gfn_IsNull(sACPA_NM) || sACPA_NM == "") {		
				if (this.gfn_IsNull(sRSPA_NM) || sRSPA_NM == "") { 		
					alertMsg = alertMsg + ", 조치방안";
				} else {
					alertMsg = "조치방안";
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(12,iLoop);
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();					
				}
			}
			
			if (this.gfn_IsNull(sACPA_DD) || sACPA_DD == "") {		
				if (this.gfn_IsNull(sRSPA_NM) || sRSPA_NM == "" || this.gfn_IsNull(sACPA_NM) || sACPA_NM == "") { 		
					alertMsg = alertMsg + ", 조치일정";
				} else {
					alertMsg = "조치일정";
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(13,iLoop);
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();					
				}
			}
				
			var iRow = iLoop + 1;
			var arrMsgArg = [iRow+"번째행 "+alertMsg];
			this.gfn_Alert("SM008", arrMsgArg);//견적정보 제출단가는 필수 입력 항목입니다.
			return false;
			
			break;			
		}
		
	}
	
	var arrMsgArg = ["부적합통보확인"];
	this.gfn_Confirm("HR008", arrMsgArg,  "CONF.CONFIRM", "fn_MsgCallback");//부적합통보확인 하시겠습니까?
}

