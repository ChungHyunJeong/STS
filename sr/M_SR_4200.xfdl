/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	//요청수량
	if (e.columnid == "NGBB_QT") {	
 		var sMopbQt = nexacro.toNumber(this.ds_Detail10.getColumn(e.row, "MOPB_QT"));
 		var sNgbbQt = nexacro.toNumber(e.newvalue);		
		var sNgbbQtOrg = e.oldvalue;
		
		if(sNgbbQt <= 0){
			var arrMsgArg = ["반품수량"];
			this.ds_Detail10.setColumn(e.row, "NGBB_QT", sNgbbQtOrg)
			this.gfn_Alert("SM008", arrMsgArg); //반품수량 필수 입력 항목입니다.
			return;
		}
		
		if(sMopbQt < sNgbbQt)
		{				
			this.ds_Detail10.setColumn(e.row, "NGBB_QT", sNgbbQtOrg)
	
			var arrMsgArg = ["가능한 반품수량"];			
			this.gfn_Alert("SR002", arrMsgArg); //&ITM0을(를) 초과했습니다.
			return;
		 }
	}
};

