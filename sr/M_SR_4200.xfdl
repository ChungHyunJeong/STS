/***********************************************************************************************
 * 02.DG1.05. [Detail그리드2-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Choice = function()
{
	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {
			
			var arrCheckRow = [];//체크된 Row
			
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
			
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					var sMophNo = this.ds_Detail20.getColumn(iLoop, "MOPH_NO");
					var sMopbSe = this.ds_Detail20.getColumn(iLoop, "MOPB_SE");
					var iFvFindRow = this.ds_Detail10.findRowExpr("NGCH_NO=='" + sMophNo + "' && NGCB_SE=='" + sMopbSe + "'");//중복안되도록
					
					if (iFvFindRow < 0) {
						var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)
					
						this.ds_Detail10.copyRow(iAddRow, this.ds_Detail20, iLoop);
						this.ds_Detail10.setColumn(iAddRow, "SGBH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SGBH_NO"));//반품요청번호
						this.ds_Detail10.setColumn(iAddRow, "SGBB_SE", "");//반품요청순번
						
						this.ds_Detail10.setColumn(iAddRow, "NGCH_NO", this.ds_Detail20.getColumn(iLoop, "MOPH_NO"));//출고번호
						this.ds_Detail10.setColumn(iAddRow, "NGCB_SE", this.ds_Detail20.getColumn(iLoop, "MOPB_SE"));//출고순번
						this.ds_Detail10.setColumn(iAddRow, "DELE_YN", "0");//삭제여부
						
						arrCheckRow.push(iLoop );
					} else {
						var crudType = this.ds_Detail10.getColumn(iFvFindRow, "crudType");
						this.ds_Detail10.setColumn(iFvFindRow, "NGBB_QT", this.ds_Detail20.getColumn(iLoop, "NGBB_QT"));
						
						if (crudType == "I") {
							this.ds_Detail10.setColumn(iFvFindRow, "NGBB_QT", this.ds_Detail20.getColumn(iLoop, "NGBB_QT"));
						} else {
							var sOrgNgbbQt = nexacro.toNumber(this.ds_Detail10.getOrgColumn(iFvFindRow, "NGBB_QT"));
							var sNgbbQt = this.ds_Detail20.getColumn(iLoop, "NGBB_QT");
							this.ds_Detail10.setColumn(iFvFindRow, "NGBB_QT", sOrgNgbbQt + sNgbbQt);
							this.ds_Detail10.setColumn(iFvFindRow, "crudType", "U");
						}
					}
					
					//선택항목 비활성화처리
					this.ds_Detail20.setColumn(iLoop, "s_conf", "1");
					this.ds_Detail20.setColumn(iLoop, "s_chk", "0");//Uncheck로 변경
				}
			}

			this.ds_Detail20.set_rowposition(-1);
			//출고상세로 Tab이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);
		}
	}
};

/***********************************************************************************************
 * 02.DG1.06. [Detail그리드2-버튼] 취소 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Cancel = function()
{
	this.ds_Detail10.reset();
	this.ds_Detail20.reset();

	//기존 체크정보 초기화
	for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
	
		if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
			this.ds_Detail20.setColumn(iLoop, "s_chk", "0");//Uncheck로 변경
		}
		
		if (this.ds_Detail20.getColumn(iLoop, "s_conf") == "1") {
			this.ds_Detail20.setColumn(iLoop, "s_conf", "0");
		}		
	}
};
 
