		if(sNgbbQt <= 0){
			var arrMsgArg = ["반품수량"];
			this.ds_Detail20.setColumn(e.row, "NGBB_QT", sNgbbQtOrg)
			this.gfn_Alert("SM008", arrMsgArg); //반품수량 필수 입력 항목입니다.
