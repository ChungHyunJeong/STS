this.fn_ComBtnAdd = function()
{
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}
	var arrArgs = ["this.div_Search.form.edt_CusmCd^" + this.div_Search.form.sta_CusmCd.text + "|NULL"
				  ]; 
				  
	var sCheckGroupTitle = "검색조건";//체크그룹 타이틀
	
	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
		return;
	}
	
	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	
	var iAddRow = this.ds_Master10.addRow();	
	this.ds_Master10.setColumn(iAddRow, "SGBH_DD", sToDay);//반품요청일자-오늘날짜로 설정
	this.ds_Master10.setColumn(iAddRow, "DELE_YN", "0");//삭제여부	
	this.ds_Master10.setColumn(iAddRow, "CSPM_CD", this.gfn_GetSessionValue("SYST_ID"));//출고자사번
	this.ds_Master10.setColumn(iAddRow, "CSPM_NM", this.gfn_GetSessionValue("USEM_NM"));//출고자명


	//파트너사로 로그인시처리 - 비활성화
	if (this.gfn_GetSessionValue("CMDS_CD") == "P") {
		//파트너사
		this.ds_Master10.setColumn(iAddRow, "CUSM_CD", this.gfn_GetSessionValue("CUSM_CD"));
		this.ds_Master10.setColumn(iAddRow, "CUSM_NM", this.gfn_GetSessionValue("CUSM_NM"));
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmNm.set_readonly(true);
		
	} else {
		this.ds_Master10.setColumn(iAddRow, "CUSM_CD", this.div_Search.form.edt_CusmCd.value);
		this.ds_Master10.setColumn(iAddRow, "CUSM_NM", this.div_Search.form.edt_CusmNm.value);
		//this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_readonly(false);
	}

	//입력영역 활성화
 	this.div_MasterInput.set_enable(true);
	this.div_DetailInput.form.set_enable(true);
 	this.div_DetailInput.form.tab_Detail.set_enable(true);
 	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	
	//처음 입력 항목에 포커스
	this.div_DetailInput.form.tab_Detail.set_tabindex(1);
	
	this.ds_Detail10.clearData();//상세정보 초기화
	this.ds_Detail20.clearData();//검색 초기화
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
	//첨부파일정보 권한설정
	var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
	oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
	this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
	
	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
}

