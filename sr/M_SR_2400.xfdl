this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	//this.fvModiChk = true;
	//this.alert(this.gfn_DsIsUpdated(this.ds_Master10));
	
	if (this.gfn_DsIsUpdated(this.ds_Master10)){
		if(obj.getRowType(e.row)==4){
			this.fvModiChk = true;
		}
	}	
	
	if(e.columnid == "s_chk"){
		if(e.newvalue =="0"){
			obj.setColumn(e.row, "crudType", "");
		}else{
			if(this.fvModiChk){	
				obj.setColumn(e.row, "crudType", "U");
			}
		}
	}
};
