/***********************************************************************************************
 * 02.MG0.05. [그리드-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
this.fn_MasterGrdChoice = function()
{
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_IN01) 
			&&!this.gfn_DsIsUpdated(this.ds_IN02) 
			&&!this.gfn_DsIsUpdated(this.ds_IN03) 
			&&!this.gfn_DsIsUpdated(this.ds_IN04) 
			&&!this.gfn_DsIsUpdated(this.ds_IN99) ) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}
	if(this.gfn_DsIsUpdated(this.ds_IN01)  && (this.gfn_Nvl(this.ds_IN01.getColumn(0, "IN_RT"),0)!=0.0 && this.gfn_IsNull(this.ds_IN01.getColumn(0, "IN_DD")))){	
		this.gfn_Alert("SM008", ["1차 납품예정일"]);
		return;
	}
	if(this.gfn_DsIsUpdated(this.ds_IN02)  && (this.gfn_Nvl(this.ds_IN02.getColumn(0, "IN_RT"),0)!=0.0 && this.gfn_IsNull(this.ds_IN02.getColumn(0, "IN_DD")))){	
		this.gfn_Alert("SM008", ["2차 납품예정일"]);
		return;
	}
	if(this.gfn_DsIsUpdated(this.ds_IN03)  && (this.gfn_Nvl(this.ds_IN03.getColumn(0, "IN_RT"),0)!=0.0 && this.gfn_IsNull(this.ds_IN03.getColumn(0, "IN_DD")))){	
		this.gfn_Alert("SM008", ["3차 납품예정일"]);
		return;
	}
	if(this.gfn_DsIsUpdated(this.ds_IN04)  && (this.gfn_Nvl(this.ds_IN04.getColumn(0, "IN_RT"),0)!=0.0 && this.gfn_IsNull(this.ds_IN04.getColumn(0, "IN_DD")))){	
		this.gfn_Alert("SM008", ["4차 납품예정일"]);
		return;
	}	
	if(this.gfn_DsIsUpdated(this.ds_IN99)  && (this.gfn_Nvl(this.ds_IN99.getColumn(0, "IN_RT"),0)!=0.0 && this.gfn_IsNull(this.ds_IN99.getColumn(0, "IN_DD")))){			
		this.gfn_Alert("SM008", [this.ds_IN99.getColumn(0,"IN_SE")+"차 납품예정일"]);
		return;
	}
	if(this.gfn_DsIsUpdated(this.ds_IN99)  && (this.gfn_Nvl(this.ds_IN99.getColumn(0, "IN_RT"),0)!=0.0 && this.gfn_IsNull(this.ds_IN99.getColumn(0, "IN_SE")))){			
		this.gfn_Alert("SM008", ["회차선택"]);
		return;
	}
	
	var nChk = nexacro.toNumber(this.gfn_Nvl(this.ds_IN01.getColumn(0, "IN_RT"),0))
				+nexacro.toNumber(this.gfn_Nvl(this.ds_IN02.getColumn(0, "IN_RT"),0))
				+nexacro.toNumber(this.gfn_Nvl(this.ds_IN03.getColumn(0, "IN_RT"),0))
				+nexacro.toNumber(this.gfn_Nvl(this.ds_IN04.getColumn(0, "IN_RT"),0))
				+nexacro.toNumber(this.gfn_Nvl(this.ds_IN99.getColumn(0, "IN_RT"),0));
	if(nChk>100){			
		this.gfn_Alert("SM056", ["비중(합계)", "100%"]);
		return false;
	}
	if (this.ds_Master10.getCaseCount("s_chk=='1'") > 0) {
		for(var i=0; i<this.ds_Master10.getRowCount(); i++){
			if(this.ds_Master10.getColumn(i, "s_chk")=="1"){
				if(this.gfn_DsIsUpdated(this.ds_IN01)){
					this.ds_Master10.setColumn(i, "DELI_DD_01", this.ds_IN01.getColumn(0, "IN_DD"));
					this.ds_Master10.setColumn(i, "PCOC_QT_01", nexacro.round(this.ds_Master10.getColumn(i,"BALJU_PCOB_QT")*(this.ds_IN01.getColumn(0, "IN_RT")/100),2));
				}
				if(this.gfn_DsIsUpdated(this.ds_IN02)){
					this.ds_Master10.setColumn(i, "DELI_DD_02", this.ds_IN02.getColumn(0, "IN_DD"));
					this.ds_Master10.setColumn(i, "PCOC_QT_02", nexacro.round(this.ds_Master10.getColumn(i,"BALJU_PCOB_QT")*(this.ds_IN02.getColumn(0, "IN_RT")/100),2));
				}
				if(this.gfn_DsIsUpdated(this.ds_IN03)){
					this.ds_Master10.setColumn(i, "DELI_DD_03", this.ds_IN03.getColumn(0, "IN_DD"));
					this.ds_Master10.setColumn(i, "PCOC_QT_03", nexacro.round(this.ds_Master10.getColumn(i,"BALJU_PCOB_QT")*(this.ds_IN03.getColumn(0, "IN_RT")/100),2));
				}
				if(this.gfn_DsIsUpdated(this.ds_IN04)){
					this.ds_Master10.setColumn(i, "DELI_DD_04", this.ds_IN04.getColumn(0, "IN_DD"));
					this.ds_Master10.setColumn(i, "PCOC_QT_04", nexacro.round(this.ds_Master10.getColumn(i,"BALJU_PCOB_QT")*(this.ds_IN04.getColumn(0, "IN_RT")/100),2));
				}
				if(this.gfn_DsIsUpdated(this.ds_IN99)){
					var nSe = this.ds_IN99.getColumn(0,"IN_SE").padLeft(2,"0");
					this.ds_Master10.setColumn(i, "DELI_DD_"+nSe, this.ds_IN99.getColumn(0, "IN_DD"));
					this.ds_Master10.setColumn(i, "PCOC_QT_"+nSe, nexacro.round(this.ds_Master10.getColumn(i,"BALJU_PCOB_QT")*(this.ds_IN99.getColumn(0, "IN_RT")/100),2));
				}
				this.ds_Master10.setColumn(i, "crudType","U");
			}
		}
	}
	else{
		this.gfn_Alert("HR003");//일괄적용할 데이터를 선택하십시요.
		return;
	}
};
