/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 

	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}

	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (!this.fn_VaildMasterSave()) return;
	
	//선택된 row 를 master11 로 입력
	this.ds_Master11.clearData();
	for(var i=0; i<this.ds_Master10.getRowCount(); i++){
		if(this.ds_Master10.getColumn(i,"s_chk")=="1"){
			var iAddRow = this.ds_Master11.addRow();
			this.ds_Master11.copyRow(iAddRow, this.ds_Master10, i);
			this.ds_Master11.setColumn(iAddRow, "crudType","U");
		}
	}
	
	//선택여부 확인
	if(this.ds_Master11.getRowCount()<=0){
		this.gfn_Alert("SM046");	//선택된 내용이 없습니다.
		return false;
	}
	
	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	//[공통]서비스 정보 추가
	svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object			 
	svcAddObj.sInsertSql = "";//저장Sql			 
	svcAddObj.sUpdateSql = "sr:M_SR_2400_M_UP10";//수정Sql
	svcAddObj.sDeleteSql = "";//삭제Sql
	svcAddObj.sSaveFlagColumn = "crudType";
	svcAddObj.execType = "N";
	this.gfn_AddComSaveService(this, svcAddObj);
		
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "SAVE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Master11=ds_Master11:U" //입력 데이터셋
	svcCallObj.sOutDatset = "";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명		
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);
}

/***********************************************************************************************
 * 05.02. [Vaildation] Master 저장 필수값 체크 - 입력항목 
 ***********************************************************************************************/ 
this.fn_VaildMasterSave = function()
{
	for(var i=0; i<this.ds_Master10.getRowCount(); i++){
		if(this.ds_Master10.getColumn(i,"s_chk")=="1"){			
			var nSum = 0.0;
			for(var n=1;n<=20;n++){
				nSum = nexacro.round(nSum + nexacro.toNumber(this.gfn_Nvl(this.ds_Master10.getColumn(i, "PCOC_QT_"+(n+"").padLeft(2,"0")),"0")),2);
			}
			if(nexacro.toNumber(this.ds_Master10.getColumn(i, "BALJU_PCOB_QT")) != nSum){
				var arrMsgArg = ["납품수량합계", "발주수량"];
				this.gfn_Alert("SM063", arrMsgArg);
				return false;
			}
		}
	}
	return true;
}
 
