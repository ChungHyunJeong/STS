this.fn_ComBtnSearch = function()
{ 

	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Input.clearData();
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "SESU_ID", this.gfn_GetSessionValue("SYST_ID"));//SYSTEM ID
	this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_TemhDtFr.value);
	this.ds_Input.setColumn(0, "CLOS_DD", this.div_Search.form.cal_TemhDtTo.value);
	this.ds_Input.setColumn(0, "CNTH_NO", this.div_Search.form.mae_CnthNo.value);
	this.ds_Input.setColumn(0, "CUSM_CD", this.div_Search.form.edt_CusmCd.value);
	this.ds_Input.setColumn(0, "WORB_NO", this.div_Search.form.edt_WorbNo.value);
	this.ds_Input.setColumn(0, "PCOH_GU", this.div_Search.form.cbo_PcohGu.value);
	this.ds_Input.setColumn(0, "DEPM_CD", this.div_Search.form.edt_DepmCd.value);
	this.ds_Input.setColumn(0, "SESU_ID", this.div_Search.form.edt_SystId.value);
	this.ds_Input.setColumn(0, "PRGM_ID", this.fvComPrgmId);//프로그램ID
	
	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;	

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	var sSqlId = "sr:M_SR_2400_M_SL10";
	var sOutDatset = "ds_Master10=ds_Master10";
		
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = sSqlId;
	this.gfn_AddComSearchService(this, svcAddObj);	
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MST_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
    svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
}

/***********************************************************************************************
 * 01.02. [Vaildation] 조회 필수값 체크
 ***********************************************************************************************/
this.fn_VaildSearch = function()
{	

	if(!this.gfn_IsNull(this.div_Search.form.cal_TemhDtFr.value) && !this.gfn_IsNull(this.div_Search.form.cal_TemhDtTo.value) ){	
		if (!this.gfn_CheckFromToDate(this.fvComPrgmId, this.div_Search.form.cal_TemhDtFr, this.div_Search.form.cal_TemhDtTo)) return false;//조회기간 범위 체크
	}
	
	//조회조건 필수입력값 있으면 체크
	var arrArgs = [ "this.div_Search.form.edt_CusmCd^"   +  this.div_Search.form.Static02_01.text + "|NULL"  //파트너사             				   
	                , "this.div_Search.form.cal_TemhDtFr^" +  this.div_Search.form.Static00.text  + "|NULL"    //발주일자	 
					, "this.div_Search.form.cal_TemhDtTo^" +  this.div_Search.form.Static00.text  + "|NULL"   
			      ]; 
					   
					   
	var sCheckGroupTitle = "조회조건";//체크그룹 타이틀

	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
		return false;
	}

	//조회조건 필수입력값 있으면 체크			  
// 	var arrArgs = ["this.div_Search.form.edt_DepmCd^" + this.div_Search.form.sta_DepmCd.text + "|NULL" //등록부서
// 			      ]; 

	//필수 체크 제외
// 	if(this.gfn_IsNull(this.ds_Input.getColumn(0,"CNTH_NO")) && this.gfn_IsNull(this.ds_Input.getColumn(0,"WORB_NO"))){
// 		var arrMsgArg = ["계약번호", "W/O번호"];
// 		this.gfn_Alert("SM053", arrMsgArg);
// 		return false;
// 	}

	return true;
}

