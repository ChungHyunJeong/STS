					svcCallObj.sCallBackFnc = "";//서버에서 처리가 완료된 후 Callback 받을 Function 명
					
					//[공통]서비스 호출
					this.gfn_CallComService(this, svcCallObj);	
					
					
									
				} else {
					var arrMsgArg = [nApp.gds_WfRst.getColumn(0, "O_RET_CMSG")];
					this.gfn_Alert("WF013", oArg);//재기안 처리시 오류가 발생하였습니다.\n&ITM0				
