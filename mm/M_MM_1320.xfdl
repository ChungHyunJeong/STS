	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");//최종여부	
	this.ds_Master10.setColumn(iAddRow, "IPRH_DD", this.gfn_GetCommDate("toDay"));//요청일자-현재날자로 설정
	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//요청부서코드	
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//요청부서명	
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//요청자사번	
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//요청자명		
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//요청자ID	
	this.ds_Master10.setColumn(iAddRow, "DELE_YN", "0");//삭제여부	
	this.ds_Master10.setColumn(iAddRow, "IPRH_YN", "0");//요청상태(0:요청/1:확인)
	this.ds_Master10.setColumn(iAddRow, "RQRE_NM", this.ds_Master10.getColumn(iOldRow, "RQRE_NM"));//사유 - 복사대상
	this.ds_Master10.setColumn(iAddRow, "REMK_NM", this.ds_Master10.getColumn(iOldRow, "REMK_NM"));//비고 - 복사대상
	this.ds_Master10.setColumn(iAddRow, "IPRH_NM", "");//공백 (비고-파트너사)
	
	this.ds_Master10.set_enableevent(true); //row 생성속도 향상을 위한 이벤트 true
	
	//Detail Insert 	
	this.ds_Detail10.set_enableevent(false); //row 생성속도 향상을 위한 이벤트 false
	this.ds_Detail10.reset();	//reset;

	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		this.ds_Detail10.setColumn(iLoop, "IPRH_NO", "");	//요청번호 - 초기화
		this.ds_Detail10.setColumn(iLoop, "WOFH_SQ", "");	//결재차수 - 초기화
		this.ds_Detail10.setColumn(iLoop, "IPRB_SE", "");	//순번 - 초기화
		this.ds_Detail10.setColumn(iLoop, "IPRB_PR", "");	//단가 - 초기화
		this.ds_Detail10.setColumn(iLoop, "IPRB_AM", "");	//금액 - 초기화
		this.ds_Detail10.setColumn(iLoop, "IPRB_CT", "");	//납기예상수 - 초기화
		this.ds_Detail10.setColumn(iLoop, "IPRB_NM", "");	//비고(파트너) - 초기화
		this.ds_Detail10.setColumn(iLoop, "crudType", "I");	//순번 - 입력(I)
	}
	
