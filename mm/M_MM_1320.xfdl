/*---------------------------------------------------------------------------------------------
 *    [Master 입력영역-Button] 엑셀업로드 Call Back 함수
  ---------------------------------------------------------------------------------------------*/
this.fn_ImportCallback = function(sImportId)
{	

	var nRow = 0;

	this.ds_Detail10.set_enableevent(false);//데이터셋 이벤트-비활성화
	
	for (var iLoop=0; iLoop<this.ds_Import.getRowCount(); iLoop++) {

		nRow = this.ds_Detail10.addRow();
		
		this.ds_Detail10.setColumn(nRow, "ITEM_NM", this.ds_Import.getColumn(iLoop, "Column0")); //품목명
		this.ds_Detail10.setColumn(nRow, "SPEC_NM", this.ds_Import.getColumn(iLoop, "Column1")); //규격
		this.ds_Detail10.setColumn(nRow, "UNIT_CD", this.ds_Import.getColumn(iLoop, "Column2")); //단위
		this.ds_Detail10.setColumn(nRow, "IPRB_QT", this.ds_Import.getColumn(iLoop, "Column3")); //수량
		this.ds_Detail10.setColumn(nRow, "REMK_NM", this.ds_Import.getColumn(iLoop, "Column4")); //비고		
		
		this.ds_Detail10.setColumn(nRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD")); //회사코드(PK)
		this.ds_Detail10.setColumn(nRow, "IPRH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "IPRH_NO")); //요청번호(PK)
		this.ds_Detail10.setColumn(nRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ")); //결재차수(PK)
		this.ds_Detail10.setColumn(nRow, "crudType", "I"); //저장시 ds_detail10 의 내용을 저장키 위함.
	
	}		
	
	//[데이터 유효성체크] 시작
	//1. 품목명 존재유무 체크 - 데이터 미존재의 경우 저장시 체크
	
	//2. 컬럼 중복 데이터 체크 - 품목명
	var sDupColCheck = this.gfn_VaildDupDatasetCol(this.ds_Detail10, "ITEM_NM");
	
	if (!this.gfn_IsNull(sDupColCheck)) {
		var arrMsgArg = ["품목명", sDupColCheck];
		this.gfn_Alert("SM080", arrMsgArg);//[ &ITM0 ] 컬럼의 데이터중 [ &ITM1 ] 행 데이터가 중복입니다.
		this.ds_Detail10.set_enableevent(true);//데이터셋 이벤트-활성화
		return;		
	}
	
	//3. 숫자타입 체크 - 숫자타입 없음
	var iIPRB_QT = 0;//수량
	var sCheckMsg = "";
	
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		iIPRB_QT = this.ds_Detail10.getColumn(iLoop, "IPRB_QT");//수량
		
		//수량 숫자형이 아니면
		if (!this.gfn_IsDigit(iIPRB_QT)) { 
			if (this.gfn_IsDigit(iIPRB_QT)) {
				this.ds_Detail10.setColumn(iLoop, "IPRB_QT", "");//수량
			}
			
			if (this.gfn_IsNull(sCheckMsg)) {
				sCheckMsg = iLoop + 1;
			} else {
				sCheckMsg = sCheckMsg + ", " + (iLoop + 1);
			}
		}
	}	

	//4. 체크메시지가 있으면 - 코드값이 존재하지 않아 유효값 체크 안함
	
	//[데이터 유효성체크] 끝
		
	this.ds_Detail10.set_enableevent(true);//데이터셋 이벤트-활성화
	
}


