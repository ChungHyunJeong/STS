/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Detail20] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/ 
this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	// 같은 계약번호 일괄 선택 기능 시작 
	//체크시 해당 프로그램 전체선택
	if (e.columnid == "s_chk") {
		if (this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "s_chk") == '1') {		
			//Detail20에 계약번호가 최초 1건만 체크됐을 경우에만 처리함(2건일 경우 return)
			var sCnthNo = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "CNTH_NO");//계약번호
			var iChkCnt = this.ds_Detail20.getCaseCount("CNTH_NO == '"+ sCnthNo +"' && s_chk == '1'"); //체크건수
			if (iChkCnt > 1) return;
			
			//최초 1건만 체크시 로직 처리
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				//Detail10 중복여부 체크
				iDupCnt = this.ds_Detail10.getCaseCount("CNTH_NO == '"+ sCnthNo +"'"); 
			
				//계약번호가 같고 Detail10에 계약번호가 미존재하고 색상변경이 안된 대상				
				if (this.ds_Detail20.getColumn(iLoop, "CNTH_NO") == sCnthNo 
				 && iDupCnt == 0
				 && (this.gfn_IsNull(this.ds_Detail20.getColumn(iLoop, "s_conf")) || this.ds_Detail20.getColumn(iLoop, "s_conf") == '0')) {
								
					//체크박스 Check
					this.ds_Detail20.setColumn(iLoop, "s_chk", "1");//Check로 변경
					
				}
			}
		}
	}
	//같은 계약번호 일괄 선택 기능 끝 
};

