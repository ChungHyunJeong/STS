/***********************************************************************************************
 * 02.DG1.05. [Detail그리드2-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
 /*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 적용 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.fn_DetailGrd02Choice = function()
{
	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {
			
			//1. 기성년월, 계약번호 중복 체크
			var sIdntNoChk=""; //Key(계약번호) 체크			
			var sCorhYmNoChk=""; //Key(기성년월) 체크			
			var sCothYm = this.div_MasterInput.form.tab_Master.tpg_01.form.cal_CothYm.value;
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {

					//1. Detail20 Tab과 Detail10 화면 항목 비교 : 기성년월, 계약번호
					if(this.ds_Detail10.rowcount > 0 ){						
						// 기성년월 체크						
						if(sCothYm.substr(0,6) != this.ds_Detail20.getColumn(iLoop,"CORH_YM") ){	//기성년월 비교
							var arrMsgArg = ["기성년월"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 기성년월만 적용 가능합니다.
							return;

						//계약번호 체크
						} else if(this.ds_Detail10.getColumn(0,"CNTH_NO") != this.ds_Detail20.getColumn(iLoop,"CNTH_NO") ){
							var arrMsgArg = ["계약번호"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 [계약번호]만 적용 가능합니다.
							return;
						}
					}					
					
					//2. Detail20 그리드내 체크된 대상 동일여부 비교 : 기성년월 비교
					if(!this.gfn_IsNull(sCorhYmNoChk) ){
						if(sCorhYmNoChk != this.ds_Detail20.getColumn(iLoop,"CORH_YM") ){
							var arrMsgArg = ["기성년월"];
							this.gfn_Alert("SM057", arrMsgArg);//동일한 기성년월만 적용 가능합니다.
							return;
						}
					}
					
					//3. Detail20 그리드내 체크된 대상 동일여부 비교 : 계약번호
					if(!this.gfn_IsNull(sIdntNoChk) ){
						if(sIdntNoChk != this.ds_Detail20.getColumn(iLoop,"CNTH_NO") ){
							var arrMsgArg = ["계약번호"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 [계약번호]만 적용 가능합니다.
							return;
						}
					}
					sIdntNoChk = this.ds_Detail20.getColumn(iLoop,"CNTH_NO"); //for문 비교를 위한 이전값 저장 계약번호
					sCorhYmNoChk = this.ds_Detail20.getColumn(iLoop,"CORH_YM"); //for문 비교를 위한 이전값 저장 기성년월
				}
			}
			
			
			var arrCheckRow = [];//체크된 Row
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
			
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)
					
					//첫번째일 경우 마스터 업데이트
					if(this.ds_Detail10.rowcount == 1 ){
					
						this.div_MasterInput.form.tab_Master.tpg_01.form.cal_CothYm.set_value(this.ds_Detail20.getColumn(iLoop,"CORH_YM"));	//기성년도
						this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_value(this.ds_Detail20.getColumn(iLoop,"CNTH_NO"));	//계약번호
     					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CnthNm.set_value(this.ds_Detail20.getColumn(iLoop,"CNTH_NM"));	//계약명
						
						
						//계약번호 format 변경 (9자리, 7자리)
						var strCnthNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO");
						if (strCnthNo.length == 9) 
							this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_format("####-@-####");		
						else 
							this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_format("@##-####");
					}
					
					//기성청구검색에서 기성검토상세로 이동
					this.ds_Detail10.copyRow(iAddRow, this.ds_Detail20, iLoop);
					this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
					this.ds_Detail10.setColumn(iAddRow, "COTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTH_NO"));
					this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
					this.ds_Detail10.setColumn(iAddRow, "REMK_NM","");//비교 초기화
										
					arrCheckRow.push(iLoop );
					
					//선택항목 비활성화처리
					this.ds_Detail20.setColumn(iLoop, "s_conf", "1");	//기성청구검색 컬러 변경 구분자 업데이트
					this.ds_Detail20.setColumn(iLoop, "s_chk", "0");	//Uncheck로 변경
				}
			}
			
			//마스터 기성 금액 계산
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "COTH_AM", this.ds_Detail10.getCaseSum("crudType !='D'","ADCR_AM"));//청구금액
			//trace("청구합계-->"+this.ds_Detail10.getCaseSum("crudType !='D'","ADCR_AM"));
		
			//발주상세로 Tab이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);
		}
	}
};


  
