                  ];
	this.gfn_SetCommEmpmCd(this.fvComPrgmId, arrComp);

	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	var objCondList = [
						 "SM020|ds_Sm020"	//화폐코드 / 수금화폐
						,"SD212|ds_Sd212"	//도착지 출발지
						,"MM650|ds_Mm650"	//선적방법
						,"MM655|ds_Mm655"	//선적단위(FCL)
					  ];
	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, "");

	//[업무] 일반콤보조회
	this.fn_ComboSearch();
}

