{
	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {
			
			var arrCheckRow = [];//체크된 Row
			
			var sCnthNoChk; //화폐
			var sCusmCdChk; //거래처
			
			var sAddnAm;
			//동일화폐 추가 체크
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
				
					//선적상세 탭 화폐, 거래처 체크
					if(this.ds_Detail10.rowcount > 0 ){
						if(this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CurrCd.value != this.ds_Detail20.getColumn(iLoop,"CURR_CD") &&
						   this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.value != this.ds_Detail20.getColumn(iLoop,"CUSM_CD")){
							var arrMsgArg = ["거래처, 화폐"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 [ITM0]만 적용 가능합니다.
							return;
						}
					}
					
					//그리드내 화폐, 거래처 체크
					if(!this.gfn_IsNull(sCnthNoChk) && !this.gfn_IsNull(sCusmCdChk) ){
						if(sCnthNoChk != this.ds_Detail20.getColumn(iLoop,"CURR_CD") || sCusmCdChk != this.ds_Detail20.getColumn(iLoop,"CUSM_CD") ){
							var arrMsgArg = ["거래처, 화폐"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 [&ITM0]만 적용 가능합니다.
							return;
						}
					}
					
					sCnthNoChk = this.ds_Detail20.getColumn(iLoop,"CURR_CD");
					sCusmCdChk = this.ds_Detail20.getColumn(iLoop,"CUSM_CD");
					
					
					//선적수량 체크 
					sBolbQt =this.ds_Detail20.getColumn(iLoop, "BOLB_QT");
					sBolbQtSum =this.ds_Detail20.getColumn(iLoop, "BOLB_QT_SUM");
					if(sBolbQt == 0){
						var arrMsgArg = ["선적수량","1"];
						this.gfn_Alert("SM055", arrMsgArg); //[선적수량]을(를) 1 이상 입력해 주세요.
						return;
					}
					
					if((sBolbQt+sBolbQtSum) > this.ds_Detail20.getColumn(iLoop, "PCOB_QT") ){
						var arrMsgArg = ["기선적수량, 선적수량 합계", "발주수량"];
						this.gfn_Alert("SM056", arrMsgArg); //[기선적수량, 선적수량 합계]이(가) [발주수량]을(를) 초과할 수 없습니다.
						return;
					}
					
				}
			}
			
			
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
			
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)
					
					this.ds_Detail10.copyRow(iAddRow, this.ds_Detail20, iLoop);
					this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
					this.ds_Detail10.setColumn(iAddRow, "BOLH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BOLH_NO"));	//선적관리번호
					this.ds_Detail10.setColumn(iAddRow, "BOLB_SE", "");//선적순번
					
					arrCheckRow.push(iLoop );
					
					//선택항목 비활성화처리
					this.ds_Detail20.setColumn(iLoop, "s_conf", "1");
					this.ds_Detail20.setColumn(iLoop, "s_chk", "0");//Uncheck로 변경
				}
			}
			
			//마스터 업데이트 
			var ds10CurrCd = this.ds_Detail10.getColumn(0,"CURR_CD");
			var ds10BolbQtSum = this.ds_Detail10.getCaseSum("crudType!='D'","BOLB_AM"); 
			var ds10CusmCd = this.ds_Detail10.getColumn(0,"CUSM_CD");
			var ds10CusmNm = this.ds_Detail10.getColumn(0,"CUSM_NM");
			//alert(this.ds_Detail10.getCaseSum("crudType!='D'","BOLB_AM"));
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CURR_CD", ds10CurrCd);	//화폐코드
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BOLH_AM", ds10BolbQtSum);	//선적금액
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CUSM_CD", ds10CusmCd); //거래처코드
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CUSM_NM", ds10CusmNm); //거래처명
			
			
			this.ds_Detail20.set_rowposition(-1);
			//선적상세로 Tab이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);
		}
	}
};

