/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}

	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		//첨부파일정보만 변경시
		if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && this.ds_Master10.getRowCount() > 0) {
			//첨부파일정보 저장
			fvFileInfo = new Object();//[공통] 첨부파일정보
			fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, B-상세(그리드Row), E-기타
			fvFileInfo.CALLBACK_FUNC = "";//호출할 콜백함수
			fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
			fvFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCWH_NO");//PK번호2-입고번호
			//첨부파일Div 화면에 Key세팅하게 변경
			this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);
			return;	
		}
	}

	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		if (!this.fn_VaildMasterSave()) return;
	}
	
	if (!this.fn_Magam()) {return;}
	
	//Detail 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		if (!this.fn_VaildDetailSave()) return;
	}	
	
	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);

	//Master 입력 상태이면
	if (sChkRowType == "I") {
	
		//채번
		var sNewKeyNo = this.gfn_GetTbnmNo(this.fvComPrgmId, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCWH_DD"));//프로그램ID, 기준일자
		if (this.gfn_IsNull(sNewKeyNo)) {
			//채번번호를 가져오는데 실패했습니다.
			return;
		}
		
		var sChkKeyNo = "";//채번번호-체크용
		var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position
		
		//Master 데이터셋에 채번정보 입력-출고번호
		this.ds_Master10.setColumn(iMasterRowPos, "PCWH_NO", sNewKeyNo);
		
		var sCommCd = this.ds_Master10.getColumn(iMasterRowPos, "COMM_CD");//회사코드
		var sPcwhNo = this.ds_Master10.getColumn(iMasterRowPos, "PCWH_NO");//입고번호
		
		//Detail 데이터셋에 기본정보 입력-회사코드/입고번호
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			sChkKeyNo = this.ds_Detail10.getColumn(iLoop, "PCWH_NO");//입고번호
			
			if (this.gfn_IsNull(sChkKeyNo)) {
				this.ds_Detail10.setColumn(iLoop, "COMM_CD", sCommCd); //회사코드
				this.ds_Detail10.setColumn(iLoop, "PCWH_NO", sPcwhNo); //입고번호
				this.ds_Detail10.setColumn(iLoop, "SAUM_CD", this.ds_Master10.getColumn(iMasterRowPos, "SAUM_CD")); //사업장코드
				this.ds_Detail10.setColumn(iLoop, "WARM_CD", this.ds_Master10.getColumn(iMasterRowPos, "WARM_CD")); //창고코드
				this.ds_Detail10.setColumn(iLoop, "RADM_CD", this.ds_Master10.getColumn(iMasterRowPos, "RADM_CD")); //수불코드
				this.ds_Detail10.setColumn(iLoop, "RADD_CD", this.ds_Master10.getColumn(iMasterRowPos, "RADD_CD")); //수불사유코드
				
			}
		}		
	}	
	
	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		fvFileInfo = new Object();//[공통] 첨부파일정보
		fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
		fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
		fvFileInfo.CALLBACK_FUNC = "fn_ComBtnSearch";//호출할 콜백함수
		fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
		fvFileInfo.PK01_NO = nApp.gvCompany;//PK번호1-회사코드
		fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCWH_NO");//PK번호2-입고번호
	}		
	
	//저장시 재조회 Key설정
	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCWH_NO");//조회Key01

	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	//[공통]서비스 호출 변수
	var sInputDaset = "" //입력 데이터셋
	var sOutDatset = "";//출력 데이터셋
	var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		
	//Master 변경건이 있으면 Master 저장
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
		svcAddObj.sInsertSql = "mm:M_MM_4100_M_IN10";//저장Sql
		svcAddObj.sUpdateSql = "mm:M_MM_4100_M_UP10";//수정Sql
		svcAddObj.sDeleteSql = "mm:M_MM_4100_M_DL10";//삭제Sql
		svcAddObj.sSaveFlagColumn = "";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

		this.gfn_AddComSaveService(this, svcAddObj);
		
		sInputDaset = "ds_Master10=ds_Master10:U" //입력 데이터셋
	}
	
	//추가대상 기본정보 값 입력
	var sCommCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//회사번호
	var sPcwhNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCWH_NO");//입고번호
	var sPcwhDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCWH_DD");//입고일자			
	var sSaumCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SAUM_CD");//사업장코드
	var sWarmCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WARM_CD");//창고코드
	var sRadmCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RADM_CD");//수불코드			
	var sRaddCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RADD_CD");//수불사유코드		
	
	
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		this.ds_Detail10.setColumn(iLoop, "COMM_CD", sCommCd); //회사코드
		this.ds_Detail10.setColumn(iLoop, "PCWH_NO", sPcwhNo); //입고번호
		this.ds_Detail10.setColumn(iLoop, "PCWH_DD", sPcwhDd); //입고일자
		this.ds_Detail10.setColumn(iLoop, "SAUM_CD", sSaumCd); //사업장코드
		this.ds_Detail10.setColumn(iLoop, "WARM_CD", sWarmCd); //창고코드
		this.ds_Detail10.setColumn(iLoop, "RADM_CD", sRadmCd); //수불코드
		this.ds_Detail10.setColumn(iLoop, "RADD_CD", sRaddCd); //수불사유코드
	}			

	//Detail 변경건이 있으면 Detail 저장
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		if (this.fvDeliveryInfoYn) {	//납품번호(출하번호) 조회버튼 클릭시

			//추가대상 기본정보 값 입력
			 sCommCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//회사번호
			 sPcwhNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCWH_NO");//입고번호
			 sPcwhDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCWH_DD");//입고일자			
			 sSaumCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SAUM_CD");//사업장코드
			 sWarmCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WARM_CD");//창고코드
			 sRadmCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RADM_CD");//수불코드			
			 sRaddCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RADD_CD");//수불사유코드			
			
			for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
				this.ds_Detail10.setColumn(iLoop, "COMM_CD", sCommCd); //회사코드
				this.ds_Detail10.setColumn(iLoop, "PCWH_NO", sPcwhNo); //입고번호
