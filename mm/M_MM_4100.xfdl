this.fn_ComBtnAdd = function()
{
	this.ds_Master10.addColumn("s_chk", "string");
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10) || this.gfn_DsIsUpdated(this.ds_Detail10) || this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	
	var iAddRow = this.ds_Master10.addRow();

	this.fvMmCloseDay = sToDay;//[월마감] 마감일자

	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//출고부서코드	
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//출고부서명	
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//출고자사번	
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//출고자명		
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//출고자ID	
	this.ds_Master10.setColumn(iAddRow, "PCWH_DD", sToDay);//입고일자-오늘날자로 설정
	this.ds_Master10.setColumn(iAddRow, "PCWH_GU", '0020');//입고구분 - 일반구매(0020)
	this.ds_Master10.setColumn(iAddRow, "RADM_CD", 'MIPN');//수불코드 - 구매입고(MIPN)
	/* Default 설정추가(2022-03-11) */
	this.ds_Master10.setColumn(iAddRow, "SAUM_CD", '0400');//수업장 - 음성공장(0400)
	this.ds_Master10.setColumn(iAddRow, "WARM_CD", '5010');//창고 - 자재창고(5010 - 0010)
	this.ds_Master10.setColumn(iAddRow, "RADD_CD", '0010');//수불사유 - 일반(0010 - MIPN)

	//구이입고상세 Tab이동 (2022-03-30 미입고검색 Tab이동에서 변경)
	this.div_DetailInput.form.tab_Detail.set_tabindex(0);

	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.div_DetailInput.form.tab_Detail.set_enable(true);
	this.div_DetailInput.form.edt_MophNo.set_enable(true);	//2022-03-14 납품번호조회 추가개발
	this.div_DetailInput.form.btn_Search.set_enable(true);	//2022-03-14 납품번호조회 추가개발
	this.div_DetailInput.form.edt_MophNo.set_value(""); 	//2022-03-14 납품번호조회 추가개발(초기화)
	this.div_DetailInput.form.div_ComGrdBtn.set_visible(true); 
	this.div_DetailInput.form.sta_MophNo.set_visible(true);	//2022-03-14 납품번호조회 추가개발
	this.div_DetailInput.form.edt_MophNo.set_visible(true);	//2022-03-14 납품번호조회 추가개발
	this.div_DetailInput.form.btn_Search.set_visible(true);	//2022-03-14 납품번호조회 추가개발
	this.div_DetailInput.form.edt_MophNo.set_value(""); 	//2022-03-14 납품번호조회 추가개발(초기화)

	//[마감관리] 입력 컴포넌트 Enable 설정
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.set_readonly(false);
	this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.set_readonly(false);
	this.fvFileDiv.fn_SetCopmGridAuth(true);
	this.fvMmCloseYn = false;//월마감여부-Tab버튼 제어용
	this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PcwhDd.set_enable(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PcwhNm.set_readonly(false);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SaumCd.set_enable(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WarmCd.set_enable(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_RaddCd.set_enable(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(false);	

	//입고구분 = 외주계약 조회결과에서 입력 버튼 클릭시
	this.gfn_SetCompGroupEnable(this.div_MasterInput.form, true);//Master 입력 프리폼
	this.gfn_SetCompGroupEnable(this.div_DetailInput.form, true);//Detail 입력			
	
	//입력시 비활성화 대상 설정
	this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PcwhNo.set_readonly(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_readonly(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmNm.set_readonly(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_readonly(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm.set_readonly(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_readonly(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmNm.set_readonly(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_PcwhGu.set_enable(false);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_DoimGu.set_enable(false);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_RadmCd.set_enable(false);
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CnthNm.set_readonly(true);

	//처음 입력 항목에 포커스 - 납품번호 검색
	this.div_DetailInput.form.edt_MophNo.setFocus();

	//Master10 거래처 활성화
	this.ds_Detail10.clearData();//상세정보 초기화
	this.ds_Detail20.clearData();//검색정보 초기화
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화

	//미출고검색 초기화
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmCd.set_value(""); //거래처코드
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmNm.set_value(""); //거래처명
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.mae_CnthNo.set_value(""); //계약번호
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CnthNm.set_value(""); //계약명
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_PcohNo.set_value(""); //발주번호

	this.fvDeliveryInfoYn = false;	//납품번호(출하번호) 생성여부 조회
	this.fvDetailSearchYn = false;	//구매입고상세 조회여부

	//첨부파일정보 권한설정
	var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
	oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
	this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
	
	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
}

