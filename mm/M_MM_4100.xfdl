  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{

	//체크시 해당 프로그램 전체선택
	if (e.columnid == "s_chk") {	
		if (this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "s_chk") == '1') {		
			var objSearchList = ["PCOH_NO|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "PCOH_NO") //발주번호
								,"PCOH_SQ|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "PCOH_SQ") //발주차수
								,"PCOB_SE|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "PCOB_SE") //발주순번
								,"PCOC_SE|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "PCOC_SE") //납품순번
								,"CNTH_NO|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "CNTH_NO") //계약번호
								,"CNTB_SE|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "CNTB_SE") //계약순번
								,"WORB_NO|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "WORB_NO") //W/O번호
								,"ITEM_CD|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "ITEM_CD") //품목번호
								];		
			//[공통] Detail10 중복건 체크후 해당건 Skip
			if (this.gfn_DsSchDupcheck(this.ds_Detail10, objSearchList)) {
				this.gfn_Alert("MM001"); //해당 입고내용은 이미 존재합니다.\n재입고시 기존 입고된 내용을 삭제하고 미입고 검색을 재조회 후 진행해 주시기 바랍니다.
				this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "s_chk", '0');// Check 해제
				return;				
			}
			
		}
	} else if (e.columnid == "PCWB_QT") {	
		//입고수량이 미입고수량 개수보다 큰 경우
		if (this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "PCWB_QT") > this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "AFTR_QT")) {
				var arrMsgArg = ["입고수량", "미입고수량"];
				this.gfn_Alert("SM056", arrMsgArg); //입고수량이 미입고수량을 초과할 수 없습니다.

				//입고수량 원상복귀
				this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "PCWB_QT", this.ds_Detail20.getOrgColumn(this.ds_Detail20.rowposition, "PCWB_QT"));
				
				return;
			
		} else {
			var objSearchList = ["PCOH_NO|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "PCOH_NO") //발주번호
								,"PCOH_SQ|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "PCOH_SQ") //발주차수
								,"PCOB_SE|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "PCOB_SE") //발주순번
								,"PCOC_SE|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "PCOC_SE") //납품순번
								,"CNTH_NO|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "CNTH_NO") //계약번호
								,"CNTB_SE|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "CNTB_SE") //계약순번
								,"WORB_NO|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "WORB_NO") //W/O번호
								,"ITEM_CD|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "ITEM_CD") //품목번호
								];		
			//[공통] Detail10 중복건 체크후 해당건 Skip
			if (this.gfn_DsSchDupcheck(this.ds_Detail10, objSearchList)) {
				this.gfn_Alert("MM001"); //해당 입고내용은 이미 존재합니다.\n재입고시 기존 입고된 내용을 삭제하고 미입고 검색을 재조회 후 진행해 주시기 바랍니다.
				
				//입고수량 원상복귀
				this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "PCWB_QT", this.ds_Detail20.getOrgColumn(this.ds_Detail20.rowposition, "PCWB_QT"));
				this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "s_chk", '0');// Check 해제
				
				return;
			}			
			
			this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "s_chk", '1');// Check 
		}
	}
	
};


/***********************************************************************************************
 * 02.DG0.08. [Detail Tab] 변경 이벤트
 ***********************************************************************************************/
this.div_DetailInput_tab_Detail_onchanged = function(obj:nexacro.Tab,e:nexacro.TabIndexChangeEventInfo)
{
	var iTabIndex = e.postindex;
	var sPcwhGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCWH_GU");	//입고구분
	
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);

	//Tab-입고상세
	if (iTabIndex == 0) {
		this.fn_SetGridInputButton();//[공통] 그리드 입력 버튼 설정
		if (sPcwhGu != "0010" && !this.fvMmCloseYn) {	//입고구분이 외주구매(0010)이 아니고 월마감이 아닌경우
			this.div_DetailInput.form.sta_MophNo.set_visible(true);	//2022-03-14 납품번호조회 추가개발
			this.div_DetailInput.form.edt_MophNo.set_visible(true);	//2022-03-14 납품번호조회 추가개발
			this.div_DetailInput.form.btn_Search.set_visible(true);	//2022-03-14 납품번호조회 추가개발
			this.div_DetailInput.form.edt_MophNo.setFocus();
		}
	//Tab-미입고검색
	} else if (iTabIndex == 1) {
		this.fn_SetGridChoiceButton();//[공통] 검색 Tab 버튼 설정
		this.div_DetailInput.form.sta_MophNo.set_visible(false);	//2022-03-14 납품번호조회 추가개발
		this.div_DetailInput.form.edt_MophNo.set_visible(false);	//2022-03-14 납품번호조회 추가개발
		this.div_DetailInput.form.btn_Search.set_visible(false);	//2022-03-14 납품번호조회 추가개발
	//Tab-첨부파일
	} else if (iTabIndex == 2) {	
		this.fn_SetGridFileButton();//[공통] 그리드 파일 버튼 설정
		this.div_DetailInput.form.sta_MophNo.set_visible(false);	//2022-03-14 납품번호조회 추가개발
		this.div_DetailInput.form.edt_MophNo.set_visible(false);	//2022-03-14 납품번호조회 추가개발
		this.div_DetailInput.form.btn_Search.set_visible(false);	//2022-03-14 납품번호조회 추가개발
	}
	
	//월마감 인경우 - 버튼 비활성화 
	if (this.fvMmCloseYn) {
		this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);
	}	
};

