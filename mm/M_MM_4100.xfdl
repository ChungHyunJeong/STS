/***********************************************************************************************
 * 타이머 이벤트[멀티인쇄]
 ***********************************************************************************************/  
this.M_MM_4100_ontimer = function(obj:nexacro.Form,e:nexacro.TimerEventInfo)
{
	//멀티인쇄관련
	if (e.timerid == 900) {			
		
		if (this.fvArrPcwhNo.length < 1) {
			//멀티인쇄건 조회중이 아닌경우
			if (!this.fvMultiSearchYn) {
				this.killTimer(900);
				this.div_DetailInput.form.pgb_PrintState.set_pos(this.fvPrintCnt);
				this.div_DetailInput.form.pgb_PrintState.stepIt();
				this.div_DetailInput.form.pgb_PrintState.set_visible(false);
				
				if (this.ds_Master10MultiRt.getRowCount() < 1) {
					this.gfn_Alert("SM083");//출력할 정보가 없습니다.
					return;
				} else {
					var oArg = {ProjectFolder:"MM"        //레포트저장 폴더
							  , ReportFile:"" //레포트파일명
							  , ReportDs:""            //레포트파라메타 - 데이터셋
							  , ReportParam:""         //레포트파라메타 - 변수
							  , MultiPrintYn:"Y"       //멀티프린트여부
							  , MultiParam:this.fvArrReportParam //멀티건 파라메터
							   };                      //Popup화면 전달 파라메터
							   
							
					this.gfn_ReportPop("REPORT", oArg, "fn_PopupCallback");//레포트 팝업 호출		
				}
			}
		} else {
			//멀티인쇄건 조회중이 아닌경우
			if (!this.fvMultiSearchYn) {
				var sPcwhNo = this.fvArrPcwhNo.shift();//배열에서 처음내용 꺼냄
				this.fvMultiSearchYn = true;//멀티인쇄조회중여부
				this.div_DetailInput.form.pgb_PrintState.set_pos((this.fvPrintCnt - this.fvArrPcwhNo.length));
				this.div_DetailInput.form.pgb_PrintState.stepIt();
				
				// 조회 
				this.fn_ComPcwhSearch(sPcwhNo);				
			}
		}
	}	
};

