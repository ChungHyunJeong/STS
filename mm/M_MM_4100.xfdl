	if (this.fvDeliveryInfoYn) {
		this.ds_Detail10.copyData(this.ds_Detail10Reset);	//기존 데이터로 원복
	} else {
		this.ds_Detail10.reset();
					
		for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
