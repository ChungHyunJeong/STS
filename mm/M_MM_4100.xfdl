/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {
		//멀티선택여부
		if (this.fvChkMultiWfYn) {
			this.fvChkMultiWfYn = false;
			return;
		}
		

		//입고번호정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCWH_NO"))) {
			//[월마감제어-버튼/상세그리드]
			var sBUDG_MM = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCWH_DD"), "");//마감년월
			
			//마감여부 체크
			var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
			var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
			paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
			paramObj.MODM_CD = "MM";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
			paramObj.ACUM_CD = "";//[선택] 회계단위
			paramObj.BUSM_CD  = "";//[선택] 사업부문
			paramObj.SAUM_CD  = "";//[선택] 사업장코드
			paramObj.DEPM_CD  = "";//[선택] 부서코드
			paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
			paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
			
			var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		
			//월마감된 경우
			if (bCheck) {
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.set_readonly(true);
				this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.set_readonly(true);
				this.fvFileDiv.fn_SetCopmGridAuth(false);			
				this.fvMmCloseYn = true;//월마감여부-Tab버튼 제어용
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PcwhDd.set_enable(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PcwhNm.set_readonly(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SaumCd.set_enable(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WarmCd.set_enable(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_RaddCd.set_enable(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(true);	

				this.div_DetailInput.form.div_ComGrdBtn.set_visible(false); //행추가 버튼
				this.div_DetailInput.form.sta_MophNo.set_visible(false);	//2022-03-14 납품번호조회 추가개발
				this.div_DetailInput.form.edt_MophNo.set_visible(false);	//2022-03-14 납품번호조회 추가개발
				this.div_DetailInput.form.btn_Search.set_visible(false);	//2022-03-14 납품번호조회 추가개발
				this.div_DetailInput.form.edt_MophNo.set_value(""); 	//2022-03-14 납품번호조회 추가개발(초기화)

			} else {
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.set_readonly(false);
				this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.set_readonly(false);
				this.fvFileDiv.fn_SetCopmGridAuth(true);
				this.fvMmCloseYn = false;//월마감여부-Tab버튼 제어용
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PcwhDd.set_enable(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PcwhNm.set_readonly(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SaumCd.set_enable(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WarmCd.set_enable(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_RaddCd.set_enable(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(false);	
			}
			
			//입고구분이 외주구매(0010)이면 - 입고내역 비활성화, 입고상세 비활성화		
			var sPcwhGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCWH_GU");
			if (sPcwhGu == '0010') {
				//입력영역 비활성화
				this.div_DetailInput.form.div_ComGrdBtn.set_visible(false); //행추가 버튼
				this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
				this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 입력			
				this.div_DetailInput.form.sta_MophNo.set_visible(false);	//2022-03-14 납품번호조회 추가개발
