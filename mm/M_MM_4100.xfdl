				this.ds_Detail10.setColumn(iLoop, "s_chk", "");
				this.ds_Detail10.setColumn(iLoop, "crudType", "");
			}
		}
		
	}
	
	this.ds_Detail20.reset();

	for (var iLoop=0;iLoop<this.ds_Detail20.getRowCount();iLoop++) {
		if (!this.gfn_IsNull(this.ds_Detail20.getColumn(iLoop, "s_conf"))) {
			this.ds_Detail20.setColumn(iLoop, "s_chk", "");
			this.ds_Detail20.setColumn(iLoop, "s_conf", "");
		}
	}

	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
}

