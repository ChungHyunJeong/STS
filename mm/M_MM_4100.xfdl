/***********************************************************************************************
 * 02.DG1.05. [Detail그리드2-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Choice = function()
{
	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {
		
			//1. 거래처 중복 체크
			var sIdntNoChk=""; //Key(거래처) 체크			
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					
					//1. Detail20 Tab과 Master Input 화면 항목 비교 : 거래처코드
					if(this.ds_Detail10.rowcount > 0 ){
						if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD") != this.ds_Detail20.getColumn(iLoop,"CUSM_CD") ){
							var arrMsgArg = ["거래처"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 거래처만 적용 가능합니다.
							return false;
						}
					}
					
					//2. Detail20 그리드내 체크된 대상 동일여부 비교 : 거래처코드
					if(!this.gfn_IsNull(sIdntNoChk) ){
						if(sIdntNoChk != this.ds_Detail20.getColumn(iLoop,"CUSM_CD") ){
							var arrMsgArg = ["거래처"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 거래처만 적용 가능합니다.
							return false;
						}
					}
					sIdntNoChk = this.ds_Detail20.getColumn(iLoop,"CUSM_CD"); //for문 비교를 위한 이전값 저장
					
					//3. 수량 체크 : 제품수량
					if(this.ds_Detail20.getColumn(iLoop, "PCWB_QT") == 0){
						var arrMsgArg = ["입고수량"];
						this.gfn_Alert("SM055", arrMsgArg); //입고수량을 1이상 입력하주세요 
						return false;
					}
				}
			}
			
			//2. Detail20 --> Detail10 복사
			var arrCheckRow = [];//체크된 Row
			
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
			
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					//Detail10에 적용되어 있는지 체크
					var objSearchList = ["PCOH_NO|" + this.ds_Detail20.getColumn(iLoop, "PCOH_NO") //발주번호
										,"PCOH_SQ|" + this.ds_Detail20.getColumn(iLoop, "PCOH_SQ") //발주차수
										,"PCOB_SE|" + this.ds_Detail20.getColumn(iLoop, "PCOB_SE") //발주순번
										,"PCOC_SE|" + this.ds_Detail20.getColumn(iLoop, "PCOC_SE") //납품순번
										,"CNTH_NO|" + this.ds_Detail20.getColumn(iLoop, "CNTH_NO") //계약번호
										,"CNTB_SE|" + this.ds_Detail20.getColumn(iLoop, "CNTB_SE") //계약순번
										,"WORB_NO|" + this.ds_Detail20.getColumn(iLoop, "WORB_NO") //W/O번호
										,"ITEM_CD|" + this.ds_Detail20.getColumn(iLoop, "ITEM_CD") //품목번호
										];		
										
					//[공통] Detail10 중복건 체크후 해당건 Skip
					if (!this.gfn_DsSchDupcheck(this.ds_Detail10, objSearchList)) {
						//Master10 입고번호가 없고(최초), Detail10 첫번째 컬럼일 경우
						if (this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCOH_NO")) && this.ds_Detail10.rowcount == 0 ) { 
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"PCWH_NM",this.ds_Detail20.getColumn(iLoop,"PCOH_NM"));	//발주명 -> 입고명
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"CUSM_CD",this.ds_Detail20.getColumn(iLoop,"CUSM_CD"));	//거래처
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"CUSM_NM",this.ds_Detail20.getColumn(iLoop,"CUSM_NM"));	//거래처명
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"SAUM_NO",this.ds_Detail20.getColumn(iLoop,"SAUM_NO"));	//사업자구분
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"DOIM_GU",this.ds_Detail20.getColumn(iLoop,"DOIM_GU"));	//내외자구분
							
						}
						
						var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)

						this.ds_Detail10.copyRow(iAddRow, this.ds_Detail20, iLoop);
						
						var iWinePr = nexacro.toNumber(this.ds_Detail20.getColumn(iLoop, "WONE_PR")); //원화단가
						var iForePr = nexacro.toNumber(this.ds_Detail20.getColumn(iLoop, "FORE_PR")); //외화단가
						var iPcwbQt = nexacro.toNumber(this.ds_Detail20.getColumn(iLoop, "PCWB_QT")); //입고수량


trace("1 @@@@@@@@@@ "+iPcwbQt);
						//원화금액 계산공식(7/26 설계자확인) - 원화단가 * 입고수량  (=물품대)
						var iWoneAm = iWinePr * iPcwbQt; //물품대 (구매발주등록BODY.원화단가 * 입고수량)
						
						//외화금액 계산공식(7/26 설계자확인) - 외화단가 * 입고수량
						var iForeAm = iForePr * iPcwbQt; //물품대 (구매발주등록BODY.원화단가 * 입고수량)

						//물품대 계산공식(6/24 설계자확인) - 구매발주등록BODY.원화단가 * 입고수량 (=원화금액)
						var iPcwbAm = iWinePr * iPcwbQt; //물품대 (구매발주등록BODY.원화단가 * 입고수량)

						//미입고수량 재계산 (미입고수량 = 발주수량 - 기입고수량 - 입고수량)
						var iPcobQt = nexacro.toNumber(this.ds_Detail20.getColumn(iLoop, "PCOB_QT")); //발주수량
						var iPrevQt = nexacro.toNumber(this.ds_Detail20.getColumn(iLoop, "PREV_QT")); //기입고수량
						var iAftrQt = iPcobQt - iPrevQt - iPcwbQt; //미입고수량 = 발주수량 - 기입고수량 - 입고수량

						this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD")); //회사코드
						this.ds_Detail10.setColumn(iAddRow, "PCWH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCWH_NO")); //입고번호
						this.ds_Detail10.setColumn(iAddRow, "PCWB_SE", "");//입고순번
						this.ds_Detail10.setColumn(iAddRow, "AFTR_QT", nexacro.floor(iAftrQt,2)); //미입고수량 재계산 (미입고수량 = 발주수량 - 기입고수량 - 입고수량)
						this.ds_Detail10.setColumn(iAddRow, "PCWH_DD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCWH_DD")); //입고일자
						this.ds_Detail10.setColumn(iAddRow, "INPU_GU", '0'); //입력구분 (수기 - 0, 바코드 - 1, RFD - 2)
						this.ds_Detail10.setColumn(iAddRow, "WONE_AM", iWoneAm); //원화금액 계산 (구매발주등록BODY.원화단가 * 입고수량)
						this.ds_Detail10.setColumn(iAddRow, "FORE_AM", iForeAm); //외화금액 계산 (구매발주등록BODY.외화단가 * 입고수량)
						this.ds_Detail10.setColumn(iAddRow, "PCWB_AM", iPcwbAm); //물품대 계산 (구매발주등록BODY.원화단가 * 입고수량) # 원화금액와 동일 #
						this.ds_Detail10.setColumn(iAddRow, "SAUM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SAUM_CD")); //사업장코드
						this.ds_Detail10.setColumn(iAddRow, "WARM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WARM_CD")); //창고코드
						this.ds_Detail10.setColumn(iAddRow, "RADM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RADM_CD")); //수불코드
						this.ds_Detail10.setColumn(iAddRow, "RADD_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RADD_CD")); //수불사유코드
						
						this.ds_Detail10.setColumn(iAddRow, "BUYB_QT", 0);  
						
						arrCheckRow.push(iLoop );
						
						//선택항목 비활성화처리
						this.ds_Detail20.setColumn(iLoop, "s_conf", "1");
						this.ds_Detail20.setColumn(iLoop, "s_chk", "0");//Uncheck로 변경
					}
				}
			}

			this.ds_Detail20.set_rowposition(-1);
			
			//입고상세로 Tab이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);
		}
	}
};

/***********************************************************************************************
 * 02.DG1.06. [Detail그리드2-버튼] 취소 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Cancel = function()
{
	this.ds_Detail10.reset();
	this.ds_Detail20.reset();

	//기존 체크정보 초기화
	for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
	
		if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
			this.ds_Detail20.setColumn(iLoop, "s_chk", "0");//Uncheck로 변경
		}
		
		if (this.ds_Detail20.getColumn(iLoop, "s_conf") == "1") {
			this.ds_Detail20.setColumn(iLoop, "s_conf", "0");
		}		
	}

	//입고번호가 없고 입고상세 내용이 미존재시 Master Clear
	if (this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition,"PCOH_NO")) && this.ds_Detail10.rowcount == 0) {
		this.fn_ClearMasterValue();
	}
