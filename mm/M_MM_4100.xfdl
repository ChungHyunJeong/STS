 ***********************************************************************************************/  
this.fn_ComBtnPrint = function()
{ 
	var sMultiPcwhNo = "";//입고번호-다건
	var sPcwhNo = "";     //입고번호
	
	//체크된 내역이 있으면
	if (this.ds_Master10.getCaseCount("s_chk=='1'") > 0) {

		//일괄인쇄 건수 제한      
		if (this.ds_Master10.getCaseCount("s_chk=='1'") > 100 ) {
			this.gfn_Alert("SM121");//일괄인쇄는 최대 100건까지 가능합니다.
			return;
		}
	  
		this.div_DetailInput.form.pgb_PrintState.set_min(0);
		
		
		for (var iLoop=0; iLoop<this.ds_Master10.getRowCount(); iLoop++) {

			//결재진행 상태가 승인일경우에만 전표보기 화면으로 이동
			if (this.ds_Master10.getColumn(iLoop, "s_chk") == "1") {
				sPcwhNo = this.ds_Master10.getColumn(iLoop, "PCWH_NO");//입고번호
				if (this.gfn_IsNull(sMultiPcwhNo)) {
					sMultiPcwhNo = sPcwhNo;
				} else {
					sMultiPcwhNo = sMultiPcwhNo + ";" + sPcwhNo;
				}
			}
		}

		this.fvMultPrintYn = true;//멀티인쇄여부
		this.fvArrPcwhNo = new Array();//입고번호 Array
		this.fvArrReportParam = new Array();//레포트파라메타 Array
		
		if (sMultiPcwhNo.indexOf(";") > 0) {
			var arrPcwhNo = sMultiPcwhNo.split(";");
			
			for (var iLoop=0; iLoop <arrPcwhNo.length; iLoop++) {
				this.fvArrPcwhNo.push(arrPcwhNo[iLoop]);//입고번호 Array
			}
		} else {
			this.fvArrPcwhNo.push(sMultiPcwhNo);//입고번호 Array
		}
		
		if (this.fvArrPcwhNo.length > 0) {
		
			this.div_DetailInput.form.pgb_PrintState.set_visible(true);
			this.div_DetailInput.form.pgb_PrintState.set_pos(0);			
			this.div_DetailInput.form.pgb_PrintState.set_min(0);
			this.fvPrintCnt = this.fvArrPcwhNo.length;//출력건수
			this.div_DetailInput.form.pgb_PrintState.set_max(this.fvPrintCnt);

			var sPcwhNo = this.fvArrPcwhNo.shift();//배열에서 처음내용 꺼냄
			this.fvMultiSearchYn = true;//멀티인쇄조회중여부
			// 조회 
			this.fn_ComPcwhSearch(sPcwhNo);	
		
			this.setTimer(900, 200);
		}
	}
}

