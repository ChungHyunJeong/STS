 ***********************************************************************************************/
this.fn_DetailGrd01DeleteRow = function()
{
	var s_chk = "";//체크
	//견적미의뢰검색 내역 체크 가능하게 변경
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		s_chk = this.ds_Detail10.getColumn(iLoop, "s_chk");
		
		//체크
		if (s_chk == "1") {
		
			//검색값설정 - 컬럼명:검색값
			var objSearchList = ["PCOH_NO|" + this.ds_Detail10.getColumn(iLoop, "PCOH_NO") //발주번호
								,"PCOH_SQ|" + this.ds_Detail10.getColumn(iLoop, "PCOH_SQ") //발주차수
								,"PCOB_SE|" + this.ds_Detail10.getColumn(iLoop, "PCOB_SE") //발주순번
								,"PCOC_SE|" + this.ds_Detail10.getColumn(iLoop, "PCOC_SE") //납품순번
								,"CNTH_NO|" + this.ds_Detail10.getColumn(iLoop, "CNTH_NO") //계약번호
								,"CNTB_SE|" + this.ds_Detail10.getColumn(iLoop, "CNTB_SE") //계약순번
								,"WORB_NO|" + this.ds_Detail10.getColumn(iLoop, "WORB_NO") //W/O번호
								,"ITEM_CD|" + this.ds_Detail10.getColumn(iLoop, "ITEM_CD") //품목번호
								];		

			//[공통] 행삭제시 검색Tab 선택Row 해제 처리
			this.gfn_DeleteRowSchTabUncheck(this.ds_Detail20, objSearchList);
						
		}
	}

	this.gfn_GridDelProcess(this, this.ds_Detail10);//crud_flag 변경-삭제(D)	

	//입고번호가 없고 입고상세 내용이 미존재시 Master Clear
	if (this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition,"PCOH_NO")) && this.ds_Detail10.rowcount == 0) {
		this.fn_ClearMasterValue();
	}
};

