				return false;
				
				break;			
				
			//제출금액04
			} else if (this.gfn_IsNull(iQu04Am) || iQu04Am < 0) {		
				//탭, 위치 이동
				this.div_DetailInput.form.tab_Detail.set_tabindex(0);			
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(20,iLoop);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();
				
				var iRow = nexacro.toNumber(this.ds_Detail10.rowposition) + 1;
				var sHeadMsg = this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getCellProperty("head", 29, "text");
