/***********************************************************************************************
 * 05.03. [Vaildation] Detail 저장 필수값 체크 - 그리드항목
 ***********************************************************************************************/ 
this.fn_VaildDetailSave = function()
{
	var sQuthGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_GU");	//의뢰구분

	//[저장] Validation 설정
	var arrArgs = ["CNTH_NO^NULL"
	              ,"CNTB_SE^NULL"
	              ,"QUSB_PR^NULL|MIN=1"
			      ]; 
				  
	//가공품 - 4개 컬럼 필수체크 추가(소재비, 가공비, 후처리, 이윤)
	if (sQuthGu == '0030') {
		arrArgs.push("QU01_AM^NULL", "QU02_AM^NULL", "QU03_AM^NULL", "QU04_AM^NULL");
	
	//외주 - - 6개 컬럼 필수체크 추가(설계비, 제작_인건비, 제작_재료비, 셋업_인건비, 부대비용(경비+기타비용), 안정화)
	} else if (sQuthGu == '0010') {	
		arrArgs.push("QU01_AM^NULL", "QU02_AM^NULL", "QU03_AM^NULL", "QU04_AM^NULL", "QU05_AM^NULL", "QU06_AM^NULL");
	
	}
	
	//납품예정일수
	arrArgs.push("PADE_CT^NULL");

	var sCheckGroupTitle = this.div_DetailInput.form.tab_Detail.tpg_01.text;//체크그룹 타이틀
	//Grid Vaild 체크
	if (!this.gfn_CheckGridValid(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10, arrArgs, sCheckGroupTitle) ) {
		return false;
	}
	
	return true;
}

/***********************************************************************************************
 * 05.04. [Vaildation] Detail10 확정시 제출단가, 제출예정일 유무 필수값 체크 - 그리드항목
 ***********************************************************************************************/ 
this.fn_VaildDetailFixSave = function()
{	
	//의뢰구분 : 구매품(0020), 가공품(0030), 외주(0010)
	var sQuthGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_GU");	//의뢰구분
	
	var iQusbPr = 0;	//제출단가
	var iQu01Am = 0;	//제출금액01
	var iQu02Am = 0;	//제출금액02
	var iQu03Am = 0;	//제출금액03
	var iQu04Am = 0;	//제출금액04
	var iQu05Am = 0;	//제출금액05
	var iQu06Am = 0;	//제출금액06
	var sPadeCt = "";	//납품예정일수
	
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
	
		//구매품
		if (sQuthGu == '0020') {
			iQusbPr = this.ds_Detail10.getColumn(iLoop, "QUSB_PR");	//제출단가
			//제출단가
			if (this.gfn_IsNull(iQusbPr) || iQusbPr < 0) {		
				//탭, 위치 이동
				this.div_DetailInput.form.tab_Detail.set_tabindex(0);			
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(15,iLoop);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();
				
				var iRow = nexacro.toNumber(this.ds_Detail10.rowposition) + 1;
				var arrMsgArg = [iRow+"번째행 견적정보 제출단가"];
				this.gfn_Alert("SM008", arrMsgArg);//견적정보 제출단가는 필수 입력 항목입니다.
				return false;
				
				break;			
			}
			
		//가공품, 외주
		} else if (sQuthGu == '0030' || sQuthGu == '0010') {
			iQu01Am = this.ds_Detail10.getColumn(iLoop, "QU01_AM");	//제출금액01
			iQu02Am = this.ds_Detail10.getColumn(iLoop, "QU02_AM");	//제출금액02
			iQu03Am = this.ds_Detail10.getColumn(iLoop, "QU03_AM");	//제출금액03
			iQu04Am = this.ds_Detail10.getColumn(iLoop, "QU04_AM");	//제출금액04
			iQu05Am = this.ds_Detail10.getColumn(iLoop, "QU05_AM");	//제출금액05
			iQu06Am = this.ds_Detail10.getColumn(iLoop, "QU06_AM");	//제출금액06
			
			//제출금액01
			if (this.gfn_IsNull(iQu01Am) || iQu01Am < 0) {		
				//탭, 위치 이동
				this.div_DetailInput.form.tab_Detail.set_tabindex(0);			
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(17,iLoop);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();
				
				var iRow = nexacro.toNumber(this.ds_Detail10.rowposition) + 1;
				var sHeadMsg = this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getCellProperty("head", 26, "text");
				var arrMsgArg = [iRow+" 번째행 견적정보 "+sHeadMsg];
				this.gfn_Alert("SM008", arrMsgArg);//견적정보 제출금액01는 필수 입력 항목입니다.
				return false;
				
				break;			
				
			//제출금액02
			} else if (this.gfn_IsNull(iQu02Am) || iQu02Am < 0) {		
				//탭, 위치 이동
				this.div_DetailInput.form.tab_Detail.set_tabindex(0);			
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(18,iLoop);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();
				
				var iRow = nexacro.toNumber(this.ds_Detail10.rowposition) + 1;
				var sHeadMsg = this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getCellProperty("head", 27, "text");
				var arrMsgArg = [iRow+" 번째행 견적정보 "+sHeadMsg];
				this.gfn_Alert("SM008", arrMsgArg);//견적정보 제출금액02는 필수 입력 항목입니다.
				return false;
				
				break;			
				
			//제출금액03
			} else if (this.gfn_IsNull(iQu03Am) || iQu03Am < 0) {		
				//탭, 위치 이동
				this.div_DetailInput.form.tab_Detail.set_tabindex(0);			
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(19,iLoop);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();
				
				var iRow = nexacro.toNumber(this.ds_Detail10.rowposition) + 1;
				var sHeadMsg = this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getCellProperty("head", 28, "text");
