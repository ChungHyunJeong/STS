/***********************************************************************************************
 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST10_SEARCH": //조회		
			this.ds_Master10.addColumn("crudType", "string");
			this.ds_Master10Update.addColumn("crudType", "string");
			this.ds_Master10Delete.addColumn("crudType", "string");
	
			if (this.ds_Master10.getRowCount() > 0) {
				var sToDay = this.gfn_GetCommDate("toDay");//현재일자		

				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master10.findRowExpr("QUSH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ=='" + this.fvSchKey02 + "' && QUTH_NO=='" + this.fvSchKey03 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}				
				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
				this.div_DetailInput.form.tab_Detail.set_enable(true);
				
				//견적확정, 견적차수증가 버튼 및 입력기능 활성화/비활성화
				this.fn_SetQushEnbaleButton();

				//견적의뢰상세 Tab으로 이동
				this.div_DetailInput.form.tab_Detail.set_tabindex(0);			

				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
				this.fvFileMst.fn_SetCommFileBtnAuth(oScrnBtnAuth);	//Mst첨부파일
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	//Div첨부파일

				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_MasterView.form.grd_Master10);
				
				this.fvMasterChangeYn = false; //Master 변경여부 false

			} else {
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			
				//입력영역 비활성화
				this.div_MasterInput.set_enable(false);
				this.div_DetailInput.form.tab_Detail.set_enable(false);
					
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = false;//권한 유무	
				this.fvFileMst.fn_SetCommFileBtnAuth(oScrnBtnAuth);	//Mst첨부파일
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	//Div첨부파일
				
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_MasterView.form.grd_Master10);
			}		

			//저장후 재조회여부-true
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}			

			break;
			
		case "SAVE":   //저장		
			// 최초 저장시
			if (this.fvNewSaveYn) {
				this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);	//첨부파일 저장
				this.fvSchKeyYn = true;//저장후 재조회여부

			//첨부파일 추가건이 없으면
			} else if (!this.fvFileDiv.fn_CheckFileAddYn()) {
				this.fvSchKeyYn = true;//저장후 재조회여부
				this.fn_ComBtnSearch();//저장후 재조회

			} else {
				this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);	//첨부파일 저장
				
			}

			this.fvMasterChangeYn = false;	//Master10 변경여부 초기화
			break;				
			
		case "FIX_SAVE":   //견적확정 저장		
			//첨부파일 저장시
			if (!this.fvFileDiv.fn_CheckFileAddYn()) {
				this.fvSchKeyYn = true;//저장후 재조회여부
				this.fn_ComBtnSearch();//저장후 재조회
			} else {
				this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);							
				this.fvSchKeyYn = true;//저장후 재조회여부
			}		
			
			break;	
			
		case "DTL10_SEARCH": //조회		
			this.ds_Detail10.addColumn("crudType", "string");

			//첨부파일 정보 조회 Mst
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = "M_MM_2100";//프로그램ID
			oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
			oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO");//PK번호2-견적의뢰번호
			oFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_SQ");//PK번호3-견적의뢰결재차수

			this.fvFileMst.fn_AtchFileSearch(oFileInfo);			

			//검색의뢰상세내용 존재시
			if (this.ds_Detail10.getRowCount() > 0) {

				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Detail10.findRowExpr("QUSH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ=='" + this.fvSchKey02 + "' && QUSB_SE=='" + this.fvSchKey04 + "' && QUTH_NO=='" + this.fvSchKey03 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Detail10.enableevent) {
						this.ds_Detail10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Detail10.set_rowposition(-1);
						} 
						this.ds_Detail10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Detail10.set_rowposition(0);
					}
				}		

				//첨부파일 정보 조회 Div
				if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUSH_NO"))) { 
					oFileInfo = new Object();//[공통] 첨부파일정보
					oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
					oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
					oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
					oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUSH_NO");//PK번호2-업체견적번호
					oFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
					
					this.fvFileDiv.fn_AtchFileSearch(oFileInfo);			
				}
				
				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);
				
				//도면정보가 있으면
				if (this.ds_Detail10.getCaseCount("DWGF_YN == 'O'") > 0) {
					this.div_DetailInput.form.btn_DwgAllSave.set_enable(true);//도면일괄저장
				} else {
					this.div_DetailInput.form.btn_DwgAllSave.set_enable(false);//도면일괄저장
				}
				
			} else {
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);		
				this.div_DetailInput.form.btn_DwgAllSave.set_enable(false);//도면일괄저장
			}
			
			//저장후 재조회여부-true 
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}	

			this.fvIncStatus = false; //견적차수증가

			break;			

		case "CHECK_SEARCH": //조회		

			//업체선정확정여부 체크
			if (this.ds_Master20Check.getRowCount() > 0) {
				this.gfn_Alert("MM005");//이미 업체선정이 확정되었습니다. 재조회 해주세요.
			
			} else {
				var arrMsgArg = ["견적차수증가","진행"];
				this.gfn_Confirm("SM077", arrMsgArg,  "CONF.INCREASE_CHANGE", "fn_MsgCallback");//견적차수증가를 진행하시겠습니까?
			}

			break;			
			
		case "ITED_FILE": //도면일괄 다운로드		

			if (nApp.gds_ItedResult.getRowCount() > 0) {
				if (nApp.gds_ItedResult.getColumn(0, "RSLT_CD") == "S") {
					try {		 			
						var sPathNm = nApp.gds_ItedResult.getColumn(0, "PATH_NM");
						var sFileNm = nApp.gds_ItedResult.getColumn(0, "FILE_NM");
						var sFileId = nApp.gds_ItedResult.getColumn(0, "FILE_NM");
							
						/*
						var sUrl = nEnv.services["svcUrl"].url+"fileDownloader?filePath=" + sPathNm;			
							sUrl += "&fileName="+encodeURI(sFileId.replace(/\+/,"%2B"));					
						*/
						var sUrl = nEnv.services["svcUrl"].url+"fileDownloader?filePath=" + sPathNm;			
							sUrl += "&fileName="+encodeURI(sFileId.replace(/\+/,"%2B"));					
							sUrl += "&orgFileName="+encodeURI(sFileNm.replace(/\+/,"%2B"));	
							sUrl += "&drmYn=N";//DRM적용여부-Zip 파일은 미적용
				
						this.fdt_File.set_downloadfilename(sFileNm);
						this.fdt_File.set_url(sUrl);

						this.fdt_File.download();
					} catch (e) {
						var arrMsgArg = [e];
						this.gfn_Alert("SM095",arrMsgArg);//일괄파일 다운로드 처리중 오류가 발생하였습니다.\n&ITM0
					}
				} else {
					var sRSLT_CT = nApp.gds_ItedResult.getColumn(0, "RSLT_CT");
					var arrMsgArg = [sRSLT_CT];
					this.gfn_Alert("SM095",arrMsgArg);//일괄파일 다운로드 처리중 오류가 발생하였습니다.\n&ITM0				
				}
			} else {
				var arrMsgArg = [];
				this.gfn_Alert("SM095",arrMsgArg);//일괄파일 다운로드 처리중 오류가 발생하였습니다.\n&ITM0
			}

			break;				

	}
};


