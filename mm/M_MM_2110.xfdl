/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	//의뢰구분 : 구매품(0020), 외주가공(0030), 턴키외주(0010)
	var sQuthGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_GU");	//의뢰구분

	//제출단가 변경시 제출금액 변경
	if (e.columnid == "QUSB_PR") {
		// 구매품
		if (sQuthGu == "0020" || sQuthGu == '0090') {
			var iCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐코드
			var iExchRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");//환율정보
			var iQutbQt = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUTB_QT");//수량
			var iQusbPr = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUSB_PR");//제출단가
			var iQusbAm = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUSB_AM");//제출금액(원화)

			if (!this.gfn_IsNull(iQutbQt)) {	//제출단가 존재시			
				var iQusbAm = nexacro.floor(iExchRt * iQutbQt * iQusbPr, 2);	//제출금액(원화) = 환율 * 수량 * 제출단가

				//그리드에 적용
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "QUSB_AM", iQusbAm);	//제출금액(원화)
			}		
		}
		
	//제출금액 합계계산 + 제출단가 계산
	} else if (e.columnid == "QU01_AM" || e.columnid == "QU02_AM" || e.columnid == "QU03_AM" || e.columnid == "QU04_AM" || e.columnid == "QU05_AM" || e.columnid == "QU06_AM") {
		// 외주가공, 턴키외주
		if (sQuthGu == "0030" || sQuthGu == "0010") {
			if (this.gfn_IsNull(this.ds_Detail10.getColumn(e.row, e.columnid))) {	//값 삭제시 0으로 변경
				this.ds_Detail10.setColumn(e.row, e.columnid, "0");
			}
			
			var iQu01Am = this.gfn_Nvl(this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QU01_AM"), 0);//제출금액01
			var iQu02Am = this.gfn_Nvl(this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QU02_AM"), 0);//제출금액02
			var iQu03Am = this.gfn_Nvl(this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QU03_AM"), 0);//제출금액03
			var iQu04Am = this.gfn_Nvl(this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QU04_AM"), 0);//제출금액04
			var iQu05Am = this.gfn_Nvl(this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QU05_AM"), 0);//제출금액05
			var iQu06Am = this.gfn_Nvl(this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QU06_AM"), 0);//제출금액06
			var iQutbQt = this.gfn_Nvl(this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUTB_QT"), 1);//수량
			var iQusbAm = 0;	//제출금액
			var iQusbPr = 0;	//제출단가
			
			iQusbPr = iQu01Am + iQu02Am + iQu03Am + iQu04Am + iQu05Am + iQu06Am;	//제출단가 합계
			iQusbAm = nexacro.floor(iQusbPr * iQutbQt, 2);							//제출금액 = 제출단가 * 수량

			//그리드에 적용
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "QUSB_PR", iQusbPr);	//제출단가
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "QUSB_AM", iQusbAm);	//제출금액(원화)

		}
	
	}
	
};


