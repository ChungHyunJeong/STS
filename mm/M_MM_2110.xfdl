		this.gfn_Alert("MM032");//의뢰구분 정보가 유효하지 않습니다.
		return;
	}
	
	this.gfn_ExcelExport(this, objGridList, sFileName);	
};

/***********************************************************************************************
 * 15.12. 엑셀업로드
 ***********************************************************************************************/ 
this.div_DetailInput_btn_ExcelUpload_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{

	//상세정보 유무 확인
	if (this.ds_Detail10.getRowCount() < 1) {
		this.gfn_Alert("MM031");//견적의뢰 상세내역 정보가 없습니다. 
		return;
	}

	this.ds_Import.clearData();

	var sQuthGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_GU");//의뢰구분
	var sImportDataSetName = "ds_Import";//Import할 데이터셋명
	var sImportSheetName = "";//Import할 엑셀 Sheet명
	var sBody = "A3";//Import할 엑셀 Body 영역
	var sCallBackFunc = "fn_ImportCallback";//Callback Func
	var sImportId = "";//Import Id

	//의뢰구분-외주
	if (sQuthGu == "0010") {
		sImportSheetName = "턴키외주";//Import할 엑셀 Sheet명
		sImportId = "DETAIL1010_IPT";//Import Id
	//의뢰구분-구매품
	} else if (sQuthGu == "0020") {
		sImportSheetName = "구매품";//Import할 엑셀 Sheet명
		sImportId = "DETAIL1020_IPT";//Import Id
	//의뢰구분-가공품
	} else if (sQuthGu == "0030") {
		sImportSheetName = "외주가공";//Import할 엑셀 Sheet명
		sImportId = "DETAIL1030_IPT";//Import Id		
	} else {
		this.gfn_Alert("MM032");//의뢰구분 정보가 유효하지 않습니다.
		return;
	}	
	
	this.gfn_ExcelImport(this, sImportDataSetName, sImportSheetName, sBody, sCallBackFunc, sImportId);		
	
};

/***********************************************************************************************
 * 15.13. 엑셀업로드-콜백함수
 ***********************************************************************************************/ 
this.fn_ImportCallback = function(sImportId)
{	
	this.ds_Detail10.set_enableevent(false);
	
	for (var iLoop=0; iLoop<this.ds_Import.getRowCount(); iLoop++) {
		
		//의뢰구분-턴키외주
		if (sImportId == "DETAIL1010_IPT") {
			var col14 = nexacro.toNumber(this.gfn_Nvl(this.ds_Import.getColumn(iLoop, "Column14"), 0));
			var col15 = nexacro.toNumber(this.gfn_Nvl(this.ds_Import.getColumn(iLoop, "Column15"), 0));
			var col16 = nexacro.toNumber(this.gfn_Nvl(this.ds_Import.getColumn(iLoop, "Column16"), 0));
			var col17 = nexacro.toNumber(this.gfn_Nvl(this.ds_Import.getColumn(iLoop, "Column17"), 0));
			var col18 = nexacro.toNumber(this.gfn_Nvl(this.ds_Import.getColumn(iLoop, "Column18"), 0));
			var col19 = nexacro.toNumber(this.gfn_Nvl(this.ds_Import.getColumn(iLoop, "Column19"), 0));
			var col20 = nexacro.toNumber(this.gfn_Nvl(this.ds_Import.getColumn(iLoop, "Column20"), ""));
			
			this.ds_Detail10.setColumn(iLoop, "QU01_AM", col14); //설계비
			this.ds_Detail10.setColumn(iLoop, "QU02_AM", col15); //제작_인건비
			this.ds_Detail10.setColumn(iLoop, "QU03_AM", col16); //제작_재료비
			this.ds_Detail10.setColumn(iLoop, "QU04_AM", col17); //셋업_인건비
			this.ds_Detail10.setColumn(iLoop, "QU05_AM", col18); //부대비용(경비+기타비용)
			this.ds_Detail10.setColumn(iLoop, "QU06_AM", col19); //안정화
			this.ds_Detail10.setColumn(iLoop, "PADE_CT", col20); //납품예정일수
			
			
		//의뢰구분-구매품
		} else if (sImportId == "DETAIL1020_IPT") { 
			var col12 = nexacro.toNumber(this.gfn_Nvl(this.ds_Import.getColumn(iLoop, "Column12"), 0));
			var col13 = nexacro.toNumber(this.gfn_Nvl(this.ds_Import.getColumn(iLoop, "Column14"), 0));
			
			this.ds_Detail10.setColumn(iLoop, "QUSB_PR", col12); //제출단가
			this.ds_Detail10.setColumn(iLoop, "PADE_CT", col13); //납품예정일수
			
		//의뢰구분-외주가공
		} else if (sImportId == "DETAIL1030_IPT") {
			var col14 = nexacro.toNumber(this.gfn_Nvl(this.ds_Import.getColumn(iLoop, "Column16"), 0));
			var col15 = nexacro.toNumber(this.gfn_Nvl(this.ds_Import.getColumn(iLoop, "Column17"), 0));
			var col16 = nexacro.toNumber(this.gfn_Nvl(this.ds_Import.getColumn(iLoop, "Column18"), 0));
			var col17 = nexacro.toNumber(this.gfn_Nvl(this.ds_Import.getColumn(iLoop, "Column19"), 0));
			var col18 = nexacro.toNumber(this.gfn_Nvl(this.ds_Import.getColumn(iLoop, "Column20"), ""));

			
			this.ds_Detail10.setColumn(iLoop, "QU01_AM", col14); //소재비
			this.ds_Detail10.setColumn(iLoop, "QU02_AM", col15); //가공비
			this.ds_Detail10.setColumn(iLoop, "QU03_AM", col16); //후처리
			this.ds_Detail10.setColumn(iLoop, "QU04_AM", col17); //이윤
			this.ds_Detail10.setColumn(iLoop, "PADE_CT", col18); //납품예정일수	

			
		}
		
		//의뢰구분-외주가공, 턴키외주
		if (sImportId == "DETAIL1010_IPT" || sImportId == "DETAIL1030_IPT") {
			var iQu01Am = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "QU01_AM"), 0);//제출금액01
			var iQu02Am = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "QU02_AM"), 0);//제출금액02
			var iQu03Am = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "QU03_AM"), 0);//제출금액03
			var iQu04Am = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "QU04_AM"), 0);//제출금액04
			var iQu05Am = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "QU05_AM"), 0);//제출금액05
			var iQu06Am = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "QU06_AM"), 0);//제출금액06
			var iQutbQt = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "QUTB_QT"), 1);//수량
			var iQusbAm = 0;	//제출금액
			var iQusbPr = 0;	//제출단가
			
			iQusbPr = iQu01Am + iQu02Am + iQu03Am + iQu04Am + iQu05Am + iQu06Am;	//제출단가 합계
			iQusbAm = nexacro.floor(iQusbPr * iQutbQt, 2);							//제출금액 = 제출단가 * 수량

			//그리드에 적용
			this.ds_Detail10.setColumn(iLoop, "QUSB_PR", iQusbPr);	//제출단가
			this.ds_Detail10.setColumn(iLoop, "QUSB_AM", iQusbAm);	//제출금액(원화)
			
		} else {	//구성품
			var iQusbPr = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "QUSB_PR"), 0);//제출단가
			var iQutbQt = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "QUTB_QT"), 1);//수량
			var iQusbAm = 0;	//제출금액
			
			iQusbAm = nexacro.floor(iQusbPr * iQutbQt, 2);							//제출금액 = 제출단가 * 수량

			//그리드에 적용
			this.ds_Detail10.setColumn(iLoop, "QUSB_PR", iQusbPr);	//제출단가
			this.ds_Detail10.setColumn(iLoop, "QUSB_AM", iQusbAm);	//제출금액(원화)
		}
		
		this.ds_Detail10.setColumn(iLoop, "crudType", "U");
		
	} 
	
	this.ds_Detail10.set_enableevent(true);  
};


