/***********************************************************************************************
 * 15.08. [실행] 견적차수증가 실행
 ***********************************************************************************************/ 
this.fn_ExecQushSeqIncrease = function()
{ 

	//견적의뢰상세 Detail Tab으로 이동
	this.div_DetailInput.form.tab_Detail.set_tabindex(0);			
	
	//견적차수증가 이전대상
	var iPrevRow = this.ds_Master10.rowposition; //이전 Row
	var sWofhSq = nexacro.toNumber(this.ds_Master10.getColumn(iPrevRow,"WOFH_SQ"));

	//ds_MasterUpdate에 최종여부, 수주확율 Update 대상 복사함
	this.ds_Master10Update.clearData();
	this.ds_Master10Update.addRow();
	this.ds_Master10Update.setColumn(0, "COMM_CD", this.ds_Master10.getColumn(iPrevRow, "COMM_CD"));
	this.ds_Master10Update.setColumn(0, "QUSH_NO", this.ds_Master10.getColumn(iPrevRow, "QUSH_NO"));
	this.ds_Master10Update.setColumn(0, "WOFH_SQ", sWofhSq);
	this.ds_Master10Update.setColumn(0, "crudType", "U");

	//견적차수증가 신규추가
	var iAddRow  = this.ds_Master10.addRow();

	//이전 Row의 값을 복사함
	this.ds_Master10.copyRow(iAddRow, this.ds_Master10, iPrevRow);			

	//견적차수 증가
	sWofhSq = sWofhSq + 1;
	
	//신규대상
	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", sWofhSq);	//견적차수
	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");	 	//최종여부 WOFH_YN = '1'
	this.ds_Master10.setColumn(iAddRow, "QUSH_CD", "0010"); 	//견적상태 (등록 = 0010, 확정 0020)
	this.ds_Master10.setColumn(iAddRow, "REGI_NM", this.gfn_GetSessionValue("USEM_NM"));	 	//로그인사용자
	this.ds_Master10.setColumn(iAddRow, "REGI_DT", "");	 	    //등록일시 초기화
	this.ds_Master10.setColumn(iAddRow, "MODI_NM", "");	 	    //수정자 초기화
	this.ds_Master10.setColumn(iAddRow, "MODI_DT", "");	 	    //수정일시 초기화	
	this.ds_Master10.setColumn(iAddRow, "crudType", "I");	 	//증가대상 crudType = 'I'
	
	//견적의뢰상세 저장전 상태로 리셋
	this.ds_Detail10.reset();
	
	//처음 입력 항목에 포커스
	this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.setFocus();	//비고

	this.fvMasterChangeYn = true; //Master변경체크
	this.fvIncStatus = true; //견적차수증가
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화

	//견적확정, 견적차수증가 버튼 및 입력기능 활성화/비활성화
	this.fn_SetQushEnbaleButton();
		
};

/***********************************************************************************************
 * 15.08. [버튼 - 활성화] 견적확정, 견적차수증가 
 ***********************************************************************************************/ 
this.fn_SetQushEnbaleButton = function()
{ 
	var sToDay  = this.gfn_GetCommDate("toDay");//현재일자		
	var sRequDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "REQU_DD") //요청기한
	var sSeleYn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SELE_YN") //발주업체선정 선정확정 여부 (확정 1, 미확정 0)
	
	this.div_MasterInput.form.sta_RequDd_Over.set_visible(false);	
	
	if (sSeleYn == '0' && sRequDd < sToDay) { 
		this.div_MasterInput.form.sta_RequDd_Over.set_visible(true);	
	}
	
	//선정미확정이거나 오늘일자보다 요청기한이 큰 경우 견적확정/견적차수증가 활성화
	if (sSeleYn == '0' && sRequDd >= sToDay) { 
	
	
		//견적상태가 확정이면 (견적승인 비활성화, 견적차수증가 활성화) - 수정불가상태 
		if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUSH_CD") == '0020') { //견적상태(QUSH_CD) - 등록 0010, 확정 0020

			this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 견적의뢰내용 tab 비활성화
			this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 견적의뢰상세 tab 비활성화
			this.div_DetailInput.form.div_ComGrdBtn.set_visible(false); //Detail 첨부파일 tab 안보임

			//견적확정 비활성화/견적차수증가 비활성화
			this.div_MasterInput.form.btn_Fix.set_enable(false);
			
			
			//견적이력이 최종이 아닌경우 비활성화
			if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_YN") == '0') {
				this.div_MasterInput.form.btn_Increase.set_enable(false);
			} else {	//최종대상 활성화
				this.div_MasterInput.form.btn_Increase.set_enable(true);
			}
			
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_RemkPop.set_enable(true); //비고 활성화
			
		//견적상태가 미확정이면 (견적확정 활성화(최종여부시), 견적차수증가 비활성화)
		} else {
			this.gfn_SetCompGroupEnable(this.div_MasterInput.form, true);//Master 견적의뢰내용 tab 활성화
			this.gfn_SetCompGroupEnable(this.div_DetailInput.form, true);//Detail 견적의뢰상세 tab 활성화
			this.div_DetailInput.form.div_ComGrdBtn.set_visible(true); //Detail 첨부파일 tab 보임
		
			//업체견적번호 NULL 여부 확인(견적이력이 최종이 아닌경우)
			if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_YN") == '0') {
				this.div_MasterInput.form.btn_Fix.set_enable(false);
			} else {	//최종대상 활성화
				this.div_MasterInput.form.btn_Fix.set_enable(true);
			}
			this.div_MasterInput.form.btn_Increase.set_enable(false);
			
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_QushNo.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_QushDd.set_enable(false);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WofhSq.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_QuthNo.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_QuthDd.set_enable(false);	
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_RequDd.set_enable(false);	
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmNm.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm.set_readonly(true);			
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_QuthNm.set_readonly(true);	
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_QuthGu.set_enable(false);	
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_EvalCd.set_enable(false);	

		}
		
	//최초 저장전 상태 or 요청기한이 오늘을 지난 경우 견적차수증가/견적승인 비활성화
	} else {
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 견적의뢰내용 tab 비활성화
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 견적의뢰상세 tab 비활성화
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(false); //Detail 첨부파일 tab 안보임

		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_RemkPop.set_enable(true); //비고 활성화
	}
};

