	var objGridList = [];
	var sFileName = "";//출력파일명
	
	//의뢰구분-턴키외주
	if (sQuthGu == "0010") {
		objGridList = ["this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10_10|턴키외주"
					  ];		 
		sFileName = "견적의뢰상세_턴키외주";//출력파일명
	//의뢰구분-구매품
	} else if (sQuthGu == "0020") {
		objGridList = ["this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10_20|구매품"
					  ];		 
		sFileName = "견적의뢰상세_구매품";//출력파일명	
	//의뢰구분-외주가공
	} else if (sQuthGu == "0030") {
		objGridList = ["this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10_30|외주가공"
					  ];		 
		sFileName = "견적의뢰상세_외주가공";//출력파일명			
