 ***********************************************************************************************/
/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 
	//변경여부 체크
	if (this.ds_Master10.getRowCount() == 0 && this.ds_Detail10.getRowCount() == 0 && this.fvFileDiv.fn_GetFileCount() == 0) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}	

	//첨부파일 유무 체크
	if (this.fvFileDiv.fn_GetFileCount() < 1 ) {
		var arrMsgArg = ["첨부파일"];
		this.gfn_Alert("SM045", arrMsgArg); //첨부파일은 1건 이상 존재해야 합니다

		//첨부파일 Tab으로 이동
		this.div_DetailInput.form.tab_Detail.set_tabindex(1);			
		
		return;
	}	

	//첨부파일정보 변경시 (최초 저장시 제외)
	if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUSH_NO")) && this.fvFileDiv.fn_CheckFileAddYn()) {
		//첨부파일정보만 변경시
		if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && this.ds_Master10.getRowCount() > 0) {
			//첨부파일정보 저장
			fvFileInfo = new Object();//[공통] 첨부파일정보
			fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, B-상세(그리드Row), E-기타
			fvFileInfo.CALLBACK_FUNC = "";//호출할 콜백함수
			fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
			fvFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUSH_NO");//PK번호2-견적의뢰번호
			fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
			//첨부파일Div 화면에 Key세팅하게 변경
			this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);
			
			return;	
		}
	}
	
	//저장전 유효성 체크
	//Detail 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		if (!this.fn_VaildDetailSave()) return;
	}	

	//Master 신규저장여부 (견젹의뢰번호가 없으면 최초저장임)
	if (this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUSH_NO"))) {
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "crudType", "I");	 //신규 - I 
		this.fvNewSaveYn = true; //최초저장

	} else {
		if (this.fvIncStatus) { //견적차수증가 진행시
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "crudType", "I");	 //신규 - I 
			
			//Detail10 신규
			for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
				this.ds_Detail10.setColumn(iLoop, "QUSH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUSH_NO"));	//견적번호
				this.ds_Detail10.setColumn(iLoop, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));	//견적차수
				this.ds_Detail10.setColumn(iLoop, "crudType", "I");	//신규 - I
			}			
			
		} else {	//단순 변경시
			//Master10 변경 사항 존재시
			if (this.fvMasterChangeYn) {	//비고 변경시
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "crudType", "U");	 //변경 - U
			}

			//Detail10 - 테스트시 오류에 의한 처리(데이터 정합후 삭제무관)
			if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
				for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
					if (this.gfn_IsNull(this.ds_Detail10.getColumn(iLoop, "QUSH_NO")) && this.ds_Detail10.getColumn(iLoop, "crudType") == "U") {	//견적의뢰번호가 없을 경우
						this.ds_Detail10.setColumn(iLoop, "QUSH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUSH_NO"));	//견적번호
						this.ds_Detail10.setColumn(iLoop, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));	//견적차수
						this.ds_Detail10.setColumn(iLoop, "crudType", "I");	//신규 - I
					}
				}			
			}

		}
		this.fvNewSaveYn = false; //기저장
		
	}
	
	//Master 신규입력 상태이면 
	if (this.fvNewSaveYn) {
		//채번
		var sTbnmNo = this.gfn_GetTbnmNo(this.fvComPrgmId, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUSH_DD"));//프로그램ID, 기준일자
		if (this.gfn_IsNull(sTbnmNo)) {
			//채번을 가져오는데 실패했습니다.
			return;
		}
		
		var sChkTbnmNo = "";//채번-체크용
		var sToDay = this.gfn_GetCommDate("toDay");//현재일자		
		var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position
		
		
		//Master 데이터셋에 채번정보 입력
		this.ds_Master10.setColumn(iMasterRowPos, "QUSH_NO", sTbnmNo);	//업체견적번호

		//Master 기본값 입력
		this.ds_Master10.setColumn(iMasterRowPos, "WOFH_SQ", "1");	//결재차수
		this.ds_Master10.setColumn(iMasterRowPos, "WOFH_YN", "1");	//최종여부
		this.ds_Master10.setColumn(iMasterRowPos, "QUSH_DD", sToDay);	//견적일자 (당일)
		this.ds_Master10.setColumn(iMasterRowPos, "QUSH_CD", "0010");	//견적상태 (등록 - 0010, 승인 - 0020)
		
		//PK값 정의[필수]
		var sCommCd = this.ds_Master10.getColumn(iMasterRowPos, "COMM_CD");//회사코드
		var sQuthNo = this.ds_Master10.getColumn(iMasterRowPos, "QUSH_NO");//업체견적번호
		var sWofhSq = this.ds_Master10.getColumn(iMasterRowPos, "WOFH_SQ");//결재차수

		//Detail10 데이터셋에 PK정보 입력
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			sChkTbnmNo = this.ds_Detail10.getColumn(iLoop, "QUSH_NO");//업체견적번호
			
			if (this.gfn_IsNull(sChkTbnmNo)) { 
				this.ds_Detail10.setColumn(iLoop, "COMM_CD", sCommCd);
				this.ds_Detail10.setColumn(iLoop, "QUSH_NO", sQuthNo);
				this.ds_Detail10.setColumn(iLoop, "WOFH_SQ", sWofhSq);
				this.ds_Detail10.setColumn(iLoop, "crudType", "I");	 //신규 - I 
				
			}
		}		
				
	}

	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		fvFileInfo = new Object();//[공통] 첨부파일정보
		fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
		fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
		fvFileInfo.CALLBACK_FUNC = "fn_ComBtnSearch";//호출할 콜백함수
		fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
		fvFileInfo.PK01_NO = nApp.gvCompany;//PK번호1-회사코드
		fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUSH_NO");//PK번호2-견적번호
		fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수			
	}		

	//저장시 재조회 Key설정 
	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUSH_NO");//조회Key01
	this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//조회Key02
	this.fvSchKey03 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO");//조회Key03
	
	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	//[공통]서비스 호출 변수
	var sInputDaset = "" //입력 데이터셋
	var sOutDatset = "";//출력 데이터셋
	var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		
	//Master 견적차수증가 상태이면 (this.fvIncStatus = true) 이전대상 최종상태를 0으로 변경
	if (this.fvIncStatus) {
	
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
		svcAddObj.sInsertSql = "";//저장Sql
		svcAddObj.sUpdateSql = "mm:M_MM_2110_M_UP11";//수정Sql
		svcAddObj.sDeleteSql = "";//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

		this.gfn_AddComSaveService(this, svcAddObj);
		
		sInputDaset = "ds_Master10Update=ds_Master10Update:U" //입력 데이터셋
	
	}
	
	//Master 변경건이 있으면 Master 저장
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
		svcAddObj.sInsertSql = "mm:M_MM_2110_M_IN10";//저장Sql
		svcAddObj.sUpdateSql = "mm:M_MM_2110_M_UP10";//수정Sql
		svcAddObj.sDeleteSql = "mm:M_MM_2110_M_DL10";//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

		this.gfn_AddComSaveService(this, svcAddObj);

		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = "ds_Master10=ds_Master10:U" //입력 데이터셋
		} else {
			sInputDaset = sInputDaset + " ds_Master10=ds_Master10:U" //입력 데이터셋
		}
		
	}
	
	//Detail10 변경건이 있으면 Detail10 저장 - 견적의뢰상세
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
	
		//Master 견적차수증가 상태이면 (this.fvIncStatus = true) Detail10은 신규로 등록함
		if (this.fvIncStatus) {
			var sWofhSq = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//결재차수		
			//결재차수, 신규			
			for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {			
				this.ds_Detail10.setColumn(iLoop, "WOFH_SQ", sWofhSq);	//결재차수
				this.ds_Detail10.setColumn(iLoop, "crudType", "I");	    //신규
			}
		}
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object	
		svcAddObj.sInsertSql = "mm:M_MM_2110_D_IN10";//저장Sql
		svcAddObj.sUpdateSql = "mm:M_MM_2110_D_UP10";//수정Sql
		svcAddObj.sDeleteSql = "";//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		
		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = "ds_Detail10=ds_Detail10:U" //입력 데이터셋
		} else {
			sInputDaset = sInputDaset + " ds_Detail10=ds_Detail10:U" //입력 데이터셋
		}

		//저장시 재조회 Key설정
		this.fvSchKey04 = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUSB_SE");//조회Key04
	}		

	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "SAVE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_SR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = sInputDaset //입력 데이터셋
	svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
	svcCallObj.sOtherArg = sOtherArg;//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명		
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
	
}

/***********************************************************************************************
 * 05.02. [Vaildation] Master 저장 필수값 체크 - 입력항목 
 ***********************************************************************************************/ 
this.fn_VaildMasterSave = function()
{
/* 
