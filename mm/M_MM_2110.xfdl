/***********************************************************************************************
 * 15.10. [초기화] 견적의뢰상세 Title 그리드 - 의뢰구분에 따른 변경
 ***********************************************************************************************/ 
this.fn_SetOrderDetailTitleGrd = function()
{ 
	//의뢰구분에 따른 견적의뢰상세 컬럼 증감
	//1. 구매품(0020) - 기본컬럼만 존재
	//2. 외주가공(0030) - 4개 컬럼(소재비, 가공비, 후처리, 이윤) : 단가는 4개컬럼 합계, 제출금액은 단가 * 수량
	//3. 턴키외주(0010) - 6개 컬럼(설계비, 제작_인건비, 제작_재료비, 셋업_인건비, 부대비용(경비+기타비용), 안정화) : 단가는 6개컬럼 합계, 제출금액합계는 단가 * 수량
	var grdOrder = this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10; //주 그리드
	var sQuthGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_GU");	//의뢰구분

	//1. 구매품 - 기본컬럼
	if (sQuthGu == '0020' || sQuthGu == '0090') {
		//재질, 후처리 숨기기(2022-07-05)
		grdOrder.setFormatColProperty(11, "size", 0);		//재질 칸사이즈 숨기기
		grdOrder.setFormatColProperty(12, "size", 0);		//후처리 칸사이즈 숨기기
		
		grdOrder.setCellProperty("head", 26, "text", ""); //칸 제목
		grdOrder.setCellProperty("head", 27, "text", ""); //칸 제목
		grdOrder.setCellProperty("head", 28, "text", ""); //칸 제목
		grdOrder.setCellProperty("head", 29, "text", ""); //칸 제목
		grdOrder.setCellProperty("head", 30, "text", ""); //칸 제목
		grdOrder.setCellProperty("head", 31, "text", ""); //칸 제목
	
		grdOrder.setFormatColProperty(17, "size", 0);	//칸사이즈
		grdOrder.setFormatColProperty(18, "size", 0);	//칸사이즈
		grdOrder.setFormatColProperty(19, "size", 0);	//칸사이즈
		grdOrder.setFormatColProperty(20, "size", 0);	//칸사이즈
		grdOrder.setFormatColProperty(21, "size", 0);	//칸사이즈
		grdOrder.setFormatColProperty(22, "size", 0);	//칸사이즈

		grdOrder.setCellProperty("body", 17, "edittype", "none");	//입력 비활성화
		grdOrder.setCellProperty("body", 18, "edittype", "none");	//입력 비활성화
		grdOrder.setCellProperty("body", 19, "edittype", "none");	//입력 비활성화
		grdOrder.setCellProperty("body", 20, "edittype", "none");	//입력 비활성화
		grdOrder.setCellProperty("body", 21, "edittype", "none");	//입력 비활성화
		grdOrder.setCellProperty("body", 22, "edittype", "none");	//입력 비활성화
		
		grdOrder.setCellProperty("body", 15, "edittype", "text");	//제품단가 활성화
	
	//2. 외주가공 - 4개 컬럼(소재비, 가공비, 후처리, 이윤)
	} else if (sQuthGu == '0030') {
		//재질, 후처리 보이기(2022-07-05 추가)
		grdOrder.setFormatColProperty(11, "size", 80);		//재질 칸사이즈 보이기
		grdOrder.setFormatColProperty(12, "size", 100);	//후처리 칸사이즈 보이기
	
