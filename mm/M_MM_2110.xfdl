/***********************************************************************************************
 * 13.03. [메세지] CallBack 함수 처리
 ***********************************************************************************************/  
this.fn_MsgCallback = function (strId, strVal)
{

	//MASTER 변경사항 저장여부 
	if (strId == "CONF.MAST10_CHANGE") {
		if(strVal )
		{
			this.fn_ComBtnSave();//저장 서비스 호출
		} else {
			//데이터 변경여부 체크
			if (this.fvMasterChangeYn) {
				this.gfn_UndoColumn(this.div_MasterView.form.grd_Master10);			
				this.fvMasterChangeYn = false;	//Master10 변경여부 초기화
			}		

			if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
				this.ds_Detail10.reset();	//설계자요청(10/8)
				for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
					this.ds_Detail10.setColumn(iLoop, "crudType", "")
				}
			
			}				
		}

	//견적확정
	} else if (strId == "CONF.FIX_CHANGE") {
		if(strVal)
		{
			//견적확정 저장호출
			this.fn_QushFixSave();
		}
		
	//견적차수증가
	} else if (strId == "CONF.INCREASE_CHANGE") {
		if(strVal)
		{
			//견적차수증가 호출 
			this.fn_ExecQushSeqIncrease(); //견적차수증가 실행
		}		
		
	}
}; 
 
