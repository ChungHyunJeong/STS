				this.gfn_Alert("SM008", arrMsgArg);//견적정보 제출금액04는 필수 입력 항목입니다.
				return false;
				
				break;			
				
			//외주이며, 제출금액05
			} else if (sQuthGu == '0010' && (this.gfn_IsNull(iQu05Am) || iQu05Am < 0)) {		
				//탭, 위치 이동
				this.div_DetailInput.form.tab_Detail.set_tabindex(0);			
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(21,iLoop);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();
				
				var iRow = nexacro.toNumber(this.ds_Detail10.rowposition) + 1;
				var sHeadMsg = this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getCellProperty("head", 30, "text");
				var arrMsgArg = [iRow+" 번째행 견적정보 "+sHeadMsg];
				this.gfn_Alert("SM008", arrMsgArg);//견적정보 제출금액05는 필수 입력 항목입니다.
				return false;
				
				break;			
				
			//외주이며, 제출금액06
			} else if (sQuthGu == '0010' && (this.gfn_IsNull(iQu06Am) || iQu06Am < 0)) {		
				//탭, 위치 이동
				this.div_DetailInput.form.tab_Detail.set_tabindex(0);			
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(22,iLoop);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();
				
				var iRow = nexacro.toNumber(this.ds_Detail10.rowposition) + 1;
				var sHeadMsg = this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getCellProperty("head", 31, "text");
				var arrMsgArg = [iRow+" 번째행 견적정보 "+sHeadMsg];
				this.gfn_Alert("SM008", arrMsgArg);//견적정보 제출금액06는 필수 입력 항목입니다.
				return false;
				
				break;			
			}		
		}
		
		//납품예정일수
		sPadeCt = this.ds_Detail10.getColumn(iLoop, "PADE_CT");	//납품예정일수
		if (this.gfn_IsNull(sPadeCt)) {	
			//탭, 위치 이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);			
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(23,iLoop);
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();
			
			var iRow = nexacro.toNumber(this.ds_Detail10.rowposition) + 1;
			var arrMsgArg = [iRow+" 번째행 납품정보 납품예정일수"];
			this.gfn_Alert("SM008", arrMsgArg);//납품정보 납품예정일수는 필수 입력 항목입니다.
			return false;
			
			break;			
		}
	}

	return true;
}


