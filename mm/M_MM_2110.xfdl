this.div_MasterInput_btn_Increase_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	
	//데이터 변경여부 체크
	if (this.fvMasterChangeYn || this.gfn_DsIsUpdated(this.ds_Detail10) || this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	} 

	//2021-10-20 요청내용
	//견적차수 증가시 업체선정 완료일 경우 견적차수증가 진행을 하면 안됨
	//fn_Callback에서 견적차수증가 진행여부 메시지 처리

	//[Check] 발주업체선정에서 업체선정완료를 하였는지 체크하는 로직 (10/20)
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Master20Check.clearData();		//발주업체선정에서 업체선정확정체크
	this.ds_Input.clearData();
	
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "QUTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO"));
	this.ds_Input.setColumn(0, "QUTH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_SQ"));

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "mm:M_MM_2110_M_SL20";	//업체선정확정 Check
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "CHECK_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Master20Check=ds_Master20";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);		

	
};


/***********************************************************************************************
 * 15.06. [버튼] Master 견적확정
 ***********************************************************************************************/ 
this.div_MasterInput_btn_Fix_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{ 

	//최초 저장 or 견적차수 증가시 Master가 먼저 저장해야 함
	if (this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUSH_NO"))) {
		this.gfn_Alert("MM008");//견적의뢰를 등록하신 후 견적확정을 진행해 주세요.
	}

	//Detail 데이터 유효성 체크
	if (!this.fn_VaildDetailFixSave()) return;

	//데이터 변경여부 체크
	if (this.fvMasterChangeYn || this.gfn_DsIsUpdated(this.ds_Detail10) || this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	} 

	//첨부파일 유무 체크
	if (this.fvFileDiv.fn_GetFileCount() < 1 ) {
		var arrMsgArg = ["첨부파일"];
		this.gfn_Alert("SM045", arrMsgArg); //첨부파일은 1건 이상 존재해야 합니다

		//첨부파일 Tab으로 이동
		this.div_DetailInput.form.tab_Detail.set_tabindex(1);			
		
		return;
	}	

	var arrMsgArg = ["견적확정","진행"];
	this.gfn_Confirm("SM077", arrMsgArg,  "CONF.FIX_CHANGE", "fn_MsgCallback");//견적확정을 진행하시겠습니까?
	
};

