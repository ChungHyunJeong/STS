	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");	//최종여부
	this.ds_Master10.setColumn(iAddRow, "WOFH_ST", "0010");	//결재상태	

	//파트너사로 로그인시처리 - 초기화
	if (this.gfn_GetSessionValue("CMDS_CD") == "P") {
		this.ds_Master10.setColumn(iAddRow, "CUSM_CD", this.gfn_GetSessionValue("CUSM_CD"));//파트너사	
		this.ds_Master10.setColumn(iAddRow, "CUSM_NM", this.gfn_GetSessionValue("CUSM_NM"));//파트너사명
		this.ds_Master10.setColumn(iAddRow, "CSPM_CD", this.gfn_GetSessionValue("SYST_ID"));//담당자사번
		this.ds_Master10.setColumn(iAddRow, "CSPM_NM", this.gfn_GetSessionValue("USEM_NM"));//담당자명
		
		//상세 파트너사 검색	
		this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmCd.set_value(this.gfn_GetSessionValue("CUSM_CD"));
		this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmNm.set_value(this.gfn_GetSessionValue("CUSM_NM"));   

	} else {
		//상세 파트너사 검색	
		this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmCd.set_value("");//파트너사코드
		this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmNm.set_value("");//파트너사명
	}

	this.ds_Master10.setColumn(iAddRow, "CORH_YM", sToDay);//기성년월-오늘날자로 설정
	this.ds_Master10.setColumn(iAddRow, "CORH_DD", sToDay);//청구일자-오늘날자로 설정
	

	this.fn_SetWfCompEnable();
	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	this.div_DetailInput.form.tab_Detail.set_enable(true);
	this.fn_SetGridInputButtonEnable();
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.div_MasterInput.form.btn_CorhM1.set_enable(false);// 기성청구변경 비활성
	this.div_MasterInput.form.btn_CorhM2.set_enable(false);// 기성청구확정 비활성


	
	//처음 입력 항목에 포커스
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.setFocus();
	this.div_DetailInput.form.tab_Detail.set_tabindex(1);	
	
	this.ds_Detail10.clearData();//상세정보 초기화
	this.ds_Detail20.clearData();//검색정보 초기화	
	
	
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
	//첨부파일정보 권한설정
	var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
	oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
	this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
	
	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
}

