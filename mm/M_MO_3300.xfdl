			return false;
		} else {		
			obj.setColumn(e.row, "ADCR_PC", nexacro.floor(obj.getColumn(e.row, "ADCR_AM") / obj.getColumn(e.row, "WONE_AM")*100, 1)); //당월기성 비중 
		}

	//당월기성 비중(%)
	}else if(e.columnid == "ADCR_PC"){
	
		var iMaxPersent = 100 - obj.getColumn(e.row, "CORB_PC");
	
		if( obj.getColumn(e.row, "ADCR_PC") > iMaxPersent) {
			obj.setColumn(e.row, "ADCR_PC", e.oldvalue);
			var arrMsgArg = ["당월기성 비중(%)",nexacro.floor(iMaxPersent,1) +"%"];
