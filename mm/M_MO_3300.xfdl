	var arrArgs = ["this.div_Search.form.edt_CusmCd^"+ this.div_Search.form.sta_CustCd.text +"|NULL"
				  ]; 
	
	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs) ) {
		return false;
	}
	
	return true;
