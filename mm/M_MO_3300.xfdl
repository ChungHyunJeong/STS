 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)

	//당월기성 금액
	if(e.columnid == "ADCR_AM"){
		var iMaxMoney = obj.getColumn(e.row, "WONE_AM") - obj.getColumn(e.row, "CORB_AM");
	
		if( obj.getColumn(e.row, "ADCR_AM") > iMaxMoney) {
			obj.setColumn(e.row, "ADCR_AM", e.oldvalue);
			var arrMsgArg = ["당월기성 금액",this.gfn_AppendComma(iMaxMoney,0)+"원"];
			this.gfn_Alert("SM056", arrMsgArg); //[&ITM0]이(가) [&ITM1]을(를) 초과할 수 없습니다.
			return false;
		} else {		
			obj.setColumn(e.row, "ADCR_PC", nexacro.floor(obj.getColumn(e.row, "ADCR_AM") / obj.getColumn(e.row, "WONE_AM")*100, 1)); //당월기성 비중 
		}

		//마스터 원화 금액 계산
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CORH_AM", this.ds_Detail10.getCaseSum("crudType !='D'","ADCR_AM"));//청구금액
	
	
	//당월기성 비중(%)
	}else if(e.columnid == "ADCR_PC"){
	
		var iMaxPersent = 100 - obj.getColumn(e.row, "CORB_PC");
	
		if( obj.getColumn(e.row, "ADCR_PC") > iMaxPersent) {
			obj.setColumn(e.row, "ADCR_PC", e.oldvalue);
			var arrMsgArg = ["당월기성 비중(%)",nexacro.floor(iMaxPersent,1) +"%"];
			this.gfn_Alert("SM056", arrMsgArg); //[&ITM0]이(가) [&ITM1]을(를) 초과할 수 없습니다.
			return false;
		} else {		
			obj.setColumn(e.row, "ADCR_AM", nexacro.floor(obj.getColumn(e.row, "WONE_AM") * (obj.getColumn(e.row, "ADCR_PC") / 100), 0)); //당월기성 금액
		}
		//마스터 원화 금액 계산
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CORH_AM", this.ds_Detail10.getCaseSum("crudType !='D'","ADCR_AM"));//청구금액
	
	}

};
