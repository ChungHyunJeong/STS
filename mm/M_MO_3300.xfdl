/***********************************************************************************************
 * 00.03. Form Onload 이벤트
 ***********************************************************************************************/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{
	//[팝업]호출 파라메타 처리 (비활성화)
	this.fvPopupGu = this.getOwnerFrame().paramPopupGu;
	this.fvCrohNo = this.getOwnerFrame().paramCrohNo;
	this.fvWofhSq = this.getOwnerFrame().paramWofhSq;
	this.fvCusmCd = this.getOwnerFrame().paramCusmCd;
	
	trace(this.fvPopupGu);
	trace(this.fvCrohNo);
	trace(this.fvWofhSq);
	trace(this.fvCusmCd);
	//[팝업]호출하지 않았을 경우
	if (this.gfn_IsNull(this.fvPopupGu)) {
