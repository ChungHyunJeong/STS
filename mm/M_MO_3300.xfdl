this.fn_SetWfCompEnable = function()
{ 	

	//trace("CORH_YN-->"+this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CORH_YN"));
	//확정상태에 따른 컴포넌트 Enable 등 처리
	if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CORH_YN") =='1'){
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 입력			
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_RemkPop.set_enable(true);
		this.div_MasterInput.form.btn_CorhM1.set_enable(true);// 기성청구변경 활성
		this.div_MasterInput.form.btn_CorhM2.set_enable(false);// 기성청구확정 비활성
	}else{
		//this.gfn_SetCompGroupEnable(this.div_MasterInput.form, true);//Master 입력 프리폼
		//최종만 수정가능
		if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_YN") =='1'){ 
			this.div_MasterInput.form.btn_CorhM1.set_enable(false);// 기성청구변경 비활성
			this.div_MasterInput.form.btn_CorhM2.set_enable(true);// 기성청구확정 활성
		}else{
			this.div_MasterInput.form.btn_CorhM1.set_enable(false);// 기성청구변경 비활성
			this.div_MasterInput.form.btn_CorhM2.set_enable(false);// 기성청구확정 비활성
		}
		
		this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(false);//비고
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form, true);//Detail 입력			
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(true);
		//trace("버튼 Visible---> "+ this.div_DetailInput.form.div_ComGrdBtn.visible);

		//2022-03-21 파트너사, 본사 로그인시 처리로직 추가
		if (this.gfn_GetSessionValue("CMDS_CD") == "P") {	//파트너사 비활성화
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_CorhYm.set_enable(true);//기성년월
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_readonly(true);//거래처코드
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_CusmCd.set_enable(false);//거래처 버튼
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CspmCd.set_readonly(true);//담당자코드
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_CspmCd.set_enable(false);//담당자버튼

			//상세 파트너사 검색	
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmCd.set_readonly(true);
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmNm.set_readonly(true);
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.btn_CusmCd.set_enable(false);
			
			//상세 계약번호 검색(2022-04-11)	
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.mae_CnthNo.set_readonly(true);
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CnthNm.set_readonly(true);
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.btn_CnthNo.set_enable(false);


		} else {	//본사 활성화
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_CorhYm.set_enable(true);//기성년월
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_readonly(false);//거래처코드
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_CusmCd.set_enable(true);//거래처 버튼
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CspmCd.set_readonly(false);//담당자코드
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_CspmCd.set_enable(true);//담당자버튼
			
			//상세 파트너사 검색	
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmCd.set_readonly(false);
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmNm.set_readonly(false);
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.btn_CusmCd.set_enable(true);

			//상세 계약번호 검색(2022-04-11)
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.mae_CnthNo.set_readonly(false);
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CnthNm.set_readonly(false);
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.btn_CnthNo.set_enable(true);

		}
		
		//Master 입력/수정/삭제 상태 체크
		var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
		
		//Master 입력 상태이면
		if (sChkRowType != "I") {
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_CorhYm.set_enable(false);
		}else{
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_CorhYm.set_enable(true);
		}
		
	}
			
	
}


