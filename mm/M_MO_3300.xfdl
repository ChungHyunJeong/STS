						trace("20거래처"+this.ds_Detail20.getColumn(0,"CUSM_CD"));
						if(this.ds_Detail10.getColumn(0,"CUSM_CD") != this.ds_Detail20.getColumn(iLoop,"CUSM_CD") ){
							var arrMsgArg = ["거래처"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 거래처만 적용 가능합니다.
							return;
						}
					}
					
					
					//2. Detail20 그리드내 체크된 대상 동일여부 비교 : 거래처코드
					if(!this.gfn_IsNull(sIdntNoChk) ){
						if(sIdntNoChk != this.ds_Detail20.getColumn(iLoop,"CUSM_CD") ){
							var arrMsgArg = ["거래처"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 거래처만 적용 가능합니다.
							return;
						}
					}
					sIdntNoChk = this.ds_Detail20.getColumn(iLoop,"CUSM_CD"); //for문 비교를 위한 이전값 저장
				}
			}	
			
			//2. 당월기성 금액이 0원(또는 비중이 0%) 대상은 메시지 알림 (2022-04-08)
			var iCntChk = this.ds_Detail20.getCaseCount("s_chk == '1' && ADCR_AM == '0'");	//체크 대상중 당월기성 금액이 0인 대상
			if (iCntChk > 0) {	//당월기성 금액이 0인 대상 존재시 return
				var iFindRow = this.ds_Detail20.findRowExpr("s_chk == '1' && ADCR_AM == '0'");
				this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.setCellPos(17, iFindRow);
				this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.setFocus();				
				
				this.gfn_Alert("MM030"); //당월기성 금액이 0원 이상 대상만 적용이 가능합니다.
				return;				
			}			
			
			//3. Detail20 -> Detail10 
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
			
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					
					
					//기 등록된 발주번호 / 발주순번 패스처리
					var iFvFindRow = this.ds_Detail10.findRowExpr("PCOH_NO=='" + this.ds_Detail20.getColumn(iLoop,"PCOH_NO") + "' && PCOB_SE=='" + this.ds_Detail20.getColumn(iLoop,"PCOB_SE") + "'");
					if(iFvFindRow == -1){
						//trace("------->"+ this.ds_FaVatm.rowposition);
						
						var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)
						
						//첫번째일 경우 마스터 업데이트
						if(this.ds_Detail10.rowcount == 1 ){
							this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_value(this.ds_Detail20.getColumn(iLoop,"CUSM_CD"));	//거래처
							this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CorhYn.set_value("0");	//확정여부 (비활성)
							
							//비활성화
							this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_readonly(true);	//거래처
							this.div_MasterInput.form.tab_Master.tpg_01.form.btn_CusmCd.set_enable(false);	//거래처버튼
							var sPopupId = "BS_CUSM_CD_01";//팝업ID -거래처정보
							this.gfn_GetCommCusm("C", this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd, this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmNm, sPopupId);//거래처 코드로 검색	
				
						}
						
						
						//미발주검색에서 발주상세로 이동
						this.ds_Detail10.copyRow(iAddRow, this.ds_Detail20, iLoop);
						this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));	//회사코드
						this.ds_Detail10.setColumn(iAddRow, "CORH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CORH_NO"));	//청구번호
						this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));	
				
						arrCheckRow.push(iLoop );
						
						//선택항목 비활성화처리
						this.ds_Detail20.setColumn(iLoop, "s_conf", "1");	//미발주검색 컬러 변경 구분자 업데이트
						this.ds_Detail20.setColumn(iLoop, "s_chk", "0");	//Uncheck로 변경
						
					} else {
						//선택항목 비활성화처리
						this.ds_Detail20.setColumn(iLoop, "s_conf", "1");	//미발주검색 컬러 변경 구분자 업데이트
						this.ds_Detail20.setColumn(iLoop, "s_chk", "0");	//Uncheck로 변경					
					}
					
				}
				
			}
			
			//마스터 원화 금액 계산
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CORH_AM", this.ds_Detail10.getCaseSum("crudType !='D'","ADCR_AM"));//청구금액
			
		
			//발주상세로 Tab이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);
		}
	}
};


  
