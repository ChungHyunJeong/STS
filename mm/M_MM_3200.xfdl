/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//trace("ds_Detail10_oncolumnchanged");
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	//체크박스 변경
	if (e.columnid == "s_chk") {
			//trace(this.gfn_GetSessionValue("USEM_ID"));
			//체크선택시
			if(e.newvalue =='1'){
				obj.setColumn(e.row, "CHSU_ID", this.gfn_GetSessionValue("USEM_ID")); //
				obj.setColumn(e.row, "CHSU_NM", this.gfn_GetSessionValue("USEM_NM")); //성명
				obj.setColumn(e.row, "CHAN_DD", sToDay);//날짜
				
				//체크시 상태가 취소이면
				if(obj.getColumn(e.row, "PCOB_ST") == 1){
					obj.setColumn(e.row, "PCOB_ST", "3");// 정정으로
					obj.setColumn(e.row, "PCOB_QT", obj.getColumn(e.row, "BFPC_QT"));// 이전발주 수량으로 발주 세팅
					obj.setColumn(e.row, "PCOE_QT", obj.getColumn(e.row, "BFPC_QT"));// 이전발주 수량으로 정정 세팅
					
					//obj.setColumn(e.row, "PCOB_ST", "3");// 정정으로				
				}else{
					
					if(obj.getColumn(e.row, "PCWB_QT") == 0){//입고가 0이면 취소
						obj.setColumn(e.row, "PCOB_ST", "1");
					}else{
						obj.setColumn(e.row, "PCOB_ST", "2");//입고가 1 이상이면 종결												
					}
				}
				trace("체크값-->"+obj.getColumn(e.row, "s_chk"));
				trace("진행상태-->"+obj.getColumn(e.row, "PCOB_ST"));
				//								
				
				
				this.div_DetailInput_grd_Detail_PcobStCheck();
				obj.setColumn(e.row, "crudType", "U"); //체크박스는 crudType 변경-수정이 안되므로 수동변경
				
			}else{
				//trace("해제");
				obj.setColumn(e.row, "CHSU_ID", "");
				obj.setColumn(e.row, "CHSU_NM", "");
				obj.setColumn(e.row, "PCOB_ST",obj.getOrgColumn(e.row, "PCOB_ST"));		// 진행상태 초기화
				obj.setColumn(e.row, "PCOE_QT",obj.getOrgColumn(e.row, "PCOE_QT"));		// 정정수량 초기화
				obj.setColumn(e.row, "PCOB_QT",obj.getOrgColumn(e.row, "PCOB_QT"));		// 발주수량 초기화
				
				
				obj.setColumn(e.row, "WONE_AM", obj.getOrgColumn(e.row, "WONE_AM"));	// 원화금액
				obj.setColumn(e.row, "VTAX_AM", obj.getOrgColumn(e.row, "VTAX_AM"));	// 부가세금액
				obj.setColumn(e.row, "FORE_AM", obj.getOrgColumn(e.row, "FORE_AM"));	// 외화금액
		
				//마스터 원화 금액 계산
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM", obj.getSum("WONE_AM"));
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "VTAX_AM", obj.getSum("VTAX_AM"));
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_AM", obj.getSum("FORE_AM"));
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "TOTL_AM", obj.getSum("WONE_AM")+obj.getSum("VTAX_AM"));
		
				obj.setColumn(e.row, "crudType", ""); //체크박스는 crudType 변경-수정이 안되므로 수동변경
				
			}
	//진행상태
	} else if (e.columnid == "PCOB_ST") {
		trace("진행상태변경-->"+obj.getColumn(e.row, "PCOB_ST"));
		if(obj.getColumn(e.row, "PCOB_ST") ==3){//정정
			obj.setColumn(e.row, "PCOE_QT", obj.getColumn(e.row, "PCOB_QT"));// 발주 수량으로 정정수량으로 세팅
		}else if (obj.getColumn(e.row, "PCOB_ST") ==1){//취소
			obj.setColumn(e.row, "PCOE_QT", 0);//0 으로 정정수량으로 세팅
			
		}
		
		this.div_DetailInput_grd_Detail_PcobStCheck(); //진행상태 변경 처리
		
	//정정
	} else if (e.columnid == "PCOE_QT") {
		this.div_DetailInput_grd_Detail_PcobQtCheck(); //발주금액정보 계산		
	}
	
};

//발주금액정보 계산
this.div_DetailInput_grd_Detail_PcobQtCheck = function(val){
		//trace("발주계산");
		var sCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");	//화폐
		var sQitbPr = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "WONE_PR");	//원화단가
		var sForePr = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "FORE_PR");	//외화단가
		var sPcoeQt = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "PCOE_QT");	//정정 발주 수량
		//if (this.gfn_IsNull(val)) {
			//sPcoeQt = val;
		//}
		 if(this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "PCOB_ST") == "1"){//취소인경우
			sPcoeQt=0;
		 }
		 trace("계산 정정수량"+sPcoeQt);
		if(sCurrCd == "0090"){//원화
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "WONE_AM", sQitbPr * sPcoeQt);			// 원화금액
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "VTAX_AM", sQitbPr * sPcoeQt * 0.1);	// 부가세금액
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "FORE_AM", 0);							// 외화금액
		}else{
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "WONE_AM", sForePr * sPcoeQt);			// 원화금액
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "VTAX_AM", sForePr * sPcoeQt * 0.1);	// 부가세금액
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "FORE_AM", sForePr * sPcoeQt);			// 외화금액
		}
		
		//마스터 원화 금액 계산
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM", this.ds_Detail10.getSum("WONE_AM"));
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "VTAX_AM", this.ds_Detail10.getSum("VTAX_AM"));
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_AM", this.ds_Detail10.getSum("FORE_AM"));
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "TOTL_AM", this.ds_Detail10.getSum("WONE_AM")+this.ds_Detail10.getSum("VTAX_AM"));
		
}

