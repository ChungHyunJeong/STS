/***********************************************************************************************
 * 15.08. [버튼] 단가적용요율 버튼 클릭
 ***********************************************************************************************/ 
this.div_DetailInput_btn_UnitRate_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{

	var iChkCount = this.ds_Detail10.getCaseCount("s_chk=='1'");	//체크박스 존재유무
	var iUnitRt = nexacro.toNumber(this.div_DetailInput.form.mae_UnitRt.value);	//적용요율
	var iItrbPr = 0;	//단가
	
	if (this.gfn_IsNull(iUnitRt) || iUnitRt == 0) {	//적용요율이 없거나 0일 경우
		this.div_DetailInput.form.mae_UnitRt.setFocus();
		var arrMsgArg = ["적용요율", "0", "10"];
		this.gfn_Alert("SM101", arrMsgArg);//적용요율은 0보다 크고 10보다 작은 숫자만 입력가능합니다.
		
		return false;
	
	} else {
		
		if (iChkCount < 1) {	//선택한 체크박스가 없음
			this.gfn_Alert("SM046"); //선택한 대상이 없습니다.
			return false;
			
		} else {	//선택한 체크박스가 존재시
			for(var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
				if (this.ds_Detail10.getColumn(iLoop, "s_chk") == "1") {
					iItrbPr = nexacro.toNumber(this.ds_Detail10.getColumn(iLoop, "ITRB_PR"));	//단가
					
					this.ds_Detail10.setColumn(iLoop, "ITRB_PR", nexacro.floor(iItrbPr * iUnitRt, 0));	//단가 * 적용요율
				}
			}
		}
	}
};


/***********************************************************************************************
 * 15.09. [메시지 발송] 품목단가확정시 잔디메시지 발송
 ***********************************************************************************************/ 
this.fn_SendJandiMessage = function()
{
	/* 메시지 내용
	[SynusTech-iERP] 품목단가요청.
	- 요청번호 : 220328-0001
	- 고객사 : (주)SK 실트론
	- 처리자 : 홍길동
	품목단가요청이 처리되었습니다.
	*/
	
	//제목 셋팅
	var sTitlNm = "품목단가요청.";

	//요청번호 셋팅
	var sJandNo = this.gfn_StringFormat(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITRH_NO"), "@@@@@@-@@@@");	//요청번호

	//고객사 셋팅
	var sRecvNm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_NM");	//고객사명

	//처리사 셋팅 (로그인한사람)
	var sRequNm = this.gfn_GetSessionValue("EMPM_NM") + " " + this.gfn_GetSessionValue("POSI_NM");
	
	//내용 셋팅
	var sCocfNm = "품목단가요청이 처리되었습니다.";

	//잔디 전송 환경
    var sRECV_ID = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SYST_ID");//받는사람 (등록자 시스템ID)
    var sEMAL_NM = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EMAL_NM");//이메일주소 (등록자 이메일)
    var sbody = "[알림]";//최상단에 노출할 메시지
    var stitle = "[SynusTech-iERP] "+sTitlNm;//메시지 하단 말풍선 내 최상단 메시지
    var sdescription = "- 요청번호 : " + sJandNo + "\n- 고객사 : " + sRecvNm + "\n- 처리자 : " + sRequNm + "\n" + sCocfNm;//메시지 하단 말풍선 내 중간 메시지

    //잔디전송 내용 데이터셋 초기화
    nApp.gds_SendJandi.clearData();      

    //전송 이메일 정보가 있으면
    if (!this.gfn_IsNull(sEMAL_NM)) {

       var paramObj = new Object();//알람메세지 파라메타 Object
       paramObj.PRGM_ID = "M_MM_1310";//프로그램ID
       paramObj.SYST_ID = this.gfn_GetSessionValue("SYST_ID");//보내는사람
	   paramObj.RECV_ID = sRECV_ID;//받는사람
       paramObj.EMAL_NM = sEMAL_NM;//이메일
       paramObj.TITL_NM = stitle;//알람제목
       paramObj.ALRH_NM = sdescription;//알람내용
       this.gfn_AlrhMsgSave(paramObj);//알람메세지정보 저장        
	   
    }

};


