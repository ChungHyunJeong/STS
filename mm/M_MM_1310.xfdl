		this.div_DetailInput.form.btn_UnitRate.set_visible(true);  	//단가적용요율 버튼 활성화
		this.div_DetailInput.form.btn_SomCheck.set_visible(true);   //부분선택
		this.div_DetailInput.form.btn_SomeUncheck.set_visible(true);//부분취소
	//Tab-첨부파일
	} else if (iTabIndex == 1) {	
		this.div_DetailInput.form.btn_CostCreate.set_visible(false); //최종단가생성 버튼 비활성화
		this.div_DetailInput.form.mae_UnitRt.set_visible(false);  	 //적용요율 EDIT 활성화
		this.div_DetailInput.form.btn_UnitRate.set_visible(false);   //단가적용요율 버튼 활성화
		this.div_DetailInput.form.btn_SomCheck.set_visible(false);   //부분선택
		this.div_DetailInput.form.btn_SomeUncheck.set_visible(false);//부분취소
	}
	
};

