/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {

		//요청번호정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITRH_NO"))) {
		
			//[요청상태 (요청/취소:0, 확정:1)]품목단가확정/품목단가취소 버튼 활성화/비활성화
			if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITRH_YN") == '0') { //요청(취소)
				//확인부서, 확인자 설정
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CODP_CD", this.gfn_GetSessionValue("DEPM_CD"));//확인부서코드	
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CODP_NM", this.gfn_GetSessionValue("DEPM_NM"));//확인부서명	
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "COSU_CD", this.gfn_GetSessionValue("USEM_ID"));//확인자사번	
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "COSU_NM", this.gfn_GetSessionValue("USEM_NM"));//확인자명		
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "COSU_ID", this.gfn_GetSessionValue("SYST_ID"));//확인자ID	
			
				this.div_MasterInput.form.btn_SelectFix.set_enable(true);     //품목단가확정
				this.div_MasterInput.form.btn_SelectCancel.set_enable(false); //품목단가취소
				this.div_DetailInput.form.btn_CostCreate.set_enable(true);    //최종단가생성
				this.div_DetailInput.form.mae_UnitRt.set_readonly(false);     //적용요율 EDIT
				this.div_DetailInput.form.btn_UnitRate.set_enable(true);      //단가적용요율 버튼
				this.div_DetailInput.form.btn_SomCheck.set_enable(true);      //부분선택
				this.div_DetailInput.form.btn_SomeUncheck.set_enable(true);   //부분취소

				//Detail10 그리드 입력 가능
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 12, "edittype", "text");	//단가
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 15, "edittype", "text");	//비고(확인)
			} else {																		  //확정
				this.div_MasterInput.form.btn_SelectFix.set_enable(false);    //품목단가확정
				this.div_MasterInput.form.btn_SelectCancel.set_enable(true);  //품목단가취소			
				this.div_DetailInput.form.btn_CostCreate.set_enable(false);   //최종단가생성
				this.div_DetailInput.form.mae_UnitRt.set_readonly(true);       //적용요율 EDIT
				this.div_DetailInput.form.btn_UnitRate.set_enable(false); 	  //단가적용요율 버튼
				this.div_DetailInput.form.btn_SomCheck.set_enable(false);      //부분선택
				this.div_DetailInput.form.btn_SomeUncheck.set_enable(false);   //부분취소
				
				//Detail10 그리드 입력 불가
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 12, "edittype", "none");	//단가
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 15, "edittype", "none");	//비고(확인)
			}			
			
			//단가적용요율 Default 표기
			this.div_DetailInput.form.mae_UnitRt.set_value("1.00");	//기본값 1.00

			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "ITRH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITRH_NO"));
			this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
					
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "mm:M_MM_1310_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);

			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);		
		}
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master10] Row 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무

	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail10) || this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	

	//요청상태(ITRH_YN)가 요청(0)의 경우 Master Row 이동시 이전값으로 변경
	if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITRH_YN") == '0') {	
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CONF_DD", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "CONF_DD"));//확인일자
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CODP_CD", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "CODP_CD"));//확인부서코드
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CODP_NM", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "CODP_NM"));//확인부서명
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "COSU_CD", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "COSU_CD"));//확인자사번
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "COSU_NM", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "COSU_NM"));//확인자명
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "COSU_ID", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "COSU_ID"));//확인자ID
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "crudType", "");//crudType='' (초기화)
		
	}

	return true;
};  

