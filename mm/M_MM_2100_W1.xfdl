		oScrnAuth.AUTH_YN = "N";//권한 유무
		oScrnAuth.FILE_CHECK_YN = "Y";//첨부파일종류 체크유무
		oScrnAuth.PRGM_ID = this.fvComPrgmId;//프로그램ID
		oScrnAuth.FILM_GU = "H";//첨부파일구분-Head		
		oScrnAuth.READ_YN = "Y";//읽기전용권한여부
		
		this.fvFileDiv.fn_SetCommFileScrnAuth(oScrnAuth);	
	}
};


/***********************************************************************************************
 * 15.05. [데이터변경] 환율정보 변경시
 ***********************************************************************************************/ 
this.fn_RatmChange  = function()
{
	//Detal10 견적단가 기준으로 원화단가/원화금액, 외화단가/외화금액 적용
	var iCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐코드
	var iExchRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");//환율정보

	var iQutrQt = 0; 	//견적의뢰상세 수량
	var iQutbPr = 0; 	//견적의뢰상세 견적단가
	var iWoneAm = 0;	//견적의뢰상세 원화금액
	var iForeAm = 0;	//견적의뢰상세 외화금액
	
	//Detail10 데이터셋에 원화금액, 외화금액 변경
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
	
		iQutbQt = this.ds_Detail10.getColumn(iLoop, "QUTB_QT");//수량
		iQutbPr = this.ds_Detail10.getColumn(iLoop, "QUTB_PR");//견적단가
		
		iWoneAm = nexacro.floor(iExchRt * iQutbQt * iQutbPr, 2);	//원화금액 = 환율 * 수량 * 견적단가

		if (iCurrCd == '0090') {			//KRW(원화)
			iForeAm = 0;					//외화금액 = 0원
		} else {
			iForeAm = iQutbQt * iQutbPr;	//외화금액 = 수량 * 견적단가
		}
		
		//Detail10 데이터셋에 적용
		this.ds_Detail10.setColumn(iLoop, "WONE_AM", iWoneAm);	//원화금액
		this.ds_Detail10.setColumn(iLoop, "FORE_AM", iForeAm);	//외화금액
	}	

};


/***********************************************************************************************
 * 15.06. [데이터변경] 내외자구분 및 연간단가적용 변경시
 ***********************************************************************************************/ 
this.fn_DoimChange  = function()
{
	var sDoimGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DOIM_GU");//내외자구분
	var sYearYn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "YEAR_YN");//연간단적적용

	//내외자구분이 import 또는 Local거나, Domestic이고 연간단가적용인 경우만 견적단가 입력가능
	if (sDoimGu == '1' && sYearYn == '0') { //Domestic이고 연간단가적용이 아닌경우
		//견적단가(col19) 입력 비활성화
		this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setCellProperty("body", 19, "edittype", "none");	
		for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
			//견적단가, 원화, 외화 초기화
			this.ds_Master10.setColumn(iLoop,"QUTB_PR","");
			this.ds_Master10.setColumn(iLoop,"WONE_AM","");
			this.ds_Master10.setColumn(iLoop,"FORE_AM","");
		}
	} else {
		//견적단가(col19) 입력 활성화
		this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setCellProperty("body", 19, "edittype", "text");				
		     
	}
};


/***********************************************************************************************
 * 15.07. [그리드변경] Detail 그리드 포멧 변경 (Master-의뢰구분, Detail-청구구분)
 ***********************************************************************************************/ 
this.fn_GridFormatChange  = function()
{
/* 2022-03-28 추가개발
    1. Master (견적의뢰내역) - 의뢰구분
		ㅇ 가공품 (0030)
		  - 보이기 : 중분류, 재질, 후처리
		  - 숨기기 : Maker
		ㅇ  가공품 이외 (구매품, 외주)
		  - 보이기 : Maker
		  - 숨기기 : 중분류, 재질, 후처리
	
*/
	var sSeleGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_GU");		//의뢰구분
	var gDetail10 = this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10; //견적의뢰상세 그리드
	

	//의뢰구분이 가공품인 경우
	if (sSeleGu == "0030") { 
		//Detail10
		gDetail10.setFormatColProperty(5, "size", 70);		//중분류 칸사이즈
		gDetail10.setFormatColProperty(9, "size", 80);		//재질 칸사이즈
		gDetail10.setCellProperty("head", 19, "text", "후처리"); //칸 제목 
	
	//그외
	} else {
		//Detail10
		gDetail10.setFormatColProperty(5, "size", 0);		//중분류 칸사이즈
		gDetail10.setFormatColProperty(9, "size", 0);		//재질 칸사이즈
		gDetail10.setCellProperty("head", 19, "text", "Maker"); //칸 제목 
	}
	
};



