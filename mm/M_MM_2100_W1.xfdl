/***********************************************************************************************
 * WF.02. [결재처리] 최종결재 [승인] 시 업무영역 전처리 함수
 ***********************************************************************************************/
this.fn_ComWfAppr = function(oWfParam)
{
	//견적의뢰업체 정보가 있으면 - 잔디로 알림메세지 전송
	if (this.ds_Master11.getRowCount() > 0) {
		var sEMAL_NM = "";//이메일주소
		var sEMAL_NM_TOT = "";//이메일주소-전체
		var sbody = "[알림]";//최상단에 노출할 메시지
		var stitle = "시너스텍에서 견적의뢰 요청하였습니다.";//메시지 하단 말풍선 내 최상단 메시지
		var sdescription = "의뢰일자 : " + this.gfn_StringFormat(this.ds_Master10.getColumn(0, "QUTH_DD"), "@@@@-@@-@@") + "\n의뢰명 : " + this.ds_Master10.getColumn(0, "QUTH_NM");//메시지 하단 말풍선 내 중간 메시지
		
		
		//잔디전송 내용 데이터셋 초기화
		nApp.gds_SendJandi.clearData();
		
		for (var iLoop=0; iLoop<this.ds_Master11.getRowCount(); iLoop++) {
			sEMAL_NM = this.ds_Master11.getColumn(iLoop, "EMAL_NM");//이메일주소
			//이메일 주소 정보가 있으면 
			if (!this.gfn_IsNull(sEMAL_NM)) {
				if (this.gfn_IsNull(sEMAL_NM_TOT)) {
					sEMAL_NM_TOT = sEMAL_NM;
				} else {
					sEMAL_NM_TOT = sEMAL_NM_TOT + "," + sEMAL_NM;
				}
			}
		}
		
		//전송 이메일 정보가 있으면
		if (!this.gfn_IsNull(sEMAL_NM_TOT)) {
			/* 바로 발송 예시
			nApp.gds_SendJandi.addRow();
			
			nApp.gds_SendJandi.setColumn(0, "email", sEMAL_NM_TOT);//이메일주소
			nApp.gds_SendJandi.setColumn(0, "body", sbody);//최상단에 노출할 메시지
			nApp.gds_SendJandi.setColumn(0, "connectColor", "#FAC11B");//메시지 하단 Bar 의 색상
			nApp.gds_SendJandi.setColumn(0, "title", stitle);//메시지 하단 말풍선 내 최상단 메시지
			nApp.gds_SendJandi.setColumn(0, "description", sdescription);//메시지 하단 말풍선 내 중간 메시지
			nApp.gds_SendJandi.setColumn(0, "imageUrl", "");//메시지 하단 말풍선 내 노출할 Link URL				
		
			var ssCallBackFunc = "";//콜백함수-필요시에만 입력	
			this.gfn_SendJandiMsg(ssCallBackFunc);//Jandi 메세지 전송		
			*/
			
			var paramObj = new Object();//알람메세지 파라메타 Object
			paramObj.PRGM_ID = "M_MM_2100";//프로그램ID
			paramObj.SYST_ID = this.gfn_GetSessionValue("SYST_ID");//보내는사람
			paramObj.RECV_ID = "";//받는사람
			paramObj.EMAL_NM = sEMAL_NM_TOT;//이메일
			paramObj.TITL_NM = stitle;//알람제목
			paramObj.ALRH_NM = sdescription;//알람내용
			this.gfn_AlrhMsgSave(paramObj);//알람메세지정보 저장			
		}
	}
	return true;
}

