/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)

	// 견적단가 변경(변경)시 원화금액/외화금액 변경
	if (e.columnid == "QUTB_PR") {
		var iCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐코드
		var iExchRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");//환율정보
		var iQutbQt = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUTB_QT");//수량
		var iQutbPr = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUTB_PR");//견적단가
		var iWoneAm = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "WONE_AM");//원화금액 
		var iForeAm = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "FORE_AM");//외화금액 

		if (!this.gfn_IsNull(iQutbQt)) {	//견적단가 존재시			
			var iWoneAm = nexacro.floor(iExchRt * iQutbQt * iQutbPr, 2);	//원화금액 = 환율 * 수량 * 견적단가

			if (this.fvCurrCd == '0090') {	//KRW(원화)
				var iForeAm = 0;	// 외화금액 = 0원
			} else {
				var iForeAm = iQutbQt * iQutbPr;	//외화금액 = 수량 * 견적단가
			}
		
			//그리드에 적용
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "WONE_AM", iWoneAm);	//원화금액
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "FORE_AM", iForeAm);	//외화금액
		}		
	}
	
};

 
