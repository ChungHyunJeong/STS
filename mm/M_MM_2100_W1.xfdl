{

	//[조회영역]
	
	//[Master영역]		
	//거래처
	if (strId == "B_CUSM_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);
			if (!this.gfn_IsNull(arrRtnVal.CUSM_CD)) {
				this.ds_Master11.setColumn(this.ds_Master11.rowposition, "CUSM_CD", arrRtnVal.CUSM_CD);
				this.ds_Master11.setColumn(this.ds_Master11.rowposition, "CUSM_NM", arrRtnVal.CUSM_NM);
