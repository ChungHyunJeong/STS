				this.ds_Master11.setColumn(this.ds_Master11.rowposition, "EMAL_NM", arrRtnVal.EMAL_NM);
			}
		} else {
			this.ds_Master11.setColumn(this.ds_Master11.rowposition, "CSPM_CD", "");
			this.ds_Master11.setColumn(this.ds_Master11.rowposition, "CSPM_NM", "");
			this.ds_Master11.setColumn(this.ds_Master11.rowposition, "RANK_NM", "");
			this.ds_Master11.setColumn(this.ds_Master11.rowposition, "MTEL_NO", "");
			this.ds_Master11.setColumn(this.ds_Master11.rowposition, "EMAL_NM", "");
