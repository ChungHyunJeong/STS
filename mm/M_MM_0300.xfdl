 ***********************************************************************************************/
this.ds_Master20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	//  중분류 사양 '품명자동완성여부' 수정값이 '0' 인 경우
	// '품명자동완성방법' 값를 Clear 한다.
	if(e.columnid == "NMAT_YN" && e.newvalue == "0"){
		obj.setColumn(e.row, "NMAT_CD", "");
	}
	//대분류
	if(e.columnid == "ITLM_CD"){	
		obj.set_enableevent(false);
		if(!this.gfn_IsNull(e.newvalue)){
			this.ds_RdItmm.filter("ITLM_CD=='"+e.newvalue+"' || ITLM_CD==''" );	
		}
		obj.setColumn(e.row, "ITMM_CD","");
		obj.set_enableevent(true);
	}
	//중분류
	if(e.columnid == "ITMM_CD"){
		var nRow = this.ds_RdItmm.findRow("ITMM_CD",e.newvalue);
		
		if(!this.gfn_IsNull(e.newvalue)){
			obj.set_enableevent(false);
			obj.setColumn(e.row, "ITLM_CD", this.ds_RdItmm.getColumn(nRow, "ITLM_CD"));
			obj.set_enableevent(true);
		}
		obj.setColumn(e.row, "NMAT_YN", this.ds_RdItmm.getColumn(nRow, "NMAT_YN"));
		obj.setColumn(e.row, "ITLM_CD", this.ds_RdItmm.getColumn(nRow, "ITLM_CD"));
		
	}
};
