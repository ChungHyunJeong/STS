/***********************************************************************************************
 * 05.02.01 [Vaildation] Master 저장 중복값 체크 
 ***********************************************************************************************/ 
this.fn_VaildMasterSave1 = function()
{
	for(var i = 0; i < this.ds_Master10.rowcount; i++) {
		var chkRow = "";
		chkRow = i;
				
		for(var j = 0; j < this.ds_Master10.rowcount; j++) {
			if(this.ds_Master10.getColumn(chkRow, "COSM_CD") == this.ds_Master10.getColumn(j, "COSM_CD"))
			
			if(chkRow != j) {
				var arrMsgArg = [(j+1),"비용코드"];	//메시지내용
				this.gfn_Alert("HR113", arrMsgArg); //[ &ITM0 ] 행 &ITM1 이 중복 되었습니다.
				return false;
			}
			
			if(this.ds_Master10.getColumn(chkRow, "COSM_NM") == this.ds_Master10.getColumn(j, "COSM_NM"))
			
			if(chkRow != j) {
				var arrMsgArg = [(j+1),"비용명"];	//메시지내용
				this.gfn_Alert("HR113", arrMsgArg); //[ &ITM0 ] 행 &ITM1 이 중복 되었습니다.
				return false;
			}
		}
	}
	
	return true;
}

