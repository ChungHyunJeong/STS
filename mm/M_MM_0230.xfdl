					this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "ITEM_NM", "");
					this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "SPEC_NM", "");
					this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "MAKE_NM", "");
					this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "UNIT_CD", "");
					
				} else {
					this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "ITLM_CD", arrRtnVal.ITLM_CD);
					this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "ITLM_NM", arrRtnVal.ITLM_NM);
					this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "ITEM_CD", arrRtnVal.ITEM_CD);
					this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "ITEM_NM", arrRtnVal.ITEM_NM);
					this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "SPEC_NM", arrRtnVal.SPEC_NM);
					this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "MAKE_NM", arrRtnVal.MAKE_NM);
					this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "UNIT_CD", arrRtnVal.UNIT_CD);
					
					//해당 품목번호의 발주정보, 전년도연간단가를 가져옴
					this.fn_SetItemInfoSearch(arrRtnVal.ITEM_CD);
				}
			}
		} else {
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "ITLM_CD", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "ITLM_NM", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "ITEM_CD", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "ITEM_NM", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "SPEC_NM", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "MAKE_NM", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "UNIT_CD", "");
		}		
		
	//[Detail영역-검색]
	}
	
};
 
/***********************************************************************************************
 * 13.03. [메세지] CallBack 함수 처리
 ***********************************************************************************************/  
this.fn_MsgCallback = function (strId, strVal)
{
	//삭제여부
	if (strId == "CONF.DELETE") {
		if(strVal )
		{
			this.fn_SvcDelete();//삭제 서비스 호출
		}
	//MASTER 변경사항 저장여부
	} else if (strId == "CONF.MAST_CHANGE") {
		if(strVal )
		{
			this.fn_ComBtnSave();//저장 서비스 호출
		} else {
			//데이터 변경여부 체크
			if (this.gfn_DsIsUpdated(this.ds_Master10)) {
				this.gfn_UndoColumn(this.div_MasterView.form.grd_Master10);
			}		
			
			if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
				this.ds_Detail10.copyData(this.ds_Detail10Reset);	//기존 데이터로 원복
			}				
			
		}

	//연간단가정보생성(Detail)
	} else if (strId == "CONF.YEARUNIT_INFO") {
		if(strVal)
		{
			//연간단가정보 대상조회
			this.fn_YearUnitInfoSearch();
		}

	//연간단가요청확정(Master)
	} else if (strId == "CONF.FIX_CHANGE") {
		if(strVal)
		{		
			//연간단가정보 확정진행
			this.fn_UnitSelectFixSave();
		}

	//요청확정취소(Master)
	} else if (strId == "CONF.CANCEL_CHANGE") {
		if(strVal)
		{
			//업체선정취소 저장호출
			this.fn_UnitSelectCancelSave();
		}		

	}
}; 


