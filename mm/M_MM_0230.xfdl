 * 02.01. [공통버튼] 입력 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnAdd = function()
{
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10) || this.gfn_DsIsUpdated(this.ds_Detail10) || this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무

	var sYprhYy = nexacro.toNumber(this.gfn_GetCommDate("nowYear")) - 1; //2022-03-28 기준년도의 1년전 기준 변경
	var sFromDd = sYprhYy-2; 
	sFromDd = sFromDd.toString()+"0101";
	var sClosDd = sYprhYy+"1231";
	
	var iAddRow = this.ds_Master10.addRow();
	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", "1");//결재차수
	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");//최종여부	
	this.ds_Master10.setColumn(iAddRow, "YPRH_DD", this.gfn_GetCommDate("toDay"));//등록일자-현재날자로 설정
	this.ds_Master10.setColumn(iAddRow, "YPRH_YY", this.gfn_GetCommDate("nowYear"));//기준년도-당해년
	this.ds_Master10.setColumn(iAddRow, "FROM_DD", sFromDd);//집계년도(시작) - 기준년도-3년(01월01일)
	this.ds_Master10.setColumn(iAddRow, "CLOS_DD", sClosDd);//집계년도(종료) - 기준년도-1년(12월31일)
	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//요청부서코드	
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//요청부서명	
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//요청자사번	
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//요청자명		
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//요청자ID	
	this.ds_Master10.setColumn(iAddRow, "REQU_ST", "0010");//진행상태 (요청등록-0010, 요청확정-0020)
	this.ds_Master10.setColumn(iAddRow, "PART_ST", "0010");//진행상태 (미확정-0010, 확정-0020)
	this.ds_Master10.setColumn(iAddRow, "DELE_YN", "0");//삭제여부	

	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.div_DetailInput.form.tab_Detail.set_enable(true);
	
	//연간단가정보생성 버튼 활성화
	this.div_DetailInput.form.btn_YearUnitInfo.set_enable(true);
	
	//처음 입력 항목에 포커스
	this.div_MasterInput.form.tab_Master.tpg_01.form.cal_RequDd.setFocus(); //요청기한
	
	this.ds_Detail10.clearData();//연간단가요청상세정보 초기화
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
	//첨부파일정보 권한설정
	var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
	oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
	this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	

	//연간단가확정/취소 버튼 활성화/비활성화 초기화
	this.div_MasterInput.form.btn_SelectFix.set_enable(false);		//단가요청확정
	this.div_MasterInput.form.btn_SelectCancel.set_enable(false);	//요청확정취소	

	//연간단가정보(초기화)
	this.fvYearUnitInfoYn = false;	 //연간단가정보 생성여부
	this.fvYearUnitSearchYn = false; //연간단가정보 기존정보 조회

	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
}

