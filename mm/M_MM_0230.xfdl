	var iTabIndex = this.div_DetailInput.form.tab_Detail.tabindex;
	
	//요청등록시(최초 포함)
	if (this.gfn_IsNull(sRequSt) || (sRequSt == '0010' && sPartSt == '0010')) {	//진행상태 (최초입력-NULL, 요청등록-0010, 요청확정-0020), (단가요청(0010)/단가제출(0020)/단가확정(0030))
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, true);//Master 입력 프리폼
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form, true);//Detail 입력			
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(true); //행추가/취소, 첨부파일
		if (iTabIndex == 0) {//Tab-연간단가상세
			this.div_DetailInput.form.btn_YearUnitInfo.set_visible(true);	//연간단가정보생성 활성화
		} else { //Tab-첨부파일
			this.div_DetailInput.form.btn_YearUnitInfo.set_visible(false);	//연간단가정보생성 비활성화
		}
		
		this.div_MasterInput.form.btn_SelectFix.set_enable(true);		//단가요청확정
		this.div_MasterInput.form.btn_SelectCancel.set_enable(false);	//요청확정취소

		//항목 Enable처리
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_YprhNo.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_YprhDd.set_enable(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CspmNm.set_readonly(true);
	//요청확정시
	} else if (sRequSt == '0020' && sPartSt == '0010') {	//진행상태 (최초입력-NULL, 요청등록-0010, 요청확정-0020), (단가요청(0010)/단가제출(0020)/단가확정(0030))
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 입력			
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(false); //행추가/취소, 첨부파일
		this.div_DetailInput.form.btn_YearUnitInfo.set_visible(false);	//연간단가정보생성 비활성화
		
		this.div_MasterInput.form.btn_SelectFix.set_enable(false);		//단가요청확정
		this.div_MasterInput.form.btn_SelectCancel.set_enable(true);	//요청확정취소	
				
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_RemkPop.set_enable(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_RequDd.set_enable(true);	//회신요청일 수정가능하도록 변경(2022-05-02 요청사항)				

	//그외
	} else {
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 입력					
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(false); //행추가/취소, 첨부파일
		this.div_DetailInput.form.btn_YearUnitInfo.set_visible(false);	//연간단가정보생성 바활성화
	
		this.div_MasterInput.form.btn_SelectFix.set_enable(false);		//단가요청확정
		this.div_MasterInput.form.btn_SelectCancel.set_enable(false);	//요청확정취소		
		
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_RemkPop.set_enable(true);
		if (sWofhSt == '0010') {	//결재상태가 "등록"인 경우 수정가능
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_RequDd.set_enable(true);	//회신요청일 수정가능하도록 변경(2022-05-02 요청사항)				
		}
	}
	
};


/***********************************************************************************************
 * 15.13. [메시지 발송] 단가요청확정시 잔디메시지 발송
 ***********************************************************************************************/ 
this.fn_SendJandiMessage = function()
{
	/* 메시지 내용
	[SynusTech-iERP] 연간단가요청.
	- 요청번호 : 220409-0002
	- 요청회신일 : 2022-04-10
	- 요청자 : 김선혜대리
	시너스텍에서 연간단가등록을 요청하였습니다.
	*/

	//제목 셋팅
	var sTitlNm = "연간단가요청.";

	//요청번호 셋팅
	var sJandNo = this.gfn_StringFormat(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "YPRH_NO"), "@@@@@@-@@@@");	//요청번호

	//회신요청일 셋팅
	var sRecvNm = this.gfn_StringFormat(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "REQU_DD"), "@@@@-@@-@@");	//회신요청일

	//요청자 셋팅 (로그인한사람)
	var sRequNm = this.gfn_GetSessionValue("EMPM_NM") + " " + this.gfn_GetSessionValue("POSI_NM");

	//내용 셋팅
	var sCocfNm = "시너스텍에서 연간단가등록을 요청하였습니다.";

	//잔디 전송 환경
    var sRECV_ID = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SACP_CD");//받는사람 (파트너사 영업담당)
    var sEMAL_NM = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JAND_ID");//이메일주소 (파트너사 잔디ID)
    var sbody = "[알림]";//최상단에 노출할 메시지
    var stitle = "[SynusTech-iERP] "+sTitlNm;//메시지 하단 말풍선 내 최상단 메시지
    var sdescription = "- 요청번호 : " + sJandNo + "\n- 회신요청일 : " + sRecvNm + "\n- 요청자 : " + sRequNm + "\n" + sCocfNm;//메시지 하단 말풍선 내 중간 메시지

    //잔디전송 내용 데이터셋 초기화
    nApp.gds_SendJandi.clearData();      
	
    //전송 이메일 정보가 있으면
    if (!this.gfn_IsNull(sEMAL_NM)) {

       var paramObj = new Object();//알람메세지 파라메타 Object
       paramObj.PRGM_ID = "M_MM_0230";//프로그램ID
       paramObj.SYST_ID = this.gfn_GetSessionValue("SYST_ID");//보내는사람
	   paramObj.RECV_ID = sRECV_ID;//받는사람
       paramObj.EMAL_NM = sEMAL_NM;//이메일
       paramObj.TITL_NM = stitle;//알람제목
       paramObj.ALRH_NM = sdescription;//알람내용
       this.gfn_AlrhMsgSave(paramObj);//알람메세지정보 저장        
	   
    }

};


