		//}
		
		//if (sPtexYn == '0' && (crudType == 'I' || crudType == 'U')) {	//연간단가제외가 아닌경우
			if (this.gfn_IsNull(iYprhPr) || iYprhPr < 1 || this.gfn_IsNull(iYprbCt) || this.gfn_IsNull(sYprbGu) || sYprbGu == "") {
				//탭, 위치 이동
				this.div_DetailInput.form.tab_Detail.set_tabindex(0);	
				
				if (this.gfn_IsNull(iYprhPr) || iYprhPr < 1) {
					alertMsg = "연간단가";
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(15,iLoop);
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();
				}
				
				if (this.gfn_IsNull(iYprbCt)) {		
					if (this.gfn_IsNull(iYprhPr) || iYprhPr < 1) { 		
						alertMsg = alertMsg + ", 통상납품일수";
					} else {
						alertMsg = "통상납품일수";
						this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(17,iLoop);
						this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();					
					}
				}
				
				if(this.gfn_IsNull(sYprbGu) || sYprbGu == ""){					
					if (this.gfn_IsNull(iYprhPr) || iYprhPr < 1 || this.gfn_IsNull(iYprbCt)) { 		
						alertMsg = alertMsg + ", 유통구조";
					} else {
						alertMsg = "유통구조";
						this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(18,iLoop);
						this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();					
					}
				}
				
				var iRow = iLoop + 1;
				var arrMsgArg = [iRow+"번째행 "+alertMsg];
				this.gfn_Alert("SM008", arrMsgArg);//연간단가, 통상납품일수는 필수 입력 항목입니다.
				return false;
				
				break;			
			}
		}
	}
	
	return true;
}

