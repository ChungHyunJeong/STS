		sYprbGu = this.ds_Detail10.getColumn(iLoop, "YPRB_GU");//유통구조
		
		if(this.gfn_IsNull(sYprbGu) || sYprbGu == ""){
			alertMsg = "유통구조";
			var arrMsgArg = [iLoop+"번째행 "+alertMsg];
			this.ds_Detail10.set_rowposition(iLoop);
			//this.gfn_Alert("SM008", arrMsgArg);//연간단가, 유통구조 필수 입력 항목입니다.
			//return false;				
