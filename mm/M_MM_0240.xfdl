 * 15.14. [메시지 발송] 연간단가제출시 잔디메시지 발송
 ***********************************************************************************************/ 
this.fn_SendJandiMessage = function()
{
	/* 메시지 내용
	[SynusTech-iERP] 연간단가입력.
	- 요청번호 : 220317-1001
	- 파트너사 : 주식회사신성씨에스
	연간단가를 제출하였습니다.
	*/

	//제목 셋팅
	var sTitlNm = "연간단가입력.";

	//요청번호 셋팅
	var sJandNo = this.gfn_StringFormat(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "YPRH_NO"), "@@@@@@-@@@@");	//요청번호

	//파트너사 셋팅
	var sRequNm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_NM");	//파트너사명

	//내용 셋팅
	var sCocfNm = "연간단가를 제출하였습니다.";

	//잔디 전송 환경
    var sRECV_ID = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SYST_ID");//받는사람 (요청자 시스템ID)
    var sEMAL_NM = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EMAL_NM");//이메일주소 (요청자 이메일)
    var sbody = "[알림]";//최상단에 노출할 메시지
    var stitle = "[SynusTech-iERP] "+sTitlNm;//메시지 하단 말풍선 내 최상단 메시지
    var sdescription = "- 요청번호 : " + sJandNo + "\n- 파트너사 : " + sRequNm + "\n" + sCocfNm;//메시지 하단 말풍선 내 중간 메시지

    //잔디전송 내용 데이터셋 초기화
    nApp.gds_SendJandi.clearData();      

    //전송 이메일 정보가 있으면
    if (!this.gfn_IsNull(sEMAL_NM)) {

       var paramObj = new Object();//알람메세지 파라메타 Object
       paramObj.PRGM_ID = "M_MM_0240";//프로그램ID
       paramObj.SYST_ID = this.gfn_GetSessionValue("SYST_ID");//보내는사람
	   paramObj.RECV_ID = sRECV_ID;//받는사람
       paramObj.EMAL_NM = sEMAL_NM;//이메일
       paramObj.TITL_NM = stitle;//알람제목
       paramObj.ALRH_NM = sdescription;//알람내용
       this.gfn_AlrhMsgSave(paramObj);//알람메세지정보 저장        
	   
    }

};

