this.fn_UnitStatusCheckSearch = function(sChekSt)
{
	//제출시 진행상태 단가요청/결재상태 신청 OR 취소시 진행상태 단가제출/결재상태 승인인지 DB체크
	this.ds_Master10Check.clearData();		//결재상태 확인
	this.ds_Input.clearData();
	
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "YPRH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "YPRH_NO")); //관리번호
	this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ")); //결재차수
	this.ds_Input.setColumn(0, "REQU_ST", "0020"); 	//요청등록(0010), 요청확정(0020)
	if (sChekSt == 'F') {			//[제출]시 체크 (요청확정이고, 파트너사 미확정 대상 검색)
		this.ds_Input.setColumn(0, "PART_ST", "0010"); 	//단가요청(0010), 단가제출(0020), 단가확정(0030)
	} else if (sChekSt == 'C') {	//[취소]시 체크 (요청확정이고, 파트너사 확정 대상 검색)
		this.ds_Input.setColumn(0, "PART_ST", "0020"); 	//단가요청(0010), 단가제출(0020), 단가확정(0030)
	}

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "mm:M_MM_0240_M_SL11";	//연간단가 단가제출/제출취소 가능여부 Check
	this.gfn_AddComSearchService(this, svcAddObj);

	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	if (sChekSt == 'F') {			//[제출]시 체크
		svcCallObj.sTrid = "FIX_CHECK";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	} else if (sChekSt == 'C') {	//[취소]시 체크
		svcCallObj.sTrid = "CANCEL_CHECK";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	}
	svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Master10Check=ds_Master11";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);		

};

