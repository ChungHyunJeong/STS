 * 15.13. 엑셀업로드-콜백함수
 ***********************************************************************************************/ 
this.fn_ImportCallback = function(sImportId)
{	
	this.ds_Detail10.set_enableevent(false);
	
	for (var iLoop=0; iLoop<this.ds_Import.getRowCount(); iLoop++) {
		
		var col14 = nexacro.toNumber(this.gfn_Nvl(this.ds_Import.getColumn(iLoop, "Column15"), 0));	//연간단가
		var col15 = this.gfn_Nvl(this.ds_Import.getColumn(iLoop, "Column16"), "0");					//연간단가제외
		var col16 = nexacro.toNumber(this.gfn_Nvl(this.ds_Import.getColumn(iLoop, "Column17"), 0));	//통상납품일수
		var col17 = this.ds_Import.getColumn(iLoop, "Column18");									//비고(파트너사)
		
		if (col15 != "0") col15 = "1";	//연간단가제외값이 0이 아닌경우 1로 변경
		
		this.ds_Detail10.setColumn(iLoop, "YPRH_PR", col14); //연간단가
		this.ds_Detail10.setColumn(iLoop, "PTEX_YN", col15); //연간단가제외
		this.ds_Detail10.setColumn(iLoop, "YRPB_CT", col16); //통상납품일수
		this.ds_Detail10.setColumn(iLoop, "PART_NM", col17); //비고(파트너사)
		
		this.ds_Detail10.setColumn(iLoop, "crudType", "U");
		
	} 
	
	this.ds_Detail10.set_enableevent(true);  
}

