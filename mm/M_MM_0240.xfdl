 * 15.10. [환경설정] 진행상태에 따른 컴포넌트 Enable 등 처리
 ***********************************************************************************************/ 
this.fn_SetFrameCompEnable = function()
{
	var sPartSt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PART_ST"); //진행상태(단가요청(0010), 단가제출(0020), 단가확정(0030))
	var sWofhSt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"); //결재상태
	var sRequDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "REQU_DD"); //요청일자
	var toDay = this.gfn_GetCommDate("toDay"); //당일
	var iTabIndex = this.div_DetailInput.form.tab_Detail.tabindex;

	//단가요청시
	if (sPartSt == '0010' && sWofhSt == '0010' && sRequDd >= toDay) {	//진행상태 (단가요청(0010), 단가제출(0020), 단가확정(0030))
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, true);//Master 입력 프리폼
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form, true);//Detail 입력			
		
		this.div_MasterInput.form.btn_SelectFix.set_enable(true);		//단가제출
		this.div_MasterInput.form.btn_SelectCancel.set_enable(false);	//제출취소

		//항목 Enable처리
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ConfNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ConfDd.set_enable(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_YprhNo.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_YprhDd.set_enable(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_RequDd.set_enable(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_YprhYy.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FromDd.set_enable(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.set_enable(false);
		
		this.fvFixYn = false;	//제출여부
		if (iTabIndex == 0) {
			this.div_DetailInput.form.btn_ExcelDown.set_visible(true);     //엑셀다운로드
			this.div_DetailInput.form.btn_ExcelUpload.set_visible(true);   //엑셀업로드
		} else {
			this.div_DetailInput.form.div_ComGrdBtn.set_visible(true); 	//첨부파일
		}
		
	//단가제출시
	} else if (sPartSt == '0020' && sWofhSt == '0010' && sRequDd >= toDay) {	//진행상태 (미확정(0010), 확정(0020))
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 입력			
		
		this.div_MasterInput.form.btn_SelectFix.set_enable(false);		//단가제출
		this.div_MasterInput.form.btn_SelectCancel.set_enable(true);	//제출취소	
				
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_RemkPop.set_enable(true);

		this.fvFixYn = true;	//제출여부
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(false); 	//첨부파일
		this.div_DetailInput.form.btn_ExcelDown.set_visible(false);     //엑셀다운로드
		this.div_DetailInput.form.btn_ExcelUpload.set_visible(false);   //엑셀업로드
	//그외 (단가확정)
	} else { 
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 입력					
	
		this.div_MasterInput.form.btn_SelectFix.set_enable(false);		//단가제출
		this.div_MasterInput.form.btn_SelectCancel.set_enable(false);	//제출취소
		
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_RemkPop.set_enable(true);

		this.fvFixYn = true;	//제출여부
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(false); 	//첨부파일
		this.div_DetailInput.form.btn_ExcelDown.set_visible(false);     //엑셀다운로드
		this.div_DetailInput.form.btn_ExcelUpload.set_visible(false);   //엑셀업로드
	}
	
};


