 ***********************************************************************************************/
/***********************************************************************************************
 * 00.01. Form Include 선언
 ***********************************************************************************************/
include "comLib::CommonUtil.xjs";//공통함수
include "comLib::lib_splitter.xjs";            

/***********************************************************************************************
 * 00.02. Form 변수 선언
 ***********************************************************************************************/
this.fvComPrgmId = "";//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
this.fvComMenuId = "";//[선택]메뉴ID-메인화면기준, 팝업일경우 해당 메인화면의 메뉴ID => 한화면을 여러 메뉴에서 사용하는 화면에서는 꼭 사용해야함
this.fvFormComBtnAuthYn = false;//그리드 공통버튼 권한 유무
this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
this.fvSchKeyYn = false;//저장후 재조회여부
this.fvSchKey01 = "";//조회Key01
this.fvSchKey02 = "";//조회Key02
this.fvSchKey03 = "";//조회Key03
this.fvSchKey04 = "";//조회Key04

this.fvPlanChange = false;//계획변경
this.fvMasterSearchMode = false;//마스터내역 조회중여부

//첨부파일 탭이 없음

/***********************************************************************************************
 * 00.03. Form Onload 이벤트
 ***********************************************************************************************/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{
	//[팝업]호출 파라메타 처리 (비활성화)
	this.fvPopupGu = this.getOwnerFrame().paramPopupGu;
	this.fvPcohNo = this.getOwnerFrame().paramPcohNo;
	
	this.fvPcrhNo = this.getOwnerFrame().paramPcrhNo; // 청구번호 ( 정충현 추가 - 2022022 )
	this.fvPcrbSe = this.getOwnerFrame().paramPcrbSe; // 청구순번 ( 정충현 추가 - 2022022 )

	//[팝업]호출하지 않았을 경우
	if (this.gfn_IsNull(this.fvPopupGu)) {
		this.fvPopupGu = false;		//팝업 호출 없을경우
	}

	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 
}

/***********************************************************************************************
 * 00.04. Form [공통] 초기설정 함수 - 필수
 ***********************************************************************************************/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	this.fvComPrgmId = this.gfn_CommLoadFormPrgmId(this);//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID	
	this.fvComMenuId = this.gfn_CommLoadFormMenuId(this);//[선택]메뉴ID-메인화면기준, 팝업일경우 해당 메인화면의 메뉴ID => 한화면을 여러 메뉴에서 사용하는 화면에서는 꼭 사용해야함
	// 그리드에서 오른쪽 마우스 버튼 사용
	this.gfn_SetGrid(this.div_MasterView.form.grd_Master10);//[공통]그리드 이벤트 설정 - 발주목록
	this.gfn_SetGrid(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);//[공통]그리드 이벤트 설정 - 납기예정일상세
	//스플리터 초기화 함수 호출
	this.gfn_InitSpliter(this.sta_HSplitter, "horizontal", -1, -1);
	
	//스플리터 초기화 함수 호출
	this.gfn_InitSpliter(this.sta_VSplitter, "vertical", -1, -1);
	//권한체크 할 컴포넌트 : 컴포넌트 Full Path
	var objCompCheckList = ["this.div_DetailInput.form.div_ComGrdBtn"];		
	
	if (this.gfn_CommCompAuthCheck(this, objCompCheckList))
	{
		this.fvFormComBtnAuthYn = true;//그리드 공통버튼 권한 유무
	}
	
	this.fn_SetGridInputButton();//[공통] 그리드 입력 버튼 설정 
					 
}

/***********************************************************************************************
 * 00.05. Form [업무] 초기설정 함수
 ***********************************************************************************************/
this.fn_FormBizInit = function() 
{ 
	//[공통] 날짜정보 설정	
	this.gfn_SetCommFromToDate(this.fvComPrgmId, "MS", this.div_Search.form.cal_FromDd, this.div_Search.form.cal_ClosDd);//조회조건영역	
	
	//[공통] 부서정보 설정
	var arrComp = ["this.div_Search.form.edt_DepmCd^this.div_Search.form.edt_DepmNm"
				  ];
	this.gfn_SetCommDepmCd(this.fvComMenuId, arrComp);//한화면을 여러메뉴에서 사용하는 경우 권한 체크는 메뉴기준을 따름
	
	//[공통] 사원정보 설정
	var arrComp = ["this.div_Search.form.edt_EmpmCd^this.div_Search.form.edt_EmpmNm^this.div_Search.form.edt_SystId"
                  ];
	this.gfn_SetCommEmpmCd(this.fvComMenuId, arrComp);//한화면을 여러메뉴에서 사용하는 경우 권한 체크는 메뉴기준을 따름
	
	//파트너사로 로그인시처리 - 비활성화
	if (this.gfn_GetSessionValue("CMDS_CD") == "P") {
		this.div_Search.form.edt_CusmCd.set_value(this.gfn_GetSessionValue("CUSM_CD"));
		this.div_Search.form.edt_CusmNm.set_value(this.gfn_GetSessionValue("CUSM_NM"));	
		this.div_Search.form.edt_CusmCd.set_readonly(true);
		this.div_Search.form.edt_CusmNm.set_readonly(true);
		this.div_Search.form.btn_CusmCd.set_enable(false);
		
		//계약번호가 있을경우 계약번호 팝업버튼 비활성화
		this.div_Search.form.btn_CnthNo.set_enable(false);
	}	
	

	//[팝업]호출시 자동조회
	if (this.fvPopupGu) {
		this.fn_ComBtnSearch();
	}

}

/***********************************************************************************************
 * 00.06. Form [업무] 일반콤보 조회 함수
 ***********************************************************************************************/


/***********************************************************************************************
 * 00.07. Form OnClose 이벤트
 ***********************************************************************************************/
this.fn_FormClose = function(obj:nexacro.Form,e:nexacro.CloseEventInfo)
{
	return true;
};

/***********************************************************************************************
 * 1. 조회 영역
 ***********************************************************************************************/
/***********************************************************************************************
 * 01.01. [공통버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.fn_ComBtnSearch = function()
{ 
	this.fvMasterSearchMode = false;//마스터내역 조회중여부

	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;	

	//저장후 재조회시-마스터 데이터셋 이벤트 비활성화
	if (this.fvSchKeyYn) {
		this.ds_Master10.set_enableevent(false);
	}

	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Master10.clearData();
	this.ds_Detail10.clearData();
	this.ds_Detail11.clearData();
	this.ds_Detail10Update.clearData();
	this.ds_Input.clearData();
	
	//[업무] 조회조건 입력 : 견적의뢰관리 대상 필수값
	this.ds_Input.addRow();
	var sSqlId = "";//[팝업]쿼리ID

	//[팝업] 발주업체선정 호출 팝업여부
	if (!this.fvPopupGu) {		
		this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_FromDd.value);
		this.ds_Input.setColumn(0, "CLOS_DD", this.div_Search.form.cal_ClosDd.value);
		this.ds_Input.setColumn(0, "PCOH_NO", this.div_Search.form.edt_PcohNo.value);
		this.ds_Input.setColumn(0, "CNTH_NO", this.div_Search.form.mae_CnthNo.value);
		this.ds_Input.setColumn(0, "CUSM_CD", this.div_Search.form.edt_CusmCd.value);
		this.ds_Input.setColumn(0, "DEPM_CD", this.div_Search.form.edt_DepmCd.value);
		this.ds_Input.setColumn(0, "SESU_ID", this.div_Search.form.edt_SystId.value);
		this.ds_Input.setColumn(0, "PCRH_NO", this.div_Search.form.edt_PcrhNo.value);
		sSqlId = "mm:M_MO_3400_M_SL10";//쿼리ID
	} else {
		this.ds_Input.setColumn(0, "PCOH_NO", this.fvPcohNo);//발주번호
		this.ds_Input.setColumn(0, "PCRH_NO", this.fvPcrhNo);//청구번호
		this.ds_Input.setColumn(0, "PCRB_SE", this.fvPcrbSe);//청구순번
		sSqlId = "mm:M_MO_3400_M_SL11";//쿼리ID
	}

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = sSqlId;	//[팝업]
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MST10_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
