	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	} 

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	
	//초기설정값
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자		
	var sToDayFull = this.gfn_GetCommDate("toDayFull");//현재일자 Full
	this.ds_Detail10.reset(); //리셋
	
	//계획변경으로 차수증가 이전대상
	var nPrevRow = this.ds_Detail10.rowposition; //이전 Row
	var sPcocSe = nexacro.toNumber(this.ds_Detail10.getColumn(nPrevRow,"PCOC_SE")); //납품순번
	var sPcocSq = nexacro.toNumber(this.ds_Detail10.getColumn(nPrevRow,"PCOC_SQ")); //납품차수

	//ds_Detail10Update에 최종여부 대상 복사함
	this.ds_Detail10Update.clearData();
	this.ds_Detail10Update.copyData(this.ds_Detail10);	//업데이트 대상 복사
	
	for (var iLoop=0; iLoop<this.ds_Detail10Update.getRowCount(); iLoop++) {
		this.ds_Detail10Update.setColumn(iLoop, "crudType", "U"); //crudType 수정
	}
	
	//변경차수 증가
	sPcocSq = sPcocSq + 1;

	//변경차수, 로그인사용자/등록일시 수정
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {		
		this.ds_Detail10.setColumn(iLoop, "PCOC_SQ", sPcocSq);	//변경차수(납품차수)
		this.ds_Detail10.setColumn(iLoop, "WORH_YN", "1");		//최종여부
		this.ds_Detail10.setColumn(iLoop, "REGI_NM", this.gfn_GetSessionValue("USEM_NM"));	//로그인사용자
		this.ds_Detail10.setColumn(iLoop, "REGI_DT", sToDayFull); 							//등록일시 초기화
		this.ds_Detail10.setColumn(iLoop, "crudType", "I");	 	 //증가대상 crudType = 'I'
	}
	
	//계획변경 상태
	this.fvPlanChange = true;//계획변경
	
	//계획변경 비활성화, 분할 활성화
	this.div_DetailInput.form.btn_PlanChange.set_enable(false); //계획변경
	this.div_DetailInput.form.btn_TaskSplit.set_enable(true); //분할

	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무	

};



/***********************************************************************************************
 * 15.07. [버튼] Detail 분할
 ***********************************************************************************************/ 
this.div_DetailInput_btn_TaskSplit_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{ 
	
	//계획변경여부
	if (this.fvPlanChange) {

		//초기설정
		var sToDay = this.gfn_GetCommDate("toDay");//현재일자		
		var sToDayFull = this.gfn_GetCommDate("toDayFull");//현재일자 Full
		
		//분할대상 	
		var nLastRow = this.ds_Detail10.getRowCount() - 1 ; //마지막 Row 
		var iPcocSe = nexacro.toNumber(this.ds_Detail10.getColumn(nLastRow, "PCOC_SE")); //납품순번(최종)
		var iPcocQtSum = this.ds_Detail10.getCaseSum("crudType != 'D'", "PCOC_QT"); //납품수량 합계
		var iPcobQt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCOB_QT"); //발주수량
		var iPcocQt = 0; // 남은 납품수량(계산)

		//납품수량 >= 발주수량 return
		if (iPcocQtSum >= iPcobQt) {
			var arrMsgArg = ["납품수량 합계","발주수량","수량"];
			this.gfn_Alert("SM060", arrMsgArg); //납품수량 합계가 발주수량 이상의 수량을 입력 할 수 없습니다
		
			return;
		} else {		
			iPcocQt = nexacro.toNumber(iPcobQt) - nexacro.toNumber(iPcocQtSum) ; // 남은 납품수량 = 발주수량 - 납품수량합계
		}

		this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무

		//신규추가
		var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)
		
		//납품순번 증가
		iPcocSe = iPcocSe + 1;
		
		//분할대상 셋팅
		this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));	//회사코드
		this.ds_Detail10.setColumn(iAddRow, "PCOH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCOH_NO"));	//발주번호
		this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));	//발주차수
		this.ds_Detail10.setColumn(iAddRow, "PCOB_SE", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCOB_SE"));	//발주순번		
		this.ds_Detail10.setColumn(iAddRow, "PCOC_SE", iPcocSe);	//납품순번
		this.ds_Detail10.setColumn(iAddRow, "PCOC_SQ", this.ds_Detail10.getColumn(0, "PCOC_SQ"));	//납품차수
		this.ds_Detail10.setColumn(iAddRow, "WORH_YN", "1");		//최종여부		
		this.ds_Detail10.setColumn(iAddRow, "PCOC_DD", sToDay);	    //변경일자
		this.ds_Detail10.setColumn(iAddRow, "PCOC_QT", nexacro.round(iPcocQt,2));	//남은 납품수량
		this.ds_Detail10.setColumn(iAddRow, "REGI_NM", this.gfn_GetSessionValue("USEM_NM"));	 	//로그인사용자
		this.ds_Detail10.setColumn(iAddRow, "REGI_DT", sToDayFull); 							    //등록일시 초기화
		this.ds_Detail10.setColumn(iAddRow, "DELE_YN", "0");	    //삭제여부
			
		//계획변경 비활성화
		this.div_DetailInput.form.btn_PlanChange.set_enable(false); //계획변경

		this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무	
	}
};

/***********************************************************************************************
 * 15.07. [SET] Detail 계획변경, 분할 버튼 활성화/비활성화
 ***********************************************************************************************/ 
this.fn_SetEnablePlanSplitButton = function()
{
	//Detail 계획변경, 분할 버튼 제어
	var sPcohNo = this.ds_Detail10.getColumn(0, "PCOH_NO"); //발주번호
	var sWorhYn = this.ds_Detail10.getColumn(0, "WORH_YN"); //최종여부

	//계획변경, 분할 버튼 활성화/비활성화
	if (this.ds_Detail10.getRowCount() > 0 && !this.gfn_IsNull(sPcohNo) && sWorhYn == '1' && !this.fvPlanChange) { //발주번호가 존재하고, 최종여부가 1이고, 계획변경이 아니면
		//계획변경 활성화, 분할 비활성화
		this.div_DetailInput.form.btn_PlanChange.set_enable(true); //계획변경
		this.div_DetailInput.form.btn_TaskSplit.set_enable(false); //분할
		
	} else if (this.fvPlanChange) {	//계획변경이면 
		//계획변경 비활성화, 분할 활성화
		this.div_DetailInput.form.btn_PlanChange.set_enable(false); //계획변경
		this.div_DetailInput.form.btn_TaskSplit.set_enable(true);  //분할
		
	} else {
		//계획변경 비활성화, 분할 비활성화
		this.div_DetailInput.form.btn_PlanChange.set_enable(false); //계획변경
		this.div_DetailInput.form.btn_TaskSplit.set_enable(false);  //분할
	}
};			


/***********************************************************************************************
 * 15.08. [이벤트] 이력포함 체크박스 클릭시 조회
 ***********************************************************************************************/ 
this.div_DetailInput_chk_WorhYn_onclick = function(obj:nexacro.CheckBox,e:nexacro.ClickEventInfo)
{
	if (this.ds_Detail10.getRowCount() > 0) {	//기존에 Detail10이 존재할 경우
	
		//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
		this.ds_Detail10.clearData();		//납기예정일상세
		this.ds_Input.clearData();
		
		this.ds_Input.addRow();
		this.ds_Input.setColumn(0, "PCOH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCOH_NO"));
		this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
		this.ds_Input.setColumn(0, "PCOB_SE", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCOB_SE"));
		this.ds_Input.setColumn(0, "WORH_YN", this.div_DetailInput.form.chk_WorhYn.value);
				
		//[공통]서비스정보 초기화
		this.gfn_InitComService(this);
		
		//[공통]서비스정보 추가
		var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
		svcAddObj.sSelectSql = "mm:M_MO_3400_D_SL10";	//납기예정일상세
		this.gfn_AddComSearchService(this, svcAddObj);
		
		//[공통]서비스 호출 변수
		var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
		svcCallObj.sTrid = "DTL10_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
		svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
		svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
		svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10";//출력 데이터셋
		svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

		//[공통]서비스 호출
		this.gfn_CallComService(this, svcCallObj);		
	}
	
};



