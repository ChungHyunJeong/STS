	var oArg = {ProjectFolder:"MM"        //레포트저장 폴더
			  , ReportFile:sReportFile //레포트파일명
			  , ReportDs:dsObj            //레포트파라메타 - 데이터셋
			  , ReportParam:paObj         //레포트파라메타 - 변수
			   };                         //Popup화면 전달 파라메터
			   
	this.gfn_ReportPop("REPORT", oArg, "fn_PopupCallback");//레포트 팝업 호출	

