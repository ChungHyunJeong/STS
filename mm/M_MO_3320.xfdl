this.div_MasterInput_btn_CorhM1_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		var arrMsgArg = [""];
		this.gfn_Alert("SM002", arrMsgArg);//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.
		return false;
	}	
	
	if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTH_NO"))) {
	
		//[기성확정제어-확정]
		var sBUDG_MM = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COCO_DD"), "");//마감년월
		
