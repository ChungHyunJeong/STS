		this.fvArrWofhSq = new Array();//결재차수 Array
		this.fvArrReportParam = new Array();//레포트파라메타 Array
		
		//멀티건 선택시
		if (sKeyNoMulti.indexOf(";") > 0) {
		
			var arrKeyNo = sKeyNoMulti.split(";");
			var arrWofhSq = sWofhSqMulti.split(";");		
			
			for (var iLoop=0; iLoop <arrKeyNo.length; iLoop++) {
				this.fvArrKeyNo.push(arrKeyNo[iLoop]);//키번호 Array
				this.fvArrWofhSq.push(arrWofhSq[iLoop]);//결재차수 Array
			}
		} else {
			this.fvArrKeyNo.push(sKeyNoMulti);//키번호 Array
			this.fvArrWofhSq.push(sWofhSqMulti);//결재차수 Array		
		}

		if (this.fvArrKeyNo.length > 0) {
		
			this.div_DetailInput.form.pgb_PrintState.set_visible(true);
			this.div_DetailInput.form.pgb_PrintState.set_pos(0);			
			this.div_DetailInput.form.pgb_PrintState.set_min(0);
			this.fvPrintCnt = this.fvArrKeyNo.length;//출력건수
			this.div_DetailInput.form.pgb_PrintState.set_max(this.fvPrintCnt);

			sKeyNo = this.fvArrKeyNo.shift();//배열에서 처음내용 꺼냄 - 키번호
			sWofhSq = this.fvArrWofhSq.shift();//배열에서 처음내용 꺼냄 - 결재차수
			this.fvMultiSearchYn = true;//멀티인쇄조회중여부
			trace(sKeyNo);
			// 조회 
			this.fn_ComPrintSearch(sKeyNo, sWofhSq);	
		
			this.setTimer(900, 200);
		}
	} else {
		this.gfn_Alert("SM083");//출력할 정보가 없습니다.
	}
	
}

