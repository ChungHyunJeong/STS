this.fn_SendJandiMessage = function()
{

	//계약번호 셋팅
	var sCnthNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO");
	if (sCnthNo.length > 8) 
		sCnthNo = this.gfn_StringFormat(sCnthNo, "@@@@-@-@@@@");	//계약번호
	else 
		sCnthNo = this.gfn_StringFormat(sCnthNo, "@@@-@@@@");		//계약번호
		
	//요청자 셋팅 (로그인한사람 - 부서명/이름/직위)
	var sRequNm = this.gfn_GetSessionValue("DEPM_NM") + " " + this.gfn_GetSessionValue("EMPM_NM") + this.gfn_GetSessionValue("POSI_NM");
	
	//재검토사유 셋팅
	var sCocfNm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COCF_NM");

	//잔디 전송 환경
    var sRECV_ID = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SYST_ID");//받는사람 (검토자 시스템ID)
    var sEMAL_NM = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EMAL_NM");//이메일주소 (검토자 이메일)
    var sbody = "[알림]";//최상단에 노출할 메시지
    var stitle = "[SynusTech-iERP] 외주기성 재검토요청.";//메시지 하단 말풍선 내 최상단 메시지
    var sdescription = "  - 계약번호 : " + sCnthNo + "\n- 요청자 : " + sRequNm + "\n- 재검토사유 : \n" + sCocfNm;//메시지 하단 말풍선 내 중간 메시지

    //잔디전송 내용 데이터셋 초기화
    nApp.gds_SendJandi.clearData();      
		 
    //전송 이메일 정보가 있으면
    if (!this.gfn_IsNull(sEMAL_NM)) {

       var paramObj = new Object();//알람메세지 파라메타 Object
       paramObj.PRGM_ID = "M_MO_3320";//프로그램ID
       paramObj.SYST_ID = this.gfn_GetSessionValue("SYST_ID");//보내는사람
	   paramObj.RECV_ID = sRECV_ID;//받는사람
       paramObj.EMAL_NM = sEMAL_NM;//이메일
       paramObj.TITL_NM = stitle;//알람제목
       paramObj.ALRH_NM = sdescription;//알람내용
       this.gfn_AlrhMsgSave(paramObj);//알람메세지정보 저장        
	   
    }

};


