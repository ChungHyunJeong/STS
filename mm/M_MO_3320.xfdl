/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{


	if (e.newrow > -1) {
		trace("멀티선택여부->"+this.fvChkMultiWfYn)
		//멀티선택여부
		if (this.fvChkMultiWfYn) return;
		
		//기성검토정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTH_NO"))) {
			
			//[결재] 결재상태에 따른 컴포넌트 제어
			this.fn_SetWfCompEnable();		
			
			//기성검토 format 변경 (10자리, 20자리)
			var strCothNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTH_NO")
			if (strCothNo.length == 10)
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CothNo.set_format("######-####");
			else
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CothNo.set_format("####-@-####-###########");
			
			//계약번호 format 변경 (9자리, 7자리)
			var strCnthNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO");
			if (strCnthNo.length == 9) 
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_format("####-@-####");		
			else 
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_format("@##-####");
				
		
			//기성확정이 아니면 기본정보 표시
			//trace(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTH_ST"));
			if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTH_ST") != "0020"){
				/*this.ds_Master10.setColumn(this.ds_Master10.rowposition, "COCO_DD", sToDay);//기성검토 일자-오늘날자로 설정
				
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CODP_CD", this.gfn_GetSessionValue("DEPM_CD"));//등록부서코드	
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CODP_NM", this.gfn_GetSessionValue("DEPM_NM"));//등록부서명	
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "COSU_CD", this.gfn_GetSessionValue("USEM_ID"));//등록자사번	
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "COSU_NM", this.gfn_GetSessionValue("USEM_NM"));//등록자명		
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "COSU_ID", this.gfn_GetSessionValue("SYST_ID"));//등록자ID	
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "COCO_DD", sToDay);//기성검토 일자-오늘날자로 설정
				
				//기본정보 표시로 인한 ROW TYPE 초기화
				this.ds_Master10.set_updatecontrol(false);
				this.ds_Master10.setRowType(this.ds_Master10.rowposition, Dataset.ROWTYPE_NORMAL); // ROW TYPE 초기화
				this.ds_Master10.set_updatecontrol(true);
				*/
			}
			
			//return;
			
		
		
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "COTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTH_NO"));
			this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
			this.ds_Input.setColumn(0, "CUSM_CD", this.div_Search.form.edt_CusmCd.value);	//거래처
			//this.ds_Input.setColumn(0, "PRGM_ID", this.fvComPrgmId);
				
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "mm:M_MO_3320_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);
			
		
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);		
		}
	}
};

