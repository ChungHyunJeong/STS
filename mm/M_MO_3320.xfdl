	var sKeyNoMulti  = "";//키번호-다건
	var sWofhSqMulti  = "";//결재차수-다건
	var sKeyNo  = "";//키번호
	var sWofhSq  = "";//결재차수
	
	//체크된 내역이 있으면
	if (this.ds_Master10.getCaseCount("s_chk=='1'") > 0) {
		
