		if (this.ds_Master10.getCaseCount("s_chk=='1'") > 100 ) {
			this.gfn_Alert("SM121");//일괄인쇄는 최대 100건까지 가능합니다.
			return;
		}
	  
		this.div_DetailInput.form.pgb_PrintState.set_min(0);
		
		
		for (var iLoop=0; iLoop<this.ds_Master10.getRowCount(); iLoop++) {

			//체크된 내용만
			if (this.ds_Master10.getColumn(iLoop, "s_chk") == "1") {
				sKeyNo = this.ds_Master10.getColumn(iLoop, "COTH_NO");//키번호
				sWofhSq = this.ds_Master10.getColumn(iLoop, "WOFH_SQ");//결재차수
				if (this.gfn_IsNull(sKeyNoMulti)) {
					sKeyNoMulti = sKeyNo;
					sWofhSqMulti = sWofhSq;
				} else {
					sKeyNoMulti = sKeyNoMulti + ";" + sKeyNo;
					sWofhSqMulti = sWofhSqMulti + ";" + sWofhSq;
