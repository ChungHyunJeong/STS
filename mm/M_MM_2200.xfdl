/***********************************************************************************************
 * 05.03. [Vaildation] Detail10 저장 필수값 체크 - 그리드항목
 ***********************************************************************************************/ 
this.fn_VaildDetailSave = function()
{

	//[저장] Validation 설정
	var arrArgs = [//"SLCS_CD^NULL"
	               "QUTB_PR^NULL"
			      ,"WONE_AM^NULL"
			      ]; 
				  
	var sCheckGroupTitle = this.div_DetailInput.form.tab_Detail.tpg_01.text;//체크그룹 타이틀
				  
	//Grid Vaild 체크
// 	if (!this.gfn_CheckGridValid(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10, arrArgs, sCheckGroupTitle) ) {
// 		return false;
// 	}
	
	var PcrbQt = 0;
	var SlcsCd = "";
	
	var SlcsCd = "";
	var ItemCd = "";
	var nRow   = 0;
	var iLoopChk = false;
	
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		PcrbQt = this.ds_Detail10.getColumn(iLoop, "PCRB_QT");
		PcrbQt = nexacro.toNumber(PcrbQt);
		if(this.gfn_IsNull(PcrbQt)) {PcrbQt = 0;}
		
		SlcsCd = this.ds_Detail10.getColumn(iLoop, "SLCS_CD");
		
		if(PcrbQt != 0 && (this.gfn_IsNull(SlcsCd) || SlcsCd =="")){
			var arrMsgArg = ["파트너사"];
			this.gfn_Alert("SM008",arrMsgArg);
			return false;
		}		
	
		if(PcrbQt == 0 && !this.gfn_IsNull(SlcsCd) && SlcsCd !=""){
			iLoopChk = true;		
			
		}
	}
	
	if(iLoopChk) {
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
				PcrbQt = this.ds_Detail10.getColumn(iLoop, "PCRB_QT");
				PcrbQt = nexacro.toNumber(PcrbQt);
				if(this.gfn_IsNull(PcrbQt)) {PcrbQt = 0;}
				
				SlcsCd = this.ds_Detail10.getColumn(iLoop, "SLCS_CD");
			
				if(PcrbQt == 0 && !this.gfn_IsNull(SlcsCd) && SlcsCd !=""){					
					SlcsCd = this.ds_Detail20.getColumn(iLoop, "SLCS_CD");
					ItemCd = this.ds_Detail20.getColumn(iLoop, "ITEM_CD");			
					
					this.ds_Detail20.setColumn(iLoop, "SLCS_CD", null);
					this.ds_Detail20.setColumn(iLoop, "SLCS_NM", null);
					this.ds_Detail20.setColumn(iLoop, "QUTB_PR", 0);
					this.ds_Detail20.setColumn(iLoop, "WONE_AM", 0);
					this.ds_Detail20.setColumn(iLoop, "FORE_AM", 0);
					this.ds_Detail20.setColumn(iLoop, "QUSH_NO", null);
					this.ds_Detail20.setColumn(iLoop, "QUSH_SQ", null);
					
					nRow = this.ds_Detail10.findRowExpr("SLCS_CD== '" + SlcsCd + "'  &&  ItemCd == '" + ItemCd + "'  && PCRB_QT == 0");											
								
					//this.alert(nRow);	
					if(nRow > -1){
						this.ds_Detail10.setColumn(nRow, "SLCS_CD", null);
						this.ds_Detail10.setColumn(nRow, "SLCS_NM", null);
						this.ds_Detail10.setColumn(nRow, "QUTB_PR", 0);
						this.ds_Detail10.setColumn(nRow, "WONE_AM", 0);
						this.ds_Detail10.setColumn(nRow, "FORE_AM", 0);
						this.ds_Detail10.setColumn(nRow, "QUSH_NO", null);
						this.ds_Detail10.setColumn(nRow, "QUSH_SQ", null);
					}
					
				}
			}			
		var arrMsgArg = ["청구수량이 0이면 파트너사를 등록할 수"];
		this.gfn_Alert("MM051",arrMsgArg);
		return false;

	}

	return true;
}

/***********************************************************************************************
 * 05.04. [Vaildation] 업체확정 필수값 체크 - 그리드항목
 ***********************************************************************************************/ 
this.fn_VaildSelectFixSave = function()
{
	var sSlcsCd = 0;	//거래처
	var iQutbPr = "";	//견적단가
	var iWoneAm = "";	//견적금액
	var PcrbQt  = 0;
	var nRow   = 0;
	
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		sSlcsCd = this.ds_Detail10.getColumn(iLoop, "SLCS_CD");	//거래처
		iQutbPr = this.ds_Detail10.getColumn(iLoop, "QUTB_PR");	//견적단가
		iWoneAm = this.ds_Detail10.getColumn(iLoop, "WONE_AM");	//견적금액
		
		PcrbQt = this.ds_Detail10.getColumn(iLoop, "PCRB_QT");
		PcrbQt = nexacro.toNumber(PcrbQt);
		if(this.gfn_IsNull(PcrbQt)) {PcrbQt = 0;}

		//-- 청구수량이 0이 아닐떄만 체크
		if(PcrbQt != 0){
			//거래처
			if (this.gfn_IsNull(sSlcsCd)) {	
				//탭, 위치 이동
				this.div_DetailInput.form.tab_Detail.set_tabindex(1);			
				this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.setCellPos(9,iLoop);
				this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.setFocus();
				
				var arrMsgArg = ["거래처"];
				this.gfn_Alert("SM008", arrMsgArg);//거래처은 필수 입력 항목입니다.
				return false;
				
				break;			
			}		
		
			//견적단가
			if (this.gfn_IsNull(iQutbPr) || iQutbPr < 1) {		
				//탭, 위치 이동
				this.div_DetailInput.form.tab_Detail.set_tabindex(1);			
				this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.setCellPos(10,iLoop);
				this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.setFocus();
				
				var arrMsgArg = ["견적단가"];
				this.gfn_Alert("SM055", arrMsgArg);//견적단가를 1이상 입력해주세요.
				return false;
				
				break;			
			}

			
			//견적금액
			if (this.gfn_IsNull(iWoneAm) || iWoneAm < 1) {		
				//탭, 위치 이동
				this.div_DetailInput.form.tab_Detail.set_tabindex(1);			
				this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.setCellPos(11,iLoop);
				this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.setFocus();
				
				var arrMsgArg = ["견적금액"];
				this.gfn_Alert("SM055", arrMsgArg);//견적금액을 1이상 입력해주세요.
				return false;
				
				break;			
			}
		}
	}
	
	this.ds_Input_99.clearData();
	this.ds_Master10_chk.clearData();
	this.ds_Input_99.addRow();

	this.ds_Input_99.setColumn(0,	"QUTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO"));
	this.ds_Input_99.setColumn(0,	"WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "mm:M_MM_2200_M_SL19";	
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MST_SEARCH_EXISTS";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input_99=ds_Input_99" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Master10_chk=ds_Master10_chk";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID		
	svcCallObj.isSync = true;//동기식으로 설정할지 여부

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);
	
	if(this.ds_Master10_chk.rowcount > 0){
		this.gfn_Alert("MM054");//청구건으로 발주 가능 수량을 넘었습니다.
		return false;
	}			

	return true;
}
 
