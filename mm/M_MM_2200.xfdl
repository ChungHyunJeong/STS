/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Detail20] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{

	//체크시 해당 프로그램 로직처리
	if (e.columnid == "s_chk") {	
		var sCusmCd = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "SLCS_CD"); //거래처코드
		var sQuthNo = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "QUTH_NO"); //견적의뢰번호
		var sWofhSq = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "WOFH_SQ"); //견적의뢰결재차수
		var sQutbSe = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "QUTB_SE"); //견적의뢰번호
		var sItemCd = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "ITEM_CD"); //품목코드
		var sChkYn  = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "s_chk"); //체크여부
		
		var iDtlFindRow = this.ds_Detail10.findRowExpr("QUTH_NO=='" + sQuthNo + "' && WOFH_SQ=='" + sWofhSq + "' && QUTB_SE=='" + sQutbSe + "' && ITEM_CD=='" + sItemCd + "'");//Detail10 위치찾기
	
		if (sChkYn == '0') { //체크 해제
			if (!this.gfn_IsNull(this.ds_Detail10.getColumn(iDtlFindRow, "crudType"))) {	//Detail10 crudType이 존재할 경우에만 진행
				this.ds_Detail10.setColumn(iDtlFindRow, "SLCS_CD", this.ds_Detail10.getOrgColumn(iDtlFindRow, "SLCS_CD")); //거래처코드
				this.ds_Detail10.setColumn(iDtlFindRow, "SLCS_NM", this.ds_Detail10.getOrgColumn(iDtlFindRow, "SLCS_NM")); //거래처명
				this.ds_Detail10.setColumn(iDtlFindRow, "QUTB_PR", this.ds_Detail10.getOrgColumn(iDtlFindRow, "QUTB_PR")); //견적단가
				this.ds_Detail10.setColumn(iDtlFindRow, "WONE_AM", this.ds_Detail10.getOrgColumn(iDtlFindRow, "WONE_AM")); //견적금액		
				this.ds_Detail10.setColumn(iDtlFindRow, "FORE_AM", this.ds_Detail10.getOrgColumn(iDtlFindRow, "FORE_AM")); //외화금액		
				this.ds_Detail10.setColumn(iDtlFindRow, "QUSH_NO", this.ds_Detail10.getOrgColumn(iDtlFindRow, "QUSH_NO")); //업체견적번호		
				this.ds_Detail10.setColumn(iDtlFindRow, "QUSH_SQ", this.ds_Detail10.getOrgColumn(iDtlFindRow, "QUSH_SQ")); //업체견적결재차수		
				this.ds_Detail10.setColumn(iDtlFindRow, "crudType","") //crudType 초기화
			}
			
		} else { //체크
			if (sCusmCd != this.ds_Detail10.getColumn(iDtlFindRow, "SLCS_CD")) { //Detail10의 거래처와 동일하지 않을경우
				this.ds_Detail10.setColumn(iDtlFindRow, "SLCS_CD", this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "SLCS_CD")); //거래처코드
				this.ds_Detail10.setColumn(iDtlFindRow, "SLCS_NM", this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "SLCS_NM")); //거래처명
				this.ds_Detail10.setColumn(iDtlFindRow, "QUTB_PR", this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "QUTB_PR")); //견적단가
				this.ds_Detail10.setColumn(iDtlFindRow, "WONE_AM", this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "WONE_AM")); //견적금액		
				this.ds_Detail10.setColumn(iDtlFindRow, "FORE_AM", this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "FORE_AM")); //외화금액		
				this.ds_Detail10.setColumn(iDtlFindRow, "QUSH_NO", this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "QUSH_NO")); //업체견적번호		
				this.ds_Detail10.setColumn(iDtlFindRow, "QUSH_SQ", this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "QUSH_SQ")); //업체견적결재차수	
			}
		}
		
	//거래처 코드	
	} else if (e.columnid == "SLCS_CD") {	
		var sQuthNo = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "QUTH_NO"); //견적의뢰번호
		var sWofhSq = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "WOFH_SQ"); //견적의뢰결재차수
		var sQutbSe = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "QUTB_SE"); //견적의뢰번호
		var sItemCd = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "ITEM_CD"); //품목코드
		var sCusmCd = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "SLCS_CD"); //거래처코드
		
		//폼목코드, 거래처코드 존재하는 Row 찾기
		var nFindRow = this.ds_Detail21.findRowExpr("ITEM_CD == '"+sItemCd+"' && CUSM_CD == '"+sCusmCd+"'"); //견적단가, 견적금액 찾기

		//외화금액 계산
		var iQusbPr = this.ds_Detail21.getColumn(nFindRow, "QUSB_PR") //견적단가
		var iQutbQt = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "QUTB_QT"); //수량
		var sCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐코드
		var iExchRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");//환율 ### 견적단가가 외화의 경우 필요 ###

		if (sCurrCd == '0090') {			//KRW(원화)
			var iForeAm = 0;					//외화금액 = 0원
		} else {
			var iForeAm = iQutbQt * iQusbPr;	//외화금액 = 수량 * 견적단가(외회견적단가의 경우)
		}

		//Detail20 찾은 Row의 견적단가/견적금액, 업체견적번호, 업체견적결재차수 저장
		this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "SLCS_NM", this.ds_Detail21.getColumn(nFindRow, "CUSM_NM")); //거래처명
		this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "QUTB_PR", this.ds_Detail21.getColumn(nFindRow, "QUSB_PR")); //견적단가
		this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "WONE_AM", this.ds_Detail21.getColumn(nFindRow, "QUSB_AM")); //견적금액
		this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "FORE_AM", iForeAm); //외화금액
		this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "QUSH_NO", this.ds_Detail21.getColumn(nFindRow, "QUSH_NO")); //업체견적번호
		this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "QUSH_SQ", this.ds_Detail21.getColumn(nFindRow, "QUSH_SQ")); //업체견적결재차수


		//Detail10에 거래처, 견적단가, 견적금액 적용 (7/15 요청추가)
		var iDtlFindRow = this.ds_Detail10.findRowExpr("QUTH_NO=='" + sQuthNo + "' && WOFH_SQ=='" + sWofhSq + "' && QUTB_SE=='" + sQutbSe + "' && ITEM_CD=='" + sItemCd + "'");//Detail10 위치찾기

		this.ds_Detail10.setColumn(iDtlFindRow, "SLCS_CD", sCusmCd); //거래처코드
		this.ds_Detail10.setColumn(iDtlFindRow, "SLCS_NM", this.ds_Detail21.getColumn(nFindRow, "CUSM_NM")); //거래처명
		this.ds_Detail10.setColumn(iDtlFindRow, "QUTB_PR", this.ds_Detail21.getColumn(nFindRow, "QUSB_PR")); //견적단가
		this.ds_Detail10.setColumn(iDtlFindRow, "WONE_AM", this.ds_Detail21.getColumn(nFindRow, "QUSB_AM")); //견적금액
		this.ds_Detail10.setColumn(iDtlFindRow, "FORE_AM", iForeAm); //외화금액
		this.ds_Detail10.setColumn(iDtlFindRow, "QUSH_NO", this.ds_Detail21.getColumn(nFindRow, "QUSH_NO")); //업체견적번호
		this.ds_Detail10.setColumn(iDtlFindRow, "QUSH_SQ", this.ds_Detail21.getColumn(nFindRow, "QUSH_SQ")); //업체견적결재차수
		
		//'구매견적의뢰'의 비고 컬럼을 '업체선정상세'탭 '비고'값으로 가져옴(2022-03-28 추가)
		this.ds_Detail10.setColumn(iDtlFindRow, "REMK_NM", this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "REMK_NM")); //비고

		this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "s_chk","1"); //체크
	}
	
};


