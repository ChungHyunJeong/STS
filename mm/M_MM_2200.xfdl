 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST10_SEARCH": //조회		
			if (this.ds_Master10.getRowCount() > 0) {
				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master10.findRowExpr("QUTH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ=='" + this.fvSchKey02 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}				
			

				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
				this.div_DetailInput.form.tab_Detail.set_enable(true);

				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_MasterView.form.grd_Master10);

				this.fvMasterChangeYn = false; //Master 변경여부 false

			} else {
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			
				//입력영역 비활성화
				this.div_MasterInput.set_enable(false);
				this.div_DetailInput.form.tab_Detail.set_enable(false);
					
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_MasterView.form.grd_Master10);
			}		

			
			break;
		case "SAVE":   //저장		

			this.fvSchKeyYn = true;//저장후 재조회여부
			this.fn_ComBtnSearch();//저장후 재조회			
			this.fvMasterChangeYn = false;	//Master10 변경여부 초기화
			
			break;	

		case "FIX_SAVE":   //선정업체 확정저장		
			var arrMsgArg = ["업체선정확정"];
			this.gfn_Alert("SM066", arrMsgArg);//업체선정확정 처리가 완료되었습니다.

			this.fn_ComBtnSearch();//저장후 재조회
			this.fvMasterChangeYn = false;	//Master10 변경여부 초기화
			
			break;	

		case "DTL10_SEARCH": //조회		
			this.ds_Detail10.addColumn("crudType", "string");
			this.ds_Detail20.addColumn("s_chk", "string");

			//검색의뢰업체내용 존재시
			if (this.ds_Master11.getRowCount() > 0) {
				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master11);
				
			} else {
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master11);					
			}

			//업체선정상세내용 존재시
			if (this.ds_Detail10.getRowCount() > 0) {

				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Detail10.findRowExpr("QUTH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ=='" + this.fvSchKey02 + "' && QUTB_SE=='" + this.fvSchKey03 + "'");//업무Key Row 찾기
					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Detail10.enableevent) {
						this.ds_Detail10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Detail10.set_rowposition(-1);
						} 
						this.ds_Detail10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Detail10.set_rowposition(0);
					}
				}		

				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);
			
				//선정여부 완료 및 업체일괄선정 대상 협력사가 1건이상 여부 확인
				var sSeleYn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SELE_YN");//선정여부				
				var iCustCnt = this.ds_Detail20.getSum("MINC_SQ");	//협력사 존재 건수
				if (sSeleYn == '0' && iCustCnt > 0) {	//미선정이고,  협력사가 1건이상 존재시
					this.div_DetailInput.form.btn_CustPkgSelect.set_enable(true);
					this.div_DetailInput.form.btn_CustAllSelect.set_enable(true);
				} else {
					this.div_DetailInput.form.btn_CustPkgSelect.set_enable(false);
					this.div_DetailInput.form.btn_CustAllSelect.set_enable(false);
				}

				this.div_DetailInput.form.btn_CustCompSearch.set_enable(true); //업체견적비교 버튼 활성화

			} else {
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);					

				this.div_DetailInput.form.btn_CustCompSearch.set_enable(false); //업체견적비교 버튼 비활성화
			}

			//업체견적비교내용 존재시
			if (this.ds_Detail20.getRowCount() > 0) {

				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Detail20.findRowExpr("QUTH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ=='" + this.fvSchKey02 + "' && QUTB_SE=='" + this.fvSchKey04 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Detail20.enableevent) {
						this.ds_Detail20.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Detail20.set_rowposition(-1);
						} 
						this.ds_Detail20.set_rowposition(iFvFindRow);
					} else {
						this.ds_Detail20.set_rowposition(0);
					}
				}		

				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20);
				
			} else {
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20);
			}				
			
			//저장후 재조회여부-true 
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}	

			this.fvMasterSearchMode = false;//마스터내역 조회중여부
			break;	
			
		case "CHECK_SEARCH": //조회		
			
			//구매발주등록 결재여부 체크
			var sWofhStChk = this.ds_Master20Check.getColumn(0, "WOFH_ST");	//구매발주등록 결재상태
			
			if (this.ds_Master20Check.getRowCount() == 0 || sWofhStChk == '0010') {	//구매발주등록에 미존재 or 결재상태 등록(0010)
				var arrMsgArg = ["업체선정취소","진행"];
				this.gfn_Confirm("SM077", arrMsgArg,  "CONF.CANCEL_CHANGE", "fn_MsgCallback");//업체선정취소를 진행하시겠습니까?
			} else {
				this.gfn_Alert("MM006");//구매발주등록의 결재가 진행중으로 업체선정취소를 할 수 없습니다.
			}

			break;			
			
			
		case "CANCEL_SAVE":   //업체선정취소 저장					
			/*----------------------------------------------------------------*/
			this.ds_Master10Cancel.clearData();
	
			this.ds_Master10Cancel.addRow();
			this.ds_Master10Cancel.setColumn(0, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD")); 	//회사코드
