this.fn_SearchAllSelect = function()
{ 
	//2022-07-04 요청내용
	//업체일괄선정 --> 업체선정(개별)로 변경
	//업체선정(총괄) 추가개발 : 견적의뢰업체에서 견적번호가 존재하는 대상중 견적금액 합계가 최저인 대상의 업체견적번호, 품목코드별 데이터 조회

	var iMinQusbAm = this.ds_Master11.getCaseMin("QUSH_NO != null && QUSB_AM > 0", "QUSB_AM");	//견적의뢰업체 견적금액 최저금액 견적확정금액 > 0 이상 추가

	//최저금액 대상이 없을경우 메시지를 표기
	if (this.gfn_IsNull(iMinQusbAm)) {
		this.gfn_Alert("MM044");	//견적의뢰업체 견적금액이 존재하지 않아 업체선정(총괄)을 진행 할 수 없습니다.
		
