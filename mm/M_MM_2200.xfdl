/***********************************************************************************************
 * 02.MF0.03. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master] 데이터 Row 변경 이벤트 - ds_Master10
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1 && !this.fvMasterSearchMode) {
		//조회된 정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO"))) {
			this.fvMasterSearchMode = true;//마스터내역 조회중여부

			//견적의뢰번호 마스킹 포멧
			var iQuthLen = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO").length;
			
			//견적의뢰번호 format 변경 (10자리, 8자리)
			if (iQuthLen > 8) {
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_QuthNo.set_format("@@@@@@-@@@@");		
			} else {
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_QuthNo.set_format("@@@@@@-@@");
			}

			//선정완료/발주등록 결재상태에 따른 업체선정확정, 업체선정취소 활성화/비활성화
			var sSeleYn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SELE_YN");//선정여부
			var sCnclYn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNCL_YN");//취소가능여부(10/8 설계자추가요청) - 발주등록 결재상태가 등록(0010)이고 선정일자가 NOT NULL일 경우만 업체선정취소 가능
			if (sSeleYn == '1') { 	//선정완료
				this.div_MasterInput.form.btn_SelectFix.set_enable(false);      //업체선정확정버튼 비활성화
				if (sCnclYn == '1') { 	//선정 취소가능여부
					this.div_MasterInput.form.btn_SelectCancel.set_enable(true);    //업체선정취소버튼 활성화				
				} else {
					this.div_MasterInput.form.btn_SelectCancel.set_enable(false);    //업체선정취소버튼 비활성화
				}
			} else {             						//미선정, 선정취소
				this.div_MasterInput.form.btn_SelectFix.set_enable(true);       //업체선정확정버튼 활성화
				this.div_MasterInput.form.btn_SelectCancel.set_enable(false);   //업체선정취소버튼 비활성화

				//선정확정은 선정일자 = 현재일자, 선정부서/담당자 = 로그인 정보 저장
				var sToDay = this.gfn_GetCommDate("toDay");//현재일자
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "SELE_DD", sToDay);//선정일자-오늘날자로 설정			

			}

			//Detail10, Detail20 Tab 조회만 가능(선정완료 또는 내외자구분(국내외구분 OP01_NM = 0020)의 경우) - 10/6 설계자 요청
			var sDoimGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DOIM_GU");//내외자구분 
			var iFindRow = this.ds_Mm010.findRowExpr("CODM_CD=='" + sDoimGu +"'");
			var sDoimOp = this.ds_Mm010.getColumn(iFindRow,"OP01_NM"); //국내외구분
			if (sSeleYn == '1' || sDoimOp == '0020') { 			//선정완료 or 내외자구분(국내외구분 0020)
				this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);  //Detail 입력 비활성화							
				this.div_DetailInput.form.tab_Detail.set_tabindex(0);			//업체선정상세 tab으로 강제이동
			} else {											//미선정(선정취소) or 의뢰구분(외주아님)
				this.gfn_SetCompGroupEnable(this.div_DetailInput.form, true);   //Detail 입력 활성화
				
			}

			//2022-05-23 [실무자]규격. Maker > 재질, 후처리로 표현 될 수 있도록 변경 요청
			var sQuthGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_GU");	//의뢰구분
			var gDetail10 = this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10; 		//업체선정 그리드			
			if (sQuthGu == '0030') {
				gDetail10.setCellProperty("head", 18, "text", "재질"); 		//재질 칸 제목 
				gDetail10.setCellProperty("head", 19, "text", "후처리"); 	//후처리 칸 제목 
			} else {
				gDetail10.setCellProperty("head", 18, "text", "규격"); 		//규격 칸 제목 
				gDetail10.setCellProperty("head", 19, "text", "Maker"); 	//Maker 칸 제목 
			}
			
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Master11.clearData();		//견적의뢰업체
			this.ds_Detail10.clearData();		//업체선정상세
			this.ds_Detail20.clearData();		//업체견적비교
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "QUTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO"));
			this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
			
			if (sSeleYn == '1') { 	//선정완료시에 조회조건 추가
				this.ds_Input.setColumn(0, "SELE_DD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SELE_DD")); //선정일자(업체견적비교 사용)			
			}
					
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "mm:M_MM_2200_M_SL11";	//견적의뢰업체
			this.gfn_AddComSearchService(this, svcAddObj);

			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "mm:M_MM_2200_D_SL10";	//업체선정상세
			this.gfn_AddComSearchService(this, svcAddObj);

			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "mm:M_MM_2200_D_SL20";	//업체견적비교
			this.gfn_AddComSearchService(this, svcAddObj);

			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "mm:M_MM_2200_D_SL21";	//업체견적비교 - 거래처/견적단가/견적금액
			this.gfn_AddComSearchService(this, svcAddObj);
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL10_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Master11=ds_Master11 ds_Detail10=ds_Detail10 ds_Detail20=ds_Detail20 ds_Detail21=ds_Detail21";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);		
		}
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master] Row 변경전 이벤트 - ds_Master10
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무

	//데이터 변경여부 체크	
	if (this.fvMasterChangeYn || this.gfn_DsIsUpdated(this.ds_Detail10)) {
		this.gfn_Confirm("SM003", "", "CONF.MAST10_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
		
	} 
	
	//미선정 대상은 선택일자 원상복구
	var sSeleYn = this.ds_Master10.getColumn(e.oldrow, "SELE_YN");//선정여부
	if (sSeleYn == '0') {
		this.gfn_UndoColumn(this.div_MasterView.form.grd_Master10);
	}	

	return true;
};  

/*---------------------------------------------------------------------------------------------
 *    03. [데이터셋-ds_Master] Row 변경시 이벤트 - ds_Master10
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{

	if (e.columnid == "REMK_NM") {

		var sNewRemkNm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "REMK_NM");//비고(New)
		var sQrgRemkNm = this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "REMK_NM");//비고(Org)

		if (sNewRemkNm != sQrgRemkNm) {
			this.fvMasterChangeYn = true; //Master 비고 변경 여부
		}
	}
	
};


