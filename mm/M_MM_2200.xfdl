 ***********************************************************************************************/
this.fn_FormClose = function(obj:nexacro.Form,e:nexacro.CloseEventInfo)
{
	return true;
};

/***********************************************************************************************
 * 1. 조회 영역
 ***********************************************************************************************/
/***********************************************************************************************
 * 01.01. [공통버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.fn_ComBtnSearch = function()
{ 
	this.fvMasterSearchMode = false;//마스터내역 조회중여부
	
	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;	

	//저장후 재조회시-마스터 데이터셋 이벤트 비활성화
	if (this.fvSchKeyYn) {
		this.ds_Master10.set_enableevent(false);
	}

	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Master10.clearData();
	this.ds_Master11.clearData();
	this.ds_Detail10.clearData();
	this.ds_Detail20.clearData();
	this.ds_Input.clearData();
	
	//[업무] 조회조건 입력 : 견적의뢰관리 대상 필수값
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_FromDd.value);
	this.ds_Input.setColumn(0, "CLOS_DD", this.div_Search.form.cal_ClosDd.value);
	this.ds_Input.setColumn(0, "CNTH_NO", this.div_Search.form.mae_CnthNo.value);
	this.ds_Input.setColumn(0, "CUSM_CD", this.div_Search.form.edt_CusmCd.value);
	this.ds_Input.setColumn(0, "SEAR_GU", this.div_Search.form.cbo_SearGu.value);
	this.ds_Input.setColumn(0, "SEAR_NM", this.div_Search.form.edt_SearNm.value);
	this.ds_Input.setColumn(0, "DEPM_CD", this.div_Search.form.edt_DepmCd.value);
	this.ds_Input.setColumn(0, "SESU_ID", this.div_Search.form.edt_SystId.value);
	this.ds_Input.setColumn(0, "QUTH_GU", this.div_Search.form.cbo_QuthGu.value);

	var sRecvYn = this.div_Search.form.chk_RecvYn.value;	//견적접수 - 접수
	var sBfrvYn = this.div_Search.form.chk_BfrvYn.value;	//견적접수 - 미접수
	if (sRecvYn == '1' && sBfrvYn == '1') {
		this.ds_Input.setColumn(0, "RECV_YN", "");
	} else if (sRecvYn == '1' && sBfrvYn == '0') {
		this.ds_Input.setColumn(0, "RECV_YN", "1");
	} else if (sRecvYn == '0' && sBfrvYn == '1') {
		this.ds_Input.setColumn(0, "RECV_YN", "0");
	}

	var sSeleYn = this.div_Search.form.chk_SeleYn.value;	//업체선정 - 선정
	var sUnseYn = this.div_Search.form.chk_UnseYn.value;	//업체선정 - 미선정
	if (sSeleYn == '1' && sUnseYn == '1') {
		this.ds_Input.setColumn(0, "SELE_YN", "");
	} else if (sSeleYn == '1' && sUnseYn == '0') {
		this.ds_Input.setColumn(0, "SELE_YN", "1");
	} else if (sSeleYn == '0' && sUnseYn == '1') {
		this.ds_Input.setColumn(0, "SELE_YN", "0");
	}

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "mm:M_MM_2200_M_SL10";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MST10_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Master10=ds_Master10";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
}

