/***********************************************************************************************
 * 15.10. [SET] Detail 업체선정(개별) 셋팅
 ***********************************************************************************************/ 
this.fn_SetPkgSelectFix = function()
{
	this.ds_Detail20.set_enableevent(false); //이벤트 false
	
	var sCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐코드	
	var iExchRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");//환율 ### 견적단가가 외화의 경우 필요 ###
	for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {

		var sQuthNo = this.ds_Detail20.getColumn(iLoop, "QUTH_NO"); //견적의뢰번호
		var sWofhSq = this.ds_Detail20.getColumn(iLoop, "WOFH_SQ"); //견적의뢰결재차수
		var sQutbSe = this.ds_Detail20.getColumn(iLoop, "QUTB_SE"); //견적의뢰번호
		var sItemCd = this.ds_Detail20.getColumn(iLoop, "ITEM_CD"); //품목코드
		
		//외화금액 계산
		var iQusbPr = this.ds_Detail20.getColumn(iLoop, "MINC_PR") //최저견적단가
		var iQutbQt = this.ds_Detail20.getColumn(iLoop, "QUTB_QT"); //수량

		if (sCurrCd == '0090') {			//KRW(원화)
			var iForeAm = 0;					//외화금액 = 0원
		} else {
			var iForeAm = iQutbQt * iQusbPr;	//외화금액 = 수량 * 견적단가(외화견적단가의 경우)
		}

		//Detail10 최저견적단가 적용
		this.ds_Detail20.setColumn(iLoop, "SLCS_CD", this.ds_Detail20.getColumn(iLoop, "MINC_CD")); //최저거래처코드
		this.ds_Detail20.setColumn(iLoop, "SLCS_NM", this.ds_Detail20.getColumn(iLoop, "MINC_NM")); //거래처명
		this.ds_Detail20.setColumn(iLoop, "QUTB_PR", this.ds_Detail20.getColumn(iLoop, "MINC_PR")); //최저견적단가
		this.ds_Detail20.setColumn(iLoop, "WONE_AM", this.ds_Detail20.getColumn(iLoop, "MINC_AM")); //최저견적금액
		this.ds_Detail20.setColumn(iLoop, "FORE_AM", iForeAm); //외화금액계산
		this.ds_Detail20.setColumn(iLoop, "QUSH_NO", this.ds_Detail20.getColumn(iLoop, "MINC_NO")); //최저업체견적번호
		this.ds_Detail20.setColumn(iLoop, "QUSH_SQ", this.ds_Detail20.getColumn(iLoop, "MINC_SQ")); //최저업체견적결재차수

		//Detail10에 거래처, 견적단가, 견적금액 적용
		var iDtlFindRow = this.ds_Detail10.findRowExpr("QUTH_NO=='" + sQuthNo + "' && WOFH_SQ=='" + sWofhSq + "' && QUTB_SE=='" + sQutbSe + "' && ITEM_CD=='" + sItemCd + "'");//Detail10 위치찾기

		this.ds_Detail10.setColumn(iDtlFindRow, "SLCS_CD", this.ds_Detail20.getColumn(iLoop, "MINC_CD")); //최저거래처코드
		this.ds_Detail10.setColumn(iDtlFindRow, "SLCS_NM", this.ds_Detail20.getColumn(iLoop, "MINC_NM")); //거래처명
		this.ds_Detail10.setColumn(iDtlFindRow, "QUTB_PR", this.ds_Detail20.getColumn(iLoop, "MINC_PR")); //최저견적단가
		this.ds_Detail10.setColumn(iDtlFindRow, "WONE_AM", this.ds_Detail20.getColumn(iLoop, "MINC_AM")); //최저견적금액
		this.ds_Detail10.setColumn(iDtlFindRow, "FORE_AM", iForeAm); //외화금액
		this.ds_Detail10.setColumn(iDtlFindRow, "QUSH_NO", this.ds_Detail20.getColumn(iLoop, "MINC_NO")); //최저업체견적번호
		this.ds_Detail10.setColumn(iDtlFindRow, "QUSH_SQ", this.ds_Detail20.getColumn(iLoop, "MINC_SQ")); //최저업체견적결재차수

		this.ds_Detail20.setColumn(iLoop, "s_chk","1"); //체크

	}
	this.ds_Detail20.set_enableevent(true); //이벤트 false
};


