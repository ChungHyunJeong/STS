		iDtlFindRow = this.ds_Detail10.findRowExpr("QUTH_NO=='" + sQuthNo + "' && WOFH_SQ=='" + sWofhSq + "' && QUTB_SE=='" + sQutbSe + "' && ITEM_CD=='" + sItemCd + "'");//Detail20 위치찾기		

		//외화금액 계산
		var iQusbPr = this.ds_Detail23.getColumn(iLoop, "QUSB_PR") //최저견적단가
		var iQutbQt = this.ds_Detail20.getColumn(iDtlFindRow, "QUTB_QT"); //수량 (이미 검색한 업체선정비교에서 가져옴)

		if (sCurrCd == '0090') {			//KRW(원화)
			var iForeAm = 0;					//외화금액 = 0원
		} else {
			var iForeAm = iQutbQt * iQusbPr;	//외화금액 = 수량 * 견적단가(외화견적단가의 경우)
		}

		//Detail20 최저견적단가 적용
		this.ds_Detail20.setColumn(iDtlFindRow, "SLCS_CD", this.ds_Detail23.getColumn(iLoop, "CUSM_CD")); //거래처코드
		this.ds_Detail20.setColumn(iDtlFindRow, "SLCS_NM", this.ds_Detail23.getColumn(iLoop, "CUSM_NM")); //거래처명
		this.ds_Detail20.setColumn(iDtlFindRow, "QUTB_PR", this.ds_Detail23.getColumn(iLoop, "QUSB_PR")); //견적단가
		this.ds_Detail20.setColumn(iDtlFindRow, "WONE_AM", this.ds_Detail23.getColumn(iLoop, "QUSB_AM")); //견적금액
		this.ds_Detail20.setColumn(iDtlFindRow, "FORE_AM", iForeAm); //외화금액계산
		this.ds_Detail20.setColumn(iDtlFindRow, "QUSH_NO", this.ds_Detail23.getColumn(iLoop, "QUSH_NO")); //업체견적번호
		this.ds_Detail20.setColumn(iDtlFindRow, "QUSH_SQ", this.ds_Detail23.getColumn(iLoop, "QUSH_SQ")); //업체견적결재차수

		//Detail10에 거래처, 견적단가, 견적금액 적용
		iDtlFindRow = this.ds_Detail10.findRowExpr("QUTH_NO=='" + sQuthNo + "' && WOFH_SQ=='" + sWofhSq + "' && QUTB_SE=='" + sQutbSe + "' && ITEM_CD=='" + sItemCd + "'");//Detail10 위치찾기

		this.ds_Detail10.setColumn(iDtlFindRow, "SLCS_CD", this.ds_Detail23.getColumn(iLoop, "CUSM_CD")); //거래처코드
		this.ds_Detail10.setColumn(iDtlFindRow, "SLCS_NM", this.ds_Detail23.getColumn(iLoop, "CUSM_NM")); //거래처명
		this.ds_Detail10.setColumn(iDtlFindRow, "QUTB_PR", this.ds_Detail23.getColumn(iLoop, "QUSB_PR")); //견적단가
		this.ds_Detail10.setColumn(iDtlFindRow, "WONE_AM", this.ds_Detail23.getColumn(iLoop, "QUSB_AM")); //견적금액
		this.ds_Detail10.setColumn(iDtlFindRow, "FORE_AM", iForeAm); //외화금액
		this.ds_Detail10.setColumn(iDtlFindRow, "QUSH_NO", this.ds_Detail23.getColumn(iLoop, "QUSH_NO")); //업체견적번호
		this.ds_Detail10.setColumn(iDtlFindRow, "QUSH_SQ", this.ds_Detail23.getColumn(iLoop, "QUSH_SQ")); //업체견적결재차수

		this.ds_Detail20.setColumn(iLoop, "s_chk","1"); //체크

	}
	this.ds_Detail20.set_enableevent(true); //이벤트 false
};


