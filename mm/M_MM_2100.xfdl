 * 5. 저장
 ***********************************************************************************************/
/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Master11) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}	

	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		//첨부파일정보만 변경시
		if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Master11) && !this.gfn_DsIsUpdated(this.ds_Detail10) && this.ds_Master10.getRowCount() > 0) {
			//첨부파일정보 저장
			fvFileInfo = new Object();//[공통] 첨부파일정보
			fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, B-상세(그리드Row), E-기타
			fvFileInfo.CALLBACK_FUNC = "";//호출할 콜백함수
			fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
			fvFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO");//PK번호2-견적의뢰번호
			fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
			//첨부파일Div 화면에 Key세팅하게 변경
			this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);
			return;	
		}
	}

	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		if (!this.fn_VaildMasterSave()) return;
	}
	
	//Master11 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master11)) {
		if (!this.fn_VaildMaster11Save()) return;
	}

	//[요청]Master11 견적의뢰업체 저장시 1Row 필수 (7/15)
	var sDoimGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DOIM_GU");//내외자구분 
	var iFindRow = this.ds_Mm010.findRowExpr("CODM_CD=='" + sDoimGu +"'");
	var sDoimOp = this.ds_Mm010.getColumn(iFindRow,"OP01_NM"); //국내외구분
	var sDoimNm = this.ds_Mm010.getColumn(iFindRow,"CODM_NM"); //내외자구분명
	var sEvalCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EVAL_CD");//평가기준 (2022-04-21)
	iFindRow = this.ds_Mm215.findRowExpr("CODM_CD=='" + sEvalCd +"'");
	var sEvalOp = this.ds_Mm215.getColumn(iFindRow,"OP01_NM"); //업체직접선정		
	var iMst11Count = this.ds_Master11.getCaseCount("crudType != 'D'");	//삭제대상이 아닌 건수

	//Master11 견적의뢰업체가 1Row 이상이어야 함 (7/15 추가요청, 7/22 내외자구분Domestic/연간단가 미적용 조건적용 해제
	if (iMst11Count < 1) { 
		var arrMsgArg = ["견적의뢰업체"];
		this.gfn_Alert("SM045", arrMsgArg); //견적의뢰업체는 1건이 이상 존재해야 합니다.
		return false;

	//평가기준이 업체직접선정 대상의 경우 1개의 업체만 등록 가능해야 함(2022-04-21)
	} else if (sEvalOp == '1' && iMst11Count > 1) {	//평가기준 - 업체직접선정, 견적의뢰업체가 1건 초과인 경우
		var arrMsgArg = ["평가기준", "업체직접선정"];
		this.gfn_Alert("MM003", arrMsgArg);  //평가기준이 업체직접선정일 경우에는 견적의뢰업체를 1건만 등록해야 합니다.
		return false;

	//내외자구분 - 국내외구분(0020)의 경우 1건만 저장되어야 함 (7/15)
	} else if (sDoimOp == '0020' && iMst11Count > 1) { 
		var arrMsgArg = ["내외자구분", sDoimNm];
		this.gfn_Alert("MM003", arrMsgArg); //내외자구분이 &ITM0일 경우에는 견적의뢰업체를 1건만 등록해야 합니다.
		return false;
		
	}


	//Detail 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		if (!this.fn_VaildDetailSave()) return;
	}	

	//Detail10 견적의뢰상세가 1Row 이상이어야 함 (10/19 설계자요청)
	if (this.ds_Detail10.getRowCount() < 1) { 
		this.div_DetailInput.form.tab_Detail.set_tabindex(0); //견적의뢰상세 Tab으로 이동
		
		var arrMsgArg = ["견적의뢰상세"];
		this.gfn_Alert("SM045", arrMsgArg); //견적의뢰상세는 1건이 이상 존재해야 합니다.
		return false;

	} else {
		//Detail10 거래처코드에 견적의뢰업체 저장
		//내외자구분 국내외구분, 평가기준 업체직접선정(2022-04-21 추가)
		if ((sDoimOp == '0020' || sEvalOp == '1') && iMst11Count == 1) { 
			for (var iLoop=0;iLoop<this.ds_Master11.getRowCount();iLoop++) {	//삭제 대상이 아닌 견적의뢰업체
				if (this.ds_Master11.getColumn(iLoop, "crudType") != 'D') {
					var sCusmCd = this.ds_Master11.getColumn(iLoop, "CUSM_CD");
					break;
				}
			}
		
			for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
				this.ds_Detail10.setColumn(iLoop,"SLCS_CD",sCusmCd); //거래처코드 = 견적의뢰업체 처음 Row
			}
			
		} else {
			for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
				this.ds_Detail10.setColumn(iLoop,"SLCS_CD",""); //거래처코드 = 초기화
			}
		}
		
	}	

	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);

	//Master 입력 상태이면
	if (sChkRowType == "I") {
	
		//채번
		var sTbnmNo = this.gfn_GetTbnmNo(this.fvComPrgmId, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_DD"));//프로그램ID, 기준일자
		if (this.gfn_IsNull(sTbnmNo)) {
			//채번을 가져오는데 실패했습니다.
			return;
		}
		
		var sChkTbnmNo = "";//채번-체크용
		var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position
		
		//Master 데이터셋에 채번정보 입력
		this.ds_Master10.setColumn(iMasterRowPos, "QUTH_NO", sTbnmNo);	//견적의뢰번호

		//PK값 정의[필수]
		var sCommCd = this.ds_Master10.getColumn(iMasterRowPos, "COMM_CD");//회사코드
		var sQuthNo = this.ds_Master10.getColumn(iMasterRowPos, "QUTH_NO");//견적의뢰번호
		var sWofhSq = this.ds_Master10.getColumn(iMasterRowPos, "WOFH_SQ");//결재차수

		//Master11 데이터셋에 PK정보 입력
		for (var iLoop=0; iLoop<this.ds_Master11.getRowCount(); iLoop++) {
			sChkTbnmNo = this.ds_Master11.getColumn(iLoop, "QUTH_NO");//견적의뢰번호
			
			if (this.gfn_IsNull(sChkTbnmNo)) { 
				this.ds_Master11.setColumn(iLoop, "COMM_CD", sCommCd);
				this.ds_Master11.setColumn(iLoop, "QUTH_NO", sQuthNo);
				this.ds_Master11.setColumn(iLoop, "WOFH_SQ", sWofhSq);
			}
		}
		
		//Detail10 데이터셋에 PK정보 입력
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			sChkTbnmNo = this.ds_Detail10.getColumn(iLoop, "QUTH_NO");//견적의뢰번호
			
			if (this.gfn_IsNull(sChkTbnmNo)) { 
				this.ds_Detail10.setColumn(iLoop, "COMM_CD", sCommCd);
				this.ds_Detail10.setColumn(iLoop, "QUTH_NO", sQuthNo);
				this.ds_Detail10.setColumn(iLoop, "WOFH_SQ", sWofhSq);
			}
		}			
	}
	
	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		fvFileInfo = new Object();//[공통] 첨부파일정보
		fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
		fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
		fvFileInfo.CALLBACK_FUNC = "fn_ComBtnSearch";//호출할 콜백함수
		fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
		fvFileInfo.PK01_NO = nApp.gvCompany;//PK번호1-회사코드
		fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO");//PK번호2-견적의뢰번호
		fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
	}			
	
	//저장시 재조회 Key설정
	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO");//조회Key01
	this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//조회Key02
	
	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	//[공통]서비스 호출 변수
	var sInputDaset = "" //입력 데이터셋
	var sOutDatset = "";//출력 데이터셋
	var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		
	//Master10 변경건이 있으면 Master 저장
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
		svcAddObj.sInsertSql = "mm:M_MM_2100_M_IN10";//저장Sql
		svcAddObj.sUpdateSql = "mm:M_MM_2100_M_UP10";//수정Sql
		svcAddObj.sDeleteSql = "mm:M_MM_2100_M_DL10";//삭제Sql
		svcAddObj.sSaveFlagColumn = "";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

		this.gfn_AddComSaveService(this, svcAddObj);
		
		sInputDaset = "ds_Master10=ds_Master10:U" //입력 데이터셋

	}

	//Master11 변경건이 있으면 Master 저장
	if (this.gfn_DsIsUpdated(this.ds_Master11)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
		svcAddObj.sInsertSql = "mm:M_MM_2100_M_IN11";//저장Sql
		svcAddObj.sUpdateSql = "mm:M_MM_2100_M_UP11";//수정Sql
		svcAddObj.sDeleteSql = "mm:M_MM_2100_M_DL11";//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

		this.gfn_AddComSaveService(this, svcAddObj);

		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = "ds_Master11=ds_Master11:U" //입력 데이터셋
		} else {
			sInputDaset = sInputDaset + " ds_Master11=ds_Master11:U" //입력 데이터셋
		}

		//저장시 재조회 Key설정
		this.fvSchKey04 = this.ds_Master11.getColumn(this.ds_Master11.rowposition, "QUTC_SE");//조회Key04

	}
	
	//Detail10 변경건이 있으면 Detail10 저장 - 견적상세
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object	
		svcAddObj.sInsertSql = "mm:M_MM_2100_D_IN10";//저장Sql
		svcAddObj.sUpdateSql = "mm:M_MM_2100_D_UP10";//수정Sql
		svcAddObj.sDeleteSql = "mm:M_MM_2100_D_DL10";//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		
		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = "ds_Detail10=ds_Detail10:U" //입력 데이터셋
		} else {
			sInputDaset = sInputDaset + " ds_Detail10=ds_Detail10:U" //입력 데이터셋
		}

		//저장시 재조회 Key설정
		this.fvSchKey03 = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUTB_SE");//조회Key03
	}		

	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "SAVE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = sInputDaset //입력 데이터셋
	svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
	svcCallObj.sOtherArg = sOtherArg;//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명		
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	

}

