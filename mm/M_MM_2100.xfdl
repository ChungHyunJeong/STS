	var oArg = { CommCd:this.gfn_GetSessionValue("COMM_CD")//회사코드
	           , ModuCd:"MM"//모듈구분
			   , WctmId:"WF_M_MM_2100_W1"//결재업무ID
			   , SystId:this.gfn_GetSessionValue("SYST_ID")//결재자_시스템사번
			   , WofhNm:sWofhNm//결재명
			   , WofhNo:this.ds_Master10.getColumn(iRow, "WOFH_NO")//결재PID
			   , InnrGb:"1"//내부결재구분(1:신규, 2:내부)
			   , CharSystId:""//담당자_시스템사번
			   , CharWfguCd:""//담당자 결재구분
			   , SworqVr:"3"//결재요청버전(1,2,3)
			   , WfamAmtr:"0"//금액
			   , WfamOpid:""//원천PIID
			   , WfPkvl:sPks//PK값 정보 - PK 값을 순서대로 전달, Row구분자는 ⓡ, 조건 구분자는 ⓐ
	           };//Argument처리
	this.gfn_WfRequestPop(sPopupId, oArg);	
}

/***********************************************************************************************
 * 09.02. [공통버튼] 결재요청 유효성체크
 ***********************************************************************************************/  
this.fn_VaildWfDecide = function() {
	
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Master11) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.fvFileDiv.fn_CheckFileAddYn()) {
		var sWOFH_ST = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST");//결재진행상태
		//결재상태에 따른 결재요청 가능여부 체크		
		if (!this.gfn_CheckDecideWf(sWOFH_ST)) {			
			return false;		
		}
		
	} else {
		var arrMsgArg = ["결재요청"];
		this.gfn_Alert("SM064", arrMsgArg);//변경된 내용이 있습니다.\n저장 후 &ITM0하시기 바랍니다.
		return false;
	}

	//결재진행내역체크
	if (this.ds_Master10.getRowCount() < 1 || this.ds_Master10.rowposition < 0) {
		this.gfn_Alert("WF016", arrMsgArg);//결재내역을 선택한 후 결재요청하세요.
		return false;
	}

	//견적의뢰상세 구매청구 진행상태 (2022-06-08 추가)
	if (this.ds_Detail10.getCaseCount("PROC_ST == '1'") > 0) {
		this.gfn_Alert("MM043");//[견적의뢰상세]에서 [청구정보 - 진행상태]에 대한 취소정보가 존재하여 결재를 진행할 수 없습니다.
		return false;
	}
	
	
	return true;
}


//-- 견적의뢰 수량이 청구수량보타 클 경우 결재요청 불가(2022.09.20 이성수차장 요청)

this.fn_VailQtChk = function() {
	var QutbQt = 0;
	var PcrbQt = 0;
	
	for(var iLoop = 0; iLoop < this.ds_Detail10.rowcount; iLoop++){	
		QutbQt = this.ds_Detail10.getColumn(iLoop, "QUTB_QT");
		PcrbQt = this.ds_Detail10.getColumn(iLoop, "PCRB_QT");
		
		if(this.gfn_IsNull(QutbQt)) {QutbQt = 0;}
		if(this.gfn_IsNull(PcrbQt)) {PcrbQt = 0;}
		
		QutbQt = nexacro.toNumber(QutbQt);
		PcrbQt = nexacro.toNumber(PcrbQt);
				
		if(QutbQt > PcrbQt){
			this.ds_Detail10.set_rowposition(iLoop);
			var arrMsgArg = ["견적의뢰 수량이 청구수량보다 큰 경우 결재요청할 수" ];	
			this.gfn_Alert("MM046", arrMsgArg); //..없습니다.	
			return false;
		}
	}
	
	return true;
}
