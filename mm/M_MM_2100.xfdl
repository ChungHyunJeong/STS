/***********************************************************************************************
 * 02.DG1.05. [Detail그리드2-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Choice = function()
{

	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {
		
			//1. 동일대상만 체크 대상 값 Flag
			var sDupKeyChk=""; //Key(청구구분) 중복체크
			
			//청구구분 중복체크
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1" || this.ds_Detail20.getColumn(iLoop, "s_conf") == "1") {
					
					//Detail20 그리드내 체크된 대상 동일여부 비교 : 청구구분
					if(!this.gfn_IsNull(sDupKeyChk) ){
						if(sDupKeyChk != this.ds_Detail20.getColumn(iLoop,"PCRH_GU") ){
							var arrMsgArg = ["청구구분"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 청구구분만 적용 가능합니다.
							return;
						}
					}
					sDupKeyChk = this.ds_Detail20.getColumn(iLoop,"PCRH_GU"); //for문 비교를 위한 이전값 저장
				}
			}		
		
			//2. Detail20 -> Detail10 적용
			var arrCheckRow = [];//체크된 Row

			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				//체크 대상여부 확인
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {

					//Detail10에 적용되어 있는지 체크
					var objSearchList = ["PCRH_NO|" + this.ds_Detail20.getColumn(iLoop, "PCRH_NO") //청구번호
										,"PCRB_SE|" + this.ds_Detail20.getColumn(iLoop, "PCRB_SE") //청구순번
										];		

					//[공통] Detail10 중복건 체크후 해당건 Skip
					if (!this.gfn_DsSchDupcheck(this.ds_Detail10, objSearchList)) {
										
						// 라인추가 후 Detail20 -> Detail10으로 적용
						var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)
						
						//선택항목 비활성화처리 (색상변경 및 체크박스 Lock)
						this.ds_Detail20.setColumn(iLoop, "s_conf", "1");
						this.ds_Detail20.setColumn(iLoop, "s_chk", "0");//Uncheck로 변경

						//견적미의뢰검색(Detail20) -> 견적의뢰상세(Detail10) Copy : 컬럼명 동일
						this.ds_Detail10.copyRow(iAddRow, this.ds_Detail20, iLoop);		
						
						//견적의뢰상세 Master에서 Key값 적용
						this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
						this.ds_Detail10.setColumn(iAddRow, "QUTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO")); //견적의뢰번호
						this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ")); //결재번호
						this.ds_Detail10.setColumn(iAddRow, "QUTB_SE", "1"); //견적의뢰순번 (신규의 경우 1으로 셋팅)
						this.ds_Detail10.setColumn(iAddRow, "s_chk", "0");
						
						arrCheckRow.push(iLoop );						
					}
					

					//최근발주업체 견적의뢰업체(Master11) 추가 (2022-04-27)
					var objCompareList = ["CUSM_CD|" + this.ds_Detail20.getColumn(iLoop, "CUPM_CD") //파트너사
										];		
					var sCupmCd = this.ds_Detail20.getColumn(iLoop, "CUPM_CD"); //최근발주업체 파트너사

					//최근발주업체가 존재하지 않거나, Master11 중복건 체크후 해당건 Skip
					if (!this.gfn_DsSchDupcheck(this.ds_Master11, objCompareList) && !this.gfn_IsNull(sCupmCd)) {
										
						// 라인추가 후 Detail20 -> Master11로 적용
						var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Master11);//crud_flag 변경-입력(I)
						
						//견적의뢰업체 M적용
						this.ds_Master11.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
						this.ds_Master11.setColumn(iAddRow, "QUTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO")); //견적의뢰번호
						this.ds_Master11.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ")); //결재번호
						this.ds_Master11.setColumn(iAddRow, "CUSM_CD", this.ds_Detail20.getColumn(iLoop, "CUPM_CD")); //파트너사
						this.ds_Master11.setColumn(iAddRow, "CUSM_NM", this.ds_Detail20.getColumn(iLoop, "CUPM_NM")); //파트너사명
						this.ds_Master11.setColumn(iAddRow, "CSPM_CD", this.ds_Detail20.getColumn(iLoop, "CSPM_CD")); //고객번호
						this.ds_Master11.setColumn(iAddRow, "CSPM_NM", this.ds_Detail20.getColumn(iLoop, "CSPM_NM")); //고객명
						this.ds_Master11.setColumn(iAddRow, "RANK_NM", this.ds_Detail20.getColumn(iLoop, "RANK_NM")); //직급
						this.ds_Master11.setColumn(iAddRow, "MTEL_NO", this.ds_Detail20.getColumn(iLoop, "MTEL_NO")); //연락처
						this.ds_Master11.setColumn(iAddRow, "EMAL_NM", this.ds_Detail20.getColumn(iLoop, "EMAL_NM")); //이메일
						this.ds_Master11.setColumn(iAddRow, "s_chk", "0");
						
						arrCheckRow.push(iLoop );						
					}


				}
			}
					
			this.ds_Detail20.set_rowposition(-1);
			
			//적용 후 견적의뢰상세 Tab이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);
		}
	}
};


