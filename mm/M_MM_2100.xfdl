/***********************************************************************************************
 * 15.09. [그리드변경] Detail 그리드 포멧 변경 (Master-의뢰구분, Detail-청구구분)
 ***********************************************************************************************/ 
this.fn_GridFormatChange  = function(sGridGu)
{
/* 2022-03-28 추가개발
    1. Master (견적의뢰내역) - 의뢰구분
		ㅇ 외주가공 (0030)
		  - 보이기 : 중분류, 재질, 후처리
		  - 숨기기 : Maker
		ㅇ 구매품 (0020) / 턴키외주(0010)
		  - 보이기 : Maker
		  - 숨기기 : 중분류, 재질, 후처리
	
	2. Detail (견적미의뢰검색) - 청구구분
		ㅇ 구매품 : (0010), 턴키외주(0200) 등 그외
		  - 보이기 : Maker
		  - 숨기기 : 중분류, 재질, 후처리
		ㅇ 외주가공 (0020)
		  - 보이기 : 중분류, 재질, 후처리
		  - 숨기기 : Maker
*/
	var gDetail10 = this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10; //견적의뢰상세 그리드
	var gDetail20 = this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20; //견적미의뢰검색 그리드
	var sQuthGu = "";	//의뢰구분
	var sPcrhGu = "";	//청구구분
	
	//Master - 의뢰구분, Detail - 청구구분	
	if (sGridGu == 'Master') {
		sQuthGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_GU");	//의뢰구분
		
		if (sQuthGu == "0010") {	//의뢰구분 턴키외주(0010) 선택시 -> 청구구분 턴키외주(0200) 콤보
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_PcrhGu.set_index(2);  //청구구분 - 턴키외주(0030)
			
		} else if (sQuthGu == "0020") {	//의뢰구분 구매품(0020) 선택시 -> 청구구분 구매품(0010) 콤보
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_PcrhGu.set_index(0);  //청구구분 - 구매품(0010)
			
		} else if (sQuthGu == "0030") {	//의뢰구분 외주가공(0030) 선택시 -> 청구구분 외주가공(0020) 콤보
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_PcrhGu.set_index(1);  //청구구분 - 외주가공(0020)
			
