				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_DoimGu.set_enable(false); //내외자구분 비활성화(7/15 수정 요청)
				this.div_MasterInput.form.tab_Master.tpg_01.form.chk_YearYn.set_enable(false); //연간단가적용 비활성화(7/15 수정 요청)

			} else {
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);					
				
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_DoimGu.set_enable(true); //내외자구분 활성화(7/15 수정 요청)
				this.div_MasterInput.form.tab_Master.tpg_01.form.chk_YearYn.set_enable(true); //연간단가적용 활성화(7/15 수정 요청)
			}

			//견적미의뢰검색 초기화
			this.ds_Detail20.clearData();//견적미의뢰검색 데이터셋 초기화			
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_EmpmCd.set_value(this.gfn_GetSessionValue("")); //청구자 ID (청구부서 -> 로그인 정보 2021-02-07 -> 빈값 2022-02-21)
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_EmpmNm.set_value(this.gfn_GetSessionValue("")); //청구자명 (청구부서 -> 로그인 정보 2021-02-07 -> 빈값 2022-02-21)
