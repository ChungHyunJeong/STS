			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.mae_CnthNo.set_value(""); //계약번호
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CnthNm.set_value("");
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_PcrhNo.set_value(""); //청구번호
			
			
