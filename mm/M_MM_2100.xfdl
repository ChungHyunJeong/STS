		case "WF_RECIDE": //재기안처리		
			if (nApp.gds_WfRst.getRowCount() > 0) {
				if (nApp.gds_WfRst.getColumn(0, "O_RET_CODE") == "TRUE") {
					this.gfn_Alert("WF012");//재기안 처리가 완료되었습니다.
					this.fvSchKeyYn = true;//저장후 재조회여부
