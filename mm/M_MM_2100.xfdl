/***********************************************************************************************
 * 02.MG0.02. [Master그리드-버튼] 행추가 클릭 이벤트
 ***********************************************************************************************/
this.fn_MasterGrd01AddRow = function()
{
	//[요청]Master11 견적의뢰업체 저장시 1Row 필수 (7/15)
	var sDoimGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DOIM_GU");//내외자구분 
	var iFindRow = this.ds_Mm010.findRowExpr("CODM_CD=='" + sDoimGu +"'");
	var sDoimOp = this.ds_Mm010.getColumn(iFindRow,"OP01_NM"); //국내외구분
	var sDoimNm = this.ds_Mm010.getColumn(iFindRow,"CODM_NM"); //내외자구분명
	var sEvalCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EVAL_CD");//평가기준
	iFindRow = this.ds_Mm215.findRowExpr("CODM_CD=='" + sEvalCd +"'");
	var sEvalOp = this.ds_Mm215.getColumn(iFindRow,"OP01_NM"); //업체직접선정	
	var iMst11Count = this.ds_Master11.getCaseCount("crudType != 'D'");	//삭제대상이 아닌 건수

	//평가기준이 업체직접선정 대상의 경우 1개의 업체만 등록 가능해야 함(2022-04-21)
	if (sEvalOp == '1' && iMst11Count > 0) {	//평가기준 - 업체직접선정, 견적의뢰업체가 1건 초과인 경우
		var arrMsgArg = ["평가기준", "업체직접선정"];
		this.gfn_Alert("MM003", arrMsgArg);  //평가기준이 업체직접선정일 경우에는 견적의뢰업체를 1건만 등록해야 합니다.
		
		return false;

	//내외자구분 - 국내외구분(0020)의 경우 1건만 저장되어야 함 (7/15)
	} else if (sDoimOp == '0020' && iMst11Count > 0) { 
		var arrMsgArg = ["내외자구분", sDoimNm];
		this.gfn_Alert("MM003", arrMsgArg); //내외자구분이 &ITM0일 경우에는 견적의뢰업체를 1건만 등록해야 합니다.
		return false;

	} else {
		var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Master11);//crud_flag 변경-입력(I)

		this.ds_Master11.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
		this.ds_Master11.setColumn(iAddRow, "QUTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO"));
		this.ds_Master11.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));

		//견적의뢰업체 행추가시 추가된 Row 거래처 항목으로 Focus
		this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master11.setCellPos(3, iAddRow);
		this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master11.setFocus();
	}
};

