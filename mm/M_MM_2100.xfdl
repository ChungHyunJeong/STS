this.fn_ComBtnAdd = function()
{
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10) || this.gfn_DsIsUpdated(this.ds_Master11) || this.gfn_DsIsUpdated(this.ds_Detail10) || this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	
	var iAddRow = this.ds_Master10.addRow();
	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", "1");//결재차수
	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");//최종여부	
	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//의뢰부서코드	
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//의뢰부서명	
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//의뢰자사번	
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//의뢰자명		
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//의뢰자ID	
	this.ds_Master10.setColumn(iAddRow, "QUTH_DD", sToDay);   //의뢰일자-오늘날자로 설정
	this.ds_Master10.setColumn(iAddRow, "REQU_DD", this.gfn_AddDate(sToDay, 7));   //요청일자-오늘날자+7로 설정(2022-05-18 요청)
	this.ds_Master10.setColumn(iAddRow, "EVAL_CD", "0020");   //평가기준 (9/28 Default -  경쟁:0020)
	this.ds_Master10.setColumn(iAddRow, "DOIM_GU", "1");   //내외자구분 (9/28 Default - Domestic:1)
	this.ds_Master10.setColumn(iAddRow, "YEAR_YN", "0");      //연간단적적용 (9/28 비활성화)
	this.ds_Master10.setColumn(iAddRow, "CURR_CD", "0090");   // 화폐코드 Default "KRW"
	this.ds_Master10.setColumn(iAddRow, "EXCH_RT", "1.00");   // 환율 Default "1.00"	

	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.div_ComGrdBtn.set_enable(true);
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.div_DetailInput.form.tab_Detail.set_enable(true);

	//견적미의뢰 Detail Tab으로 이동
	this.div_DetailInput.form.tab_Detail.set_tabindex(1);			

	//처음 입력 항목에 포커스
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_QuthNm.setFocus();

	//견적의뢰번호 포멧 변경
	this.div_MasterInput.form.tab_Master.tpg_01.form.mae_QuthNo.set_format("@@@@@@-@@@@");		

	//견적미의뢰검색 TAB - 청구자
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_EmpmCd.set_value(this.gfn_GetSessionValue("")); //청구자 ID (청구부서 -> 로그인 정보 2021-02-07 -> 빈값 2022-02-21)
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_EmpmNm.set_value(this.gfn_GetSessionValue("")); //청구자명 (청구부서 -> 로그인 정보 2021-02-07 -> 빈값 2022-02-21)
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_SystId.set_value(this.gfn_GetSessionValue("")); //청구자사번 (청구부서 -> 로그인 정보 2021-02-07 -> 빈값 2022-02-21)
		
	this.ds_Master11.clearData();//견적의뢰업체 초기화
	this.ds_Detail10.clearData();//견적의뢰상세 초기화
	this.ds_Detail20.clearData();//견적미의뢰검색 초기화
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
	//첨부파일정보 권한설정
	var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
	oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
	this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
	
	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
	
}

