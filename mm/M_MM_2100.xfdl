/***********************************************************************************************
 * 02.MG0.07. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
			
	//  견적의뢰일자 입력(변경)시
	if (e.columnid == "QUTH_DD") {
		var sCurrCd  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐코드
		var sQuthDd  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_DD");//견적의뢰일자
		var nOrgRatm = this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "EXCH_RT");//이전 환율정보

		//화폐코드
		if (!this.gfn_IsNull(sCurrCd)) {	//화폐코드 존재할 경우
			var nRatm = this.gfn_GetCommRatm(sQuthDd, sCurrCd);//환율정보
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EXCH_RT", nRatm);	//환율정보

			//이전환율정보와 재조회된 환율정보가 서로 다를경우 Detail10 원화, 외화 데이터 변경
			if (!this.gfn_IsNull(nOrgRatm) && nOrgRatm != nRatm) {
				this.fn_RatmChange();
			}
		} 
		
		
	// 화폐코드 변경시
	} else if (e.columnid == "CURR_CD") {
		var sCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐코드
		var sQuthDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_DD");//견적의뢰일자
		var nOrgRatm = this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "EXCH_RT");//이전 환율정보

		//견적의뢰일자
		if (!this.gfn_IsNull(sQuthDd)) {	//견적일자 존재할 경우
			var nRatm = this.gfn_GetCommRatm(sQuthDd, sCurrCd);//환율정보
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EXCH_RT", nRatm);	//환율정보

			//이전환율정보와 재조회된 환율정보가 서로 다를경우 Detail10 원화, 외화 데이터 변경
			if (!this.gfn_IsNull(nOrgRatm) && nOrgRatm != nRatm) {
				this.fn_RatmChange();
			}
		} 		
		
	//내외자구분
	} else if (e.columnid == "DOIM_GU") {		

		//견적금액정보(견적단가, 원화금액, 외화금액) 활성화 유무 체크
		this.fn_DoimEvalCdChange();

	//의뢰구분
	} else if (e.columnid == "QUTH_GU") {	
		this.ds_Detail20.clearData(); //의뢰구분 선택시 견적미의뢰검색 초기화	
	
		var sQuthGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_GU");
		
		//의뢰구분 값이 선택될 경우만 조회
		if (!this.gfn_IsNull(sQuthGu)) {
