/***********************************************************************************************
 * 02.DG0.07. [Detail 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)

	// 견적단가 변경(변경)시 원화금액/외화금액 변경
	if (e.columnid == "QUTB_PR") {
		var sCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐코드
		var iExchRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");//환율정보
		var iQutbQt = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUTB_QT");//수량
		var iQutbPr = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUTB_PR");//견적단가
		var iWoneAm = 0; //원화금액 
		var iForeAm = 0; //외화금액 

		if (!this.gfn_IsNull(iQutbPr)) {	//견적단가 존재시			
			var iWoneAm = nexacro.floor(iExchRt * iQutbQt * iQutbPr, 2);	//원화금액 = 환율 * 수량 * 견적단가

			if (sCurrCd == '0090') {	//KRW(원화)
				var iForeAm = 0;	// 외화금액 = 0원
			} else {
				var iForeAm = iQutbQt * iQutbPr;	//외화금액 = 수량 * 견적단가
			}
		
			//그리드에 적용
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "WONE_AM", iWoneAm);	//원화금액
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "FORE_AM", iForeAm);	//외화금액
			
		} else { //공백시 초기화
			//그리드에 적용
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "WONE_AM", "");	//원화금액
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "FORE_AM", "");	//외화금액
		}
	}
	
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Detail20] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{

	//체크시 해당 프로그램 전체선택
	if (e.columnid == "s_chk") {	
		if (this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "s_chk") == '1') {		
			var sPcrhNo = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "PCRH_NO");//청구번호
			var sPcrbSe = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "PCRB_SE");//청구순번
			var sPcrhGu = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "PCRH_GU");//청구구분

			//Detail10에 청구번호가 존재하는지 확인
			var iDupCnt = this.ds_Detail10.getCaseCount("PCRH_NO == '"+sPcrhNo+"' && PCRB_SE == '"+sPcrbSe+"'"); 
			if(iDupCnt > 0 ) {
				var arrMsgArg = ["청구번호"];
				this.gfn_Alert("SD008", arrMsgArg); //해당 청구번호가 존재합니다.
				this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "s_chk","0");//체크해제
				return;
			}			
			
			//Detail20에 청구구분이 존재하는지 확인 : 중복체크(2022-03-15 추가요청)
			var iChkCnt = this.ds_Detail20.getCaseCount("s_chk == '1' || s_conf == '1'");	//체크건수
			if(iChkCnt > 1 ) {
				iDupCnt = this.ds_Detail20.getCaseCount("(s_chk == '1' || s_conf == '1') && PCRH_GU == '"+sPcrhGu+"'"); //청구구분건수
				if(iDupCnt == 1 ) {					
					var arrMsgArg = ["청구구분"];
					this.gfn_Alert("SM057", arrMsgArg); //동일한 청구구분만 적용 가능합니다.
					this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "s_chk","0");//체크해제
					return;
				}
			}			
			
		}
		
	//견적수량 입력시
	} else if (e.columnid == "QUTB_QT") {	
		var iNqutQt = nexacro.toNumber(this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "NQUT_QT"));//미의뢰수량
		var iQutbQt = nexacro.toNumber(this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "QUTB_QT"));//견적수량

		if (this.gfn_IsNull(iNqutQt)) {
			iNqutQt = 0;
		}

		if (this.gfn_IsNull(iQutbQt)) {
			iQutbQt = 0;
		}
		
		//견적수량 > 미의뢰수량 시 메시지 표기
		//공백이거나 1 이하 입력시 메시지 및 미의뢰수량의로 원복
		if (iQutbQt < 1) {
			this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "QUTB_QT", this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "NQUT_QT")); //견적수량 = 미의뢰수량 초기화


			var arrMsgArg = ["견적수량"];
			this.gfn_Alert("SM045", arrMsgArg); //견적수량은 1이상의 입력하셔야 합니다.
			return false;		
			
		//견적수량 > 미의뢰수량의 경우 메시지 및 미의뢰수량의로 원복
		} else if (iQutbQt > iNqutQt) {
			this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "QUTB_QT", this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "NQUT_QT")); //견적수량 = 미의뢰수량 초기화
			
			var arrMsgArg = ["견적수량", "미의뢰수량"];
			this.gfn_Alert("SM056", arrMsgArg); //견적수량이 미의뢰수량을 초가할 수 없습니다.
			return false;		

		//정상처리시
		} else {
			this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "s_chk", "1"); //값 변경시 자동체크
		}
	}
	
};

