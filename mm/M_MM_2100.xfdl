		}
		sPcrhGu = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_PcrhGu.value;	//청구구분(그리드 컨트롤를 하기 위해 재조회)
		
	} else if (sGridGu == 'Detail') {
		sPcrhGu = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_PcrhGu.value;	//청구구분
		
		if (sPcrhGu == "0010") { 	//청구구분 구매품(0010) 선택시 의뢰구분 구매품(0020) 변경
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "QUTH_GU", "0020");	//의뢰구분 - 구매품(0020)
		
		} else if (sPcrhGu == "0020") { 	//청구구분 외주가공(0020) 선택시 의뢰구분 외주가공(0030) 변경
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "QUTH_GU", "0030");	//의뢰구분 - 외주가공(0030)

