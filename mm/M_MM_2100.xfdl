			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "QUTH_GU", "0010");	//의뢰구분 - 턴키외주(0010)

		} else {	//의뢰구분 - 초기화
			//this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_QuthGu.set_index(-1);		//의뢰구분 - 초기화
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "QUTH_GU", "0090");	//의뢰구분 - 턴키외주(0010)
			
		}
	}
	
	//청구구분이 외주가공(0020)인 경우 
	if (sPcrhGu == "0020") { 	
		//Detail10
		gDetail10.setFormatColProperty(8, "size", 70);		//중분류 칸사이즈 복구
