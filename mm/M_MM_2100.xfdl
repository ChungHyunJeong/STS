			}	
			
			if (this.div_DetailInput.form.tab_Detail.tabindex == 0) {
				//도면정보가 있으면
				if (this.ds_Detail10.getCaseCount("DWGF_YN == 'O'") > 0) {
					this.div_DetailInput.form.btn_DwgAllSave.set_enable(true);//도면일괄저장
				} else {
					this.div_DetailInput.form.btn_DwgAllSave.set_enable(false);//도면일괄저장
				}		
			} else {
				this.div_DetailInput.form.btn_DwgAllSave.set_enable(false);//도면일괄저장
			}			
			
			break;			
			
		case "DTL20_SEARCH": //제품사양검색		
			this.ds_Detail20.addColumn("s_chk", "string");		
			this.ds_Detail20.addColumn("s_conf", "string");

			if (this.ds_Detail20.getRowCount() > 0) {
