/***********************************************************************************************
 * 15.05. [데이터변경] 환율정보 변경시
 ***********************************************************************************************/ 
this.fn_RatmChange  = function()
{
	//Detal10 견적단가 기준으로 원화단가/원화금액, 외화단가/외화금액 적용
	var sCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐코드
	var iExchRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");//환율정보

	var iQutrQt = 0; 	//견적의뢰상세 수량
	var iQutbPr = 0; 	//견적의뢰상세 견적단가
	var iWoneAm = 0;	//견적의뢰상세 원화금액
	var iForeAm = 0;	//견적의뢰상세 외화금액
	
	//Detail10 데이터셋에 원화금액, 외화금액 변경
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
	
		iQutbQt = this.ds_Detail10.getColumn(iLoop, "QUTB_QT");//수량
		iQutbPr = this.ds_Detail10.getColumn(iLoop, "QUTB_PR");//견적단가
		
		iWoneAm = nexacro.floor(iExchRt * iQutbQt * iQutbPr, 2);	//원화금액 = 환율 * 수량 * 견적단가

		if (sCurrCd == '0090') {			//KRW(원화)
			iForeAm = 0;					//외화금액 = 0원
		} else {
			iForeAm = iQutbQt * iQutbPr;	//외화금액 = 수량 * 견적단가
		}
		
		//Detail10 데이터셋에 적용
		this.ds_Detail10.setColumn(iLoop, "WONE_AM", iWoneAm);	//원화금액
		this.ds_Detail10.setColumn(iLoop, "FORE_AM", iForeAm);	//외화금액
	}	

}		


/***********************************************************************************************
 * 15.06. [데이터변경] 내외자구분 및 평가기준 변경시
 ***********************************************************************************************/ 
this.fn_DoimEvalCdChange  = function()
{
	var sDoimGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DOIM_GU");//내외자구분
	var iFindRow = this.ds_Mm010.findRowExpr("CODM_CD=='" + sDoimGu +"'");
	var sDoimOp = this.ds_Mm010.getColumn(iFindRow,"OP01_NM"); //국내외구분 
	var sEvalCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EVAL_CD");//평가기준
	iFindRow = this.ds_Mm215.findRowExpr("CODM_CD=='" + sEvalCd +"'");
	var sEvalOp = this.ds_Mm215.getColumn(iFindRow,"OP01_NM"); //업체직접선정
	
	var sCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐
	var iExchRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");//환율
	var iQutbQt = 0;//수량
	var iQutbPr = 0;//견적단가
	var iWoneAm = 0;//원화금액
	var iForeAm = 0;//외화금액

	//내외자구분으로 국내외구분(0020) 대상인 경우만 견적단가 입력가능
	//평가기준 업체직접선정 대상 (2022-04-21 추가개발)
	if (sDoimOp == '0020' || sEvalOp == '1') { //내외자구분 - 국내외구분(0020) 대상, 평가기준 - 업체직접선정
		//Detail10 견적단가(col25) 입력 활성화, Head 필수표시
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 27, "edittype", "text");				
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 37, "cssclass", "essential");

		for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
			//견적단가, 원화, 외화 계산
			iQutbPr = this.ds_Detail10.getColumn(iLoop, "QUTB_PR");	//견적단가
			
			if (!this.gfn_IsNull(iQutbPr)) {	//견적단가 존재시
				iQutbQt = this.ds_Detail10.getColumn(iLoop, "QUTB_QT");	//수량
				iWoneAm = nexacro.floor(iExchRt * iQutbQt * iQutbPr, 2);	//원화금액 = 환율 * 수량 * 견적단가

				if (sCurrCd == '0090') {	//KRW(원화)
					iForeAm = 0;	// 외화금액 = 0원
				} else {
					iForeAm = iQutbQt * iQutbPr;	//외화금액 = 수량 * 견적단가
				}

				this.ds_Detail10.setColumn(iLoop,"WONE_AM", iWoneAm);	//원화금액
				this.ds_Detail10.setColumn(iLoop,"FORE_AM", iForeAm); //외화금액
			}
		}

		
	} else {
		//Detail10 견적단가(col25) 입력 비활성화, Head 필수표시 해제
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 27, "edittype", "none");	
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 37, "cssclass", "");
		
		for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
			//견적단가, 원화, 외화 초기화
			this.ds_Detail10.setColumn(iLoop,"QUTB_PR","");	//견적단가
			this.ds_Detail10.setColumn(iLoop,"WONE_AM","");	//원화금액
			this.ds_Detail10.setColumn(iLoop,"FORE_AM","");	//외화금액
		}
	}
};

