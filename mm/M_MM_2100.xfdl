			this.ds_Detail20.setColumn(iLoop, "s_chk", "0"); //Uncheck로 변경
		}
		
		if (this.ds_Detail20.getColumn(iLoop, "s_conf") == "1") {
			this.ds_Detail20.setColumn(iLoop, "s_conf", "0");
		}		
	}
}

