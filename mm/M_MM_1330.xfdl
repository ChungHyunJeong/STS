/***********************************************************************************************
 * 13.03. [메세지] CallBack 함수 처리
 ***********************************************************************************************/  
this.fn_MsgCallback = function (strId, strVal)
{
	
	//MASTER 변경사항 저장여부
	if (strId == "CONF.MAST_CHANGE") {
		if(strVal )
		{
			this.fn_ComBtnSave();//저장 서비스 호출
		} else {
			//데이터 변경여부 체크
			if (this.gfn_DsIsUpdated(this.ds_Master10)) {
				this.gfn_UndoColumn(this.div_MasterView.form.grd_Master10);
			}		
			
			if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
				this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);
			}	
		}
			
	//품목단가확정
	} else if (strId == "CONF.FIX_CHANGE") {
		if(strVal)
		{
			//확인일자, 요청상태 확정으로 변경
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CONF_DD", this.gfn_GetCommDate("toDay"));//확인일자-현재날자로 설정
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "IPRH_YN", "1");//요청상태(요청/취소:0, 확정:1)
			
			//확인부서, 확인자 설정
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CODP_CD", this.gfn_GetSessionValue("DEPM_CD"));//확인부서코드	
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "COSU_CD", this.gfn_GetSessionValue("USEM_ID"));//확인자사번	
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "COSU_NM", this.gfn_GetSessionValue("USEM_NM"));//확인자명		
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "COSU_ID", this.gfn_GetSessionValue("SYST_ID"));//확인자ID	


			//품목단가확정 저장
			this.fn_ItemUnitFixSave();
		}
		
	//품목단가취소
	} else if (strId == "CONF.CANCEL_CHANGE") {
		if(strVal)
		{
			//요청상태 요청(취소)로 변경, 확인일자/부서/사번 초기화
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "IPRH_YN", "0");//요청상태(요청/취소:0, 확정:1)			
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CONF_DD", "");//확인일자-현재날자로 설정
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CODP_CD", "");//확인부서코드	
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "COSU_CD", "");//확인자사번	
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "COSU_NM", "");//확인자명		
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "COSU_ID", "");//확인자ID	

			//저장시 재조회 Key설정
			this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "IPRH_NO");//조회Key01
			this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//조회Key02

			//품목단가취소 저장호출
			this.fn_ItemUnitCancelSave();
		}		

	}
}; 


