/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {

		//요청번호정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "IPRH_NO"))) {
		
			//## 요청기한에 의한 처리로직은 설계자 확인 후 추가하지 않음(추후 검토 2020-01-14))
			//[요청상태 (요청/취소:0, 확정:1)]품목단가확정/품목단가취소 버튼 활성화/비활성화
			if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "IPRH_YN") == '0') { //요청(취소)
				this.div_MasterInput.form.btn_SelectFix.set_enable(true);     //품목단가확정
				this.div_MasterInput.form.btn_SelectCancel.set_enable(false); //품목단가취소

				//Master10 비고(파트너사) 입력가능
				this.div_MasterInput.form.tab_Master.tpg_01.form.txt_IprhNm.set_readonly(false);
				
				//Detail10 그리드 입력가능
				this.gfn_SetCompGroupEnable(this.div_DetailInput.form, true);//Detail 입력			
			} else {																		  //확정
				this.div_MasterInput.form.btn_SelectFix.set_enable(false);    //품목단가확정
				this.div_MasterInput.form.btn_SelectCancel.set_enable(true);  //품목단가취소			

				//Master10 비고(파트너사) 입력불가
				this.div_MasterInput.form.tab_Master.tpg_01.form.txt_IprhNm.set_readonly(true);
				
				//Detail10 그리드 입력불가
				this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 입력			
			}			

			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Input.clearData();
			this.fvFileDiv.fn_ClearAtchFile();//Div첨부파일 정보 초기화
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "IPRH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "IPRH_NO"));
			this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
					
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "mm:M_MM_1330_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);

			//[공통]서비스 호출 변수
