 ***********************************************************************************************/ 
this.fn_VaildDetailFixSave = function()
{	
	var iItrbPr = 0;	//단가
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		iItrbPr = this.ds_Detail10.getColumn(iLoop, "IPRB_PR");	//단가

		if (this.gfn_IsNull(iItrbPr) || iItrbPr < 1) {
			//탭, 위치 이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);			
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(9,iLoop);
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();
			
			var arrMsgArg = ["품목단가"];
			this.gfn_Alert("SM055", arrMsgArg);//품목단가를 1이상 입력해주세요.
			return false;
			
			break;			
		}
	}

	return true;
}


