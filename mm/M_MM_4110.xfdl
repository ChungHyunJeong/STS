	this.ds_Input.setColumn(0, "ITEM_NM", this.div_Search.form.edt_ItemNm.value);	
	this.ds_Input.setColumn(0, "PRGM_ID", "M_MM_4110");//프로그램ID
	
	var sGroup = "";
	var arrCode = this.div_Search.form.edt_Ct01Cd.code.split(",");//조건1
	var allCheck = this.div_Search.form.edt_Ct01Cd.value.split(",");//ALL선택체크(2022-06-30 요청 - ALL인경우 전체조회)	
	
	for (var iLoop=0;iLoop<arrCode.length;iLoop++) {
		if (!this.gfn_IsNull(arrCode[iLoop])) {
			//마지막은 컴마를 붙이지 않음
			if (iLoop == arrCode.length-1) {
				sGroup += "'" + arrCode[iLoop] + "'";
			} else {
				sGroup += "'" + arrCode[iLoop] + "',";
			}
		}
	}	
	//-- 전체 선택이면 공백처리(공통사항에서 미사용 자료는 여기에 안나오므로 SELECT가 안되서 
	
	if (this.gfn_IsNull(arrCode[0]) || allCheck[0] == "ALL") {sGroup= "";}
	
	
// 	trace("arrCode.toString() --> " + arrCode.toString());
