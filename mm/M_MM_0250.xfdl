 ***********************************************************************************************/ 
this.fn_YearUnitSumProc = function()
{
	/* 2022-04-08 자동입력 설정되는 문구, 주석처리 요청 (김형수부장)	
	
	// 1. 연간단가상세내용 sum
	// 2. 연간단가제외 대상 제외
    // 전년도 연간단가 * 전년도 발주수량(최근1년) = 전체금액
    // 연간단가 * 전년도 발주수량(최근1년) = 전체금액
    // 전년도 연간단가 전체금액 / 연간단가 전체금액 = 요츌

	var sBefoTotMemo  = "전년도 연간단가 * 전년도 발주수량(최근1년) = 전체금액\n";		//전년도   연간단가 전체금액
	var sNowTotMemo   = "연간단가 * 전년도 발주수량(최근1년) = 전체금액\n";				//당해년도 연간단가 전체금액
	var sUnitRateMemo = "전년도 연간단가 전체금액 / 연간단가 전체금액 = 요율\n";	//연간단가 요율
	
	var iBfypPrSum = this.gfn_Nvl(this.ds_Detail10.getCaseSum("PTEX_YN = '0'", "BFYP_PR"), 0); //전년도 연간단가 합계
	var iOneyQtSum = this.gfn_Nvl(this.ds_Detail10.getCaseSum("PTEX_YN = '0'", "ONEY_QT"), 0); //전년도 발주수량(최근1년) 합계
	var iYprhPrSum = this.gfn_Nvl(this.ds_Detail10.getCaseSum("PTEX_YN = '0'", "YPRH_PR"), 0); //연간단가 합계 
	var iBfypPrTotal = iBfypPrSum * iOneyQtSum; //전년도 연간단가 전체금액 = 전년도 연간단가 * 전년도 발주수량(최근1년)
	var iYprhPrTotal = iYprhPrSum * iOneyQtSum; //연간단가 전체금액 = 연간단가 * 전년도 발주수량(최근1년)
	var iYprhPrRate = 0;                        //연간단가 요율 = 전년도 연간단가 전체금액 / 연간단가 전체금액

	if (iBfypPrTotal != 0) {	//(분모)연간단가 전체금액이 0이 아닌경우
		iYprhPrRate = iBfypPrTotal / iYprhPrTotal;	//연간단가 요율계산
	}
	
	sBefoTotMemo  = sBefoTotMemo  + this.gfn_AppendComma(iBfypPrSum,0) + " * " + this.gfn_AppendComma(iOneyQtSum,0) + " = " + this.gfn_AppendComma(iBfypPrTotal,0) + "\n";	//전년도 연간단가 전체금액
	sNowTotMemo   = sNowTotMemo   + this.gfn_AppendComma(iYprhPrSum,0) + " * " + this.gfn_AppendComma(iOneyQtSum,0) + " = " + this.gfn_AppendComma(iYprhPrTotal,0) + "\n";	//당해년도 연간단가 전체금액
	sUnitRateMemo = sUnitRateMemo + this.gfn_AppendComma(iBfypPrTotal,0) + " / " + this.gfn_AppendComma(iYprhPrTotal,0) + " = " + this.gfn_AppendComma(iYprhPrRate,0) + "%";	    //연간단가 요율
	
	var sRemkNm = sBefoTotMemo + sNowTotMemo + sUnitRateMemo;	//비고에 입력되는 내용 최종

	this.ds_Master10.setColumn(this.ds_Master10.rowposition, "REMK_NM", sRemkNm);
	
	
	*/
};

