this.cbo_DoimGu_onitemchanged = function(obj){
	
	//[결재] 결재내역 조회가 아니면
	if (!this.fvWfSearchMode) {
		this.ds_Detail10.reset();
		//내외자상세내역 타이틀
		this.div_MasterInput.form.tab_Master.tpg_02.form.sta_02_Title.set_text("["+obj.text+"]");
		//trace("내외자값"+ obj.value);
		//trace(this.ds_Master10.getRowType(this.ds_Master10.rowposition));
		//trace(this.fvWofhStatus);
		//결재상태가 "등록"이 아닐경우
		if (this.fvWofhStatus) {
			this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);
			//[결재] Div하위의 컴포넌트 Enable 처리
			this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
			this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 입력			
			
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_ParePop.set_enable(true);// 비고(내부)
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_RemkPop.set_enable(true);// 비고(협력사)
			this.div_MasterInput.form.btn_PcohM1.set_enable(true);	//파트너사 견적보기버튼
		}else{
			if(obj.value == "1"){//Domestic
				
				//지불조건(내자), 포장방법, 인도조건, 검사방법, 부가세코드, 발주금액, 부가세금액.
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_PaymCd.set_enable(true);//지불조건(내자)
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_PackGu.set_enable(true);//포장방법
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_TermCd.set_enable(true);//인도조건
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_InspCd.set_enable(true);//검사방법
					
				if(this.ds_Master10.getRowType(this.ds_Master10.rowposition) =="2"){//입력시에만
					this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_PaymCd.set_index(0); 	//지불조건(내자) 기본값
					this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_PackGu.set_index(0); 	//포장방법	기본값
					this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_TermCd.set_index(0);	//인도조건 기본값
					this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_InspCd.set_index(0); 	//검사방법	기본값
				}
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_ImpaCd.set_enable(false);//지불조건(외자)
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_PricCd.set_enable(false);//가격조건
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_NatmCd.set_enable(false);//원산지
				this.div_MasterInput.form.tab_Master.tpg_02.form.cal_ShipDd.set_enable(false);//Shipping Date(S/D)
				this.div_MasterInput.form.tab_Master.tpg_02.form.cal_ExpiDd.set_enable(false);//Expire Date(E/D)
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_ShipCd.set_enable(false);//선적항
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_ArriCd.set_enable(false);//도착항
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_ExchCd.set_enable(false);//환율기준
				this.div_MasterInput.form.tab_Master.tpg_02.form.cal_ExchDd.set_enable(false);//환율적용일자
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_TranCd.set_enable(false);//운송방법
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_PatyCd.set_enable(false);//포장형태
				
				
			}else if(obj.value == "2"){//Import
				//지불조건(외자), 가격조건, 원산지, Shipping Date(S/D), Expire Date(E/D), 선적항, 도착항, 환율기준, 환율적용일자, 운송방법, 포장형태
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_PaymCd.set_enable(false);//지불조건(내자)
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_PackGu.set_enable(false);//포장방법
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_TermCd.set_enable(false);//인도조건
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_InspCd.set_enable(false);//검사방법
				
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_ImpaCd.set_enable(true);//지불조건(외자)
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_PricCd.set_enable(true);//가격조건
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_NatmCd.set_enable(true);//원산지
				this.div_MasterInput.form.tab_Master.tpg_02.form.cal_ShipDd.set_enable(true);//Shipping Date(S/D)
				this.div_MasterInput.form.tab_Master.tpg_02.form.cal_ExpiDd.set_enable(true);//Expire Date(E/D)
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_ShipCd.set_enable(true);//선적항
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_ArriCd.set_enable(true);//도착항
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_ExchCd.set_enable(true);//환율기준
				this.div_MasterInput.form.tab_Master.tpg_02.form.cal_ExchDd.set_enable(true);//환율적용일자
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_TranCd.set_enable(true);//운송방법
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_PatyCd.set_enable(true);//포장형태
				if(this.ds_Master10.getRowType(this.ds_Master10.rowposition) =="2"){//입력시에만
					this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_PaymCd.set_index(-1); 	//지불조건(내자) 기본값 재거
					this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_PackGu.set_index(-1); 	//포장방법	기본값 재거
					this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_TermCd.set_index(-1);	//인도조건 기본값 재거
					this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_InspCd.set_index(-1); 	//검사방법	기본값 재거
				}
				
			}else if(obj.value == "3"){//Local
				//지불조건(내자), 포장방법, 인도조건, 검사방법, Shipping Date(S/D), Expire Date(E/D), 환율기준, 환율적용일자
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_PaymCd.set_enable(true);//지불조건(내자)
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_PackGu.set_enable(true);//포장방법
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_TermCd.set_enable(true);//인도조건
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_InspCd.set_enable(true);//검사방법
				
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_ImpaCd.set_enable(false);//지불조건(외자)
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_PricCd.set_enable(false);//가격조건
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_NatmCd.set_enable(false);//원산지
				this.div_MasterInput.form.tab_Master.tpg_02.form.cal_ShipDd.set_enable(true);//Shipping Date(S/D)
				this.div_MasterInput.form.tab_Master.tpg_02.form.cal_ExpiDd.set_enable(true);//Expire Date(E/D)
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_ShipCd.set_enable(false);//선적항
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_ArriCd.set_enable(false);//도착항
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_ExchCd.set_enable(true);//환율기준
				this.div_MasterInput.form.tab_Master.tpg_02.form.cal_ExchDd.set_enable(true);//환율적용일자
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_TranCd.set_enable(false);//운송방법
				this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_PatyCd.set_enable(false);//포장형태
				
			}
		}
	}
	
	
}

