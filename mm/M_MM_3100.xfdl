		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm.set_readonly(true);	//발주자명
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_QuthNo.set_readonly(true);	//견적의뢰번호
		
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_readonly(true);	//발주업체코드
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmNm.set_readonly(true);	//발주업체명
