		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_VatmNm.set_readonly(true);	//부가세코드
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_VatmRt.set_readonly(true);	//부가세요율
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_WoneAm.set_readonly(true);	//발주금액
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_VtaxAm.set_readonly(true);	//부가세금액
