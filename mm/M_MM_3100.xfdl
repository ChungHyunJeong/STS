 /*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 적용 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.fn_DetailGrd02Choice = function()
{
	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {
			
			//부가세 코드 체크
			if(this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "VATM_RT"))   ){
				var arrMsgArg = ["부가세코드"];
				this.gfn_Alert("SM008", arrMsgArg); //[부가세코드]은(는) 필수 입력 항목입니다.
				return;
			}
			
			var sCurrCdChk, sPcthGuChk, sDoimGuChk;
			//동일 (발주,청구)구분 체크
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					
					
					//매출연계 탭 화폐 체크
					if(this.ds_Detail10.rowcount > 0 ){
						if(this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CurrCd.value != this.ds_Detail20.getColumn(iLoop,"CURR_CD") ){
							var arrMsgArg = ["화폐"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 [화폐]만 적용 가능합니다.
							return;
						}
					}
					
					//그리드내 화폐 체크
					if(!this.gfn_IsNull(sCurrCdChk) ){
						if(sCurrCdChk != this.ds_Detail20.getColumn(iLoop,"CURR_CD") ){
							var arrMsgArg = ["화폐"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 [화폐]만 적용 가능합니다.
							return;
						}
					}
					sCurrCdChk = this.ds_Detail20.getColumn(iLoop,"CURR_CD");
					
					trace("발주구분"+this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_PcohGu.value);
					trace("청구구분"+this.ds_Detail20.getColumn(iLoop,"PCRH_GU"));
					//발주상세 탭 청구구분 체크
					if(this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_PcohGu.value != this.ds_Detail20.getColumn(iLoop,"PCRH_GU") ){
						var arrMsgArg = ["발주구분","청구구분"];
						this.gfn_Alert("SM058", arrMsgArg); //[발주구분]와(과) 동일한 [청구구분]만 적용 가능합니다.
						return;
					}

					//동일 청구구분 체크
					if(this.ds_Detail10.rowcount > 0 ){
						if(this.ds_Detail10.findRowExpr("PCRH_GU=='" + this.ds_Detail20.getColumn(iLoop,"PCRH_GU") + "'") == -1){
							var arrMsgArg = ["청구구분"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 [청구구분]만 적용 가능합니다.
							return;
						}
					}
					
					
					//그리드내 청구구분 체크
					if(!this.gfn_IsNull(sPcthGuChk) ){
						if(sPcthGuChk != this.ds_Detail20.getColumn(iLoop,"PCRH_GU") ){
							var arrMsgArg = ["청구구분"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 [청구구분]만 적용 가능합니다.
							return;
						}
					}
					sPcthGuChk = this.ds_Detail20.getColumn(iLoop,"PCRH_GU");
					
					//trace("발주 내외자"+this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_DoimGu.value);
					//trace("청구 내외자"+this.ds_Detail20.getColumn(iLoop,"DOIM_GU"));
					//내외자구분 체크 
					if(this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_DoimGu.value != this.ds_Detail20.getColumn(iLoop,"DOIM_GU") ){
						var arrMsgArg = ["내외자구분"];
						this.gfn_Alert("SM057", arrMsgArg); //동일한 [내외자구분]만 적용 가능합니다.
						return;
					}
					
					
					//그리드내 청구구분 체크
					if(!this.gfn_IsNull(sDoimGuChk) ){
						if(sDoimGuChk != this.ds_Detail20.getColumn(iLoop,"DOIM_GU") ){
							var arrMsgArg = ["내외자구분"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 [내외자구분]만 적용 가능합니다.
							return;
						}
					}
					sDoimGuChk = this.ds_Detail20.getColumn(iLoop,"DOIM_GU");
					
					//발주수량 체크 
					if(this.ds_Detail20.getColumn(iLoop, "PCOB_QT") == 0){
						var arrMsgArg = ["발주수량"];
						this.gfn_Alert("SM055", arrMsgArg); //[매입 수량]을(를) 1 이상 입력해 주세요.
						return;
					}
					
					//trace("가능발주 수량"+this.ds_Detail20.getOrgColumn(iLoop, "PCOB_QT"));
					//trace("입력발주 수량"+this.ds_Detail20.getColumn(iLoop, "PCOB_QT"));
					 
					if(this.ds_Detail20.getOrgColumn(iLoop, "PCOB_QT")  < this.ds_Detail20.getColumn(iLoop, "PCOB_QT") ){
						var arrMsgArg = ["발주수량", "미발주 수량"];
						this.gfn_Alert("SM056", arrMsgArg); //[매출반영금액]이(가) [미매출금액]을(를) 초과할 수 없습니다.
						return;
					}
				}
			 }
			
			var arrCheckRow = [];//체크된 Row
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
			
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					
					//trace("------->"+ this.ds_FaVatm.rowposition);
					
					var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)
					
					//첫번째일 경우 마스터 업데이트
					if(this.ds_Detail10.rowcount == 1 ){
					
						var nRatm = this.gfn_GetCommRatm(sToDay, this.ds_Master10.getColumn(this.ds_Master10.rowposition,"CURR_CD"));//환율정보
						this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EXCH_RT", nRatm);		// 환율
						this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CurrCd.set_enable(false);	//화폐
						
						//발주업체  
						this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CUSM_CD", this.ds_Detail20.getColumn(iLoop, "SLCS_CD"));
						this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CUSM_NM", this.ds_Detail20.getColumn(iLoop, "CUSM_NM"));
						
					}
					
					
					//미발주검색에서 발주상세로 이동
					this.ds_Detail10.copyRow(iAddRow, this.ds_Detail20, iLoop);
					this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));	//회사코드
					this.ds_Detail10.setColumn(iAddRow, "PCOH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCOH_NO"));	//발주번호
					this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));	
					//this.ds_Detail10.setColumn(iAddRow, "PDRB_SE", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PDRB_SE"));
					
					
					var sExchRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");	//환율
					var sQitbPr = this.ds_Detail20.getColumn(iLoop, "QUTB_PR")
					
					if(this.ds_Master10.getColumn(this.ds_Master10.rowposition,"CURR_CD") =="0090"){//원화
						this.ds_Detail10.setColumn(iAddRow, "WONE_AM", sQitbPr);	//금액
						this.ds_Detail10.setColumn(iAddRow, "FORE_AM", 0);	//외화단가		
					}else{//외화
						this.ds_Detail10.setColumn(iAddRow, "WONE_AM", sExchRt * sQitbPr);	//금액
						this.ds_Detail10.setColumn(iAddRow, "FORE_AM", sQitbPr);	//외화단가		
					}
					
					arrCheckRow.push(iLoop );
					
					//선택항목 비활성화처리
					this.ds_Detail20.setColumn(iLoop, "s_conf", "1");	//미발주검색 컬러 변경 구분자 업데이트
					this.ds_Detail20.setColumn(iLoop, "s_chk", "0");	//Uncheck로 변경
					
				}
				
			}
			
			//환율계산
			this.fn_Reckoning();
			//발주상세로 Tab이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);
		}
	}
};



 /*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Detail10] 환율계산 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.fn_Reckoning = function(){
				

			//발주금액정보 계산
			var sExchRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");	//환율
			var sCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");	//화폐
			//var sQitbPr = this.ds_Detail20.getColumn(iLoop, "QUTB_PR");							//단가
			var sPcobQt = this.ds_Detail20.getColumn(iLoop, "PCOB_QT");							//발주 수량
			var sVatmRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "VATM_RT");	//부가세 요율
			
			
			for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
				if(this.ds_Detail10.getColumn(iLoop, "crudType") != "D"){//삭제 아닌것만
					var sPcobQt = this.ds_Detail10.getColumn(iLoop, "PCOB_QT");							//발주 수량
					if(sCurrCd == "0090"){//원화
						sQitbPr = this.ds_Detail10.getColumn(iLoop, "WONE_PR");
						this.ds_Detail10.setColumn(iLoop, "WONE_AM", nexacro.floor(sQitbPr * sPcobQt,2));			// 원화금액
						this.ds_Detail10.setColumn(iLoop, "VTAX_AM", nexacro.floor(sQitbPr * sPcobQt * sVatmRt,2));	// 부가세금액
						this.ds_Detail10.setColumn(iLoop, "FORE_PR", 0);											// 외화단가
						this.ds_Detail10.setColumn(iLoop, "FORE_AM", 0);											// 외화금액
					}else{
						sQitbPr = this.ds_Detail10.getColumn(iLoop, "FORE_PR");
						this.ds_Detail10.setColumn(iLoop, "WONE_PR", nexacro.floor(sQitbPr * sExchRt,2));			// 원화단가
						this.ds_Detail10.setColumn(iLoop, "WONE_AM", nexacro.floor(sQitbPr * sPcobQt * sExchRt,2));	// 원화금액
						this.ds_Detail10.setColumn(iLoop, "VTAX_AM", nexacro.floor(sQitbPr * sPcobQt * sVatmRt,2));	// 부가세금액
					}
				}	
			}
			
			//마스터 원화 금액 계산
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM", this.ds_Detail10.getCaseSum("crudType !='D'","WONE_AM"));
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_AM", this.ds_Detail10.getCaseSum("crudType !='D'","FORE_AM"));
			//2022-07-08 부가세와 총합계는 부가세합 -> 원화금액 * 부가세율로 변경
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "VTAX_AM", nexacro.floor(this.ds_Detail10.getCaseSum("crudType !='D'","WONE_AM")*sVatmRt,2));
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "TOTL_AM", this.ds_Detail10.getCaseSum("crudType !='D'","WONE_AM") + nexacro.floor(this.ds_Detail10.getCaseSum("crudType !='D'","WONE_AM")*sVatmRt,2));
		
}

  
