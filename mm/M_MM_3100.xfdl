/***********************************************************************************************
 * 09.02. [공통버튼] 결재요청 유효성체크
 ***********************************************************************************************/  
this.fn_VaildWfDecide = function() {
	
	
	
	//변경여부 체크
	//if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.fvFileDiv.fn_CheckFileAddYn()) {
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10)) {
		//결재진행내역체크
	   if (this.ds_Master10.getRowCount() < 1 || this.ds_Master10.rowposition < 0) {
		  this.gfn_Alert("WF016", arrMsgArg);//결재내역을 선택한 후 결재요청하세요.
		  return false;
	   }
   
		var sWOFH_ST = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST");//결재진행상태
		//결재상태에 따른 결재요청 가능여부 체크		
		if (!this.gfn_CheckDecideWf(sWOFH_ST)) {			
			return false;		
		}

		
	} else {
		var arrMsgArg = ["결재요청"];
		this.gfn_Alert("SM064", arrMsgArg);//변경된 내용이 있습니다.\n저장 후 &ITM0하시기 바랍니다.
		return false;
	}
	
	//구매전행상태에 취소건이 있을경우 결재 불가
	for(var i=0;i<this.ds_Detail10.rowcount; i++) {
		var sProcSt = this.ds_Detail10.getColumn(i, "PROC_ST"); //구매진행상태(MM120)
		if(sProcSt == "1"){
			var arrMsgArg = [""];
			this.gfn_Alert("MM045", arrMsgArg);//구매진행상태 [취소] 건이 있습니다. 삭제 후 결재요청이 가능합니다.
			return false;
		}
	}
	
	/* 
		정충현 추가 : 실행예산 제어 ( 누적투입합계 + 현구매발주합계 > 집행실행예산 )
		메세지 : '집행실행예산을 초과하여 결재요청 할 수 없습니다
				   집행실행예산을 확인하세요 '
		적용일 : 2022.08.22
		해제 : 2022.12.13 ( 다시 알람만 띄우고 발주 나갈 수 있게.. )
	*/
	
	var sPcohGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCOH_GU"); // 발주구분
	var sWofhSt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"); // 결재상태
	
	var dicnFlag = true;
   
   //trace("sPcohGu --> " + sPcohGu);
   if(sPcohGu == "0010" || sPcohGu == "0020" || sPcohGu == "0200") { // 0010:구매품, 0020:외주가공, 0200:턴키외주
      
      if(this.ds_Master30.rowcount > 0) {
      
         for(var i=0;i<this.ds_Master30.rowcount; i++) {
            
            var sCnthNo = this.ds_Master30.getColumn(i, "CNTH_NO"); // 계약번호
            var sCntbSe = this.ds_Master30.getColumn(i, "CNTB_SE"); // 계약순번
            var sConhGu = this.ds_Master30.getColumn(i, "CONH_GU"); // 실행예산구분
            var nujuk_am = parseFloat(this.ds_Master30.getColumn(i, "NUJUK_BALJU_WONE_AM")); // 누적투입합계
            var this_balju_am = parseFloat(this.ds_Master30.getColumn(i, "BALJU_WONE_AM")); // 현구매발주합계
            var excute_am = parseFloat(this.ds_Master30.getColumn(i, "DICN_AM")); // 집행실행예산 합계
            
            
            var excute_per = parseFloat(this.ds_Master30.getColumn(i, "B_DICN_PER_REAL")); // 누적투입진행율
            
            if(sCnthNo != "Z1000") { // 공통계약이 아닐때만
            
               if(sWofhSt == '0010') { // 등록 상태일때만 체크
               
                  if(sConhGu == '집행실행') { // 집행실행에 한해서
                  
                     if(this_balju_am > 0) { // 현발주금액이 존재할 경우에 한해서
                     
                        //if(excute_per >= 100) { // 누적투입진행율이 100 이상일 경우에 한해서
                        if(excute_per > 100) { // 누적투입진행율이 100 초과일 경우에 한해서 23.01.02
                        
                           dicnFlag = false;
                           var arrMsgArg = ["집행실행예산"];
                           this.gfn_Alert("MM035", arrMsgArg);//집행실행예산을 초과하여 결재요청 할 수 없습니다.
                           i = 999999;   continue; // for문 종료
                           return false;
                           
                        }
                        
                     }
                     
                  }
               }
            }
         }
      }
   }
   
   if(!dicnFlag) { return false; }
   return true;
}


