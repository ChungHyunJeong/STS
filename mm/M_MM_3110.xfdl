		var sPcohNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition,"PCOH_NO");
	}else{
		var sPcohNo = this.ds_Master20.getColumn(this.ds_Master20.rowposition,"PCOH_NO");
	}	
	
//	sPcohNo = "A2201050003";
	//this.alert(sPcohNo);
	
	if (this.gfn_IsNull(sPcohNo) ) {
		var arrMsgArg = ["발주번호"];
		this.gfn_Alert("SM028", arrMsgArg); //해당컬럼의 발주번호가 존재하지 않습니다
		
	} else {
		var sPopupId = "M_MO_3400";
		var sPopupTitle = "납품예정일관리"; //Popup화면 타이틀
