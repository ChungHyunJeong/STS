		var  ChanNm = "";
		var  ModiQt = "";
		
		var  sCrudType = "";
		
 		for(var iLoop = 0 ; iLoop < this.ds_Detail10.rowcount; iLoop++){
			
			sCrudType = this.ds_Detail10.getColumn(iLoop,"crudType")
			
			ProcSt = this.ds_Detail10.getColumn(iLoop, "PROC_ST");
			ChanCd = this.ds_Detail10.getColumn(iLoop, "CHAN_CD");
			ChanNm = this.ds_Detail10.getColumn(iLoop, "CHAN_NM");
			
			ModiQt = this.ds_Detail10.getColumn(iLoop, "MODI_QT");
			ModiQt = nexacro.toNumber(ModiQt);
 			
 			//취소, 정정일때
 			if(ProcSt=="1" || ProcSt=="3"){
				if(this.gfn_IsNull(ChanCd)){
					this.ds_Detail10.set_rowposition(iLoop);
					 var arrMsgArg = ["정정사유"];
					 this.gfn_Alert("SM008", arrMsgArg);//[필수 입력 항목입니다.					
