this.ds_Detail10_cancolumnchange = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	if (e.columnid == "MODI_QT") {	
	
// 		var QutbQt =  this.ds_Detail10.getColumn(e.row, "QUTB_QT");
// 		var PcobQt =  this.ds_Detail10.getColumn(e.row, "PCOB_QT");
	
		var ProcSt 		= this.ds_Detail10.getColumn(e.row, "PROC_ST");
		var ModiQt		= e.newvalue;
		
		var PcrbQtOri 	= this.ds_Detail10.getOrgColumn(e.row, "PCRB_QT"); // 청구수량ORI
		var ProbQtOri 	= this.ds_Detail10.getOrgColumn(e.row, "PCOB_QT"); // 발주수량ORI
		var QutbQt 		= this.ds_Detail10.getOrgColumn(e.row, "QUTB_QT"); // 견적의뢰수량
		var BfpcQtOri 	= this.ds_Detail10.getOrgColumn(e.row, "BFPC_QT"); //이전청구수량 ORI		
		
		ModiQt			= nexacro.toNumber(ModiQt);
		ProbQtOri 		= nexacro.toNumber(ProbQtOri);
	
		
		if(ProcSt=="3"){		
			//-- 발주수량이 0보다 클때
			if(!this.gfn_IsNull(ProbQtOri) && ProbQtOri > 0  && ModiQt != 0){
				if(ModiQt  < ProbQtOri){
					this.gfn_Alert("MM036");
					return false;
				}
			}	
			
			//-- 견적의뢰수량이 0보다 클때
			if(!this.gfn_IsNull(QutbQt) && QutbQt > 0  && ModiQt != 0){
			
				if(ModiQt  < QutbQt){
					this.gfn_Alert("MM048");
					return false;
				}
			}
			
			//-- 이전 청구수량보다 크거나 같을때
			
			if(!this.gfn_IsNull(PcrbQtOri) && PcrbQtOri > 0  && ModiQt != 0){
				if(ModiQt  > PcrbQtOri){
					this.gfn_Alert("MM037");
					return false;
				}			
			}	
			
		}
	} else if (e.columnid == "PROC_ST") {	
		var ProbQt	= this.ds_Detail10.getColumn(e.row, "PCOB_QT"); // 발주수량
		var QutbQt 	= this.ds_Detail10.getColumn(e.row, "QUTB_QT"); // 견적의뢰수량
		
		ProbQt		= nexacro.toNumber(ProbQt);
		QutbQt 		= nexacro.toNumber(QutbQt);

		
		if(e.newvalue=="1"){
			//if(ProbQt > 0 || QutbQt > 0){
			if(ProbQt > 0){  //-- 2022.08.31 이성수차장 요청
				var arrMsgArg = ["발주수량이 있으면 취소할 수"];
				this.gfn_Alert("MM046",arrMsgArg);
				return false;
			}			
		}
		
	}
};

