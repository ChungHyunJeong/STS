				}
				
				if(this.gfn_IsNull(ChanNm)){
					this.ds_Detail10.set_rowposition(iLoop);
					var arrMsgArg = ["정정상세사유"];
					this.gfn_Alert("SM008", arrMsgArg);//[필수 입력 항목입니다.					
					return false;
				}
				
				if(ProcSt=="3"){
					if(sCrudType == "U"){				
						if(this.gfn_IsNull(ModiQt) || ModiQt == 0){
							this.ds_Detail10.set_rowposition(iLoop);
							var arrMsgArg = ["정정수량"];
							this.gfn_Alert("SM008", arrMsgArg);//[필수 입력 항목입니다.					
							return false;					
						}
					}
				}				
				
				
			}
 		}	
	
	return true;
}
 
