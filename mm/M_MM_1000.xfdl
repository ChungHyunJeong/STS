 ***********************************************************************************************/
this.fn_DetailGrd01Creat = function()
{
	if(this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO"))){
		return false;
	}
	
	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	if(sChkRowType=="I") this.ds_Detail20.clearData();
	this.ds_Detail21.clearData();
	this.ds_Input.clearData();
	
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "CNTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO"));
	this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
	this.ds_Input.setColumn(0, "CNTH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_SQ"));
	this.ds_Input.setColumn(0, "CONH_GU", this.fvConhGu);
	this.ds_Input.setColumn(0, "PRGM_ID", this.fvComPrgmId);
			
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "mm:M_MM_1000_D_SL21";
	this.gfn_AddComSearchService(this, svcAddObj);	
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "DTL20_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_PS;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Detail21=ds_Detail20";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);
};

