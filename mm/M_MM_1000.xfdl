/*---------------------------------------------------------------------------------------------
 *    [Master 입력영역-Edit] 계약번호 변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.div_MasterInput_tab_Master_tpg_01_mae_CnthNo_onchanged = function(obj:nexacro.MaskEdit,e:nexacro.ChangeEventInfo)
{

	var dsObj = this.ds_Master10;
	var code = this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.value;
	var codeNm = this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CnthNm.value;
	var iRow = dsObj.rowposition;
	
	if (this.gfn_IsNull(code)) {
		dsObj.setColumn(iRow, code, "");
		dsObj.setColumn(iRow, codeNm, "");
		return;
	}	
	nApp.gds_Input.clearData();	
	nApp.gds_Cnth.clearData();		
	
	nApp.gds_Input.addRow();
	nApp.gds_Input.setColumn(0, "G_COMM", nApp.gvCompany);
	nApp.gds_Input.setColumn(0, "CNTH_NO", code);//계약번호
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "mm:M_MM_1000_M_SL14";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "COMM_CNTH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = "";//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=gds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "gds_Cnth=ds_Master10";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.isSync = true;//동기식으로 설정할지 여부
	svcCallObj.sCallBackFnc = "_gfn_Callback_ComFunc";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
	
	var objCommDs = nApp.gds_Cnth;
	var iRowCnt = objCommDs.getRowCount();
	if (iRowCnt > 0) {
		dsObj.setColumn(iRow, "CNTH_NO", objCommDs.getColumn(0, "CNTH_NO"));
		dsObj.setColumn(iRow, "CNTH_NM", objCommDs.getColumn(0, "CNTH_NM"));
		
		var rtnValue = "({" +
							" COMM_CD:'" + objCommDs.getColumn(0, "COMM_CD") + "'"+  //0.회사코드
							",CNTH_NO:'" + objCommDs.getColumn(0, "CNTH_NO") + "'"+  //1.계약번호
							",CNTH_NM:'" + objCommDs.getColumn(0, "CNTH_NM") + "'"+  //2.계약명
							",WOFH_SQ:'" + objCommDs.getColumn(0, "WOFH_SQ") + "'"+  //3.결재차수
							",CUSM_CD:'" + objCommDs.getColumn(0, "CUSM_CD") + "'"+  //8.계약처코드
							",CUSM_NM:'" + objCommDs.getColumn(0, "CUSM_NM") + "'"+  //9.계약처명
							",FROM_DD:'" + objCommDs.getColumn(0, "FROM_DD") + "'"+  //32.계약시작일
							",CLOS_DD:'" + objCommDs.getColumn(0, "CLOS_DD") + "'"+  //33.계약종료일
							",WONE_AM_M:'" + objCommDs.getColumn(0, "WONE_AM") + "'"+  //37.원화금액
							",BSAC_AM:'" + objCommDs.getColumn(0, "BSAC_AM") + "'"+  //39.영업실행금액
						"})";
		this.fn_PopupCallback("M_CNTH_NO_01", rtnValue)	;
		
	} else {		
		dsObj.setColumn(iRow, "CNTH_NO", "");
		dsObj.setColumn(iRow, "CNTH_NM", "");
	}
};  
