					,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd^this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm^this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SystId"
                  ];
	this.gfn_SetCommEmpmCd(this.fvComPrgmId, arrComp);
	
	//콤보 인덱스 설정
	//this.div_Search.form.cbo_Ct01Cd.set_index(0);
	this.div_Search.form.cbo_WofhSt.set_index(0);//결재상태 ALL

