/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	//this.gfn_SetGridSelectRowColor(this.div_MasterView.form.grd_Master10, e.newrow);//선택한 Row BackgroundColor 설정

	if (e.newrow > -1) {
		//[결재] 결재상태에 따른 컴포넌트 제어
		this.fn_SetWfCompEnable();
		
		///계약번호 자리수 확인
		if(!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO"))){
		var iCnthLen = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO").length;
		
		//계약번호 format 변경 (9자리, 8자리)
		if (iCnthLen > 8) 
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_format("@@@@-@-@@@@");		
		else 
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_format("@@@-@@@@");
		}
		//Master 입력/수정/삭제 상태 체크
		var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
		
		//Master 입력 상태이면
		if (sChkRowType != "I") {
			this.fn_GetStchData();
			this.fn_GetDetailData();
		}
		
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master10] Row 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10) || this.gfn_DsIsUpdated(this.ds_Detail10)) {
		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	

	return true;
};  

/*---------------------------------------------------------------------------------------------
 *    03. [데이터셋-ds_Master10] Column 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/

this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//경비기준 변경시..
	if (e.columnid == "CONH_CD") {	
	    // 현장경비관리:0010, 표준요율적용:0020, 임의요율적용:0040, 현장경비없음:0030
		if(e.newvalue == '0010'){
			this.ds_Master10.setColumn(e.row, "CONH_RT", "0");
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_STCH_SQ.set_enable(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_ConhRt.set_readonly(true);
			this.fn_GetStchData();
		}
		else if(e.newvalue == "0040"){
			this.ds_Master10.setColumn(e.row, "CONH_RT", '0');
			this.ds_Master10.setColumn(e.row, "STCH_SQ", "");
			this.ds_Master10.setColumn(e.row, "TCOD_AM", "");
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_ConhRt.set_readonly(false);
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_STCH_SQ.set_enable(false);
		}else{
			this.ds_Master10.setColumn(e.row, "CONH_RT", "0");
			this.ds_Master10.setColumn(e.row, "STCH_SQ", "");
			this.ds_Master10.setColumn(e.row, "TCOD_AM", "");
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_ConhRt.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_STCH_SQ.set_enable(false);
		}
	}
	//계약금액 변경
	else if(e.columnid == "CNTH_AM" 
			&& this.gfn_Nvl(this.ds_Master10.getColumn(e.row, "OLD_CNTH_AM"),0) > 0
			&& this.gfn_Nvl(this.ds_Master10.getColumn(e.row, "OLD_CNTH_AM"),0) != e.newvalue){
		if(this.ds_Master10.getColIndex("CNTH_AM_BF")<0) this.ds_Master10.addColumn("CNTH_AM_BF", "string");
		this.ds_Master10.setColumn(e.row, "CNTH_AM_BF", e.oldvalue);
	}
	//집행실행예산변경
	else if(e.columnid == "CONH_AM" 
			&& this.gfn_Nvl(this.ds_Master10.getColumn(e.row, "OLD_CONH_AM"),0) > 0
			&& this.gfn_Nvl(this.ds_Master10.getColumn(e.row, "OLD_CONH_AM"),0) != e.newvalue){
		if(this.ds_Master10.getColIndex("CONH_AM_BF")<0) this.ds_Master10.addColumn("CONH_AM_BF", "string");
		this.ds_Master10.setColumn(e.row, "CONH_AM_BF", e.oldvalue);
	}
	//경비 임의요율
	else if(e.columnid == "CONH_RT"){
		if(nexacro.toNumber(e.newvalue)>10){
			var arrMsgArg = ["경비(직접)요율", "10"];
			this.gfn_Alert("SM056", arrMsgArg);//[경비(직접)요율]이(가) [10]을(를) 초과할 수 없습니다.
			this.ds_Master10.setColumn(e.row, "CONH_RT", e.oldvalue);
			return false;
		}
		
		this.ds_Master10.setColumn(e.row, "CONH_RT",nexacro.round(e.newvalue,3));
	}
	//경비차수
	else if(e.columnid == "STCH_SQ"){
		if(this.gfn_Nvl(this.ds_Master10.getColumn(e.row, "STCH_SQ"),"")==""){
			this.ds_Detail40.clearData();
			return;
		}
		this.fn_GetDetail40Data();
	}
};

