/***********************************************************************************************
 * 02.DG1.05. [Detail그리드2-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Choice = function()
{
	var sRepaGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "REPA_GU");
	
	var nRow1 = this.ds_mmRepaGu.findRowExpr("CODM_CD == '"+ sRepaGu + "'");
	var sOp02Nm = this.ds_mmRepaGu.getColumn(nRow1, "OP02_NM"); 
	
	var nRow = this.ds_mmPcrhGu.findRow( "CODM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCRH_GU"));
	var sOp01Nm = this.ds_mmPcrhGu.getColumn(nRow, "OP01_NM") 
	
	var sPrchGu = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_Ct12cd.value;
	
	this.gfn_FilterCommDtCode(this.ds_Mm110, this.ds_Mm110Filter, "CODM_CD=='"+this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCRH_GU")+"'");
	
	this.gfn_FilterCommDtCode(this.ds_Mm110, this.ds_Mm110FilterChoi, "CODM_CD=='"+this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCRH_GU")+"'  && OD01_NM== '" + sOp02Nm + "'");
	
		
	//this.alert(this.ds_Mm110Filter.rowcount);
	//this.alert(this.ds_Mm110FilterChoi.rowcount);	
	//this.alert(this.ds_Mm110Filter.rowcount);
		
	if (sOp01Nm != "0")
	{
		var arrMsgArg = ["상단 청구내역의 청구구분항목 중 생산자재 "];
		this.gfn_Alert("SM040", arrMsgArg); 
		return;
	}
	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {
			var arrCheckRow = [];//체크된 Row
			var sCnthNo		= "";
			var sRepaGu 	=  this.ds_Master10.getColumn(this.ds_Master10.rowposition, "REPA_GU");
			
			
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				//--------------------------------------------------------------------------------------------------
					var sCnthNo = "";
					
					if(sRepaGu == "0020") {
						
						sCnthNo  = this.ds_Detail20.getColumn(iLoop, "CNTH_NO");
						
						this.ds_Exists.clearData();
						this.ds_Input.clearData();
						this.ds_Input.addRow();
						
						this.ds_Input.setColumn(0,	"CNTH_NO", sCnthNo);
						
						//[공통]서비스정보 초기화
						this.gfn_InitComService(this);
						
						//[공통]서비스정보 추가
						var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
						svcAddObj.sSelectSql = "mm:M_MM_1100_M_SL14";
						this.gfn_AddComSearchService(this, svcAddObj);
						
						//[공통]서비스 호출 변수
						var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
						svcCallObj.sTrid = "MST_SEARCH_EXISTS";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
						svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
						svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
						svcCallObj.sOutDatset = "ds_Exists=ds_Exists";//출력 데이터셋
						svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
						svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
						svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
						svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID		
						svcCallObj.isSync = true;//동기식으로 설정할지 여부

						//[공통]서비스 호출
						this.gfn_CallComService(this, svcCallObj);
						
						if(this.ds_Exists.rowcount < 1){
							var arrMsgArg = ["준공관리의 계약번호"];
							this.ds_Detail10.set_rowposition(nRow);
							//this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.setCellPos(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.getBindCellIndex("body","CREM_NO")); 																					  								 
							//this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.showEditor(true);
							this.gfn_Alert("HS010", arrMsgArg); //&ITM0(가)이 존재하지 않습니다.	
							return false; 
						}
					};		
			
			
				//----------------------------------------------------------------------------------------------------
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					//Detail10에 적용되어 있는지 체크
					var objSearchList = ["CNTH_NO|" + this.ds_Detail20.getColumn(iLoop, "CNTH_NO") //계약번호
										,"CNTB_SE|" + this.ds_Detail20.getColumn(iLoop, "CNTB_SE") //계약순번
										,"WORB_NO|" + this.ds_Detail20.getColumn(iLoop, "WORB_NO") //W/O번호
										,"ITEM_CD|" + this.ds_Detail20.getColumn(iLoop, "ITEM_CD") //품목코드
										,"MRPH_NO|" + this.ds_Detail20.getColumn(iLoop, "MRPH_NO") //자재소요계획번호
										,"MRPH_SQ|" + this.ds_Detail20.getColumn(iLoop, "MRPH_SQ") //결재차수
										,"MRPB_SE|" + this.ds_Detail20.getColumn(iLoop, "MRPB_SE") //순번								
										];		

					//[공통] Detail10 중복건 체크후 해당건 Skip
					if (!this.gfn_DsSchDupcheck(this.ds_Detail10, objSearchList) && this.ds_Detail20.getColumn(iLoop, "S_QT") > 0) {
										
