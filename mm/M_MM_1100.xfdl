{
	var CnthNo = this.ds_Detail10.getColumn(e.row, "CNTH_NO");	
	
	if (e.columnid == "ACOM_CD" && !this.gfn_IsNull(CnthNo)) {
	
		var sCnthNo = this.ds_Detail10.getColumn(e.row, "CNTH_NO").substr(0,1);
		var nRow  = this.ds_Mm110Filter.findRowExpr("CODD_CD == '" + e.newvalue + "'");		
		var sOd01 = this.ds_Mm110Filter.getColumn(nRow, "OD01_NM");
		var sOdNm = this.ds_Mm110Filter.getColumn(nRow, "CODD_NM");
		
		if(sCnthNo == "R"){
			if(sOd01 != "3"){
				var arrMsgArg = ["연구개발이면 " + sOdNm + " 계정을 선택할 수 " ];
				this.gfn_Alert("MM046", arrMsgArg); //..없습니다.
				return false;
			}
