			sBUDG_MM = this.gfn_Nvl(this.fvMmCloseDay, "");//마감년월
		} else {
			sBUDG_MM = this.gfn_Nvl(e.newvalue, "");//마감년월
		}
		
		this.fvMmCloseDay = "";

		//마감여부 체크
		var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
		var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
		paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
		paramObj.MODM_CD = "MM";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
		paramObj.ACUM_CD = "";//[선택] 회계단위
		paramObj.BUSM_CD  = "";//[선택] 사업부문
		paramObj.SAUM_CD  = "";//[선택] 사업장코드
		paramObj.DEPM_CD  = "";//[선택] 부서코드
		paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
		paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
		
		var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		

		//월마감된 경우
		if (bCheck) {
			this.gfn_Alert("CA025");//해당월의 정보가 마감이 되어서 등록/수정 할 수 없습니다.
			
			var sOrgDate = this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "PCRH_DD");//변경전값
			
			//변경전값이 없을경우
			if (this.gfn_IsNull(sOrgDate)) {				
				var sToDay = this.gfn_GetCommDate("toDay");//현재일자
				
				if (sBUDG_MM.substr(0,6) == sToDay.substr(0,6)) {
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PCRH_DD", "");//입력날짜가 마감된경우 오늘날짜로 세팅				
				} else {
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PCRH_DD", sToDay);//입력날짜가 마감된경우 오늘날짜로 세팅
				}
			} else {
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PCRH_DD", sOrgDate);//입력날짜가 마감된경우 변경전 값으로 세팅
			}
			
			return false;
		} else {
			return true;
		}

	}else if(e.columnid == "REPA_GU" || e.columnid == "PCRH_GU"){
		if(e.columnid == "REPA_GU"){
			var PdrhNo = this.div_MasterInput.form.tab_Master.tpg_01.form.edt_Pbrh_No.value;			
			var Haja   = e.newvalue;
		
			if(!this.gfn_IsNull(PdrhNo) && PdrhNo != ""){
				if(Haja == "0010"){		
					var arrMsgArg = ["문제점발생번호 입력시 하자보수구분은 정상을 입력할 수"];
					this.gfn_Alert("MM046",arrMsgArg);
					this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_Ct04Cd00.setFocus();
					return false;
				}
			}
		}	
	
		if (this.ds_Detail10.rowcount > 0){
			this.gfn_Alert("SM043");		
			return false;	
		}
	}
};

