		this.div_DetailInput.form.btn_DwgAllSave.set_visible(false);
		this.div_DetailInput.form.sta_DeliDt.set_visible(false);
		this.div_DetailInput.form.cal_DeliDtFr.set_visible(false);
		this.div_DetailInput.form.btn_CorhM2.set_visible(false);
