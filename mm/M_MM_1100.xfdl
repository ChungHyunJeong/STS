					return false; 
				}			
			}		
		}	
	}	
	
	for(var iLoop = 0; iLoop < this.ds_Detail10.rowcount; iLoop++){
		
		var sCnthNo = this.ds_Detail10.getColumn(iLoop, "CNTH_NO").substr(0,1);
		var sAcomCd = this.ds_Detail10.getColumn(iLoop, "ACOM_CD");
		
