				if(this.ds_Exists.rowcount < 1){
					var arrMsgArg = ["SPEC확인서"];
					this.ds_Detail10.set_rowposition(nRow);
					//this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.setCellPos(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.getBindCellIndex("body","CREM_NO")); 																					  
						 
