		}else{
			if(sOd01 == "3"){
				this.ds_Detail10.set_rowposition(iLoop);
				var arrMsgArg = ["일반계정이면 " + sOdNm + " 계정을 선택할 수 " ];
				this.gfn_Alert("MM046", arrMsgArg); //..없습니다.
				return false;
			}		
		}
	}	
	
	
	return true;
}
 
