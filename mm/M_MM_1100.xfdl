 * 15.09. [그리드변경] Detail 그리드 포멧 변경 (Master-의뢰구분, Detail-청구구분)
 ***********************************************************************************************/ 
this.fn_GridFormatChange  = function(sGridGu)
{
/* 
    1. Master (견적의뢰내역) - 의뢰구분
		ㅇ 외주가공 (0020)
		  - 보이기 : 중분류, 재질, 후처리
		  - 숨기기 : 규격, Maker
		ㅇ 구매품 (0010) / 턴키외주(0200)
		  - 보이기 : 규격, Maker
		  - 숨기기 : 중분류, 재질, 후처리
	
	2. Detail (견적미의뢰검색) - 청구구분
		ㅇ 구매품 : (0010), 턴키외주(0050) 등 그외
		  - 보이기 : 규격, Maker
		  - 숨기기 : 중분류, 재질, 후처리
		ㅇ 외주가공 (0020)
		  - 보이기 : 중분류, 재질, 후처리
		  - 숨기기 : 규격, Maker			
*/
	var gDetail10 = this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10; //견적의뢰상세 그리드
	var gDetail20 = this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20; //견적미의뢰검색 그리드
	var sQuthGu = "";	//의뢰구분
	var sPcrhGu = "";	//청구구분	
	
	
	//Master - 의뢰구분, Detail - 청구구분	
	if (sGridGu == 'Master') {
		sQuthGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PCRH_GU");	//청구구분		
		
		if (sQuthGu == "0200" || sQuthGu == "0070" ) {	//의뢰구분 턴키외주(0010) 선택시 -> 청구구분 턴키외주(0200) 콤보
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_Ct12cd.set_index(0);  //청구구분 - 턴키외주(0030)
			
		} else if (sQuthGu == "0010") {	//의뢰구분 구매품(0020) 선택시 -> 청구구분 구매품(0010) 콤보
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_Ct12cd.set_index(0);  //청구구분 - 구매품(0010)
			
		} else if (sQuthGu == "0020") {	//의뢰구분 외주가공(0030) 선택시 -> 청구구분 외주가공(0020) 콤보
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_Ct12cd.set_index(1);  //청구구분 - 외주가공(0020)		
			
			
		} else if (sQuthGu == "0050") {	//의뢰구분 연구개발(005) 선택시 -> 품목구분 연구개발(0050) 콤보
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_Ct12cd.set_index(2);  //청구구분 - 외주가공(0020)			
			
		} else {
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_Ct12cd.set_index(-1);  //청구구분 - 초기화
		}
		sPcrhGu = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_Ct12cd.value;	//청구구분(그리드 컨트롤를 하기 위해 재조회)
		
	} else if (sGridGu == 'Detail') {
		//sPcrhGu = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_PcrhGu.value;	//청구구분
		sPcrhGu = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_Ct12cd.value;	//청구구분
		
		
		if (sPcrhGu == "0010") { 	//청구구분 구매품(0010) 선택시 의뢰구분 구매품(0020) 변경
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PCRH_GU", "0010");	//의뢰구분 - 구매품(0020)
		
		} else if (sPcrhGu == "0020") { 	//청구구분 외주가공(0020) 선택시 의뢰구분 외주가공(0030) 변경
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PCRH_GU", "0020");	//의뢰구분 - 외주가공(0030)

		} else if (sPcrhGu == "0050") { 	//청구구분 연구개발(0050) 선택시 의뢰구분 턴키외주(0050) 변경
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PCRH_GU", "0050");	//의뢰구분 - 턴키외주(0010)

		} else {	//의뢰구분 - 초기화
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_QuthGu.set_index(-1);		//의뢰구분 - 초기화
			
		}
	}	
	
	
	sQuthGu = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_Ct12cd.value;	//청구구분		
	
	
	//청구구분이 외주가공(0020)인 경우 
	if (sQuthGu == "0020") {	
		
		//Detail10
		gDetail10.setFormatColProperty(10, "size", 80);		//중분류 칸사이즈 복구		
		gDetail10.setFormatColProperty(15, "size", 0);		//규격 칸사이즈 최소화
		gDetail10.setFormatColProperty(16, "size", 0);		//MAKER 칸사이즈 최소화
		
		gDetail10.setFormatColProperty(17, "size", 80);		//재질 칸사이즈 복구
		gDetail10.setFormatColProperty(18, "size", 100);	//MAKER 칸사이즈 복구
		
// 		gDetail10.setFormatColProperty(20, "size", 0);		//일회성_품명
// 		gDetail10.setFormatColProperty(21, "size", 0);		//일회성_규격
// 		gDetail10.setFormatColProperty(22, "size", 0);		//일회성_단위
		
		//gDetail10.setCellProperty("head", 25, "text", "후처리"); //후처리(Maker) 칸 제목 
		
		//Detail20
		gDetail20.setFormatColProperty(7, "size", 70);		//중분류 칸사이즈 복구
		gDetail20.setFormatColProperty(11, "size", 0);		//규격 칸사이즈 최소화
		gDetail20.setFormatColProperty(12, "size", 0);	
		gDetail20.setFormatColProperty(13, "size", 80);		//재질 칸사이즈 복구
		gDetail20.setFormatColProperty(14, "size", 120);	//후처리 칸사이즈 복구
		
	}else if (sQuthGu == "0070") { 	
		//Detail10		
		
		gDetail10.setFormatColProperty(10, "size", 0);		//중분류 칸사이즈 복구				
		gDetail10.setFormatColProperty(15, "size", 150);		//규격 칸사이즈 최소화
		gDetail10.setFormatColProperty(16, "size", 120);		//MAKER 칸사이즈 최소화
		
		//this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 11, "cssclass", "");		
		
		//this.alert(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getBindCellIndex("body", "ITEM_CD" ));
		
		
		gDetail10.setFormatColProperty(17, "size", 0);		//재질 칸사이즈 복구
		gDetail10.setFormatColProperty(18, "size", 0);		//MAKER 칸사이즈 복구
		
// 		gDetail10.setFormatColProperty(20, "size", 150);		//일회성_품명
// 		gDetail10.setFormatColProperty(21, "size", 120);		//일회성_규격
// 		gDetail10.setFormatColProperty(22, "size", 60);		//일회성_단위
		
		//gDetail10.setCellProperty("head", 25, "text", "후처리"); //후처리(Maker) 칸 제목 
		
		//Detail20
		gDetail20.setFormatColProperty(7, "size", 70);		//중분류 칸사이즈 복구
		gDetail20.setFormatColProperty(11, "size", 0);		//규격 칸사이즈 최소화
