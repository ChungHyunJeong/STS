/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	//출고부서
	if (e.columnid == "DEPM_CD") {
		this.gfn_GetCommDepmGrid(obj, "DEPM_CD", "DEPM_NM");//부서명 검색
	//출고자
	} else if (e.columnid == "EMPM_CD") {
		this.gfn_GetCommEmpmGrid(obj, "EMPM_CD", "EMPM_NM", "SYST_ID");//사원명 검색
	//영업부서
	} else if (e.columnid == "SDDP_CD") {
		this.gfn_GetCommDepmGrid(obj, "SDDP_CD", "SDDP_NM");//부서명 검색
	//영업자
	} else if (e.columnid == "SDSU_CD") {
		this.gfn_GetCommEmpmGrid(obj, "SDSU_CD", "SDSU_NM", "SDSU_ID");//사원명 검색
	//거래처
	} else if (e.columnid == "CUSM_CD") {
		this.gfn_GetCommCusmGrid(obj, "CUSM_CD", "CUSM_NM");//거래처명 검색
	//계약번호
	} else if (e.columnid == "CNTH_NO") {
		this.gfn_GetCommCnthGrid(obj, "CNTH_NO", "CNTH_NM");//거래처명 검색				
		
	//프로젝트명
	} else if (e.columnid == "PJTH_NO") {
		this.gfn_GetCommPjthGrid(obj, "PJTH_NO", "PJTH_NM");//거래처명 검색			
	//고객명
	} else if (e.columnid == "CSPM_CD") {
		this.gfn_GetCommCspmGrid(obj, "CSPM_CD", "CSPM_NM");//거래처명 검색			
	//품명
	} else if (e.columnid == "ITEM_CD") {
		this.gfn_GetCommItemGrid(obj, "ITEM_CD", "ITEM_NM");//거래처명 검색			
	} else if (e.columnid == "ACOM_NM") {
		this.ds_Detail10.setColumn(e.row, "ACOM_CD", e.newvalue);
	} else if (e.columnid == "WORB_NO") {
		var sWorbNo = e.newvalue;
		
	    var sRepaGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "REPA_GU");
		
		var nRow = this.ds_mmRepaGu.findRow( "CODM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "REPA_GU"));
		var sOp02Nm = this.ds_mmRepaGu.getColumn(nRow, "OP02_NM") 
		
		// 0010 : 정상 0020 : 하자보수 0030 : 정상_추가
		
		if(sRepaGu=="0020"){
			this.gfn_FilterCommDtCode(this.ds_Mm110, this.ds_Mm110Filter, "CODM_CD=='"+this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_Ct03Cd00.value+"' && OD01_NM=='2'");
		}else{
			if(this.gfn_IsNull(sWorbNo)){sWorbNo="A";}
			if(sWorbNo.substr(0,1) == "R"){
				this.gfn_FilterCommDtCode(this.ds_Mm110, this.ds_Mm110Filter, "CODM_CD=='"+this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_Ct03Cd00.value+"' && OD01_NM=='3'");				
				obj.setColumn(e.row,"ACOM_CD", this.ds_Mm110Filter.getColumn(0, "CODD_CD"));
				
			}else{
				this.gfn_FilterCommDtCode(this.ds_Mm110, this.ds_Mm110Filter, "CODM_CD=='"+this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_Ct03Cd00.value+"' && OD01_NM=='1'");	
			}
			obj.setColumn(e.row,"ACOM_CD", this.ds_Mm110Filter.getColumn(0, "CODD_CD"));
		}
	}

};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Detail20] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	
	//체크시 해당 프로그램 전체선택
	if (e.columnid == "s_chk") {	
		if (this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "s_chk") == '1') {		
			var sCNTH_NO = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "CNTH_NO");//
			var sCNTB_SE = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "CNTB_SE");//
			var sWORB_NO = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "WORB_NO");//
			var sITEM_CD = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "ITEM_CD");//
			var sMRPH_NO = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "MRPH_NO");//
			var sMRPH_SQ = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "MRPH_SQ");//
			var sMRPB_SE = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "MRPB_SE");//
			//Detail10에 프로젝트에 대한 청구번호가 존재하는지 확인
			var iDupCnt = this.ds_Detail10.getCaseCount("CNTH_NO == '"+sCNTH_NO+"' && CNTB_SE == '"+sCNTB_SE+"' && WORB_NO == '"+sWORB_NO+"' && ITEM_CD == '"+sITEM_CD+"' && MRPH_NO == '"+sMRPH_NO+"' && MRPH_SQ == '"+sMRPH_SQ+"' && MRPB_SE == '"+sMRPB_SE+"'"); 
			if(iDupCnt > 0 ){
				var arrMsgArg = ["W/O"];
				this.gfn_Alert("SD008", arrMsgArg); //해당 품목코드가 존재합니다.
				return;
			}	
			//Detail20에 청구번호가 최초 1건만 체크됐을 경우에만 처리함(2건일 경우 return)
			//var iChkCnt = this.ds_Detail20.getCaseCount("CNTH_NO == '"+sCNTH_NO+"' && CNTB_SE == '"+sCNTB_SE+"' && WORB_NO == '"+sWORB_NO+"' && ITEM_CD == '"+sITEM_CD+"' && s_chk == '1'");  //체크건수		
			var iChkCnt = this.ds_Detail20.getCaseCount("CNTH_NO == '"+sCNTH_NO+"' && CNTB_SE == '"+sCNTB_SE+"' && WORB_NO == '"+sWORB_NO+"' && s_chk == '1'");  //체크건수		

			if (iChkCnt > 1) return;
			
			//최초 1건만 체크시 로직 처리
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				//Detail10 중복여부 체크	

				iDupCnt = this.ds_Detail10.getCaseCount("CNTH_NO == '"+sCNTH_NO+"' && CNTB_SE == '"+sCNTB_SE+"' && WORB_NO == '"+sWORB_NO+"'");// && ITEM_CD == '"+sITEM_CD+"'");  
			
				//청구번호가 같고 Detail10에 청구번호가 미존재하고 색상변경이 안된 대상				
				if (this.ds_Detail20.getColumn(iLoop, "CNTH_NO") == sCNTH_NO && this.ds_Detail20.getColumn(iLoop, "CNTB_SE") == sCNTB_SE && this.ds_Detail20.getColumn(iLoop, "WORB_NO") == sWORB_NO
				 //&& this.ds_Detail20.getColumn(iLoop, "ITEM_CD") == sITEM_CD
				 && iDupCnt == 0
				 && (this.gfn_IsNull(this.ds_Detail20.getColumn(iLoop, "s_conf")) || this.ds_Detail20.getColumn(iLoop, "s_conf") == '0')) {
								
					//체크박스 Check
					this.ds_Detail20.setColumn(iLoop, "s_chk", "1");//Check로 변경
					
					//this.ds_Detail20.setColumn(iLoop, "S_QT", this.ds_Detail20.getColumn(iLoop, "RE_PURC_QT"));//Check로 변경
				}
			}
		}
	}
	if (e.columnid == "S_QT") {	
		if (this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "S_QT") > this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "RE_PURC_QT")){
			var arrMsgArg = ["청구수량","미청구수량"];
			this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "S_QT", 0);
			this.gfn_Alert("SM056", arrMsgArg); //[청구수량]이(가) [미청구수량]을(를) 초과할 수 없습니다.
						
		}
	}
};

