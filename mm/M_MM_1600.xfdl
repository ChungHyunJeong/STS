          주의 : Open(popuptype=modeless)창은 팝업콜백을 지원하지 않음
 ***********************************************************************************************/ 
this.fn_PopupCallback = function(strId, rtnValue)
{
	//[조회영역]
	//W/O번호	
	if (strId == "S_WO_NO_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);
			
			if (!this.gfn_IsNull(arrRtnVal.WORB_NO)){	
				this.div_Search.form.mae_WorbNo.set_value(arrRtnVal.WORB_NO);//WO코드 					 
				this.div_Search.form.edt_WorbNm.set_value(arrRtnVal.HOGI_NM);//호기명
			}	
		} else {
				this.div_Search.form.mae_WorbNo.set_value("");//WO코드 					 
				this.div_Search.form.edt_WorbNm.set_value("");//호기명
		}
	}
			
};
 
