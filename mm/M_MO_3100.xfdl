 /*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 적용 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.fn_DetailGrd02Choice = function()
{
	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {
			
			/*
			김형수 부장 요청 - 멀티선택으로 변경 -2019-09-30
			if(this.ds_Detail10.rowcount > 0 ){ 
				var arrMsgArg = [""];
				this.gfn_Alert("SM043", arrMsgArg); //상세정보가 존재합니다.\n상세정보를 삭제하신 후 작업하세요.
				return;
			}
			*/
			//	trace("------->")
			
			//1. 거래처 중복 체크
			var sIdntNoChk=""; //Key(거래처) 체크			
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					
					//1. Detail20 Tab과 Detail10 화면 항목 비교 : 거래처코드
					if(this.ds_Detail10.rowcount > 0 ){
						if(this.ds_Detail10.getColumn(0,"CUSM_CD") != this.ds_Detail20.getColumn(iLoop,"CUSM_CD") ){
							var arrMsgArg = ["거래처"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 거래처만 적용 가능합니다.
							return;
						}
					}
					
					
					//2. Detail20 그리드내 체크된 대상 동일여부 비교 : 거래처코드
					if(!this.gfn_IsNull(sIdntNoChk) ){
						if(sIdntNoChk != this.ds_Detail20.getColumn(iLoop,"CUSM_CD") ){
							var arrMsgArg = ["거래처"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 거래처만 적용 가능합니다.
							return;
						}
					}
					sIdntNoChk = this.ds_Detail20.getColumn(iLoop,"CUSM_CD"); //for문 비교를 위한 이전값 저장
				}
			}	
			
			var arrCheckRow = [];//체크된 Row
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
		
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					
					//trace("------->"+ this.ds_Detail10.rowcount);
					
					var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)
					
					//첫번째일 경우 마스터 업데이트
					if(this.ds_Detail10.rowcount == 1 ){
						this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_value(this.ds_Detail20.getColumn(iLoop,"CNTH_NO"));	//계약코드
						this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CnthNm.set_value(this.ds_Detail20.getColumn(iLoop,"CNTH_NM"));	//계약명
						this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CtcsCd.set_value(this.ds_Detail20.getColumn(iLoop,"CTCS_CD"));	//계약 거래처코드
						this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CtcsNm.set_value(this.ds_Detail20.getColumn(iLoop,"CTCS_NM"));	//계약 거래처명
						this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_value(this.ds_Detail20.getColumn(iLoop,"CUSM_CD"));	//외주 계약처코드
						this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmNm.set_value(this.ds_Detail20.getColumn(iLoop,"CUSM_NM"));	//외주 계약처명
						this.div_MasterInput.form.tab_Master.tpg_01.form.edt_OcnhNm.set_value(this.ds_Detail20.getColumn(iLoop,"PCOH_NM"));	//외주 계약명 (발주명을 외주계약명으로)
						
						this.ds_Master10.setColumn(this.ds_Master10.rowposition, "VATM_CD", this.ds_Detail20.getColumn(iLoop,"VATM_CD"));	 //부가세코드
						this.ds_Master10.setColumn(this.ds_Master10.rowposition, "VATM_NM", this.ds_Detail20.getColumn(iLoop,"VATM_NM"));	 //부가세명
						this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PJTH_NM", this.ds_Detail20.getColumn(iLoop,"PLAC_NM"));	 //프로젝트장소
						
						
						//계약번호 format 변경 (9자리, 7자리)
						var strCnthNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO");
						if (strCnthNo.length == 9) 
							this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_format("####-@-####");		
						else 
							this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_format("@##-####");
						
					}
					
					//외주계약미등록검색에서 외주계약상세로 이동
					this.ds_Detail10.copyRow(iAddRow, this.ds_Detail20, iLoop);
					this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));	//회사코드
					this.ds_Detail10.setColumn(iAddRow, "OCNH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OCNH_NO"));	//외주계약번호
					this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));	//결제차수
					
					this.ds_Detail10.setColumn(iAddRow, "REMK_NM", this.ds_Detail20.getColumn(iLoop, "PORM_NM"));	//구매발주상세 비고--> 외주계약비고
					
					arrCheckRow.push(iLoop );
					
					//선택항목 비활성화처리
					this.ds_Detail20.setColumn(iLoop, "s_conf", "1");	//미발주검색 컬러 변경 구분자 업데이트
					this.ds_Detail20.setColumn(iLoop, "s_chk", "0");	//Uncheck로 변경
					
				}
				
			}
			
			//마스터 금액 계산
			var sWoneAm = this.ds_Detail10.getCaseSum("crudType !='D'","WONE_AM");
			var sVtaxAm = this.ds_Detail10.getCaseSum("crudType !='D'","VTAX_AM");
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "OCNH_AM", sWoneAm + sVtaxAm);	//계약금액
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "SUPL_AM", sWoneAm);		//공급금액
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "VTAX_AM", sVtaxAm);		//부가세금액
			
			
			//trace(sVtaxAm);
			if(!this.gfn_IsNull(sVtaxAm)){
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "VHAN_AM",this.gfn_Num2han(sVtaxAm));			//한글부가세액
			}else{
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "VHAN_AM","영");			//한글부가세액
			}

		
			//발주상세로 Tab이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);
		}
	}
};



