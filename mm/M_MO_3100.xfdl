/***********************************************************************************************
 *    07. [계약확정]
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    [Master 입력영역-Button] 계약확정 버튼 클릭 이벤트
  ---------------------------------------------------------------------------------------------*/
this.div_MasterInput_btn_CnthM1_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		var arrMsgArg = [""];
		this.gfn_Alert("SM002", arrMsgArg);//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.
		return false;
	}	
	
	if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OCNH_NO"))) {
		
		var iRow = this.ds_Master10.rowposition;
		
		//전자계약구분 - 전자인경우에만 스마일 EDI 전자계약서 정보 생성
		if (this.ds_Master10.getColumn(iRow, "ETAX_GU") == "1") {
			this.ds_EdiInput.clearData();
			this.ds_EdiInput.addRow();
			this.ds_EdiInput.setColumn(0, "COMM_CD",  this.ds_Master10.getColumn(iRow, "COMM_CD"));//회사코드
			this.ds_EdiInput.setColumn(0, "OCNH_NO",  this.ds_Master10.getColumn(iRow, "OCNH_NO"));//외주계약번호
			this.ds_EdiInput.setColumn(0, "WOFH_SQ",  this.ds_Master10.getColumn(iRow, "WOFH_SQ"));//변경차수
			this.ds_EdiInput.setColumn(0, "CUSM_CD",  this.ds_Master10.getColumn(iRow, "CUSM_CD"));//거래처코드
			this.ds_EdiInput.setColumn(0, "OCNH_CD",  this.ds_Master10.getColumn(iRow, "OCNH_CD"));//외주계약구분코드
			//this.ds_EdiInput.setColumn(0, "SERI_NO",  this.ds_Master10.getColumn(iRow, "SERI_NO"));//전자계약서 일련번호
			//SERI_NO 전자계약시리얼No. ==> 회사코드 + 외주계약번호 + 외주계약차수
			var sSERI_NO = this.ds_Master10.getColumn(iRow, "COMM_CD") + this.ds_Master10.getColumn(iRow, "OCNH_NO") + "" + this.ds_Master10.getColumn(iRow, "WOFH_SQ");
			this.ds_EdiInput.setColumn(0, "SERI_NO",  sSERI_NO);//전자계약서 일련번호
			this.ds_EdiInput.setColumn(0, "PRGM_ID",  this.fvComPrgmId);//프로그램ID
			this.ds_EdiInput.setColumn(0, "FILH_GU",  "H");//첨부파일구분
			this.ds_EdiInput.setColumn(0, "PK01_NO",  this.ds_Master10.getColumn(iRow, "COMM_CD"));//첨부파일Key1
			this.ds_EdiInput.setColumn(0, "PK02_NO",  this.ds_Master10.getColumn(iRow, "OCNH_NO"));//첨부파일Key2
			this.ds_EdiInput.setColumn(0, "PK03_NO",  this.ds_Master10.getColumn(iRow, "WOFH_SQ"));//첨부파일Key3		

			//스마일EDI 전자계약서 정보 생성
			this.gfn_InitComService(this);	
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "MAKE_EDI";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = "MoCnthAction";//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sMethodName = "makeEdi";// Action 안에서 호출해야 하는 메소드 명, 기본은 execute를 호출하도록 되어있음	
			svcCallObj.sInputDaset = "ds_EdiInput=ds_EdiInput" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_CnthResult=ds_CnthResult";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명	
			svcCallObj.isSync = true;//동기식으로 설정할지 여부
			
			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);		
			
			if (this.ds_CnthResult.getRowCount() < 1) {
				var arrMsgArg = [""];
				this.gfn_Alert("MM034", arrMsgArg);//전자계약서 데이터 생성중 오류가 발생하였습니다.\n&ITM0
				return;
			} else {
				//전자계약서 생성중 오류 인경우
				if (this.ds_CnthResult.getColumn(0, "RSLT_CD") != "S") {
					var arrMsgArg = [this.ds_CnthResult.getColumn(0, "RSLT_CT")];
					this.gfn_Alert("MM034", arrMsgArg);//전자계약서 데이터 생성중 오류가 발생하였습니다.\n&ITM0
					return;			
				}
			}
		
		}
			
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "OCNH_YN", "1");// 외주계약확정 처리
		
		//저장시 재조회 Key설정
		this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OCNH_NO");//조회Key01
		this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//조회Key02
	
		//[공통]서비스 정보 초기화
		this.gfn_InitComService(this);
				
		//[공통]서비스정보 추가
		var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
		svcAddObj.sUpdateSql = "mm:M_MO_3100_M_UP10";
		svcAddObj.sSaveFlagColumn = "";
		this.gfn_AddComSaveService(this, svcAddObj);
		
		
		//[공통]서비스 호출 변수
		var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
		svcCallObj.sTrid = "MO_CONFIRM";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
		svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
		svcCallObj.sInputDaset = "ds_Master10=ds_Master10:U" //입력 데이터셋
		svcCallObj.sOutDatset = "";//출력 데이터셋
		svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
		
		//[공통]서비스 호출
		this.gfn_CallComService(this, svcCallObj);	
	
	}
		
};

