  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{

	if (e.newrow > -1) {
		
		//외주계약정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OCNH_NO"))) {
			
			//지불조건 합계계산 PTCT_RT PTMD_RT PTTS_RT
			this.fn_PtotAm_compute("PTCT_RT", this.div_MasterInput.form.tab_Master.tpg_02.form.mae_PtctAm);
			this.fn_PtotAm_compute("PTMD_RT", this.div_MasterInput.form.tab_Master.tpg_02.form.mae_PtmdAm);
			this.fn_PtotAm_compute("PTTS_RT", this.div_MasterInput.form.tab_Master.tpg_02.form.mae_PttsAm);
			
			//this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.set_enable(false);
			
			//외주계약코드 포멧 설정 변경 (기존데이터용)
			var strOcnhNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OCNH_NO")//외주계약번호
			//trace(strOcnhNo.substr(0,1));
			if(strOcnhNo.length == 15){
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_OcnhNo.set_format("####-@-####-######");
			}else{
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_OcnhNo.set_format("######-####");
			}
			
			//계약번호 format 변경 (9자리, 7자리)
			var strCnthNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO");
			if (strCnthNo.length == 9) 
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_format("####-@-####");		
			else 
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_format("@##-####");
			
			//외주계약 확정/변경에 따른 컴포넌트 Enable 등 처리
			this.fn_SetMoCompEnable();
			
			
			
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail20.clearData();
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "OCNH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OCNH_NO"));
			this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
			
			
			//Master 입력/수정/삭제 상태 체크
			//var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
			//this.ds_InputSch.setColumn(0, "crudType", sChkRowType);
			
			
			//this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무	
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "mm:M_MO_3100_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명


			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);	
			
		}
	}
};

