this.fn_PayTermSum  = function()
{
	var iPtotRt = 0;	//지불조건합계
	var iPtctRt = 0;	//지불조건-계약금
	var iPtmdRt = 0;	//지불조건-중도금
	var iPttsRt = 0;	//지불조건-시운전후
	//var iPtfiRt = 0;	//지불조건-설치완료
	//var iPtetRt = 0;	//지불조건-기타
	
	// 값이 0이 아닌 경우 가져오긴(Number 변환)
	if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PTCT_RT")))
		iPtctRt = nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PTCT_RT"));
	if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PTMD_RT")))
		iPtmdRt = nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PTMD_RT"));
	if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PTTS_RT")))
		iPttsRt = nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PTTS_RT"));
	
	/*
	if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PTFI_RT")))
		iPtfiRt = nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PTFI_RT"));
	if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PTET_RT")))
		iPtetRt = nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PTET_RT"));
	*/
	
	//trace(iPtfiRt);
	
	// 지불조건 합계 계산	
	//iPtotRt = iPtctRt + iPtmdRt + iPttsRt + iPtfiRt + iPtetRt;
	iPtotRt = iPtctRt + iPtmdRt + iPttsRt ;
	
	// 지불조건 합계 적용
	this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PTOT_RT", iPtotRt);

}




