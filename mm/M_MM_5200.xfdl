/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{

	var sRegiYn = this.ds_Master10.getColumn(e.row, "REGI_YN");
	if ( sRegiYn == '1') { //등록여부 1
		this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	} 

	//기타금액 입력시 
	if (e.columnid == "BY07_AM") {	
		var iBy07Am = this.ds_Master10.getColumn(e.row, "BY07_AM");
		
		if (!this.gfn_IsNull(iBy07Am)) {
			//구매매입정보 합계금액
			var iBsumAm = this.ds_Master10.getColumn(e.row, "BSUM_AM");
			
			//60일자 지급건 합계
			var iSum60Am = this.ds_Master10.getColumn(e.row, "BY01_AM")
			             + this.ds_Master10.getColumn(e.row, "BY02_AM")
			             + this.ds_Master10.getColumn(e.row, "BY03_AM")
			             + this.ds_Master10.getColumn(e.row, "BY04_AM")
			             + this.ds_Master10.getColumn(e.row, "BY05_AM")
			             + this.ds_Master10.getColumn(e.row, "BY06_AM")
			             + this.ds_Master10.getColumn(e.row, "BY07_AM");
		
			//CASE1. 60일자지급건 합계 > 구매매입정보 합계
			if (iSum60Am > iBsumAm) {
				this.gfn_Alert("MM011");//지급 합계금액은 구매매입 합계금액을 초과 할 수 없습니다.
				this.ds_Master10.setColumn(e.row, "BY07_AM", this.ds_Master10.getOrgColumn(e.row, "BY07_AM"));
				
			//CASE2. 90일자지급건 = 구매매입 합계금액 - 60일자지급건 합계금액
			} else {
				var iBy08Am = iBsumAm - iSum60Am; //구매매입 합계금액 - 60일자지급건 합계금액
				this.ds_Master10.setColumn(e.row, "BY08_AM", iBy08Am); //90일자지급건 
				
			}
			
		}
	
	}
	
};

