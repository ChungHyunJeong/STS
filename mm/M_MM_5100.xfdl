/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{  
	if (e.newrow > -1) {	
		//멀티선택여부
		if (this.fvChkMultiWfYn) return;
		var strAcomCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ACOM_CD");
		var sDoimGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DOIM_GU");
		
		if(sDoimGu=="1"){
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_VtaxAm.set_cssclass("essential");
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_AcnmNo.set_cssclass("essential");
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_VatmCd.set_cssclass("essential");			
		}else{
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_VtaxAm.set_cssclass("");
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_AcnmNo.set_cssclass("");
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_VatmCd.set_cssclass("");					
		}
		
		/*
		//원재료-제조
		if(strAcomCd == "101080901000") {
			this.fvAutmCd = "MM_5100_10";
		//외주가공비
		} else if(strAcomCd == "503210000000") {
			this.fvAutmCd = "MM_5100_11";
		//유형자산-공구와기구-금형
		} else if(strAcomCd == "102102001000") {
			this.fvAutmCd = "MM_5100_12";	
		//유형자산-공구과기구-기타공구
		} else if(strAcomCd == "102102002000") {
			this.fvAutmCd = "MM_5100_13";			
		//외주비-용역	
		} else if(strAcomCd == "503280100000") {
			this.fvAutmCd = "MM_5100_14";						
		}
		*/
		//[결재] 결재상태에 따른 컴포넌트 제어
		this.fn_SetWfCompEnable();
		
		this.fn_PrtAll();
		
		var strWofhSt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST");		
		
		this.div_DetailInput.form.btn_InptAll.set_visible(false);
 		if(this.gfn_IsNull(strWofhSt) || strWofhSt=="0010"){
			var iTabIdx = this.div_DetailInput.form.tab_Detail.tabindex;
			if(iTabIdx == 1)	this.div_DetailInput.form.btn_InptAll.set_visible(true);
		}
		
		//거래처계좌 filter
		var sCusmCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD");
		
		this.ds_SdCbabFull.filter("");
		this.ds_SdCbabFull.filter("CUSM_CD == '" + sCusmCd + "'");	
		this.ds_SdCbabFull.set_keystring( "S:-CBAB_YN" );
		
		//매입No정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BUYH_NO"))) {	
			//[월마감제어-버튼/상세그리드]
			var sBUDG_MM = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BUYH_DD"), "");//마감년월
			
			//마감여부 체크
			var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
			var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
			paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
			paramObj.MODM_CD = "MM";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
			paramObj.ACUM_CD = "";//[선택] 회계단위
			paramObj.BUSM_CD  = "";//[선택] 사업부문
			paramObj.SAUM_CD  = "";//[선택] 사업장코드
			paramObj.DEPM_CD  = "";//[선택] 부서코드
			paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
			paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
			
			var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		

			//월마감된 경우
			if (bCheck) {		
				this.fvMmCloseYn = true;//월마감여부-Tab버튼 제어용
			} else {
				this.fvMmCloseYn = false;//월마감여부-Tab버튼 제어용
			}		
			
			
			var sDate = this.ds_Master10.getColumn(e.newrow, "BUYH_DD");
	
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtFr.set_value(sDate.substr(0,6) + "01");
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtTo.set_value(this.gfn_GetLastDate(sDate));
			
						
			//this.ds_PpRadd.filter("RADM_CD == '"+ this.ds_Master10.getColumn(this.ds_Master10.rowposition,"RADM_CD")  +"'");	//수불사유필터
			//입력영역 버튼 비활성화
			//this.fn_SetGridInputButtonEnable();			
			
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmCd.set_value("");
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmNm.set_value("");
			
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmCd.set_value(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD"));
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmNm.set_value(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_NM"));
			 
			if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST") == "0040") {
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(true);
			} else {
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(false);
			}
			
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Detail20.clearData();
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "BUYH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BUYH_NO"));			
