		this.ds_Master10.setColumn(this.ds_Master10.rowposition,  "BANM_CD",  this.ds_SdCbabFull.getColumn(nRow, "BANM_CD"));
		this.ds_Master10.setColumn(this.ds_Master10.rowposition,  "ACNM_NO",  this.ds_SdCbabFull.getColumn(nRow, "ACNM_NO"));
	} else if(e.columnid == "VATM_RT"){
		this.fn_VatAmCalc();
		//this.fn_VatAmSet();
	} else if(e.columnid == "BUYH_DD"){	
		var sDate = e.newvalue;
	
		this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtFr.set_value(sDate.substr(0,6) + "01");
		this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtTo.set_value(this.gfn_GetLastDate(sDate));
	}
	
};
