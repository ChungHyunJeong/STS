		//조회조건 필수입력값 있으면 체크
			//[저장] Validation 설정
		var arrArgs = ["this.div_Search.form.edt_DepmCd^매입부서|NULL"
					  //,"this.div_Search.form.cbo_SaumCd^사업장|NULL"
					  ]; 
					  
		var sCheckGroupTitle = "검색조건";//체크그룹 타이틀

		//컴포넌트 Vaild 체크
		if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
			return false;
		}
	}
	return true;
}

