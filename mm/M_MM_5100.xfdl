this.fn_Calc = function() 
{
		
	nApp.gds_WfRst.clearData();//글로벌 데이터셋
	this.ds_Input.clearData();
	this.ds_Input.addRow();	
	
	var strFromDd = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtFr.value;
	var strClosDd = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtTo.value;
	var strDepmCd = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_DepmCd.value;
	var strCusmCd = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmCd.value;	
	var strBuyhDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BUYH_DD"); 
	
	if(this.gfn_IsNull(strDepmCd)) {strDepmCd = "";}
	if(this.gfn_IsNull(strCusmCd)) {strCusmCd = "";}
	
	this.ds_Input.setColumn(0, "BUYH_DD", strBuyhDd);	
	this.ds_Input.setColumn(0, "FROM_DD", strFromDd);
	this.ds_Input.setColumn(0, "CLOS_DD", strClosDd);
	this.ds_Input.setColumn(0, "DEPM_CD", strDepmCd);	//입고부서(조건)
	this.ds_Input.setColumn(0, "CUSM_CD", strCusmCd);		
	this.ds_Input.setColumn(0, "ACUM_CD", this.gfn_GetSessionValue("ACUM_CD"));		//회계단위	

	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "mm:M_MM_5100_M_UP15";
	this.gfn_AddComSearchService(this, svcAddObj);	
	
	//[공통]서비스 호출 변수
	var sInputDaset = "";//입력 데이터셋
	var sOutDatset  = "";//출력 데이터셋
	var sOtherArg   = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2			
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	
	svcCallObj.sTrid = "PROC_CALC";//Transaction ID 등 callback에서 넘겨 받을 String을 지정	
	svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset 	= "ds_Input=ds_Input" 	//입력 데이터셋
	svcCallObj.sOutDatset 	= "gds_WfRst=gds_WfRst";//출력 데이터셋	
	svcCallObj.sOtherArg 	= "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";		//서버에서 처리가 완료된 후 Callback 받을 Function 명	

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);		
}

