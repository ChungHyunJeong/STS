/*---------------------------------------------------------------------------------------------
 *    01. 구매매입일괄등록 클릭 이벤트
  ---------------------------------------------------------------------------------------------*/
this.div_DetailInput_btn_allCreate_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{	
	//var strBuyhDd =  this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BUYH_DD").substr(0,6);		
	
	var strFromDd = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtFr.value.substr(0,6);
	var strClosDd = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtTo.value.substr(0,6);
	var strBuyhDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BUYH_DD");
	
	if(strFromDd != strBuyhDd.substr(0,6) ){	
		this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtFr.setFocus();
		this.gfn_Alert("MM014");//입고일자와 매입일자의 년월이 같아야 합니다.
		return ;
	}	
	
	if(strClosDd != strBuyhDd.substr(0,6) ){	
		this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtTo.setFocus();
		this.gfn_Alert("MM014");//입고일자와 매입일자의 년월이 같아야 합니다.
		return ;
	}	

	if(this.gfn_IsNull(strBuyhDd) || strBuyhDd == ""){
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PdrhDd.setFocus();
		var arrMsgArg = ["매입일자"];
		this.gfn_Alert("SM008", arrMsgArg); 
		return;	
	}	
	
	if(this.gfn_IsNull(strFromDd) || strFromDd == ""){
		this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtFr.setFocus();
		var arrMsgArg = ["입고시작일자"];
		this.gfn_Alert("SM008", arrMsgArg); 
		return;
	}
	
	if(this.gfn_IsNull(strClosDd) || strClosDd == ""){
		this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtTo.setFocus();
		var arrMsgArg = ["입고종료일자"];
		this.gfn_Alert("SM008", arrMsgArg); 
		return;
	}
	
	if(strFromDd != strClosDd){
		this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtTo.setFocus();
		this.gfn_Alert("MM014");//입고일자와 매입일자의 년월이 같아야 합니다.
		return;
	}
	
	if(this.ds_Detail20.rowcount < 1){
		this.gfn_Alert("MM013");//미매입검색을 하신다음 작업을 하십시요.
		return;
	}
	
	var arrMsgArg = ["구매매입 일괄생성"];
	this.gfn_Confirm("HR008", arrMsgArg,  "CONF.CALCPROC", "fn_MsgCallback");//구매매입 일괄생성 하시겠습니까?	
	
};

