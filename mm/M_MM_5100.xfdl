this.fn_VatAmSet = function()
{	
	var nBuyhAm   = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BUYH_AM");
	var nVatmAm   = 0;	
	var nVatmRt   = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "VATM_RT");	
	var sDoimGu	  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DOIM_GU");
	
	if(this.gfn_IsNull(nVatmRt)){ 
		nVatmRt = 0;
	}
	//--- 2022.06.16 BODY의 부가세는 사용하지 않고 합계금액을 기준으로 10%
	
	//nVatmAm = this.ds_Detail10.getCaseSum("crudType != 'D'","VTAX_AM");
	
	if(sDoimGu == "1" && !this.gfn_IsNull(sDoimGu)) {
		nVatmAm = nexacro.floor(nBuyhAm / 10);
	}else{
		nVatmAm = 0;
	}
		
	this.ds_Master10.setColumn(this.ds_Master10.rowposition,  "VTAX_AM", nVatmAm);
	this.div_MasterInput.form.tab_Master.tpg_01.form.mae_HapAm.set_value( nBuyhAm + nVatmAm);	
	
}
// 부가세코드가 수정되었을때 적용

this.fn_VatAmCalc = function(){
	var strCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");	
	var nVatmRt   = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "VATM_RT");	
	var nWoneAm   = 0;
	var vTaxAm	  = 0;
	
	if(this.gfn_IsNull(nVatmRt)){ 
		nVatmRt = 0;
	}
	
	for(var iLoop = 0 ; iLoop < this.ds_Detail10.rowcount; iLoop++){
		if(strCurrCd == "0090"){
			nWoneAm  = this.ds_Detail10.getColumn(iLoop, "WONE_AM");
			vTaxAm 	 = nexacro.floor(nWoneAm * nVatmRt);
		}else{
			vTaxAm	 = 0;
		}
		this.ds_Detail10.setColumn(iLoop, "VTAX_AM", vTaxAm);
	}
	
	this.fn_VatAmSet();
}
/*---------------------------------------------------------------------------------------------
