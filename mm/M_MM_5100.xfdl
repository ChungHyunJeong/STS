/***********************************************************************************************
 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
	
		case "COMBO_SEARCH" : // 공통코드 조회	
		
			if(this.ds_Mm010.getRowCount() > 0){
				this.ds_Mm010Sch.copyData(this.ds_Mm010);
				this.ds_Mm010Sch.insertRow(0);
				this.ds_Mm010Sch.setColumn(0,"CODM_NM","ALL");			
				
			}
					
			if(this.ds_Mm510.getRowCount() > 0){
				this.ds_Mm510Sch.copyData(this.ds_Mm510);
				this.ds_Mm510Sch.insertRow(0);
				this.ds_Mm510Sch.setColumn(0,"CODM_NM","ALL");
			}
			
			this.ds_FaVatm.filter("VATM_GU == '0'");			
			
			break;
			
		case "MST_SEARCH": //조회
			
			if (this.ds_Master10.getRowCount() > 0) {
			
				this.ds_Master10.addColumn("s_chk", "string");
				this.ds_Master10.addColumn("crudType", "string");
				
				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {				
					var iFvFindRow = this.ds_Master10.findRowExpr("BUYH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ=='" + this.fvSchKey02 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}
				
				var strSaumCd = this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SaumCd.value;		
				
				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
				this.fn_SetGridInputButtonEnable();
				this.div_DetailInput.form.tab_Detail.set_enable(true);
				
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
				
			} else {
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			
				//입력영역 비활성화
				this.div_MasterInput.set_enable(false);				
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);
				this.div_DetailInput.form.tab_Detail.set_enable(false);
				
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterView.form.grd_Master10);
				
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = false;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);		
				
				this.div_DetailInput.form.btn_InptAll.set_enable(false);
			}
			
			//저장후 재조회여부-true
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}
			
			break;
		case "SAVE":   //저장		

			//첨부파일 추가건이 없으면
			if (!this.fvFileDiv.fn_CheckFileAddYn()) {
				this.fvSchKeyYn = true;//저장후 재조회여부
				this.fn_ComBtnSearch();//저장후 재조회
			} else {
				this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);			
			}
			
			this.fn_MrpSearch();			
			
			break;	
		case "DTL_SEARCH": //조회
			//[공통-자동전표상세] 조회
			if (this.ds_Master10.getRowCount() > 0) {
				var sPRGM_ID = this.fvComPrgmId;//프로그램ID
				var sMANA_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BUYH_NO");//관리번호-업무Key
				var sWOFH_ST = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST");//결재상태
				var sWOFH_SQ = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//결재차수
				var atobObj = new Object();//[공통] 자동전표 파라메타 정보 Object
				atobObj.PRGM_ID = sPRGM_ID;
				atobObj.MANA_NO = sMANA_NO;
				atobObj.WOFH_ST = sWOFH_ST;
				atobObj.WOFH_SQ = sWOFH_SQ;
				
				eval(this.fvAtobDiv).fn_ComBtnSearch(atobObj);//자동전표 상세내역 조회
			}
		
			this.ds_Detail10.addColumn("s_chk", "string");
			this.ds_Detail10.addColumn("crudType", "string");
			this.ds_Detail10.addColumn("ITEM_NM", "string");
			this.ds_Detail10.addColumn("prt_chk", "string");
			
			//this.cbo_PdrhGu_onitemchanged(this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_PdrhGu);	
			
			//첨부파일 정보 조회			
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID			
			oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타			
			oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BUYH_NO");//PK번호2-매입요청번호	
			oFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수

			this.fvFileDiv.fn_AtchFileSearch(oFileInfo);
			
			//var BuyhAm = nexacro.toNumber(this.div_MasterInput.form.tab_Master.tpg_01.form.mae_BuyhAm.value);
			//var VtaxAm = nexacro.toNumber(this.div_MasterInput.form.tab_Master.tpg_01.form.mae_VtaxAm.value);
			var BuyhAm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BUYH_AM");
			var VtaxAm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "VTAX_AM");
			
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_HapAm.set_value( BuyhAm + VtaxAm );
			
						
			break;			 
			
		case "DTLS_SEARCH": //미매입검색		
			this.ds_Detail20.addColumn("s_chk", "string");		
			this.ds_Detail20.addColumn("s_conf", "string");

			break;			
			
		case "DELETE": //삭제		
			this.fn_ComBtnSearch();//조회
			break;		
			
		case "WF_RECIDE": //[결재]재기안처리		
			if (nApp.gds_WfRst.getRowCount() > 0) {
				if (nApp.gds_WfRst.getColumn(0, "O_RET_CODE") == "TRUE") {
					this.gfn_Alert("WF012");//재기안 처리가 완료되었습니다.
					this.fvSchKeyYn = true;//저장후 재조회여부
					this.fn_ComBtnSearch();//재기안 재조회
				} else {
					var arrMsgArg = [nApp.gds_WfRst.getColumn(0, "O_RET_CMSG")];
					this.gfn_Alert("WF013", arrMsgArg);//재기안 처리시 오류가 발생하였습니다.\n&ITM0				
				}
			} else {
				var arrMsgArg = [""];
				this.gfn_Alert("WF013", arrMsgArg);//재기안 처리시 오류가 발생하였습니다.\n&ITM0
			}
			break;	
			
		case "PROC_CALC": //구매매입 일괄등록
			if (nApp.gds_WfRst.getRowCount() > 0) {
			
				//this.alert(nApp.gds_WfRst.getColumn(0, "O_RET_CODE"));
			
				if (nApp.gds_WfRst.getColumn(0, "O_RET_CODE") == "TRUE") {
					var arrMsgArg = ["구매매입 일괄등록"];					
					this.fvSchKeyYn = false;//저장후 재조회여부
					this.fn_ComBtnSearch();// 재조회					
					this.gfn_Alert("MM015", arrMsgArg);//구매매입 일괄등록 처리가 완료되었습니다.
					
					
				} else if (nApp.gds_WfRst.getColumn(0, "O_RET_CODE") == "EXIT") {
					var arrMsgArg = [nApp.gds_WfRst.getColumn(0, "O_RET_CMSG")];
					this.gfn_Alert("HS007", arrMsgArg);//아래와 같은 이유로 처리할 수 없습니다.\n&ITM0					
				} else {
					var arrMsgArg = [nApp.gds_WfRst.getColumn(0, "O_RET_CMSG")];
					this.gfn_Alert("MM016", arrMsgArg);//구매매입 일괄등록 처리시 오류가 발생하였습니다..\n&ITM0				
				}
			} else {
				var arrMsgArg = [""];
				this.gfn_Alert("MM016", arrMsgArg);//구매매입 일괄등록 처리시 오류가 발생하였습니다..\n&ITM0
			}
			
			this.fn_setButtonEnable();
			
			break;			
			
		case "MST_ACCH_SEARCH": //전표마스터 일괄조회		
			if (this.ds_Master10Multi.getRowCount() > 0) {
				this.fn_ComAccbSearch();
			} else {
				//멀티인쇄로 호출한경우
				if (this.fvMultPrintYn) {
					this.fvMultiSearchYn = false;//멀티인쇄조회중여부;
				}			
			}
			break;	
			
		case "DTL_ACCB_SEARCH": //전표상세 일괄조회		

			this.fn_ComMakeReportParam();//레포트생성 파라메타 생성
			break;		
			
		case "MST_PCWH_SEARCH": //인쇄용 일괄조회	- 마스터		
			//case "MST_PCWH_SEARCH_IP": //인쇄용 일괄조회	- 마스터		
		            
			if (this.ds_Master10Multi_Ip.getRowCount() > 0) {
				this.fn_ComPcwbSearch_IP(); //입고일괄출력용
			} else {
				//멀티인쇄로 호출한경우
				if (this.fvMultPrintYn) {
					this.fvMultiSearchYn = false;//멀티인쇄조회중여부
				}			
			}
			break;	
		case "DTL_PCWB_SEARCH": //인쇄용 일괄조회	- 상세	
			this.fn_ComMakeReportParam_IP();//레포트생성 파라메타 생성
			break;	
			
		case "MST_PCOH_SEARCH": //인쇄용 일괄조회	- 마스터					
		            
			if (this.ds_Master10Multi_Pcoh.getRowCount() > 0) {
				this.fn_ComPcobSearch(); //발주일괄출력용
			} else {
				//멀티인쇄로 호출한경우
				if (this.fvMultPrintYn) {
					this.fvMultiSearchYn = false;//멀티인쇄조회중여부
				}			
			}
			break;	
		case "DTL_PCOB_SEARCH": //인쇄용 일괄조회	- 상세	
			this.fn_ComMakeReportParam_Pcoh();//레포트생성 파라메타 생성
			break;	
	}
};

/***********************************************************************************************
 * 13.02. [팝업] CallBack 함수 처리
          strId : 팝업호출시 지정한 팝업ID
	      rtnValue : 팝업창에서 close시 지정한 반환값
          주의 : Open(popuptype=modeless)창은 팝업콜백을 지원하지 않음
 ***********************************************************************************************/ 
this.fn_PopupCallback = function(strId, rtnValue)
{
	//[결재]요청/결재취소]
	if (strId == "C_WF_REQUEST") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);
			
			//리턴메세지코드
			if (!this.gfn_IsNull(arrRtnVal.MSG_CODE)) {
				//결재정상처리 된경우
				if (arrRtnVal.MSG_CODE == "0") {
					//재조회
					//[공통] 결재요청/결재취소 재조회시 KEY값 설정
				   this.fvSchKeyYn = true;
				   this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BUYH_NO");//조회Key01
				   this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//조회Key02  
					this.fn_ComBtnSearch();
				}
			}	
		}

	//거래처
	} else if (strId == "S_CUSM_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);
			
			if (!this.gfn_IsNull(arrRtnVal.CUSM_CD)) {
				this.div_Search.form.edt_CusmCd.set_value(arrRtnVal.CUSM_CD);
				this.div_Search.form.edt_CusmNm.set_value(arrRtnVal.CUSM_NM);
			}
		}else{
				this.div_Search.form.edt_CusmCd.set_value("");
				this.div_Search.form.edt_CusmNm.set_value("");
		}
	//거래처(Detail)
	} else if (strId == "S_CUSM_CD_02") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);
			
			if (!this.gfn_IsNull(arrRtnVal.CUSM_CD)) {
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmCd.set_value(arrRtnVal.CUSM_CD);
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmNm.set_value(arrRtnVal.CUSM_NM);
			}
		}else{
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmCd.set_value("");
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmNm.set_value("");
		}
		
	//등록부서
	} else if (strId == "S_DEPM_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);
			
			if (!this.gfn_IsNull(arrRtnVal.DEPM_CD)) {
				this.div_Search.form.edt_DepmCd.set_value(arrRtnVal.DEPM_CD);
				this.div_Search.form.edt_DepmNm.set_value(arrRtnVal.DEPM_NM);
			}
		}else{
			//조회조건영역 - 전체권한여부 체크
			if (this.gfn_CheckAllAuth(this.fvComPrgmId)) {
				this.div_Search.form.edt_DepmCd.set_value("");
				this.div_Search.form.edt_DepmNm.set_value("");		
			} else {
				this.div_Search.form.edt_DepmCd.set_value(this.gfn_GetSessionValue("DEPM_CD"));
				this.div_Search.form.edt_DepmNm.set_value(this.gfn_GetSessionValue("DEPM_NM"));					
			}
		}	
	//부가세코드		
	} else if (strId == "M_VATM_ID_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);
			
			if (!this.gfn_IsNull(arrRtnVal.VATM_CD)) {
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_VatmCd.set_value(arrRtnVal.VATM_CD);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_VatmNm.set_value(arrRtnVal.VATM_NM);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_VatmRt.set_value(arrRtnVal.VATM_RT);
				
				this.fn_VatAmCalc();
			}
		}else{
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_VatmCd.set_value("");
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_VatmNm.set_value("");
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_VatmRt.set_value("");
		}
				
	//등록부서(Detail)
	} else if (strId == "S_DEPM_CD_02") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);
			
			if (!this.gfn_IsNull(arrRtnVal.DEPM_CD)) {				
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_DepmCd.set_value(arrRtnVal.DEPM_CD);
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_DepmNm.set_value(arrRtnVal.DEPM_NM);											
			}
		}else{
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_DepmCd.set_value("");
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_DepmNm.set_value("");											
		}
		
	//요청자
	} else if (strId == "S_SYST_ID_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.EMPM_CD)) {
				this.div_Search.form.edt_EmpmCd.set_value(arrRtnVal.EMPM_CD);
				this.div_Search.form.edt_EmpmNm.set_value(arrRtnVal.EMPM_NM);
				this.div_Search.form.edt_SystId.set_value(arrRtnVal.SYST_ID);
			}
		}else{
			//조회조건영역 - 자신의자료 권한여부 체크		
			if (this.gfn_CheckOwnAuth(this.fvComPrgmId)) {
				this.div_Search.form.edt_EmpmCd.set_value(this.gfn_GetSessionValue("EMPM_CD"));
				this.div_Search.form.edt_EmpmNm.set_value(this.gfn_GetSessionValue("EMPM_NM"));
				this.div_Search.form.edt_SystId.set_value(this.gfn_GetSessionValue("SYST_ID"));	
			} else {
				this.div_Search.form.edt_EmpmCd.set_value("");
				this.div_Search.form.edt_EmpmNm.set_value("");
				this.div_Search.form.edt_SystId.set_value("");				
			}
		}
	
	//[Master영역]
	//요청부서		
	} else if (strId == "M_DEPM_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.DEPM_CD)) {
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_value(arrRtnVal.DEPM_CD);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmNm.set_value(arrRtnVal.DEPM_NM);
			}
		}else{
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_value("");
