			this.gfn_RemkPop(sPopupId, sTitle, oArg);		
		}
		
	//발주이력 팝업	
	} else if (e.col == 21) {
		var sPopupId = "M_MM_2100_01";//팝업ID
		var sPopupTitle = "발주이력 상세"; //Popup화면 타이틀
		var sPopupFormUrl  = "mm::" + sPopupId + ".xfdl"; //Popup화면 URL
		var oArg = { 
					  paramItemCd:this.ds_Detail10.getColumn(this.ds_Detail10.rowposition,"ITEM_CD")		//품목코드
					, paramItemNm:this.ds_Detail10.getColumn(this.ds_Detail10.rowposition,"ITEM_NM")		//품목명
					, paramCusmCd:this.ds_Detail10.getColumn(this.ds_Detail10.rowposition,"CUSM_CD")		//조사업체코드
					, paramCusmNm:this.ds_Detail10.getColumn(this.ds_Detail10.rowposition,"CUSM_NM")		//조사업체명
					, PrgmId:this.fvComPrgmId                                                     			//프로그램ID
					}; //Popup화면 전달 파라메터 
		var oOption = {title:sPopupTitle, width:"1105", height:"650"}; //Popup화면 옵션
		var sPopupCallBack = "fn_PopupCallback"; //Popup화면 호출후 Callback 처리 함수
		this.gfn_OpenPopup(sPopupId, sPopupFormUrl, oArg, sPopupCallBack, oOption); //Popup화면 호출 함수	
	}
};




