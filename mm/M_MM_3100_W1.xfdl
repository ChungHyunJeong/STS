/***********************************************************************************************
 * WF.02. [결재처리] 최종결재 [승인] 시 업무영역 전처리 함수
 ***********************************************************************************************/
this.fn_ComWfAppr = function(oWfParam)
{
	
	//승인시 납품예정일관리 저장
	this.ds_MmPcoc.clearData();
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		this.ds_MmPcoc.addRow();
		this.ds_MmPcoc.setColumn(iLoop, "PCOH_NO", this.ds_Detail10.getColumn(iLoop, "PCOH_NO"));    /* 발주번호 */
		this.ds_MmPcoc.setColumn(iLoop, "WOFH_SQ", this.ds_Detail10.getColumn(iLoop, "WOFH_SQ"));    /* 발주차수 */
		this.ds_MmPcoc.setColumn(iLoop, "PCOB_SE", this.ds_Detail10.getColumn(iLoop, "PCOB_SE"));    /* 발주순번 */
		//this.ds_MmPcoc.setColumn(iLoop, "PCOC_SE", this.ds_Detail10.getColumn(iLoop, ""));    /* 납품순번 */
		this.ds_MmPcoc.setColumn(iLoop, "PCOC_SQ", "1");    /* 변경차수 */
		this.ds_MmPcoc.setColumn(iLoop, "WORH_YN", "1");    /* 최종여부 */
		this.ds_MmPcoc.setColumn(iLoop, "PCOC_DD", this.ds_Master10.getColumn(0, "PCOH_DD"));    /* 변경일자 */
		this.ds_MmPcoc.setColumn(iLoop, "PCOC_QT", this.ds_Detail10.getColumn(iLoop, "PCOB_QT"));    /* 납품수량 */
		this.ds_MmPcoc.setColumn(iLoop, "DELI_DD", this.ds_Detail10.getColumn(iLoop, "DELI_DD"));    /* 당사납품요청일 */
		this.ds_MmPcoc.setColumn(iLoop, "PADE_DD", this.ds_Detail10.getColumn(iLoop, ""));    /* 협력사납품예정일 */
		this.ds_MmPcoc.setColumn(iLoop, "DELE_YN", "0");    /* 삭제여부 */
		this.ds_MmPcoc.setColumn(iLoop, "REMK_NM", this.ds_Master10.getColumn(iLoop, "REMK_NM"));    /* 당사비고 */
		this.ds_MmPcoc.setColumn(iLoop, "PARE_NM", this.ds_Master10.getColumn(iLoop, "PARE_NM"));    /* 협력사비고 */
		this.ds_MmPcoc.setColumn(iLoop, "EXPT_YN", "");    /* 실적제외여부 */
		this.ds_MmPcoc.setColumn(iLoop, "EXPT_RE", "");    /* 실적제외사유 */
	}
	
	
	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	//[공통]서비스 호출 변수
	var sInputDaset = "" //입력 데이터셋
	var sOutDatset = "";//출력 데이터셋
	var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		
	//[공통]서비스 정보 추가
	svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
	svcAddObj.sInsertSql = "mm:M_MM_3100_M_IN11";//저장Sql
	svcAddObj.sUpdateSql = "";//수정Sql
	svcAddObj.sDeleteSql = "";//삭제Sql
	svcAddObj.sSaveFlagColumn = "";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
							 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

	this.gfn_AddComSaveService(this, svcAddObj);
	sInputDaset = "ds_MmPcoc=ds_MmPcoc:U" //입력 데이터셋

	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "SAVE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = sInputDaset //입력 데이터셋
	svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
	svcCallObj.sOtherArg = sOtherArg;//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "";//서버에서 처리가 완료된 후 Callback 받을 Function 명		
	svcCallObj.isSync =true;
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
	trace("최종결재 [승인] 시 업무영역처리");
	return true;
}

