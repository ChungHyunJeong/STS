		svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
		svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
		//svcCallObj.sOutDatset = "ds_Master13=ds_Master13" ;//출력 데이터셋
		svcCallObj.sOutDatset = "ds_Master13=ds_Master13 ds_Master10Rt=ds_Master10 ds_Detail10=ds_Detail10" ;//출력 데이터셋
		svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
		svcCallObj.isSync = true;//동기식으로 설정할지 여부

		//[공통]서비스 호출
		this.gfn_CallComService(this, svcCallObj);			
		
		//사업장정보가 있으면
		if (this.ds_Master13.getRowCount() > 0) {			
			this.ds_Master10Rt.setColumn(0, "ADDR_NM", this.ds_Master13.getColumn(0, "ADD1_NM"));
			this.ds_Master10Rt.setColumn(0, "STEL_NO", this.ds_Master13.getColumn(0, "OTEL_NO"));
			this.ds_Master10Rt.setColumn(0, "SFAX_NO", this.ds_Master13.getColumn(0, "OFAX_NO"));
		}
		
		//코드명정보 세팅		
		this.ds_Master10Rt.setColumn(0, "TERM_NM", this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_TermCd.text);
		this.ds_Master10Rt.setColumn(0, "INSP_NM", this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_InspCd.text);
		this.ds_Master10Rt.setColumn(0, "PACK_NM", this.div_MasterInput.form.tab_Master.tpg_02.form.cbo_PackGu.text);

		//국내& 해외
