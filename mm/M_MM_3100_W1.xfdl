this.fn_ComBtnPrint = function()
{	  
	var iSelRow = this.ds_Master10.rowposition;//선택한Row

	if (this.ds_Master10.getRowCount() > 0 && iSelRow > -1) {
	
		var sDoimGu = this.ds_Master10.getColumn(iSelRow, "DOIM_GU");//내외자구분
		
		//마스터출력할 Row 복사
		this.ds_Master10Rt.clearData();
		this.ds_Master10Rt.addRow();
		this.ds_Master10Rt.copyRow(0, this.ds_Master10, iSelRow);		

		
		//사업장정보 조회
		this.ds_Input.clearData();
		this.ds_Master13.clearData();
				
		this.ds_Input.addRow();
		this.ds_Input.setColumn(0, "SAUM_CD", "0010");//사업장코드-본사
		this.ds_Input.setColumn(0, "PCOH_NO", this.ds_Master10.getColumn(0, "PCOH_NO"));//발주번호
		this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(0, "WOFH_SQ"));//결재차수	
			
		//[공통]서비스정보 초기화
		this.gfn_InitComService(this);
		
		//[공통]서비스정보 추가 : 
		var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
		svcAddObj.sSelectSql = "mm:M_MM_3100_M_SL13";   //사업장정보
		this.gfn_AddComSearchService(this, svcAddObj);
		
		//[공통]서비스정보 추가
		var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
		svcAddObj.sSelectSql = "mm:M_MM_3100_M_SL14";   // 레포트 출력용 헤더 조회
		this.gfn_AddComSearchService(this, svcAddObj);
		
		//[공통]서비스정보 추가
		var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
		svcAddObj.sSelectSql = "mm:M_MM_3100_D_SL10";   //레포트 출력용 전표 Body 조회
		this.gfn_AddComSearchService(this, svcAddObj);		
		
		
		//[공통]서비스 호출 변수
