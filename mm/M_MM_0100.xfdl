			oFileInfo.PK01_NO = this.ds_Master20.getColumn(this.ds_Master20.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo.PK02_NO = this.ds_Master20.getColumn(this.ds_Master20.rowposition, "CSPM_CD");//PK번호2-고객번호
			
			this.fvFileDiv.fn_AtchFileSearch(oFileInfo);			
			break;			

		case "DUP_CHECK": //SRM ID 중복체크
			
			//중복여부시 메시지
			if (this.ds_Master20Dup.getColumn(0, "DUP_CNT") > 0) {	//중복시			
				this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CscmId.setFocus();
				this.gfn_Alert("MM024");//중복 ID가 존재합니다.
				
				return false;
			
			} else {	//사용가능시
				this.ds_Master20.setColumn(this.ds_Master20.rowposition, "DUPI_CK", "1");	//중복체크 여부
				this.div_MasterInput.form.tab_Master.tpg_02.form.btn_DupCheck.set_enable(false);	//버튼 비활성화

				this.gfn_Alert("MM025");//사용가능한 ID입니다.
			}
			
			break;			

		case "WF_RECIDE": //재기안처리		
			this.gfn_Alert("WF012");//재기안 처리가 완료되었습니다.
			
			this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD");//조회Key01
			this.fvSchKey02 = this.ds_Master20.getColumn(this.ds_Master20.rowposition, "CSPM_CD");//조회Key02
			this.fvSchKeyYn = true;//저장후 재조회여부
			
			this.fn_ComBtnSearch();//재기안 재조회
			
			break;		
		/*
		case "SRMID.SAVE" :	//SRM ID 생성
			this.ds_Master20.setColumn(this.ds_Master20.rowposition, "SRM_YN", "1");
			this.div_MasterInput.form.tab_Master.tpg_02.form.btn_GenID.set_enable(false);
			
			this.gfn_Alert("HR258");
				
			break;
		*/
	}
};


