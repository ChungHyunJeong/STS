 * 01.01. [공통버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.fn_ComBtnSearch = function(sWofhNo) //[결재]
{ 
	//[결재] 결재 PID가 있으면-결재Popup으로 호출시
	if (!this.gfn_IsNull(sWofhNo)) {
		this.fvWfSearchMode = true;//[결재]결재내역 조회모드
		this.fvWfPopupMode = true;//[결재]결재내역 팝업화면으로 호출여부
	}
	
	//저장후 재조회시-마스터 데이터셋 이벤트 비활성화
	if (this.fvSchKeyYn) {
		this.ds_Master10.set_enableevent(false);
	}
	
	//사업자번호 타이틀 변경
	this.div_MasterInput.form.tab_Master.tpg_01.form.sta_SaumNo.set_text("사업자번호");	

	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Master10.clearData();
	this.ds_Master20.clearData();
	this.ds_Detail10.clearData();
	this.ds_Detail20.clearData();
	this.ds_Detail30.clearData();
	this.ds_Detail40.clearData();
	this.ds_Detail50.clearData();
	this.ds_Detail60.clearData();
	this.ds_Detail70.clearData();
	this.ds_Detail80.clearData();
	this.ds_Detail90.clearData();
	this.ds_Detail100.clearData();
	this.ds_Input.clearData();
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
		
	//[업무] 조회조건 입력 : 거래처관리 대상(고객정보는 거래처 row 조회시)
	this.ds_Input.addRow();
	var sSqlId = "";//[결재]쿼리ID	
	
	//[결재] 결재내역 조회가 아니면 (if문)
	if (!this.fvWfSearchMode) {			
		this.ds_Input.setColumn(0, "CUSM_CD", this.div_Search.form.edt_CusmCd.value);
		this.ds_Input.setColumn(0, "SAUM_NO", this.div_Search.form.mae_SaumNo.value);	
		this.ds_Input.setColumn(0, "USED_YN", this.div_Search.form.cbo_UsedYn.value);

		var sGroup = "";
		var arrCode = this.div_Search.form.edt_Ct01Cd.code.split(",");//파트너유형
		var allCheck = this.div_Search.form.edt_Ct01Cd.value.split(",");//ALL선택체크(2022-06-30 요청 - ALL인경우 전체조회)

		for (var iLoop=0;iLoop<arrCode.length;iLoop++) {
			if (!this.gfn_IsNull(arrCode[iLoop])) {
				//마지막은 컴마를 붙이지 않음
				if (iLoop == arrCode.length-1) {
					sGroup += "'" + arrCode[iLoop] + "'";
				} else {
					sGroup += "'" + arrCode[iLoop] + "',";
				}
			}
		}		
		
		//전체 조회를 위해 공백을 넣음(2022-06-30 요청)
		if (allCheck[0] == "ALL") {
			sGroup = "";	
		}
		
		this.ds_Input.setColumn(0, "PART_CD", sGroup);//파트너유형
		
		sSqlId = "mm:M_MM_0100_M_SL10";//쿼리ID
	} else {
		this.ds_Input.setColumn(0, "WOFH_NO", sWofhNo);//결재PID
		sSqlId = "mm:M_MM_0100_M_SL12";//쿼리ID  (2022-03-10 추가개발)
	}

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = sSqlId;
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MST10_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Master10=ds_Master10";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
	tabIndexA = 0;
	tabIndexB = 0;
	
	this.div_MasterInput.form.tab_Master.set_tabindex(0);
	this.div_DetailInput.form.tab_Detail.set_tabindex(0);
}

