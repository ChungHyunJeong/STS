 /***********************************************************************************************
 * 15.00. [결재] 결재상태에 따른 컴포넌트 Enable 등 처리
 ***********************************************************************************************/ 
this.fn_SetWfCompEnable = function()
{ 	
	var mstTabIdx = this.div_MasterInput.form.tab_Master.tabindex;
		
	//[결재]결재내역 팝업화면으로 호출여부
	if (!this.fvWfPopupMode) {
		//[결재] 공통버튼 제어
		var bCommSaveButton = true;//저장버튼 활성화 여부-기본값은 false로 해야함. - 결재승인후 수정일 필요한 화면에서만 true로 설정해야함. 
		//this.fvWfSearchMode = this.gfn_SetCommButtonWf(this, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"), bCommSaveButton);//결재진행상태		
		this.fvWofhStatus = this.gfn_SetCommButtonWf(this, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"), bCommSaveButton);//결재진행상태		
	}
	
	
	//[결재]결재내역 조회모드이면 Visble false
	if (this.fvWfSearchMode || this.fvWofhStatus) {
		//[결재]결재내역 팝업화면으로 호출여부
		if (this.fvWfPopupMode) {
			this.div_Search.set_enable(false);//조회영역 Enable처리
		}

		if (mstTabIdx == "0" || this.fvWfPopupMode) {
			this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);
			//[결재] Div하위의 컴포넌트 Enable 처리
			this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
			this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 입력			
	    } else {
			this.div_DetailInput.form.div_ComGrdBtn.set_visible(true);
			//[결재] Div하위의 컴포넌트 Enable 처리
			this.gfn_SetCompGroupEnable(this.div_MasterInput.form, true);//Master 입력 프리폼
			this.gfn_SetCompGroupEnable(this.div_DetailInput.form, true);//Detail 입력	
			this.div_MasterInput.form.tab_Master.tpg_02.form.btn_DupCheck.set_enable(false);	//중복체크 버튼 비활성화

			this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CspmCd.set_readonly(true);		
			this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CusmCd.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CusmNm.set_readonly(true);

		}
		
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_RemkPop.set_enable(true);	
		
		this.div_MasterInput.form.tab_Master.tpg_02.form.btn_GenID.set_enable(false);	//ID생성 비활성화
		this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CscmId.set_readonly(true);
		
		if(!this.fvWfSearchMode){
			//SRM_ID 값이 없을때..
			if(this.gfn_IsNull(this.ds_Master20.getColumn(this.ds_Master20.rowposition,"CSCM_ID"))){
				this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CscmId.set_readonly(false);
			}
		}
	} else {
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(true);
		//[결재] Div하위의 컴포넌트 Enable 처리
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, true);//Master 입력 프리폼
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form, true);//Detail 입력	
		this.div_MasterInput.form.tab_Master.tpg_02.form.btn_DupCheck.set_enable(false);	//중복체크 버튼 비활성화
		
		//[결재] Div하위의 컴포넌트 Enable 처리 제외할 컴포넌트설정
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_NatmCd.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PartCd.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CspmCd.set_readonly(true);		
		this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CusmCd.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CusmNm.set_readonly(true);
		
		
		this.div_MasterInput.form.tab_Master.tpg_02.form.btn_GenID.set_enable(false);	//ID생성 비활성화
		this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CscmId.set_readonly(true);
		
		//SRM_ID 값이 없을때..
		if(this.gfn_IsNull(this.ds_Master20.getColumn(this.ds_Master20.rowposition,"CSCM_ID"))){
			this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CscmId.set_readonly(false);
		}
	}
}

