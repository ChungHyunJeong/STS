/***********************************************************************************************
 * 15.10. [Vaildation] 중복체크 - SRM ID 중복여부
 ***********************************************************************************************/ 
this.div_MasterInput_tab_Master_tpg_02_btn_DupCheck_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	//2022-03-23 요청내용
	//SRM ID가 중복인지 체크함
	var sCscmId = this.ds_Master20.getColumn(this.ds_Master20.rowposition, "CSCM_ID");
	if (!this.gfn_IsNull(sCscmId)) {		
		if (sCscmId.length < 6 || sCscmId.length > 10) {
			this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CscmId.setFocus();
			this.gfn_Alert("MM023");//SRM ID는 6자리 이상 10자리 이하로 등록하셔야 합니다.
			
			return false;
		}
	}

	this.ds_Master20Dup.clearData();		//중복여부 체크
	this.ds_Input.clearData();
	
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "CSCM_ID", this.ds_Master20.getColumn(this.ds_Master20.rowposition, "CSCM_ID")); //SCM_ID
	this.ds_Input.setColumn(0, "CSPM_CD", this.ds_Master20.getColumn(this.ds_Master20.rowposition, "CSPM_CD")); //직원번호
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "mm:M_MM_0100_M_SL25";	//SRM ID 중복체크
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "DUP_CHECK";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_MM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Master20Dup=ds_Master25";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);			
};


/***********************************************************************************************
 * 15.11. [활성화/비활성화] SRM ID Key In
 ***********************************************************************************************/ 
this.div_MasterInput_tab_Master_tpg_02_edt_CscmId_onkeyup = function(obj:nexacro.Edit,e:nexacro.KeyEventInfo)
{
	var sCscmId = this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CscmId.value;
	var sOrgCscmId = this.ds_Master20.getColumn(this.ds_Master20.rowposition, "CSCM_ID");

	if (!this.gfn_IsNull(sCscmId) && sCscmId != sOrgCscmId) {	//ID 존재시
		this.ds_Master20.setColumn(this.ds_Master20.rowposition, "DUPI_CK", "0");	//중복체크 여부
		this.div_MasterInput.form.tab_Master.tpg_02.form.btn_DupCheck.set_enable(true);	//버튼 활성화
		
		this.ds_Master21.clearData();
		this.ds_Master22.clearData();
		this.ds_Master20.setColumn(this.ds_Master20.rowposition, "SRM_YN", "0");
		
	} else if (this.gfn_IsNull(sCscmId) || sCscmId == '') {	//ID 없을 경우
		this.ds_Master20.setColumn(this.ds_Master20.rowposition, "DUPI_CK", "0");	//중복체크 여부
		this.div_MasterInput.form.tab_Master.tpg_02.form.btn_DupCheck.set_enable(false);//버튼 비활성화
		
		this.ds_Master21.clearData();
		this.ds_Master22.clearData();
		this.ds_Master20.setColumn(this.ds_Master20.rowposition, "SRM_YN", "0");
	}
	
};

/***********************************************************************************************
 * 15.12. [SRM ID] SRM ID 생성
 ***********************************************************************************************/
this.div_MasterInput_tab_Master_tpg_02_btn_GenID_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	//중복체크 되지 않은경우 중복체크 선처리 요청
	if(this.gfn_Nvl(this.ds_Master20.getColumn(this.ds_Master20.rowposition, "DUPI_CK"), "0")=="0"){
		this.gfn_Alert("HR002",["ID생성","중복체크"]);
		return;
	}
	
	//직원번호 가 비어있는경우 직원번호 채번
	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master20, this.ds_Master20.rowposition);
	
	//Master 입력 상태이면
	if (sChkRowType == "I") {	
		//채번
		this.fn_genCspmID();
	}
	//SRM ID 저장
	
	this.ds_Master21.clearData();
	this.ds_Master22.clearData();
	
	this.ds_Master21.addRow();
	this.ds_Master21.setColumn(0, "CSPM_CD", this.ds_Master20.getColumn(this.ds_Master20.rowposition, "CSPM_CD")); //직원번호
	this.ds_Master21.setColumn(0, "CSCM_ID", this.ds_Master20.getColumn(this.ds_Master20.rowposition, "CSCM_ID")); //SRM_ID
	this.ds_Master21.setColumn(0,"crudType", "I");
	
	this.ds_Master20.setColumn(this.ds_Master20.rowposition, "SRM_YN", "1");
	
	this.fn_ComBtnSave();
};


/***********************************************************************************************
 * 15.13. [직원정보] 직원번호 채번
 ***********************************************************************************************/
this.fn_genCspmID = function()
{
	var iMasterRowPos = this.ds_Master20.rowposition;//Master 현재 Row Position
	
	if(!this.gfn_IsNull(this.ds_Master20.getColumn(iMasterRowPos, "CSPM_CD"))) return;
	
	//채번
	var sTbnmNo = this.gfn_GetTbnmNo(this.fvCspmId);//채번값 (고객정보 예외 생성-설계자확인)
	if (this.gfn_IsNull(sTbnmNo)) {
		//채번을 가져오는데 실패했습니다.
		return;
	}

	var sChkTbnmNo = "";//채번-체크용

	//Master 데이터셋에 채번정보 입력
	this.ds_Master20.setColumn(iMasterRowPos, "CSPM_CD", sTbnmNo);	//고객번호

	//PK값 정의[필수]
	var sCommCd = this.ds_Master20.getColumn(iMasterRowPos, "COMM_CD");//회사코드
	var sCspmCd = this.ds_Master20.getColumn(iMasterRowPos, "CSPM_CD");//고객번호

	//Detail 데이터셋에 PK정보 입력
	for (var iLoop=0; iLoop<this.ds_Detail30.getRowCount(); iLoop++) {
		sChkTbnmNo = this.ds_Detail30.getColumn(iLoop, "CSPM_CD");//고객번호
		
		if (this.gfn_IsNull(sChkTbnmNo)) {
			this.ds_Detail30.setColumn(iLoop, "COMM_CD", sCommCd);
			this.ds_Detail30.setColumn(iLoop, "CSPM_CD", sCspmCd);
		}
	}
}


