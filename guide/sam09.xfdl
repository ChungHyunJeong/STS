//변수를 카멜케이스형태로 변경
this.fn_Under2Camel = function(str){
	return str.toLowerCase().replace(/(\_[a-z])/g, function(arg){
		return arg.toUpperCase().replace('_','');
	});
}

//컴포넌트Type PreFix
this.fn_GetCompPreFix = function(sComp)
{
	switch(sComp)
	{
		case "Static":
			return "sta_";
			break;
		case "Edit":
			return "edt_";
			break;			
		case "Edit2Pop":
			return "edt_";
			break;
		case "Calendar":
			return "cal_";
			break;
		case "MaskEdit":
			return "mae_";
			break;
		case "Combo":
			return "cbo_";
			break;
		case "TextArea":
			return "txt_";
			break;
		case "Radio":
			return "rdo_";
			break;
		case "CheckBox":
			return "chk_";
			break;			
	}
	
}

