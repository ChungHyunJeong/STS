this.tab_TableInfo_tab_Page02_btn_WidthSet_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var sCOLUMN_NM = "";//컬럼명
	var iChkRow = 0;
	var iFindRow = -1;
	for (var iLoop=0; iLoop < this.ds_MakeGridData.getRowCount(); iLoop++) {
		sCOLUMN_NM = this.ds_MakeGridData.getColumn(iLoop, "COLUMN_NM");//컬럼명
		
		if (this.ds_MakeGridData.getColumn(iLoop, "WIDTH_SKIP") != "1") {
			iChkRow = this.fn_SchColLike(sCOLUMN_NM);
			if (iChkRow > -1) {
				if (!this.gfn_IsNull(this.ds_ColWidth.getColumn(iChkRow, "maskedittype"))) {
					this.ds_MakeGridData.setColumn(iLoop, "maskedittype", this.ds_ColWidth.getColumn(iChkRow, "maskedittype"));
					this.ds_MakeGridData.setColumn(iLoop, "maskeditformat", this.ds_ColWidth.getColumn(iChkRow, "maskeditformat"));
				}
				
				this.ds_MakeGridData.setColumn(iLoop, "TEXT_ALIGN", this.ds_ColWidth.getColumn(iChkRow, "TEXT_ALIGN"));
				this.ds_MakeGridData.setColumn(iLoop, "COMP_TYPE", this.ds_ColWidth.getColumn(iChkRow, "TYPE"));
				this.ds_MakeGridData.setColumn(iLoop, "COL_WIDTH", this.ds_ColWidth.getColumn(iChkRow, "WIDTH"));
			}
			
			iFindRow = this.ds_ColWidth.findRow("COLNM", sCOLUMN_NM);
			
			if (iFindRow > -1) {
				this.ds_MakeGridData.setColumn(iLoop, "TEXT_ALIGN", this.ds_ColWidth.getColumn(iFindRow, "TEXT_ALIGN"));
				this.ds_MakeGridData.setColumn(iLoop, "COMP_TYPE", this.ds_ColWidth.getColumn(iFindRow, "TYPE"));
				this.ds_MakeGridData.setColumn(iLoop, "COL_WIDTH", this.ds_ColWidth.getColumn(iFindRow, "WIDTH"));		
				if (!this.gfn_IsNull(this.ds_ColWidth.getColumn(iFindRow, "maskedittype"))) {
					this.ds_MakeGridData.setColumn(iLoop, "maskedittype", this.ds_ColWidth.getColumn(iFindRow, "maskedittype"));
					this.ds_MakeGridData.setColumn(iLoop, "maskeditformat", this.ds_ColWidth.getColumn(iFindRow, "maskeditformat"));
				}			
			}
		}
	}
};

//컬럼명Like검색
this.fn_SchColLike = function(sCOLUMN_NM)
{
	var iRow = -1;
	var sCOLNM = "";
	for (var iLoop=0; iLoop<this.ds_ColWidth.getRowCount(); iLoop++) {
		if (this.ds_ColWidth.getColumn(iLoop, "LIKE_SCH") == "1") {
			sCOLNM = this.ds_ColWidth.getColumn(iLoop, "COLNM");
			
			if (String(sCOLUMN_NM).indexOf(sCOLNM) >= 0) {
				return iLoop;
			}
		}
	}
	
	return iRow;
}
