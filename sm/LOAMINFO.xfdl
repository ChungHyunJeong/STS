/***********************************************************************************************
 * 13.02. [사용자함수] 선택한 정보 부모창 리턴 함수 
 ***********************************************************************************************/ 
this.fn_Choice = function(iRow) {
	if (iRow > -1) {	
	
        rtnValue = "({" +
                        ' LOAM_CD:"' + this.ds_Master10.getColumn(iRow, "LOAM_CD") + '"'+  //0.대여금코드
                        ',LOAM_NM:"' + this.ds_Master10.getColumn(iRow, "LOAM_NM") + '"'+  //1.대여금명
                        ',LOAM_DD:"' + this.ds_Master10.getColumn(iRow, "LOAM_DD") + '"'+  //2.대여일자
                        ',LOAM_AM:"' + this.ds_Master10.getColumn(iRow, "LOAM_AM") + '"'+  //3.대여금액
                        ',PROC_YN:"' + this.ds_Master10.getColumn(iRow, "PROC_YN") + '"'+  //4.진행여부
                        ',CURR_CD:"' + this.ds_Master10.getColumn(iRow, "CURR_CD") + '"'+  //5.화폐
                        ',ACUM_CD:"' + this.ds_Master10.getColumn(iRow, "ACUM_CD") + '"'+  //6.회계단위
                        ',ACUM_NM:"' + this.ds_Master10.getColumn(iRow, "ACUM_NM") + '"'+  //7.회계단위명
                        ',ACNM_NO:"' + this.ds_Master10.getColumn(iRow, "ACNM_NO") + '"'+  //8.계좌번호
                        ',USED_YN:"' + this.ds_Master10.getColumn(iRow, "USED_YN") + '"'+  //9.사용여부
                        ',REMK_NM:"' + this.ds_Master10.getColumn(iRow, "REMK_NM") + '"'+  //10.비고
                "})";

		this.gfn_PopClose(rtnValue);
	} else {
		this.gfn_Alert("SM046");//선택된 내용이 없습니다.
		return;		
	}
}

