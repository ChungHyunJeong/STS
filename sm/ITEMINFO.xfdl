	var objSchEdt;
	var objSchCbo;
	for (var iLoop=1; iLoop<9; iLoop++) {
		objSchEdt = eval("this.div_SearchDetail.form.edt_ItsmCd" + iLoop);
		objSchCbo = eval("this.div_SearchDetail.form.cbo_ItsmCd" + iLoop);
		
		if (objSchCbo.visible == false) {		
			this.ds_Input.setColumn(0, "SP0" + iLoop + "_CD", objSchEdt.value);
		} else {
			this.ds_Input.setColumn(0, "SP0" + iLoop + "_CD", objSchCbo.value);
		}
	}
	 
	//사용여부-사용
	if (this.div_Search.form.cbo_UsedYn.value == "1") {
		this.ds_Input.setColumn(0, "USED_YN", "1");//사용여부
	} else if (this.div_Search.form.cbo_UsedYn.value == "0") {
		this.ds_Input.setColumn(0, "USED_YN", "0");//사용여부
	} else {
		this.ds_Input.setColumn(0, "USED_YN", "");//사용여부
	}	

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
