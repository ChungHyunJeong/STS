/***********************************************************************************************
 * 00.03. Form Onload 이벤트
 ***********************************************************************************************/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{	
	//부모창 파라메타 처리
	this.fvItemCd = this.getOwnerFrame().ItemCd;//[부모창파라메타] 품목코드
	this.fvItemNm = this.getOwnerFrame().ItemNm;//[부모창파라메타] 품목명
	this.fvMm115  = this.getOwnerFrame().Mm115;//[부모창파라메타] 품목대분류
	this.fvPp070  = this.getOwnerFrame().Pp070;//[부모창파라메타] 품목대분류 제품인 항목만 조회되게	
	
	this.fvComPrgmId = this.getOwnerFrame().PrgmId;//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
	this.fvProdGu = this.getOwnerFrame().ProdGu;//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID

	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 
}

/***********************************************************************************************
 * 00.04. Form [공통] 초기설정 함수 - 필수
 ***********************************************************************************************/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	this.gfn_SetGrid(this.grd_Master10);//[공통]그리드 이벤트 설정 - 프로젝트목록

}

/***********************************************************************************************
 * 00.05. Form [업무] 초기설정 함수
 ***********************************************************************************************/
this.fn_FormBizInit = function() 
{ 

	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	var objCondList = ["SM088|ds_Sm088","PP070|ds_PP070|Y|ALL"];
	
	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, "");
	
	//콤보 인덱스 설정
	this.div_Search.form.cbo_UsedYn.set_index(1);	
	
	//[업무]콤보 데이터 조회
	this.fn_ComboSearch();
	
	
	//파라미터 값이 있는경우
	if (!this.gfn_IsNull(this.fvItemCd)) {
		//this.div_Search.form.edt_ItemCd.set_value(this.fvItemCd);
		//this.div_PopupTitle_btn_Search_onclick();//조회
	}
	if (!this.gfn_IsNull(this.fvItemNm)) {
		//this.div_Search.form.edt_ItemNm.set_value(this.fvItemNm);
		//this.div_PopupTitle_btn_Search_onclick();//조회
	}
	if(!this.gfn_IsNull(this.fvItemCd) || !this.gfn_IsNull(this.fvItemNm)){

		this.essentialYn = "0";
		//this.div_PopupTitle_btn_Search_onclick();//조회
	}

	if(!this.gfn_IsNull(this.fvProdGu)){
		this.div_Search.form.cbo_ProdGu.set_value(this.fvProdGu);
	}else{
		this.div_Search.form.cbo_ProdGu.set_index(0);
	}	
	
	//-- 턴기외주면 대분류 설정값을 턴키외주로 설정(2022.08.16 김형수부장 요청)
	if(this.fvMm115=="0200"){
		this.div_Search.form.cbo_ItlmCd.set_value("T");

		this.ds_Itmm.filter("");
		this.ds_Itmm.filter("ITLM_CD=='T'");
		this.div_Search.form.cbo_ItmmCd.set_value("T");
		
		this.div_Search.form.cbo_ItlmCd.set_enable(false);		
	}
	
	//-- 제품사양번호관리에서 대분류가 제품인것만 조회(2022.10.14 김형수부장 요청)
	if(this.fvPp070=="0020"){		
		this.ds_Itlm.filter("");
		this.ds_ItlmSch.filter("");
		this.ds_Itlm.filter("PROD_GU=='0020'");
		
		this.ds_ItlmSch.filter("PROD_GU=='0020'");
		
		//this.div_Search.form.cbo_ItmmCd.set_value("T");
		
		//this.div_Search.form.cbo_ItlmCd.set_enable(false);		
	}
}

/***********************************************************************************************
 * 00.06. Form [업무] 일반콤보 조회 함수
 ***********************************************************************************************/
this.fn_ComboSearch = function() 
{
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Input.clearData();

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "rd:M_RD_ITLM_C_SL10";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스정보 추가
	svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "rd:M_RD_ITMM_C_SL10";
	this.gfn_AddComSearchService(this, svcAddObj);	
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "COMBO_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_RD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Itlm=ds_Itlm ds_Itmm=ds_Itmm";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);
}

