 * 01.02. [Vaildation] 조회 필수값 체크
 ***********************************************************************************************/
this.fn_VaildSearch = function()
{

	if(this.essentialYn=="0"){
		//부모창에서 파라메타가 있을시 유효성체크 없이 바로 조회한다.
		return true;
	}


	//[조회] Validation 설정
	var arrArgs = ["this.div_Search.form.cbo_ItlmCd^" + this.div_Search.form.sta_ItlmCd.text + "|NULL"
			      ]; 
				  
	var sCheckGroupTitle = "조회조건";//체크그룹 타이틀

	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
		return false;
	}
	
	if (this.gfn_IsNull(this.div_Search.form.cbo_ItmmCd.value) && this.gfn_IsNull(this.div_Search.form.edt_ItemCd.value) && this.gfn_IsNull(this.div_Search.form.edt_ItemNm.value)) {
		//[ &ITM0 ]  또는 [ &ITM1 ] , [ &ITM2]  중 하나는 필수 입력항목입니다. 
		var arrMsgArg = [this.div_Search.form.sta_ItmmCd.text, this.div_Search.form.sta_ItemCd.text, this.div_Search.form.sta_ItemNm.text];
		this.gfn_Alert("SM054", arrMsgArg);
		return false;
	}
	
	//조회조건 필수입력값 있으면 체크
	return true;
}

