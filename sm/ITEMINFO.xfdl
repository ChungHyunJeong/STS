/***********************************************************************************************
 * 15.01. 중분류 사양에 따른 SPEC 조회 조건을 설정
 ***********************************************************************************************/ 
this.fn_SetItemStruct = function()
{

	var objStatic;
	var objCombo;
	var objEdit;
	
	var objSpecStaTitle;//사양정보-타이틀
	var objSpecStaBack;//사양정보-백그라운드
	var objSpecEdtSpCd;//사양정보-코드
	var objSpecEdtSpNm;//사양정보-코드명
	
	for (var iLoop=0; iLoop < 8; iLoop++) {
		objStatic = eval("this.div_SearchDetail.form.sta_ItsmNm" + (iLoop+1));
		objCombo = eval("this.div_SearchDetail.form.cbo_ItsmCd" + (iLoop+1));
		objEdit = eval("this.div_SearchDetail.form.edt_ItsmCd" + (iLoop+1));
		
		objSpecStaTitle = eval("this.div_Itsm.form.sta_Sp0" + (iLoop+1) + "Title");//사양정보-타이틀
		objSpecStaBack = eval("this.div_Itsm.form.sta_Sp0" + (iLoop+1) + "Back");//사양정보-백그라운드
		objSpecEdtSpCd = eval("this.div_Itsm.form.edt_Sp0" + (iLoop+1) + "Cd");//사양정보-코드
		objSpecEdtSpNm = eval("this.div_Itsm.form.edt_Sp0" + (iLoop+1) + "Nm");//사양정보-코드명		
		
		objStatic.set_text(this.gfn_Nvl(this.ds_Itsm.getColumn(iLoop, "ITSM_NM")));//사양명
		
		if (this.gfn_IsNull(objStatic.text)) {
			objStatic.set_visible(false);
			objCombo.set_visible(false);
			objEdit.set_visible(false);
			
			objSpecStaTitle.set_visible(false);
			objSpecStaBack.set_visible(false);
			objSpecEdtSpCd.set_visible(false);
			objSpecEdtSpNm.set_visible(false);
		} else {
			objStatic.set_visible(true);
			
			//KeyIn여부
			if (this.ds_Itsm.getColumn(iLoop, "KEYI_YN") == "0") {
				objEdit.set_visible(false);
				objCombo.set_visible(true);
			} else {
				objEdit.set_visible(true);
				objCombo.set_visible(false);
			}
			
			objSpecEdtSpCd.set_value("");
			objSpecEdtSpNm.set_value("");	
			objSpecStaTitle.set_text(this.gfn_Nvl(this.ds_Itsm.getColumn(iLoop, "ITSM_NM")));
			objSpecStaTitle.set_visible(true);
			objSpecStaBack.set_visible(true);
			objSpecEdtSpCd.set_visible(true);
			objSpecEdtSpNm.set_visible(true);			
		}
		
		//전송된 품목코드가 있는경우 default 값
		if(iLoop == 0 && !this.gfn_IsNull(this.ds_Master11.getColumn(0,"SP01_NM"))){
			objEdit.set_value(this.ds_Master11.getColumn(0,"SP01_NM"));
		}
	}
	
	for (var iLoop=0; iLoop < 8; iLoop++) {
	
		if (this.gfn_IsNull(this.ds_Itsm.getColumn(iLoop, "ITSM_NM"))) { break;}
		
		//KeyIn여부
		if (this.ds_Itsm.getColumn(iLoop, "KEYI_YN") == "0") {
		
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Input.clearData();

			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "ITLM_CD", this.ds_Itsm.getColumn(iLoop, "ITLM_CD"));//대분류	
			this.ds_Input.setColumn(0, "ITMM_CD", this.ds_Itsm.getColumn(iLoop, "ITMM_CD"));//중분류		
			this.ds_Input.setColumn(0, "ITSM_CD", this.ds_Itsm.getColumn(iLoop, "ITSM_CD"));//사양코드
			
			this.fn_SetItemSpec("ds_Itsm0" + (iLoop+1), (iLoop + 1));
		}
	}
}

/***********************************************************************************************
 * 15.02. SPEC 정보 조회
 ***********************************************************************************************/ 
this.fn_SetItemSpec = function(sDs, cboIndex)
{
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	var objDs = eval("this." + sDs);
	var objCbo = eval("this.div_SearchDetail.form.cbo_ItsmCd" + cboIndex);
	objDs.clearData();

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "rd:M_RD_ITEM_M_SL13";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "SPEM_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_RD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = sDs + "=ds_Spem";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.isSync = true;//동기식으로 설정할지 여부

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
	
	if (objDs.getRowCount() > 0) {
		objDs.insertRow(0);
		objDs.setColumn(0, "ITLM_CD", ""); //대분류
		objDs.setColumn(0, "ITMM_CD", ""); //중분류
		objDs.setColumn(0, "SPEM_CD", ""); //SPEC코드
		objDs.setColumn(0, "SPEM_NM", "전체"); //SPEC명
		objCbo.set_index(0);
		
		//전송된 품목코드가 있는경우 default 값
		if(cboIndex == 1 && !this.gfn_IsNull(this.ds_Master11.getColumn(0,"SP01_CD"))){
			objCbo.set_value(this.ds_Master11.getColumn(0,"SP01_CD"));
		}
		
	}
	
}

