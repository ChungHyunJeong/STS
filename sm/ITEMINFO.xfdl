  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {
		var objStatic;
		var objCombo;
		var objSpecEdtSpCd;//사양정보-코드
		var objSpecEdtSpNm;//사양정보-코드명		
		
		for (var iLoop=1; iLoop < 9; iLoop++) {
			objStatic = eval("this.div_SearchDetail.form.sta_ItsmNm" + iLoop);
			objCombo = eval("this.div_SearchDetail.form.cbo_ItsmCd" + iLoop);
			objSpecEdtSpCd = eval("this.div_Itsm.form.edt_Sp0" + iLoop + "Cd");//사양정보-코드
			objSpecEdtSpNm = eval("this.div_Itsm.form.edt_Sp0" + iLoop + "Nm");//사양정보-코드명		

			if (!this.gfn_IsNull(objStatic.text)) {		
				if (objCombo.visible) {
					objSpecEdtSpCd.set_value(this.ds_Master10.getColumn(e.newrow, "SP0" + iLoop + "_CD"));
					objSpecEdtSpNm.set_value(this.ds_Master10.getColumn(e.newrow, "SP0" + iLoop + "_NM"));
				} else {
					objSpecEdtSpNm.set_value(this.ds_Master10.getColumn(e.newrow, "SP0" + iLoop + "_CD"));
				}
	
			} else {
				objSpecEdtSpCd.set_value("");
				objSpecEdtSpNm.set_value("");	
			}
		}
	}
};

