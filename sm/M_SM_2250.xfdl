			svcAddObj.sSelectSql = "sm:M_SM_2250_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);
	
			svcAddObj.sSelectSql = "sm:M_SM_2250_D_SL11";
			this.gfn_AddComSearchService(this, svcAddObj);
			
