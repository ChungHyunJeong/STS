	svcCallObj.sTrid = "USR_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_SM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Userlist=ds_Userlist";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
};

/*---------------------------------------------------------------------------------------------
 * 12.1.7. [사용자함수 - 서비스] 사용자목록 -> 그룹별관리자목록
  ---------------------------------------------------------------------------------------------*/
this.div_UserView_btn_UserIns_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if (this.ds_Userlist.rowposition > -1) { //User 그리드에서 새로운 Row가 존재하는 확인
		//var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail);//crud_flag 변경-입력(I)
		var iAddRow = this.ds_Detail10.addRow();	// 한줄추가
		var iFromRow = this.ds_Userlist.rowposition; 	// 그룹별 관리자 목록 변경 Row
		
		//임시 디테일데이터셋 세팅
		var sSystId = this.ds_Userlist.getColumn(iFromRow, "SYST_ID");
		var iFvFindRow = this.ds_DetailTemp10.findRowExpr("SYST_ID=='" + sSystId + "'");
		if (iFvFindRow > -1) {
			this.ds_DetailTemp10.setColumn(iFvFindRow, "crudType", "U");
			this.ds_DetailTemp10.setColumn(iFvFindRow, "USED_YN", "1");
		} else {
			var iAddDetailTempRow = this.ds_DetailTemp10.addRow();	// 한줄추가
			this.ds_DetailTemp10.setColumn(iAddDetailTempRow, "crudType", "I");
			this.ds_DetailTemp10.setColumn(iAddDetailTempRow, "PGRM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PGRM_CD"));//권한그룹코드
			this.ds_DetailTemp10.copyRow(iAddDetailTempRow, this.ds_Userlist, iFromRow);	
			
		}
		
		this.ds_Detail10.setColumn(iAddRow, "PGRM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PGRM_CD"));//권한그룹코드

		// 데이터셋 Copy 후 Delete
		this.ds_Detail10.copyRow(iAddRow, this.ds_Userlist, iFromRow);	
		this.ds_Userlist.deleteRow(this.ds_Userlist.rowposition);		
	}
};

/*---------------------------------------------------------------------------------------------
 * 12.1.8. [사용자함수 - 서비스] 그룹별관리자목록 -> 사용자목록
  ---------------------------------------------------------------------------------------------*/
this.div_UserView_btn_UserDel_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if (this.ds_Detail10.rowposition > -1) { //Detail 그리드에서 새로운 Row가 존재하는 확인
		var iAddRow = this.ds_Userlist.addRow();	// 한줄추가
		var iFromRow = this.ds_Detail10.rowposition;	// 그룹별 관리자 목록 변경 Row
		
		
		var sSystId = this.ds_Detail10.getColumn(iFromRow, "SYST_ID");
		var iFvFindRow = this.ds_DetailTemp10.findRowExpr("SYST_ID=='" + sSystId + "'");
		var crudType = this.ds_DetailTemp10.getColumn(iFvFindRow, "crudType");

		if (iFvFindRow > -1) {
			if (crudType == "I") {
				this.ds_DetailTemp10.deleteRow(iFvFindRow);
			} else {
				this.ds_DetailTemp10.setColumn(iFvFindRow, "crudType", "D");
			}
		}
		

		// 데이터셋 Copy 후 Delete
		this.ds_Userlist.copyRow(iAddRow, this.ds_Detail10, iFromRow);	
		this.ds_Detail10.deleteRow(iFromRow);		
	}
	
};
