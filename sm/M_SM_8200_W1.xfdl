		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_RqtoDd.set_cssclass("");	//요청기간(종료)
		
		//this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrgmNm.set_readonly(false);
