this.fn_PopupCallback = function(strId, rtnValue)
{
	
	//[Master영역]
	//업무분류		
	if (strId == "M_HCAM_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			trace(rtnValue);
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.HCAM_CD)) {
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_HcamCd.set_value(arrRtnVal.HCAM_CD);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_HcamNm.set_value(arrRtnVal.HCAM_NM);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_HcamDepth.set_value(arrRtnVal.DEPTH);
				
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_HcadCd.set_value("");// 요청유형
				
				if (arrRtnVal.EMPM_CD != 'undefined') {
					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrsuId.set_value(arrRtnVal.APSU_ID);
					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrsuCd.set_value(arrRtnVal.APSU_CD);
					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrsuNm.set_value(arrRtnVal.APSU_NM);
					
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "HR_SYST_ID", arrRtnVal.SYST_ID);//
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "HR_EMPM_CD", arrRtnVal.EMPM_CD);//
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "HR_EMPM_NM", arrRtnVal.EMPM_NM);//
				}else{
					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrsuId.set_value("");
					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrsuCd.set_value("");
					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrsuNm.set_value("");
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "HR_SYST_ID", "");//
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "HR_EMPM_CD", "");//
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "HR_EMPM_NM", "");//
				}
				trace(arrRtnVal.DEPTH);
				trace(arrRtnVal.SYST_ID);
				trace(arrRtnVal.EMPM_CD);
				trace(arrRtnVal.EMPM_NM);
				
				
				this.fn_GetSmHcad();//요청유형
				
			}
		} else {
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_value("");
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmNm.set_value("");		
		}
	//프로그램		
	} else if (strId == "M_PRGM_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);
			
			if (!this.gfn_IsNull(arrRtnVal.DISP_ID)) {
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrgmId.set_value(arrRtnVal.DISP_ID);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrgmNm.set_value(arrRtnVal.PRGL_NM);
			}
		} else {
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrgmId.set_value("");
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrgmNm.set_value("");		
		}
	}
};
 
