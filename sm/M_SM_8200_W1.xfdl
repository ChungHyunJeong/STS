/***********************************************************************************************
 * 00.02. Form 변수 선언
 ***********************************************************************************************/
this.fvComPrgmId = "";//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
this.fvFormComBtnAuthYn = false;	// 그리드 공통버튼 권한 유무
this.fvFileDiv = this.div_MasterInput.form.tab_Master.tpg_05.form.div_CommFile.form;//[첨부파일]Div
this.fvPathNm = "SM";				// [첨부파일]업무파일경로
this.fvDsRowPosChange = true;		// 데이터셋 Row 변경이벤트-실행유무
var fvFileInfo = new Object();		// [첨부파일]정보
this.fvSchKeyYn = false;//저장후 재조회여부
this.fvSchKey01 = "";//조회Key01
this.fvSchKey02 = "";//조회Key02
this.fvSchKey03 = "";//조회Key03

this.fvComWofhNo = "";//[결재]결재PID
this.fvWfSearchMode = false;//[결재]결재내역 조회모드
this.fvWfPopupMode = false;//[결재]결재내역 팝업화면으로 호출여부
this.fvWebCompLoadYn = false;//웹컴포넌트 로드 완료여부
this.fvDtlSearchYn = false;//상세조회여부
this.fvWfguCd = "";//결재구분 - 0010 결재,  0010 협조,  0070 결재(주관부서)
this.fvWfguMode = false;
this.fvProxyUser = false;	//대행여부
var sToDay = this.gfn_GetCommDate("toDay");//현재일자
/***********************************************************************************************
 * 00.03. Form Onload 이벤트
 ***********************************************************************************************/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{
	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 
}

/***********************************************************************************************
 * 00.04. Form [공통] 초기설정 함수 - 필수
 ***********************************************************************************************/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	this.fvComPrgmId = "M_SM_8200";//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID - 결재화면인 경우에는 메뉴에서 클릭한게 아니기 때문에 해당 프로그램ID로 설정
	//this.gfn_SetGrid(this.div_MasterInput.form.tab_Master.tpg_03.form.grd_Detail20);//[공통]그리드 이벤트 설정

	this.fn_SetTabComAttachFile();//[공통] 첨부파일 Tab 설정
	
}

/***********************************************************************************************
 * 00.05. Form [업무] 초기설정 함수
 ***********************************************************************************************/
this.fn_FormBizInit = function() 
{ 

	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	var objCondList = [
						 //"SM306|ds_Sm306Sch|Y|ALL",
						 "SM805|ds_Sm805"	//요청유형
						,"SM810|ds_Sm810"	//진행_상태
						,"SM825|ds_Sm825"	//중요도
						,"SM830|ds_Sm830"	//만족도
						,"WF110|ds_Wf110"	//결재상태
					  ];
	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, "");

	//[업무] 일반콤보조회
	this.fn_ComboSearch();
	
	//[업무] 내용 Web컴포넌트 Url설정
	this.div_MasterInput.form.tab_Master.tpg_04.form.web_ContNm.set_url(this.gfn_GetServerUrl("View"));//웹에디터 Url 설정
	     
	//결재함 화면에 결재번호가 있는 경우 조회 호출
	if (!this.gfn_IsNull(this.parent.parent.fvNowWofhNo)) {
		this.fn_ComBtnSearch(this.parent.parent.fvNowWofhNo); 
	}	
	
	//결재 - 0010 결재,  0020 협조,  0070 결재(주관부서)
	this.fvWfguCd = this.parent.parent.ds_Detail13.getColumn(0,"WFGU_CD");
	//if(this.fvWfguCd == '0010' || this.fvWfguCd == '0020' || this.fvWfguCd == "0070"){
	if(this.fvWfguCd == "0070"){
		this.fvWfguMode = true;
	}
	
	
	  //결재라인 정보가 있으면
	if (this.parent.parent.ds_Detail13.getRowCount() > 0) {
		var sSYST_ID = this.parent.parent.ds_Detail13.getColumn(0, "SYST_ID");//원결재자 시스템ID
		//대행자인경우
		if (this.gfn_GetSessionValue("SYST_ID") != sSYST_ID) {
			this.fvProxyUser = true;
		}
	}
}

/***********************************************************************************************
 * 00.06. Form [업무] 일반콤보 조회 함수
 ***********************************************************************************************/
this.fn_ComboSearch = function() 
{
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Input.clearData();
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "SECH_CN", "EMPM_CD");
	this.ds_Input.setColumn(0, "SECH_VL", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SYST_ID"));

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "sm:M_SM_PGRM_SL02";
	this.gfn_AddComSearchService(this, svcAddObj);
	 
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();					//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "COMBO_SEARCH";				//Transaction ID 등 callback에서 넘겨 받을 String을 지정
