  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {		

		this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
		
		//프로그램정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PRGM_ID"))) {
		
			var iTabIndex = this.div_MasterInput.form.tab_Master.tabindex;

			if (iTabIndex == 1) {	
				var sPrgmId = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PRGM_ID");//프로그램ID
				
				if (this.gfn_IsNull(sPrgmId) || sPrgmId == "*") {
					this.div_MasterInput.form.div_ComGrdBtn.set_visible(false);	
				} else {
					this.div_MasterInput.form.div_ComGrdBtn.set_visible(true);	
					this.fn_SetGridFileButton();//[공통] 그리드 파일 버튼 설정
				}
			}		
			
			//웹컴포넌트가 로드 완료되었을경우
			if (this.fvWebCompLoadYn) {
				//내용 Web컴포넌트 초기화
				var oWeb = this.div_MasterInput.form.tab_Master.tpg_01.form.web_ContNm.getProperty("window");
				oWeb.callMethod("fn_setData", "");	
			}		
			this.fn_DetailSearch();//상세조회
		}
	}
};

