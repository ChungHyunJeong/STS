	//조회조건 필수입력값 있으면 체크
	// Validation 설정
	var arrArgs = ["this.div_Search.form.cbo_ModmCd^" + this.div_Search.form.sta_ModmCd.text + "|NULL"
				  ]; 
				  
	var sCheckGroupTitle = "조회조건";//체크그룹 타이틀

	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
		return false;
	}		

	
	return true;
}

