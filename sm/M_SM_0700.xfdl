/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
			if (this.ds_Master10.getRowCount() > 0) {			

				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_MasterView.form.grd_Master10);			

			} else {
			
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_MasterView.form.grd_Master10);	

			}

			break;
		case "SAVE":   //저장		

			//첨부파일 추가건이 없으면
			if (!this.fvFileDiv.fn_CheckFileAddYn()) {
				this.fvSchKeyYn = true;//저장후 재조회여부
				this.fn_DetailSearch();//저장후 재조회
			} else {
				this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);			
			}
			break;	
			
		case "DTL_SEARCH": //조회		
			this.ds_Master11.addColumn("crudType", "string");		
			
			//저장후 재조회여부-true
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}		
			
			this.fvDtlSearchYn = true;//상세조회여부				
		
			if (this.ds_Master11.getRowCount() > 0) {
				//첨부파일 정보 조회
				var oFileInfo = new Object();//[공통] 첨부파일정보
				oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
				oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
				oFileInfo.PK01_NO = this.ds_Master11.getColumn(this.ds_Master11.rowposition, "COMM_CD");//PK번호1-회사코드
				oFileInfo.PK02_NO = this.ds_Master11.getColumn(this.ds_Master11.rowposition, "PRGM_ID");//PK번호2-회의번호
				
				this.fvFileDiv.fn_AtchFileSearch(oFileInfo);	
			
				//내용 Web컴포넌트에 값 세팅
				//웹컴포넌트가 로드 완료되었을경우				
				if (this.fvWebCompLoadYn) {
					this.setTimer(998,500);	
				}
				
				//팝업으로 호출시
				if (this.fvPopupYn) {
					//this.btn_DivMoveMi_onclick(this.btn_DivMoveMi);				
				}	
			}
			
			break;			
			
		case "DELETE": //삭제		
			this.fn_DetailSearch();//조회
			break;			
			
		case "COMBO_SEARCH": //모듈구분조회		

			this.fvFormLoadYn = true;//폼로드 완료여부
			if (this.ds_Modm.getRowCount() > 0) {
				this.div_Search.form.cbo_ModmCd.set_index(0);
				this.fn_ComBtnSearch();
			}
			
			break;								
				
	}
};

