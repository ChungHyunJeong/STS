this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
			this.ds_Master.addColumn("s_chk", "string");
			this.ds_Master.addColumn("crudType", "string");
			break;
		
		case "SAVE":   //저장		
			this.fn_ComBtnSearch();//조회
			break;	
		
		case "DELETE": //삭제		
			this.fn_ComBtnSearch();//조회
			break;							
	}
};

