this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회				
			//Tab위치 확인
			if (this.div_TabView.form.tab_Master.tabindex == '4') 
				this.ds_Master4.addColumn("crudType", "string");
			
			break;
		case "SAVE":   //저장		
			this.fn_ComBtnSearch();//조회
			break;	
		case "DELETE": //삭제		
			this.fn_ComBtnSearch();//조회
			break;							
	}
};

