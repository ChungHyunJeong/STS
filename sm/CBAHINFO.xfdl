{
	var mRow = this.ds_Master10.rowposition;
	this.fn_Choice(mRow);
}


/***********************************************************************************************
 * 13.02. [사용자함수] 선택한 정보 부모창 리턴 함수 
 ***********************************************************************************************/ 
this.fn_Choice = function(iRow) {
	if (iRow > -1) {	
        rtnValue = "({" +
                        ' COMM_CD:"' + this.ds_Master10.getColumn(iRow, "COMM_CD") + '"'+  //0.회사코드
                        ',CUSM_CD:"' + this.ds_Master10.getColumn(iRow, "CUSM_CD") + '"'+  //1.거래처코드
                        ',CUSM_NM:"' + this.ds_Master10.getColumn(iRow, "CUSM_NM") + '"'+  //2.거래처명(한글)
                        ',SAUM_NO:"' + this.ds_Master10.getColumn(iRow, "SAUM_NO") + '"'+  //3.사업자등록번호/주민등록번호
                        ',REPR_NM:"' + this.ds_Master10.getColumn(iRow, "REPR_NM") + '"'+  //4.대표자명
                        ',ACNO_GU:"' + this.ds_Master10.getColumn(iRow, "ACNO_GU") + '"'+  //5.계좌구분(공통)
                        ',ACNO_NM:"' + this.ds_Master10.getColumn(iRow, "ACNO_NM") + '"'+  //5.계좌구분명(공통)
                        ',BANM_CD:"' + this.ds_Master10.getColumn(iRow, "BANM_CD") + '"'+  //6.은행코드
                        ',BANM_NM:"' + this.ds_Master10.getColumn(iRow, "BANM_NM") + '"'+  //6.은행명
                        ',ACNM_NO:"' + this.ds_Master10.getColumn(iRow, "ACNM_NO").replace(/(\s*$)/, "") + '"'+  //7.계좌번호 => 데이터에 엔터값이 들어가서 에러가 난다, 그래서 공백제거함
                        ',ACEM_NM:"' + this.ds_Master10.getColumn(iRow, "ACEM_NM") + '"'+  //8.예금주
                        ',CBAB_YN:"' + this.ds_Master10.getColumn(iRow, "CBAB_YN") + '"'+  //9.대표계좌여부
                        ',REMK_NM:"' + this.ds_Master10.getColumn(iRow, "REMK_NM") + '"'+  //10.비고
                "})";
		trace(rtnValue)
 		this.gfn_PopClose(rtnValue);
	} else {
		this.gfn_Alert("SM046");//선택된 내용이 없습니다.
		return;		
	} 
}

