/*---------------------------------------------------------------------------------------------
 * 12.1.7. [사용자함수 - 서비스] 사용자목록 -> 그룹별관리자목록
  ---------------------------------------------------------------------------------------------*/
this.div_UserView_btn_UserIns_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if (this.ds_Userlist.rowposition > -1) { //User 그리드에서 새로운 Row가 존재하는 확인
		//var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail);//crud_flag 변경-입력(I)
		var iAddRow = this.ds_Detail.addRow();	// 한줄추가
		var iFromRow = this.ds_Userlist.rowposition; 	// 그룹별 관리자 목록 변경 Row

		this.ds_Detail.setColumn(iAddRow, "PGRM_CD", this.ds_Master.getColumn(this.ds_Master.rowposition, "PGRM_CD"));//권한그룹코드

		// 데이터셋 Copy 후 Delete
		this.ds_Detail.copyRow(iAddRow, this.ds_Userlist, iFromRow);	
		this.ds_Userlist.deleteRow(this.ds_Userlist.rowposition);		
	}
};

/*---------------------------------------------------------------------------------------------
 * 12.1.8. [사용자함수 - 서비스] 그룹별관리자목록 -> 사용자목록
  ---------------------------------------------------------------------------------------------*/
this.div_UserView_btn_UserDel_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if (this.ds_Detail.rowposition > -1) { //Detail 그리드에서 새로운 Row가 존재하는 확인
		var iAddRow = this.ds_Userlist.addRow();	// 한줄추가
		var iFromRow = this.ds_Detail.rowposition;	// 그룹별 관리자 목록 변경 Row

		// 데이터셋 Copy 후 Delete
		this.ds_Userlist.copyRow(iAddRow, this.ds_Detail, iFromRow);	
		this.ds_Detail.deleteRow(iFromRow);		
	}
	
};
