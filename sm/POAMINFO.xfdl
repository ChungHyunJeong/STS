this.fn_SelectRow = function(iRow)
{
	if (iRow > -1) {	
	
        var rtnValue = "({" +
                        ' POAM_CD:"' + this.ds_Master10.getColumn(iRow, "POAM_CD") + '"'+  //0.구역번호(우편번호)
                        ',SIDO_NM:"' + this.ds_Master10.getColumn(iRow, "SIDO_NM") + '"'+  //1.시도명
                        ',POST_ADDR1:"' + this.ds_Master10.getColumn(iRow, "POST_ADDR1") + '"'+  //2.신주소
                       ',POST_ADDR2:"' + this.ds_Master10.getColumn(iRow, "POST_ADDR2") + '"'+  //3.구주소
                "})";

					
		this.gfn_PopClose(rtnValue);
	} else {
		this.gfn_Alert("SM046");//선택된 내용이 없습니다.
		return;		
	}
}


