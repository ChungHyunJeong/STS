	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	//Master 입력 상태이면
	if (sChkRowType == "I") {
	
		//채번-회의번호
		var sMemtNo = this.gfn_GetTbnmNo(this.fvComPrgmId, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "METH_DD"));//회의번호, 기준일자
		if (this.gfn_IsNull(sMemtNo)) {
			//회의번호를 가져오는데 실패했습니다.
			return;
		}
		
		var sChkTemhNo = "";//출고번호-체크용
		var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position
		
		//Master 데이터셋에 채번정보 입력-회의번호
		this.ds_Master10.setColumn(iMasterRowPos, "METH_NO", sMemtNo);
		
		var sCommCd = this.ds_Master10.getColumn(iMasterRowPos, "COMM_CD");//회사코드
		var sMemtNo = this.ds_Master10.getColumn(iMasterRowPos, "METH_NO");//회의번호
		var sWofhSq = this.ds_Master10.getColumn(iMasterRowPos, "WOFH_SQ");//결재차수

		//Detail 데이터셋에 기본정보 입력-회사코드/회의번호/결재차수
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			sChkMetcSe = this.ds_Detail10.getColumn(iLoop, "METC_SE");//순번
			
