//  	if (this.fvChkMultiWfYn) return;
 
	//멀티선택여부 //20221013
		if (this.fvChkMultiWfYn) {
			this.fvChkMultiWfYn = false;
			
			if (iCheckBox == 0) {
				this.ds_Master10.reset();
			}
// 			
			return;
		}

	 
	//데이터 변경여부 체크 (20221013)
	if ( (this.gfn_DsIsUpdated(this.ds_Master10) || this.gfn_DsIsUpdated(this.ds_Detail20)) && iCheckBox == 0 ) {
		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	
	
