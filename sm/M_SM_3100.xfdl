this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)

	//부서
	if (e.columnid == "DEPM_CD") {
		this.gfn_GetCommDepmGrid(obj, "DEPM_CD", "DEPM_NM");//부서명 검색
	//발언자-성명
	} else if (e.columnid == "SPEM_NM") {
		if (obj.getColumn(this.ds_Detail10.rowposition, "SPIN_YN") == "0") {
			var sSPMT_GU = obj.getColumn(this.ds_Detail10.rowposition, "SPMT_GU");
			
			if (this.gfn_IsNull(sSPMT_GU)) {
				this.gfn_Alert("SM092");//구분을 선택하세요.
				return;
			}
			
			//구분이 사내직원이면
			if(sSPMT_GU=="0010"){
				this.gfn_GetCommEmpmCspmGrid(obj,"I", "SPEM_NM", "SPEM_ID", "SPCO_NM", "SPDP_NM","", null, "B_SYST_ID_11", "fn_PopupCallback");//사원명 검색
			} else {
				this.gfn_GetCommEmpmCspmGrid(obj,"O", "SPEM_NM", "SPEM_ID", "SPCO_NM", "SPDP_NM","", null, "B_SYST_ID_12", "fn_PopupCallback");//사원명 검색		
			}
		}
	//집행자-성명
	} else if (e.columnid == "EXEM_NM") {
		if (obj.getColumn(this.ds_Detail10.rowposition, "EXIN_YN") == "0") {
			var sEXMT_GU = obj.getColumn(this.ds_Detail10.rowposition, "EXMT_GU");
			
			if (this.gfn_IsNull(sEXMT_GU)) {
				this.gfn_Alert("SM092");//구분을 선택하세요.
				return;
			}
			
			//구분이 사내직원이면
			if(sEXMT_GU=="0010"){
				this.gfn_GetCommEmpmCspmGrid(obj,"I", "EXEM_NM", "EXEM_ID", "EXCO_NM", "EXDP_NM","", null, "B_SYST_ID_13", "fn_PopupCallback");//사원명 검색
			} else {
				this.gfn_GetCommEmpmCspmGrid(obj,"O", "EXEM_NM", "EXEM_ID", "EXCO_NM", "EXDP_NM","", null, "B_SYST_ID_14", "fn_PopupCallback");//사원명 검색		
			}		
		}		
	//발언자구분
	} else if (e.columnid == "SPMT_GU") {
	
		//발언자 구분이 사내직원이면
		if(obj.getColumn(this.ds_Detail10.rowposition, "SPMT_GU")=="0010"){
			obj.setColumn(this.ds_Detail10.rowposition,"SPIN_YN","0");
		}
	//발언자 직접입력 여부
	} else if (e.columnid == "SPIN_YN") {
		obj.setColumn(this.ds_Detail10.rowposition,"SPCO_NM","");
		obj.setColumn(this.ds_Detail10.rowposition,"SPEM_ID","");
		obj.setColumn(this.ds_Detail10.rowposition,"SPEM_NM","");
		obj.setColumn(this.ds_Detail10.rowposition,"SPDP_NM","");	
	//집행자구분
	} else if (e.columnid == "EXMT_GU") {
	
		//집행자 구분이 사내직원이면
		if(obj.getColumn(this.ds_Detail10.rowposition, "EXMT_GU")=="0010"){
			obj.setColumn(this.ds_Detail10.rowposition,"EXIN_YN","0");
		}
	//집행자 직접입력 여부
	} else if (e.columnid == "EXIN_YN") {
		obj.setColumn(this.ds_Detail10.rowposition,"EXCO_NM","");
		obj.setColumn(this.ds_Detail10.rowposition,"EXEM_ID","");
		obj.setColumn(this.ds_Detail10.rowposition,"EXEM_NM","");
		obj.setColumn(this.ds_Detail10.rowposition,"EXDP_NM","");			
	}	
};

