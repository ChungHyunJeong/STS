/***********************************************************************************************
 * 15.08. 메일전송데이터 생성
 ***********************************************************************************************/ 
this.fn_MakeMailInfo = function(bAddFileYn) {
	
	var iMstRowPos = this.ds_Master10.rowposition;
	var objMail = new Object();//메일정보
	var arrMailUserList = new Array();//메일 발/수신자 정보 리스트
	var objMailUser = new Object();//메일 발/수신자 정보
	var sSendId = "";//발송자ID
	var sRecvId = "";//수신자ID
	var sInUserList = "";//사내사용자 List
	var sOustUserList = "";//고객사용자 List
	var sMETC_GU = "";//회의참석자구분
	var arrMailAtchFileList = new Array();//[부모창파라메타] 첨부파일 정보
	var objMailAtchFile = new Object();//첨부파일정보
	
	sSendId = this.gfn_GetSessionValue("SYST_ID");//발송자ID
	
	//발신자 & 수신자이메일정보 조회
	sInUserList = "'" + sSendId + "'";
	
	if (this.ds_Detail20.getRowCount() > 0) {
	
		for (var iLoop=0; iLoop < this.ds_Detail20.getRowCount(); iLoop++) {
			sRecvId = this.ds_Detail20.getColumn(iLoop, "SYST_ID");//수신자ID
			
			//발신자ID와 수신자ID가 다를경우
			if (sSendId != sRecvId) {

				if (!this.gfn_IsNull(sRecvId)) {
					sMETC_GU = this.ds_Detail20.getColumn(iLoop, "METC_GU");//회의참석자구분(공통코드:SM306 참조-0010:사내,0020:파트너사,0030:고객사)
					if (sMETC_GU == "0010") {
						sInUserList += ",'" + sRecvId + "'";
					} else {
						if (this.gfn_IsNull(sOustUserList)) {
							sOustUserList = "'" + sRecvId + "'";
						} else {
							sOustUserList += ",'" + sRecvId + "'";
						}
					}
				}
			} 
		}
	}	
	
	nApp.gds_Input.clearData();	
	nApp.gds_EmpmMail.clearData();	
	nApp.gds_CspmMail.clearData();	
	
	nApp.gds_Input.addRow();
	nApp.gds_Input.setColumn(0, "G_COMM", this.ds_Master10.getColumn(iMstRowPos, "COMM_CD"));//회사코드
	nApp.gds_Input.setColumn(0, "SYST_ID", sInUserList);
	nApp.gds_Input.setColumn(0, "CSPM_CD", sOustUserList);
	
	var sOutDataset = "gds_EmpmMail=gds_EmpmMail";
	
