/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {		
		trace("멀티선택여부->"+this.fvChkMultiWfYn)
		//멀티선택여부
		if (this.fvChkMultiWfYn) return;
		
		//[결재] 결재상태에 따른 컴포넌트 제어
		this.fn_SetWfCompEnable();		
		
		//회원No정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "METH_NO"))) {
		
// 			//[결재] 결재상태에 따른 컴포넌트 제어
// 			this.fn_SetWfCompEnable();		
// 					

			//if (this.fvLoginSystId == "C029059" || this.fvLoginSystId == "I090003" || this.fvLoginSystId == "I100045" || this.fvLoginSystId == "I100171") {
				this.btn_SendMail.set_visible(true);//메일발송 버튼
			//}
			
			//회의유형이 Kick-off 이면 상세보기 버튼 보이게
			if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MTTY_CD") == "0030") {
				this.btn_DetailProj.set_visible(true);//상세보기 버튼
				this.btn_DetailSpec.set_visible(true);//상세보기 버튼
				
				//계약번호가 있으면
				if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO"))) {
					this.btn_DetailSpec.set_enable(true);
				} else {
					this.btn_DetailSpec.set_enable(false);
				}
				
				//프로젝트번호가 있으면
				if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PJTH_NO"))) {
					this.btn_DetailProj.set_enable(true);
				} else {
					this.btn_DetailProj.set_enable(false);
				}		
				//if (this.fvLoginSystId == "C029059" || this.fvLoginSystId == "I090003" || this.fvLoginSystId == "I100045" || this.fvLoginSystId == "I100171") {
					this.btn_SendMail.move(null,90,80,24,224,null);//메일발송 버튼
				//}
			} else {
				this.btn_DetailProj.set_visible(false);//상세보기 버튼
				this.btn_DetailSpec.set_visible(false);//상세보기 버튼
				//if (this.fvLoginSystId == "C029059" || this.fvLoginSystId == "I090003" || this.fvLoginSystId == "I100045" || this.fvLoginSystId == "I100171") {
					this.btn_SendMail.move(null,90,80,24,14,null);//메일발송 버튼
				//}
			}
			
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정	
			this.ds_Detail10.clearData();
			this.ds_Detail20.clearData();
			this.ds_Input.clearData();
			
			//내용 Web컴포넌트에 값 세팅
			//웹컴포넌트가 로드 완료되었을경우				
			
			if (this.fvWebCompLoadYn) {
				//var oWeb = this.div_DetailInput.form.tab_Detail.tpg_02.form.web_ContNm.getProperty("window");
				//var sContNm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CONT_NM");
				//oWeb.callMethod("fn_setData", this.gfn_ReplaceWebEditorData(sContNm));	
				this.setTimer(998,500);	
			}
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "METH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "METH_NO"));
			this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));

			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "sm:M_SM_3100_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);			
			
			//[공통]서비스정보 추가
			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "sm:M_SM_3100_D_SL20";
			this.gfn_AddComSearchService(this, svcAddObj);
		
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_SM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10 ds_Detail20=ds_Detail20";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);		
		}
	} else {
		this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);
		this.div_DetailInput.form.tab_Detail.set_enable(false);
		this.btn_DetailProj.set_visible(false);//상세보기 버튼
		this.btn_DetailSpec.set_visible(false);//상세보기 버튼
		this.btn_SendMail.set_visible(false);//메일발송 버튼
	}
};

