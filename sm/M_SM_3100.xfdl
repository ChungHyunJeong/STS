this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
			if (this.ds_Master10.getRowCount() > 0) {			
				
				this.ds_Master10.addColumn("s_chk", "string"); //(20221013)				
				
				this.fvDtlSearchYn = true;//상세조회여부
				
				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master10.findRowExpr("METH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ=='" + this.fvSchKey02 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 					
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(-1);
						this.ds_Master10.set_rowposition(0);
					}
					
					//웹컴포넌트가 로드 완료되었을경우					
					
					if (this.fvWebCompLoadYn) {					
						//내용 Web컴포넌트에 값 세팅
						//var oWeb = this.div_DetailInput.form.tab_Detail.tpg_02.form.web_ContNm.getProperty("window");
						//var sContNm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CONT_NM");
						//oWeb.callMethod("fn_setData", this.gfn_ReplaceWebEditorData(sContNm));	
						this.setTimer(998,500);
					}	
									
				}					
				
				
// 				if(this.fvPopupYn){						
// 					var oWeb = this.div_DetailInput.form.tab_Detail.tpg_02.form.web_ContNm.getProperty("window");
// 					var sContNm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CONT_NM");
// 					oWeb.callMethod("fn_setData", this.gfn_ReplaceWebEditorData(sContNm));						
// 				}

				//웹컴포넌트가 로드완료 및 결재함에서 호출한경우
				if (this.fvWebCompLoadYn && this.fvWfSearchMode) {					
					//내용 Web컴포넌트에 값 세팅
					//var oWeb = this.div_DetailInput.form.tab_Detail.tpg_02.form.web_ContNm.getProperty("window");
					//var sContNm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CONT_NM");
					//oWeb.callMethod("fn_setData", this.gfn_ReplaceWebEditorData(sContNm));	
					this.setTimer(998,500);
				}
								
				this.div_MasterInput.set_enable(true);
				this.div_DetailInput.form.tab_Detail.set_enable(true);
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
				this.fvWofhStatus = this.gfn_SetCommButtonWf(this, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"), false);//결재진행상태

				if (this.fvWofhStatus) {
					this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);
				} else {
