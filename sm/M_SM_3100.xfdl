 * 15.09. 메일정보 찾기
 ***********************************************************************************************/ 
this.fn_GetMailUser = function(sDst, oDs, sColumn, sId)
{
	var iRow = 0;
	var sRtnValue = "";
	
	if (sDst == "OUT") {
		iRow = oDs.findRowAs("CSPM_CD", sId);
		if (iRow > -1) {
			sRtnValue = oDs.getColumn(iRow, sColumn);
		}
	} else {
		iRow = oDs.findRowAs("SYST_ID", sId);
		if (iRow > -1) {
			sRtnValue = oDs.getColumn(iRow, sColumn);
		}	
	}
	
	return sRtnValue;
}

