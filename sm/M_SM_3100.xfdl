this.fn_CheckMgas = function(pObj)
{ 	
	//마감여부 체크
	var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
	var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
	paramObj.MGAS_MM = pObj.MGAS_MM;//마감년월
	paramObj.MODM_CD = pObj.MODM_CD;//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : 해당업무 마감여부
	paramObj.ACUM_CD = pObj.ACUM_CD;//[선택] 회계단위
	paramObj.BUSM_CD  = pObj.BUSM_CD;//[선택] 사업부문
	paramObj.SAUM_CD  = pObj.SAUM_CD;//[선택] 사업장코드
	paramObj.DEPM_CD  = pObj.DEPM_CD;//[선택] 부서코드
	paramObj.MODD_CD  = pObj.MODD_CD;//[선택] 모듈상세구분(CA900)
	paramObj.MGAS_DD  = pObj.MGAS_DD;//[선택] 근태마감일자
	paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
	
	var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);

	//마감여부 - true인경우
	if (bCheck) {
		var sMgasNm = pObj.MGAS_MM.substring(0,4) + "-" + pObj.MGAS_MM.substring(4,6)
