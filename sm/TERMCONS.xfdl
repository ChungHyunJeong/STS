    <Script type="xscript5.1"><![CDATA[/**
*  @화면ID   : 
*  @화면설명 : 사용약관 동의
*  @시스템명 : (SM) 시스템
*  @작성자   : 안병호
*  @작성일   : 2022.06.15
*  @Desction    
******************* 소스 수정 이력 ************************************************************
*  일자          		수정자                비고
*************************************************************************************************
*  2022.06.15    	    안병호 	           최초 생성 
*************************************************************************************************
*/

/***********************************************************************************************
 * 0. 폼로드
 ***********************************************************************************************/
/***********************************************************************************************
 * 00.01. Form Include 선언
 ***********************************************************************************************/

/************************************************************************************************
 * 2. Form 변수 선언 영역
 ************************************************************************************************/
this.fvComPrgmId = "";//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
this.fvCusmCd  = "";//[부모창파라메타] 파트너사 코드
this.fvCusmNm  = "";//[부모창파라메타] 파트너사 명
this.fvTermYn  = "";//[부모창파라메타] 사용자동의 여부
	
/***********************************************************************************************
 * 00.02. Form 변수 선언
 ***********************************************************************************************/
/***********************************************************************************************
 * 00.03. Form Onload 이벤트
 ***********************************************************************************************/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{	
	this.fvComPrgmId = this.getOwnerFrame().PrgmId;//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
	if(!this.gfn_IsNull(this.fvComPrgmId)){
		//부모창 파라메타 처리
		
		this.fvCusmCd = this.getOwnerFrame().sCusmCd;//[부모창파라메타] 파트너사 코드
		this.fvCusmNm = this.getOwnerFrame().sCusmNm;//[부모창파라메타] 파트너사 명
		this.fvTermYn = this.getOwnerFrame().sTermYn;//[부모창파라메타] 사용자동의 여부
		
	}else{
	
		//this.fvCusmCd = this.gfn_GetSessionValue("CUSM_CD");//파트너사 코드
		//this.fvCusmNm = this.gfn_GetSessionValue("CUSM_NM");//파트너사 명
		//파트너사로 로그인하면 
		//if (this.gfn_GetSessionValue("CMDS_CD") != "P") {}
		
		this.div_Top1.form.chk_CmahYn1.set_value(1);
		this.div_Top2.form.chk_CmahYn2.set_value(1);
		this.div_Top3.form.chk_CmahYn3.set_value(1);
		this.div_Top4.form.chk_CmahYn4.set_value(1);
		
		this.div_Top1.form.chk_CmahYn1.set_readonly(true);
		this.div_Top2.form.chk_CmahYn2.set_readonly(true);
		this.div_Top3.form.chk_CmahYn3.set_readonly(true);
		this.div_Top4.form.chk_CmahYn4.set_readonly(true);
		
