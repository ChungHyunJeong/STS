this.fn_FormInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	this.gfn_SetGrid(this.grd_Master00);//[공통]그리드 이벤트 설정
	
	this.ds_Input.addRow();
	this.div_Search.form.edt_PrgmCd.set_value(this.paramPrgmId);
	this.div_Search.form.edt_PrgmNm.set_value(this.paramPrgmNm);
	
	this.ds_Input.setColumn(0,"PRGM_ID",this.paramPrgmId);
	this.ds_Input.setColumn(0,"FROM_DD",this.paramDateFr);
	this.ds_Input.setColumn(0,"CLOS_DD",this.paramDateTo);
	
	this.ds_Input.setColumn(0,"DEPM_CD",this.paramDepmCd);
	this.ds_Input.setColumn(0,"MODM_CD",this.paramModmCd);
	this.ds_Input.setColumn(0,"SYST_ID",this.paramSystId);
	
	//this.ds_ParamData.loadXML(this.paramDataset);
	
	this.div_PopupTitle_btn_Search_onclick();//조회
}

