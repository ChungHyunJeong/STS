this.fn_FormOnsize = function(obj:nexacro.Form,e:nexacro.SizeEventInfo)
{ 

	if (obj.width > 1400) {
		this.grd_Master10.set_autofittype("col");
	} else {
		this.grd_Master10.set_autofittype("none");
	}

};
 
/***********************************************************************************************
 * 1. 조회 영역
 ***********************************************************************************************/
/***********************************************************************************************
 * 01.01. [팝업버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.div_PopupTitle_btn_Search_onclick = function()
{ 
	var searchText="";
	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;	
	
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Input.clearData();
	this.ds_Master10.clearData();
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();

	this.ds_Input.setColumn(0, "GRCM_CD", this.div_Search.form.cbo_GrcmCd.value);    //그룹코드
	//this.ds_Input.setColumn(0, "CLSM_CD", "SM");           
	if(this.gfn_IsNull(this.div_Search.form.edt_CusmCd.value)){
		searchText = "%";
	}else{
		searchText = this.div_Search.form.edt_CusmCd.value;
	}
	
	this.ds_Input.setColumn(0, "SEAECH_TEXT", searchText );//코드

	//사용여부-사용
	if (this.div_Search.form.cbo_UsedYn.value == "1") {
		this.ds_Input.setColumn(0, "USED_YN", "1");//사용여부
	} else if (this.div_Search.form.cbo_UsedYn.value == "0") {
		this.ds_Input.setColumn(0, "USED_YN", "0");//사용여부
	} else {
		this.ds_Input.setColumn(0, "USED_YN", "");//사용여부
	}
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql =  "sm:M_SM_CODMINFO_SL20";//"sm:M_SM_0210_SL01";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid 		= "MST_SEARCH";			//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName 	= this.G_SV_SD;			//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset 	= "ds_Input=ds_Input" 	//입력 데이터셋
	svcCallObj.sOutDatset 	= "ds_Master10=ds_Detail10";//출력 데이터셋
	svcCallObj.sOtherArg 	= "";					//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";		//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
	
	//가변 컬럼 세팅
	var arrColMaster = ["OP01_NM", "OP02_NM", "OP03_NM", "OP04_NM", "OP05_NM", "OP06_NM", "OP07_NM", "OP08_NM", "OP09_NM", "OP10_NM"];
	var arrColDetail = ["OP01_NM", "OP02_NM", "OP03_NM", "OP04_NM", "OP05_NM", "OP06_NM", "OP07_NM", "OP08_NM", "OP09_NM", "OP10_NM"];
	var sText  = "";
	var nIdxBody=0;
	var grdObj =this.grd_Master10;
	for (var idx=0; idx<arrColMaster.length; idx++) 
	{
		sText = this.ds_Grcm.getColumn(this.ds_Grcm.rowposition, arrColMaster[idx]);
		nIdxBody = grdObj.getBindCellIndex("Body",arrColDetail[idx]);
		
		if (this.gfn_IsNull(sText)) 
		{
			grdObj.setRealColSize("body",nIdxBody, 0, false );
		}  
		else 
		{
			grdObj.setCellProperty("head", nIdxBody, "text", sText);
