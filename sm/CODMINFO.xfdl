 ***********************************************************************************************/ 
this.fn_Choice = function(iRow) {
	if (iRow > -1) {	
	
        rtnValue = "({" +
                        ' COMM_CD:"' + this.ds_Master10.getColumn(iRow, "COMM_CD") + '"'+  //0.회사코드
                        ',CLSM_CD:"' + this.ds_Master10.getColumn(iRow, "CLSM_CD") + '"'+  //1.업무분류코드
                        ',GRCM_CD:"' + this.ds_Master10.getColumn(iRow, "GRCM_CD") + '"'+  //2.그룹코드
                        ',GRCM_NM:"' + this.ds_Master10.getColumn(iRow, "GRCM_NM") + '"'+  //3.그룹명
                        ',CODM_CD:"' + this.ds_Master10.getColumn(iRow, "CODM_CD") + '"'+  //4.공통코드
                        ',CODM_NM:"' + this.ds_Master10.getColumn(iRow, "CODM_NM") + '"'+  //5.공통명
                        ',OP01_NM:"' + this.ds_Master10.getColumn(iRow, "OP01_NM") + '"'+  //6.옵션1
                        ',OP02_NM:"' + this.ds_Master10.getColumn(iRow, "OP02_NM") + '"'+  //7.옵션2
                        ',OP03_NM:"' + this.ds_Master10.getColumn(iRow, "OP03_NM") + '"'+  //8.옵션3
                        ',OP04_NM:"' + this.ds_Master10.getColumn(iRow, "OP04_NM") + '"'+  //9.옵션4
                        ',OP05_NM:"' + this.ds_Master10.getColumn(iRow, "OP05_NM") + '"'+  //10.옵션5
                        ',OP06_NM:"' + this.ds_Master10.getColumn(iRow, "OP06_NM") + '"'+  //11.옵션6
                        ',OP07_NM:"' + this.ds_Master10.getColumn(iRow, "OP07_NM") + '"'+  //12.옵션7
                        ',OP08_NM:"' + this.ds_Master10.getColumn(iRow, "OP08_NM") + '"'+  //13.옵션8
                        ',OP09_NM:"' + this.ds_Master10.getColumn(iRow, "OP09_NM") + '"'+  //14.옵션9
                        ',OP10_NM:"' + this.ds_Master10.getColumn(iRow, "OP10_NM") + '"'+  //15.옵션10
                        ',SORT_NO:"' + this.ds_Master10.getColumn(iRow, "SORT_NO") + '"'+  //16.정렬순서
                        ',USED_YN:"' + this.ds_Master10.getColumn(iRow, "USED_YN") + '"'+  //17.사용여부
                        ',REMK_NM:"' + this.ds_Master10.getColumn(iRow, "REMK_NM") + '"'+  //18.비고
                        ',REGI_NM:"' + this.ds_Master10.getColumn(iRow, "REGI_NM") + '"'+  //19.등록자명
                        ',REGI_DT:"' + this.ds_Master10.getColumn(iRow, "REGI_DT") + '"'+  //20.등록일시
                        ',MODI_NM:"' + this.ds_Master10.getColumn(iRow, "MODI_NM") + '"'+  //21.수정자명
                        ',MODI_DT:"' + this.ds_Master10.getColumn(iRow, "MODI_DT") + '"'+  //22.수정일시
                "})";


					
		this.gfn_PopClose(rtnValue);
	} else {
		this.gfn_Alert("SM046");//선택된 내용이 없습니다.
