/***********************************************************************************************
 * 14. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 14.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회	
			//조회시 focus 이동
			this.grd_Master10.setFocus(0);	
			break;			
		case "COMBO_SEARCH":
			//그룹코드콤보 설정
			trace("fn_Callback fvGrcmCd = " +  this.fvGrcmCd);
			this.div_Search.form.cbo_GrcmCd.set_value(this.fvGrcmCd);
			
			
			
			this.div_PopupTitle_btn_Search_onclick();//자동조회
			break;
	}
};


