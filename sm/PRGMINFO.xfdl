this.fn_Search = function()
{ 

	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "sm:M_SM_PRGM_SL00";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid 		= "MST_SEARCH";			//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName 	= this.G_SV_HR;			//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset 	= "ds_Input=ds_Input" 	//입력 데이터셋
	svcCallObj.sOutDatset 	= "ds_Master=ds_Master";//출력 데이터셋
	svcCallObj.sOtherArg 	= "";					//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";		//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
} 


/***********************************************************************************************
 * 10.2. [사용자함수] 선택한 행 리턴
 ***********************************************************************************************/ 
this.fn_SelectRow = function(iRow)
{
	//프로그램명이 없을경우 선택불가
	if(this.ds_Master.getColumn(iRow, "PRGM_ID") =="*") return;
	
	if (iRow > -1) {	
		/*
		부모창으로 리턴값은 문자열로만 가능
		(문자열) 리턴값을 "JSONText" 사용해서 리턴 : 부모창에서 
		var arrRtnVal = eval(rtnValue);		
		arrRtnVal.SYST_ID
		*/
		
		 rtnValue = "({" +
                        ' DEPTH_SORT:"' + this.ds_Master.getColumn(iRow, "DEPTH_SORT") + '"'+  //0.DEPTH_SORT
                        ',TCLS_CD:"' + this.ds_Master.getColumn(iRow, "TCLS_CD") + '"'+  //1.상위분류코드
                        ',TREE_CD:"' + this.ds_Master.getColumn(iRow, "TREE_CD") + '"'+  //2.TREE_CD
                        ',TREE_NM:"' + this.ds_Master.getColumn(iRow, "TREE_NM") + '"'+  //3.TREE_NM
                        ',TREE_LV:"' + this.ds_Master.getColumn(iRow, "TREE_LV") + '"'+  //4.TREE_LV
                        ',MODM_CD:"' + this.ds_Master.getColumn(iRow, "MODM_CD") + '"'+  //5.모듈코드
                        ',CLSM_YN:"' + this.ds_Master.getColumn(iRow, "CLSM_YN") + '"'+  //6.CLSM_YN
                        ',PRGM_ID:"' + this.ds_Master.getColumn(iRow, "PRGM_ID") + '"'+  //7.프로그램ID
                        ',DISP_ID:"' + this.ds_Master.getColumn(iRow, "DISP_ID") + '"'+  //8.화면ID
                        ',PRGL_NM:"' + this.ds_Master.getColumn(iRow, "PRGL_NM") + '"'+  //9.프로그램언어명
                        ',CLSM_CD:"' + this.ds_Master.getColumn(iRow, "CLSM_CD") + '"'+  //10.업무분류코드
                        ',CLLM_NM:"' + this.ds_Master.getColumn(iRow, "CLLM_NM") + '"'+  //11.업무분류언어명
                        ',CALL_PA:"' + this.ds_Master.getColumn(iRow, "CALL_PA") + '"'+  //12.호출파라미터
                        ',SEAR_NM:"' + this.ds_Master.getColumn(iRow, "SEAR_NM") + '"'+  //13.검색명
                        ',PRGM_VE:"' + this.ds_Master.getColumn(iRow, "PRGM_VE") + '"'+  //14.프로그램버전
                        ',SECU_CD:"' + this.ds_Master.getColumn(iRow, "SECU_CD") + '"'+  //15.보안등급(SM105)
                        ',PRGM_YN:"' + this.ds_Master.getColumn(iRow, "PRGM_YN") + '"'+  //16.프로그램여부
                        ',MENU_YN:"' + this.ds_Master.getColumn(iRow, "MENU_YN") + '"'+  //17.메뉴표기여부
