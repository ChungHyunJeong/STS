this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
			if (this.ds_Depm.getRowCount() > 0) {
				this.grd_Master.setTreeStatus(0,true);
			}
			
			//부모창에서 전달 받은 데이터가 있으면 상세조회
			if (!this.gfn_IsNull(this.fvDepmNm)) {
				this.div_PopupTitle_btn_Search_onclick();
			}
			break;
		case "DTL_SEARCH": //상세조회
		    break;
	}
};

