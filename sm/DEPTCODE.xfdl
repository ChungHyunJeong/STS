this.fvComPrgmId = "";//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
this.fvAuthYn = "";//[부모창파라메타] 권한체크여부
this.fvDepmDdViewYn = "";//[부모창파라메타] 기준일자 View여부
this.fvDepmNm = "";//[부모창파라메타] 부서명
this.fvDepmDd = "";//[부모창파라메타] 기준일자

