		this.fn_SelectRow(this.ds_DepmDtl, e.row);
	}
};

/***********************************************************************************************
 * 6.3. [Tree-그리드] 클릭 이벤트
 ***********************************************************************************************/ 
this.grd_Master_oncellclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	var iRow = this.ds_Depm.rowposition;
	var sDepmCd = this.ds_Depm.getColumn(iRow, "DEPM_CD");
	this.ds_DepmDtl.filter("");
	this.ds_DepmDtl.clearData();
	this.ds_DepmDtl.copyData(this.ds_Depm);
	this.ds_DepmDtl.filter("TDEP_CD == '" + sDepmCd + "'");
	this.ds_DepmDtl.insertRow(0);
	this.ds_DepmDtl.copyRow(0, this.ds_Depm, iRow);
};

/***********************************************************************************************
 * 6.4. 선택된 내역 부모창으로 리턴
 ***********************************************************************************************/ 
this.fn_SelectRow = function(objDs, iRow)
{
   if (objDs.getRowCount() < 1) {
      this.gfn_Alert("SM046");//선택된 내용이 없습니다.
      return;
   }
   
   if (iRow > -1) {   
      var sDEPM_CD = objDs.getColumn(iRow, "DEPM_CD"); //조직코드
       var sDEPM_NM = objDs.getColumn(iRow, "DEPM_NM"); //조직명
      var sBFDP_NM = objDs.getColumn(iRow, "BFDP_NM"); //조직명(변경전)
      var sDEPS_NM = objDs.getColumn(iRow, "DEPS_NM"); //조직명(약어)
      var sDEPE_NM = objDs.getColumn(iRow, "DEPE_NM"); //조직명(영어)
      var sDEPM_GU = objDs.getColumn(iRow, "DEPM_GU"); //조직구분(HR110)      
      var sAPPO_CD = objDs.getColumn(iRow, "APPO_CD"); //조직변경구분(HR115)
      var sDEPM_LV = objDs.getColumn(iRow, "DEPM_LV"); //조직레벨
      var sTDEP_CD = objDs.getColumn(iRow, "TDEP_CD"); //상위조직코드
      var sACUM_CD = objDs.getColumn(iRow, "ACUM_CD"); //회계단위
      var sSAUM_CD = objDs.getColumn(iRow, "SAUM_CD"); //사업장코드
      var sPALM_CD = objDs.getColumn(iRow, "PALM_CD"); //손익코드
      var sTMSU_ID = objDs.getColumn(iRow, "TMSU_ID"); //조직장사번
   
      //권한체크인경우
      if (this.fvAuthYn == "Y") {
         //조회권한정보체크
         var authObj = this.gfn_CheckDepmAuth(this.fvComPrgmId);
         var sChkSERA_CD = authObj.SERA_CD;//조회구분
         var sChkDEPM_CD = authObj.DEPM_CD;//부서코드
         var sChkTMDE_CD = authObj.TMDE_CD;//부서팀코드
         
         var sDEPM_PH = objDs.getColumn(iRow, "DEPM_PH");//부서경로
         //trace("sChkSERA_CD --> " + sChkSERA_CD);
         //trace("sDEPM_PH --> " + sDEPM_PH);
         
         //조회구분에 따른 부서코드 검색 제한         
         //조회구분-본부, 사업부
         if (sChkSERA_CD == "M" || sChkSERA_CD == "B") {
             
            var sDepmMa = this.gfn_GetSessionValue("DEPM_MA");//겸직부서정보
            
            

            //겸직부서 정보가 있으면
            if (sDepmMa.indexOf(",") > 0) {
            
