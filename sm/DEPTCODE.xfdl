 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 * 3.1. Form Onload 이벤트
  ---------------------------------------------------------------------------------------------*/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{	
	//부모창 파라메타 처리
	this.fvDepmDdViewYn = this.getOwnerFrame().DepmDdViewYn;//[부모창파라메타] 기준일자 View여부
	this.fvDepmNm = this.getOwnerFrame().DepmNm;//[부모창파라메타] 부서명	
	this.fvComPrgmId = this.getOwnerFrame().PrgmId;//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
	this.fvAuthYn = this.getOwnerFrame().AuthYn;//[부모창파라메타] 권한체크여부
	this.fvDepmDd = this.getOwnerFrame().DepmDd;//[부모창파라메타] 기준일자

	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 
}

/*---------------------------------------------------------------------------------------------
 * 3.2. Form [공통] 초기설정 함수 - 필수
  ---------------------------------------------------------------------------------------------*/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	this.gfn_SetGrid(this.grd_Master);//[공통]그리드 이벤트 설정
	this.gfn_SetGrid(this.grd_Detail);//[공통]그리드 이벤트 설정
	

	
	
}

/*---------------------------------------------------------------------------------------------
 * 3.3. Form [업무] 초기설정 함수
  ---------------------------------------------------------------------------------------------*/
this.fn_FormBizInit = function() 
{
	//[공통] 날짜설정
	var sNowDay = this.gfn_GetCommDate("toDay");//현재일자		
	this.div_Search.form.cal_DepmDd.set_value(sNowDay);
	
	if (this.fvDepmDdViewYn == "Y") {
		this.div_Search.form.sta_DepmDd.set_visible(true);
		this.div_Search.form.cal_DepmDd.set_visible(true);
		this.div_Search.form.cal_DepmDd.set_value(this.fvDepmDd);
	}
	
	//부모창에서 전달 받은 파라메터 세팅
	if (!this.gfn_IsNull(this.fvDepmNm)) {
		this.div_Search.form.edt_DepmNm.set_value(this.fvDepmNm);
	}
	
	var objCondList = ["FA100|ds_Fa100"];
	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, "");
	
	
	/*
	  일시적으로 기준년월 은
	      재무팀, IT사업부만 보이게 함
	*/
	var findRow = this.ds_Fa100.findRowExpr("CODM_CD == '"+this.gfn_GetSessionValue("DEPM_CD")+"'");
	trace("findRow == " + findRow + "    " +  this.gfn_GetSessionValue("DEPM_CD"));
	
	if(findRow > -1){
		this.div_Search.form.sta_DepmDd.set_visible(true);
		this.div_Search.form.cal_DepmDd.set_visible(true);
	}else{
		this.div_Search.form.sta_DepmDd.set_visible(false);
		this.div_Search.form.cal_DepmDd.set_visible(false);
	}
	
	//조직조회
	this.fn_Search();
}

