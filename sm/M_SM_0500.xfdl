 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 * 3.1. Form Onload 이벤트
  ---------------------------------------------------------------------------------------------*/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{	
	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 
}

/*---------------------------------------------------------------------------------------------
 * 3.2. Form [공통] 초기설정 함수 - 필수
  ---------------------------------------------------------------------------------------------*/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	this.gfn_SetGrid(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master);//[공통]그리드 이벤트 설정
		
	//권한체크 할 컴포넌트 : 컴포넌트 Full Path
	var objCompCheckList = ["this.div_MasterInput.form.div_ComGrdBtn"];		
	
	if (this.gfn_CommCompAuthCheck(this, objCompCheckList))
	{
		//상세 그리드버튼 설정 : 버튼ID|권한(Y/N)|호출함수명
		var objBtnInfoList = ["COPY|N|fn_MasterGrdCopy"
							 ,"ADD|Y|fn_MasterGrdAddRow"
							 ,"INSERT|N|fn_MasterGrdInsertRow"
							 ,"DELETE|Y|fn_MasterGrdDeleteRow"
							 ,"CANCEL|Y|fn_MasterGrdCancel"];
		this.div_MasterInput.form.div_ComGrdBtn.form.fn_SetGrdButtonFunc(this, objBtnInfoList);
		
	}

}

/*---------------------------------------------------------------------------------------------
 * 3.3. Form [업무] 초기설정 함수
  ---------------------------------------------------------------------------------------------*/
this.fn_FormBizInit = function() 
{
	this.ds_Input.addRow();
	var dsId, dsCd, dsNm;
	
	this.ds_Master10.addColumn("s_chk", "string");
	this.ds_Master10.addColumn("crudType", "string");

	dsCd = ["GD", "MI", "SS", "DI", ""];
	dsNm = ["GRID", "MasterInput", "상단검색", "하단검색", "ALL"];
	this.fn_SetDsAdd(this.ds_SmComponentGubunSe, dsCd, dsNm);

	dsCd = ["GD", "MI", "SS", "DI"];
	dsNm = ["GRID", "MasterInput", "상단검색", "하단검색"];
	this.fn_SetDsAdd(this.ds_SmComponentGubun, dsCd, dsNm);
	
	dsCd = ["center", "left", "right", ""];
	dsNm = ["CENTER", "LEFT", "RIGHT", ""];
	this.fn_SetDsAdd(this.ds_SmAlign, dsCd, dsNm);	
	
	dsCd = ["true", "false", ""];
	dsNm = ["Y", "N", ""];
	this.fn_SetDsAdd(this.ds_SmTrueFalse, dsCd, dsNm);
	this.fn_SetDsAdd(this.ds_ASTrueFalse, dsCd, dsNm);
	
	dsCd = ["filterlike", "filter", "caseifilterlike", ""];
	dsNm = ["filterlike", "filter", "caseifilterlike", ""];
	this.fn_SetDsAdd(this.ds_SmType, dsCd, dsNm);

	dsCd = ["mask", ""];
	dsNm = ["mask", ""];
	this.fn_SetDsAdd(this.ds_SmFormat, dsCd, dsNm);
	
	dsCd = ["maskeditformat", ""];
	dsNm = ["maskeditformat", ""];
	this.fn_SetDsAdd(this.ds_SmMaskEditFormat, dsCd, dsNm);	
	
	dsCd = ["alpha", "hangul", "number", "alpha,number", "hangul,number", ""];
	dsNm = ["alpha", "hangul", "number", "alpha,number", "hangul,number", ""];
	this.fn_SetDsAdd(this.ds_SmImeMode, dsCd, dsNm);		
	
	dsCd = ["uppercase", "lowercase", ""];
	dsNm = ["대문자", "소문자", ""];
	this.fn_SetDsAdd(this.ds_SmCase, dsCd, dsNm);	
}

this.fn_SetDsAdd = function(dsId, dsCd, dsNm) {
	var k = 0;
	for(var i=0;i<dsCd.length;i++) {
		k = dsId.addRow();
		dsId.setColumn(k, "CODM_CD", dsCd[i]);
		dsId.setColumn(k, "CODM_NM", dsNm[i]);
	}
}



