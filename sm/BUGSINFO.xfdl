this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회	
			//조회시 focus 이동
		
			//분기별
			if (this.fvGuCd == "Q") {
				this.grd_Master10.setFocus();
			//월별
			} else {
				this.grd_Master20.setFocus();
			}
			
			break;						
	}
};


