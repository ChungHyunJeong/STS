this.fvComPrgmId = "";//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
this.fvFromYy = "";//[부모창파라메타] 편성년도
this.fvDepmCd = "";//부서코드
this.fvDepmNm = "";//부서명
this.fvTitle = ""; //팝업이름
this.fvGuCd = "";  //구분자코드

/***********************************************************************************************
 * 00.02. Form 변수 선언
 ***********************************************************************************************/
/***********************************************************************************************
 * 00.03. Form Onload 이벤트
 ***********************************************************************************************/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{	
	//부모창 파라메타 처리
	this.fvFromYy = this.getOwnerFrame().FromYy;//[부모창파라메타] 편성년도
	this.fvDepmCd = this.getOwnerFrame().DepmCd;//[부모창파라메타] 부서코드
	this.fvDepmNm = this.getOwnerFrame().DepmNm;//[부모창파라메타] 부서명
	this.fvTitle = this.getOwnerFrame().paramTitle;//[부모창파라메타] 팝업이름
	this.fvGuCd = this.getOwnerFrame().GuCd;//[부모창파라메타] 구분자코드
	this.fvComPrgmId = this.getOwnerFrame().PrgmId;//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID

 	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
 	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 
	
}

/***********************************************************************************************
 * 00.04. Form [공통] 초기설정 함수 - 필수
 ***********************************************************************************************/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	this.gfn_SetGrid(this.grd_Master10);//[공통]그리드 이벤트 설정
	this.gfn_SetGrid(this.grd_Master20);//[공통]그리드 이벤트 설정
}

/***********************************************************************************************
 * 00.05. Form [업무] 초기설정 함수
 ***********************************************************************************************/
this.fn_FormBizInit = function() 
{
	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	var objCondList = [
					  ];
	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, "");
	
	
	//파라미터 값이 있는경우
	if (!this.gfn_IsNull(this.fvFromYy)) {
		this.div_Search.form.spn_FromYy.set_value(this.fvFromYy);
	}
	if (!this.gfn_IsNull(this.fvDepmCd)) {
		this.div_Search.form.edt_DepmCd.set_value(this.fvDepmCd);
	}
	if (!this.gfn_IsNull(this.fvDepmNm)) {
		this.div_Search.form.edt_DepmNm.set_value(this.fvDepmNm);
	}
	
	if (!this.gfn_IsNull(this.fvTitle)) {
		this.div_PopupTitle.form.sta_PopupTitle.set_text(this.fvTitle);
	}
	
	//분기별
	if (this.fvGuCd == "Q") {
		this.grd_Master10.set_visible(true);
		this.grd_Master20.set_visible(false);
	//월별
	} else {
		this.grd_Master10.set_visible(false);
		this.grd_Master20.set_visible(true);
	}
	
	this.div_PopupTitle_btn_Search_onclick();//조회
}

