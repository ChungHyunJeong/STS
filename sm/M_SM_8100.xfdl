 * 8. [서비스] CallBack 함수 처리부분
 ***********************************************************************************************/
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
	
		case "COMBO_SEARCH": //일반콤보 조회		
			//this.div_Search.form.cbo_ClsmCd.set_index(0);
			break;
			
		case "MST_SEARCH": //조회		
		
			this.ds_Master10Tree.addColumn("s_chk", "string");
			this.ds_Master10Tree.addColumn("crudType", "string");
			//첫번째 트리 펼침
			this.div_MasterView.form.grd_Master10.setTreeStatus(0,true);
			//기존 RowPosition 위치로 이동
			if(this.fvMasterTreeRowPosition  != -1 ){
				//트리위치 저장
				this.ds_Master10Tree.set_rowposition(this.fvMasterTreeRowPosition);
				var parentRow = this.ds_Master10Tree.findRow("HCAM_CD",this.ds_Master10Tree.getColumn(this.ds_Master10Tree.rowposition,"THCA_CD"));
				this.div_MasterView.form.grd_Master10.setTreeStatus(parentRow,true);
				this.fvMasterTreeRowPosition= -1;
			}
		
			this.ds_Master10.copyData(this.ds_Master10Tree);
			//trace(this.ds_Master10.saveXML());
			
			//기존 RowPosition 위치로 이동
			//if(this.fvMasterRowPosition  != 0 ){
				//alert(1);
				//trace("조회 후 마스터 포지션 이동-->"+ this.fvMasterRowPosition );
				//분류내역 위치 저장
				//this.ds_Master10.set_rowposition(this.fvMasterRowPosition);
				//this.fvMasterRowPosition =0;
			//}
			break;
		
		case "DTL_SEARCH": //요청유형조회		
			this.ds_Detail10.addColumn("s_chk", "string");
			this.ds_Detail10.addColumn("crudType", "string");
			//trace(this.ds_Detail10.saveXML());
			break;
		/*
		case "DTL_SEARCH2": //담당자조회		
			this.ds_Detail2.addColumn("s_chk", "string");
			this.ds_Detail2.addColumn("crudType", "string");
			//trace(this.ds_Detail10.saveXML());
			break;
		*/
		case "SAVE":   //저장		
			//저장시 기존 포지션 기록
			//this.fvMasterRowPosition = this.ds_Master10.rowposition;
			this.fvMasterTreeRowPosition = this.ds_Master10Tree.rowposition;
			//this.gfn_Alert( );	//
			
			this.fn_ComBtnSearch();//조회
			//this.fn_DetailSearch();
			break;	
		
		case "DELETE": //삭제		
			this.fn_ComBtnSearch();//조회
			//this.fn_DetailSearch();
			break;							
	}
};

/***********************************************************************************************
 * 9. [팝업] CallBack 함수 처리부분
      strId : 팝업호출시 지정한 팝업ID
	  rtnValue : 팝업창에서 close시 지정한 반환값
      주의 : Open(popuptype=modeless)창은 팝업콜백을 지원하지 않음
 ***********************************************************************************************/
this.fn_PopupCallback = function(strId, rtnValue)
{
	
	if (strId == "B_SYST_ID_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var obj = eval(rtnValue);
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "SYST_ID", obj.SYST_ID);
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EMPM_CD", obj.EMPM_CD);
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EMPM_NM", obj.EMPM_NM);
		}				
	} else if (strId == "B_APSU_ID_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var obj = eval(rtnValue);
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "APSU_ID", obj.SYST_ID);
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "APSU_CD", obj.EMPM_CD);
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "APSU_NM", obj.EMPM_NM);
		}				
	}
	
}
