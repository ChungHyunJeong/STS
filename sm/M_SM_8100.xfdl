{
	this.ds_Input.addRow();

	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	var objCondList = [	
						  "SM805|ds_Sm805"	/* HelpDesk요청유형 */
						, "SM815|ds_Sm815|Y|없음"	/* HelpDesk요청유형별선택입력항목 */
						, "HR201|ds_Hr201"	/* 직책 */
					  ];
	var objFilterList = [
						  ""
						, ""
					    , "OP01_NM='1'"
	                   ];	    
	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, "");
	
	//[업무] 일반콤보조회
	this.fn_ComboSearch();
	
	//콤보 인덱스 설정
	this.div_Search.form.cbo_UsedYn.set_index(0);
	
	//조회
	this.fn_ComBtnSearch();
}

