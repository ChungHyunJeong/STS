/*---------------------------------------------------------------------------------------------
 * 3.1. Form Onload 이벤트
  ---------------------------------------------------------------------------------------------*/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{	
	//부모창 파라메타 처리
	this.fvWrkhNo = this.getOwnerFrame().WrkhNo;//[부모창파라메타] 작업보고관리번호
	this.fvWorhSq = this.getOwnerFrame().WorhSq;//[부모창파라메타] 결재차수
	this.fvComPrgmId = this.getOwnerFrame().PrgmId;//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
	
	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 
}

/*---------------------------------------------------------------------------------------------
 * 3.2. Form [공통] 초기설정 함수 - 필수
  ---------------------------------------------------------------------------------------------*/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	this.gfn_SetGrid(this.grd_Master10);//[공통]그리드 이벤트 설정 - 출력목록
}

/*---------------------------------------------------------------------------------------------
 * 3.3. Form [업무] 초기설정 함수
  ---------------------------------------------------------------------------------------------*/
this.fn_FormBizInit = function() 
{
	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	var objCondList = ["SM034|ds_Sm034", "SM036|ds_Sm036"];
	//공통코드 설정함수 호출
	this.gfn_GetCommCode(this, objCondList, "");
	
	this.gfn_SetGridChoiceBtnUrl(this.div_ComGrdBtn);//[공통]그리드 선택 버튼 URL 설정

	//상세 그리드버튼 설정 : 버튼ID|권한(Y/N)|호출함수명
	var objBtnInfo01List = ["CHECK|Y|fn_MasterGrd01AllCheck"
						 ,"UNCHK|Y|fn_MasterGrd01AllUnCheck"
						 ,"S_CHECK|Y|fn_MasterGrd01SomeCheck"
						 ,"S_UNCHK|Y|fn_MasterGrd01SomeUnCheck"
						 ,"CHOICE|N|fn_MasterGrd01Choice"
						 ,"CANCEL|N|fn_MasterGrd01Cancel"];
	this.div_ComGrdBtn.form.fn_SetGrdButtonFunc(this, objBtnInfo01List);
	
	//콤보 인덱스 설정
	
	if (!this.gfn_IsNull(this.fvWrkhNo) && !this.gfn_IsNull(this.fvWorhSq)) {
		this.div_PopupTitle_btn_Search_onclick();//조회
	}
}

