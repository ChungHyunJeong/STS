 ***********************************************************************************************/
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회
			this.ds_Master10.addColumn("s_chk", "string");
			
			if (this.ds_Master10.getRowCount() > 0) {
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.grd_Master10);
			} else {
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.grd_Master10);
			}
			break;						
	}
};

