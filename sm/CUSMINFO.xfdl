    <Script type="xscript5.1"><![CDATA[/**
*  @화면ID   : 
*  @화면설명 : 부서정보
*  @시스템명 : (SM) 시스템
*  @작성자   : 최석목
*  @작성일   : 2021.01.27
*  @Desction    
******************* 소스 수정 이력 ************************************************************
*  일자          		수정자                비고
*************************************************************************************************
*  2021.01.27     	    최석목 	           최초 생성 
*************************************************************************************************
*/

/************************************************************************************************
 * 1. Form Include 선언 영역
 ************************************************************************************************/

/************************************************************************************************
 * 2. Form 변수 선언 영역
 ************************************************************************************************/
var fvCusmTp;
 
this.fvComPrgmId = "";//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
this.fvCusmNm = "";//[부모창파라메타] 거래처명
this.fvCusmTp = "";//[부모창파라메타] 업체유형 - C : 고객사, H : 협력사, E - 기타, A - 전체선택

/***********************************************************************************************
 * 3. Form 이벤트 영역
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 * 3.1. Form Onload 이벤트
  ---------------------------------------------------------------------------------------------*/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{	
	//부모창 파라메타 처리
	this.fvCusmNm = this.getOwnerFrame().CusmNm;//[부모창파라메타] 거래처명
	this.fvCusmTp = this.getOwnerFrame().CusmTp;//[부모창파라메타] 업체유형
	this.fvComPrgmId = this.getOwnerFrame().PrgmId;//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
	
	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 
}

/*---------------------------------------------------------------------------------------------
 * 3.2. Form [공통] 초기설정 함수 - 필수
  ---------------------------------------------------------------------------------------------*/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	this.gfn_SetGrid(this.grd_Master10);//[공통]그리드 이벤트 설정 - 출력목록
}

/*---------------------------------------------------------------------------------------------
 * 3.3. Form [업무] 초기설정 함수
  ---------------------------------------------------------------------------------------------*/
this.fn_FormBizInit = function() 
{
	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	var objCondList = ["SM088|ds_Sm088", "MM035|ds_Mm035|Y|ALL"];
	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, "");
	
	//콤보 인덱스 설정
	this.div_Search.form.cbo_UsedYn.set_index(1);
		
	//업체유형 설정
	if (!this.gfn_IsNull(this.fvCusmTp)) {
		
		this.div_Search.form.chk_Ty01Yn.set_enable(false);
		this.div_Search.form.chk_Ty02Yn.set_enable(false);
		this.div_Search.form.chk_Ty03Yn.set_enable(false);
		
		//업체유형 - 고객
		if (this.fvCusmTp == "C") {
			this.div_Search.form.chk_Ty01Yn.set_value("1");
		//업체유형 - 협력사
		} else if (this.fvCusmTp == "H") {
			this.div_Search.form.chk_Ty02Yn.set_value("1");
			//체크 콤보 설정값 만들기 - 협력사유형
			var objConfig = {
								dropbutton	 : this.div_Search.form.btn_Ct01Cd, //Button 컴포너트ID
								innerdataset : this.ds_Mm035, //데이터셋
								codecolumn	 : "CODM_CD",   //코드컬럼
								datacolumn	 : "CODM_NM", //코드값컬럼
								displayRowCount : 6 //화면표시건수
							};

			//체크 콤보 초기화 함수 호출
			this.gfn_InitMultiCombo(this.div_Search.form.edt_Ct01Cd, objConfig);	
			//체크 콤보의 Code 값을 입력하는 함수 실행
			this.gfn_SetMultiComboCode(this.div_Search.form.edt_Ct01Cd, "ALL");//전체					
			this.div_Search.form.sts_Ct01Cd.set_visible(true);
			this.div_Search.form.edt_Ct01Cd.set_visible(true);
			this.div_Search.form.btn_Ct01Cd.set_visible(true);
			
			this.grd_Master11.set_visible(true);//업체유형-파트너사인경우 그리드 변경
			
		//업체유형 - 기타
		} else if (this.fvCusmTp == "E") {
			this.div_Search.form.chk_Ty03Yn.set_value("1");	
		//업체유형 - 전체
		} else if (this.fvCusmTp == "A") {
			this.div_Search.form.chk_Ty01Yn.set_value("1");
			this.div_Search.form.chk_Ty02Yn.set_value("1");
			this.div_Search.form.chk_Ty03Yn.set_value("1");				
			this.div_Search.form.chk_Ty01Yn.set_enable(true);
			this.div_Search.form.chk_Ty02Yn.set_enable(true);
			this.div_Search.form.chk_Ty03Yn.set_enable(true);
		}
	}
	
	this.div_Search.form.edt_CusmNm.setFocus();
	
	if (!this.gfn_IsNull(this.fvCusmNm)) {
		this.div_Search.form.edt_CusmNm.set_value(this.fvCusmNm);
		this.div_PopupTitle_btn_Search_onclick();//조회
	}
}

/*---------------------------------------------------------------------------------------------
 * 3.4. Form OnClose 이벤트
  ---------------------------------------------------------------------------------------------*/
this.fn_FormClose = function(obj:nexacro.Form,e:nexacro.CloseEventInfo)
{
	return true;
};

/*---------------------------------------------------------------------------------------------
 * 3.4. Form OnSize 이벤트
  ---------------------------------------------------------------------------------------------*/
this.fn_FormOnsize = function(obj:nexacro.Form,e:nexacro.SizeEventInfo)
{
	if (obj.width > 1100) {
		this.grd_Master10.set_autofittype("col");
	} else {
		this.grd_Master10.set_autofittype("none");
	}
};

