		}
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(iRow, "ADDR_NM"))) {
			sADDR_NM = this.ds_Master10.getColumn(iRow, "ADDR_NM").replace(/\n|\r|\s*/g, ""); //21.주소
		}		
	
        var rtnValue = "({" +
                        ' CUSM_CD:"' + this.ds_Master10.getColumn(iRow, "CUSM_CD") + '"'+  //0.거래처코드
                        ',CUSM_NM:"' + this.ds_Master10.getColumn(iRow, "CUSM_NM") + '"'+  //1.거래처명(한글)
                        ',CUSE_NM:"' + this.ds_Master10.getColumn(iRow, "CUSE_NM") + '"'+  //2.거래처명(영문)
                        ',CUTY_CD:"' + this.ds_Master10.getColumn(iRow, "CUTY_CD") + '"'+  //3.회사형태
                        ',SAUM_NO:"' + this.ds_Master10.getColumn(iRow, "SAUM_NO") + '"'+  //4.사업자등록번호
                        ',CORP_NO:"' + this.ds_Master10.getColumn(iRow, "CORP_NO") + '"'+  //5.법인등록번호
                        ',TY01_YN:"' + this.ds_Master10.getColumn(iRow, "TY01_YN") + '"'+  //6.거래처유형_고객사여부
                        ',TY02_YN:"' + this.ds_Master10.getColumn(iRow, "TY02_YN") + '"'+  //8.거래처유형_협력사여부
                        ',TY03_YN:"' + this.ds_Master10.getColumn(iRow, "TY03_YN") + '"'+  //8.거래처유형_기타여부
                        ',NATM_CD:"' + this.ds_Master10.getColumn(iRow, "NATM_CD") + '"'+  //9.국가코드
                        ',GROU_CD:"' + this.ds_Master10.getColumn(iRow, "GROU_CD") + '"'+  //10.그룹코드
                        ',TCOM_CD:"' + this.ds_Master10.getColumn(iRow, "TCOM_CD") + '"'+  //11.모회사코드
                        ',BUSI_CO:"' + this.ds_Master10.getColumn(iRow, "BUSI_CO") + '"'+  //12.업태
                        ',ITEM_NM:"' + this.ds_Master10.getColumn(iRow, "ITEM_NM") + '"'+  //13.종목
                        ',REPR_NM:"' + this.ds_Master10.getColumn(iRow, "REPR_NM") + '"'+  //14.대표자명
                        ',ESTA_DD:"' + this.ds_Master10.getColumn(iRow, "ESTA_DD") + '"'+  //15.설립일자
                        ',HDOF_YN:"' + this.ds_Master10.getColumn(iRow, "HDOF_YN") + '"'+  //16.본사여부
                        ',OTEL_NO:"' + this.ds_Master10.getColumn(iRow, "OTEL_NO") + '"'+  //17.전화번호
                        ',OFAX_NO:"' + this.ds_Master10.getColumn(iRow, "OFAX_NO") + '"'+  //18.팩스번호
                        ',HOME_NM:"' + this.ds_Master10.getColumn(iRow, "HOME_NM") + '"'+  //19.홈페이지
						',TAXB_GU:"' + sTAXB_GU + '"'+  //20.세금계산서발행구분
