/***********************************************************************************************
 * 13.02. [사용자함수] 선택한 정보 부모창 리턴 함수 
 ***********************************************************************************************/ 
this.fn_Choice = function(iRow) {
	if (iRow > -1) {	
	
        rtnValue = "({" +
                        ' ACNM_NO:"' + this.ds_Master10.getColumn(iRow, "ACNM_NO") + '"'+  //0.계좌번호
                        ',ACNM_NM:"' + this.ds_Master10.getColumn(iRow, "ACNM_NM") + '"'+  //1.예적금명
                        ',ACOM_CD:"' + this.ds_Master10.getColumn(iRow, "ACOM_CD") + '"'+  //2.계정코드
                        ',ACOM_NM:"' + this.ds_Master10.getColumn(iRow, "ACOM_NM") + '"'+  //3.계정명
                        ',ACUM_CD:"' + this.ds_Master10.getColumn(iRow, "ACUM_CD") + '"'+  //4.회계단위
                        ',ACUM_NM:"' + this.ds_Master10.getColumn(iRow, "ACUM_NM") + '"'+  //5.회계단위명
                        ',BANM_CD:"' + this.ds_Master10.getColumn(iRow, "BANM_CD") + '"'+  //6.은행코드
                        ',BANM_NM:"' + this.ds_Master10.getColumn(iRow, "BANM_NM") + '"'+  //7.지점명
                        ',OPEN_DD:"' + this.ds_Master10.getColumn(iRow, "OPEN_DD") + '"'+  //8.개설일자
                        ',EXPI_DD:"' + this.ds_Master10.getColumn(iRow, "EXPI_DD") + '"'+  //9.만기일자
                        ',CURR_CD:"' + this.ds_Master10.getColumn(iRow, "CURR_CD") + '"'+  //10.화폐
                        ',USED_YN:"' + this.ds_Master10.getColumn(iRow, "USED_YN") + '"'+  //11.사용여부
                        ',REMK_NM:"' + this.ds_Master10.getColumn(iRow, "REMK_NM") + '"'+  //12.비고
                "})";

		this.gfn_PopClose(rtnValue);
	} else {
		this.gfn_Alert("SM046");//선택된 내용이 없습니다.
		return;		
	}
}

/***********************************************************************************************
 * 14. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 14.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{		
		case "COMBO_SEARCH": ///계정코드조회	
			this.ds_FaAcom.insertRow();
			this.ds_FaAcom.setColumn(0, "ACOM_CD", "0");
			this.ds_FaAcom.setColumn(0, "ACOM_NM", "ALL");	
			this.ds_FaAcom.filter("ACOM_GU=='0080'||ACOM_NM=='ALL'"); //계정자료구분이 '예적금마스타등재' 인것만 + 전체추가 
			
			var iFindRow = this.ds_FaAcom.findRowExpr("ACOM_CD == '" + this.fvAcomCd + "'");			
			//없는 계정 ALL처리
			if (iFindRow < 0){ 
				this.div_Search.form.cbo_AcomCd.set_index(0);		
			} else {
				this.div_Search.form.cbo_AcomCd.set_index(iFindRow);
			}
			this.ds_FaAcom.set_keystring("S:+ACOM_CD");					
		
			this.div_PopupTitle_btn_Search_onclick();//조회
			
			break;
		case "MST_SEARCH": //조회	
			//조회시 focus 이동
			this.grd_Master10.setFocus();
		
			break;						
	}
};


