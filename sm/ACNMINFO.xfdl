this.fvComPrgmId = "";//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
this.fvAcnmNo = "";//[부모창파라메타] 계좌번호
this.fvAcomCd = ""; //계정코드
this.fvSearchText = ""; 

/***********************************************************************************************
