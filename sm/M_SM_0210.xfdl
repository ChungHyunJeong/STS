/***********************************************************************************************
 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
	
		case "COMBO_SEARCH": //일반콤보 조회		
			this.div_Search.form.cbo_ModmCd.set_index(0);
			break;
			
		case "MST_SEARCH": //조회
			if (this.ds_Master10.getRowCount() > 0) {		
				
				//입력영역 활성화
				this.div_MasterInput.form.div_ComGrdBtn.set_enable(true);
			} else {
				//입력영역 비활성화
				this.div_MasterInput.form.div_ComGrdBtn.set_enable(false);
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);
				this.gfn_Alert("SM044"); //조회된 데이터가 없습니다.
			}
			
			this.fvMasterSearchMode = false;//마스터내역 조회중여부
			
			break;
		
		case "DTL_SEARCH": //공통코드 조회
		
			this.ds_Detail10.addColumn("s_chk", "string");
			this.ds_Detail10.addColumn("crudType", "string");
			this.ds_Detail20.addColumn("s_chk", "string");
			this.ds_Detail20.addColumn("crudType", "string");
	
			if (this.ds_Detail10.getRowCount() > 0) {
			
				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					//var iFvFindRow = this.ds_Detail10.findRowExpr("GRCM_CD=='" + this.fvSchKey01 + "' && CODM_CD=='" + this.fvSchKey02 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Detail10.enableevent) {
						this.ds_Detail10.set_enableevent(true);
					}
				
// 					if (iFvFindRow > -1) {
// 						if (iFvFindRow == 0) {
// 							this.ds_Detail10.set_rowposition(-1);
// 						} 
// 						this.ds_Detail10.set_rowposition(iFvFindRow);
// 					} else {
// 						this.ds_Detail10.set_rowposition(0);
// 					}
				}				
				
				//입력영역 활성화
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
				this.div_DetailInput.form.tab_Detail.set_enable(true);
				
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.tab_Detail.tpg_01.form.grd_Detail10);
				
			} else {
				
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterInput.form.tab_Detail.tpg_01.form.grd_Detail10);
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Detail10.enableevent) {
					this.ds_Detail10.set_enableevent(true);
				}
			
				//입력영역 비활성화
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);
				this.div_DetailInput.form.tab_Detail.set_enable(false);			
			}
			
			//저장후 재조회여부-true
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}			
			
