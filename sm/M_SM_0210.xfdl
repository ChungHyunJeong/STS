{
	if (e.newrow > -1 && !this.fvDetailSearchMode) { //Master그리드에서 새로운 Row가 존재하는 확인

		//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
		this.ds_Detail20.clearData();
	
