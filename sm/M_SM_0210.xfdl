	}
};

this.fn_DetailSearch = function()
{
	//if (e.newrow > -1) {
		//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
		this.ds_Detail10.clearData();
		this.ds_Detail20.clearData();
		
		//그룹No정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "GRCM_CD"))) {
			this.fvMasterSearchMode = false;//마스터내역 조회중여부
			//this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "GRCM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "GRCM_CD"));
			//this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
					
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "sm:M_SM_0210_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_SM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);	
			
			//가변 컬럼 세팅
			var arrColMaster = ["OP01_NM", "OP02_NM", "OP03_NM", "OP04_NM", "OP05_NM", "OP06_NM", "OP07_NM", "OP08_NM", "OP09_NM", "OP10_NM"];
			var arrColDetail = ["OP01_NM", "OP02_NM", "OP03_NM", "OP04_NM", "OP05_NM", "OP06_NM", "OP07_NM", "OP08_NM", "OP09_NM", "OP10_NM"];
			var sText  = "";
			var nIdxBody=0;
			var grdObj =this.div_MasterInput.form.tab_Detail.tpg_01.form.grd_Detail10;
			for (var idx=0; idx<arrColMaster.length; idx++) 
			{
				sText = this.ds_Master10.getColumn(this.ds_Master10.rowposition, arrColMaster[idx]);
				nIdxBody = grdObj.getBindCellIndex("Body",arrColDetail[idx]);
				
				if (this.gfn_IsNull(sText)) 
				{
					grdObj.setRealColSize("body",nIdxBody, 0, false );
				}  
				else 
				{
					grdObj.setCellProperty("head", nIdxBody, "text", sText);
					grdObj.setRealColSize("body",nIdxBody, (sText.length> 7 ? 100 + sText.length * 4 : 100), false )
					
				}
			}
		}
	//}
}

