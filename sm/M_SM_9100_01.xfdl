/*---------------------------------------------------------------------------------------------
 * 12.2. [사용자함수 - Vaildation] 조회 필수값 체크
  ---------------------------------------------------------------------------------------------*/
this.fn_VaildSearch = function()
{
	//[검색] Validation 설정
	var arrArgs = [ 
					"this.div_Search.form.cal_TemhDtFr^"+this.div_Search.form.sta_TemhDtFr.text+"|NULL" 
					//"this.div_Search.form.cal_TemhDtFr^this.div_Search.form.sta_TemhDtFr|FROM_TO=this.div_Search.form.cal_TemhDtTo"
	              ]; 
	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs) ) {
		return false;
	}
	return true;
}


this.div_PopupTitle_btn_Search_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
