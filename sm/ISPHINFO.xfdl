                        ",WOFH_ST:'" + this.ds_Master10.getColumn(mRow, "WOFH_ST") + "'"+  //3.결재상태(WF110)
                        ",WOFH_SQ:'" + this.ds_Master10.getColumn(mRow, "WOFH_SQ") + "'"+  //4.결재차수
                        ",CUSM_NM:'" + this.ds_Master10.getColumn(mRow, "CUSM_NM") + "'"+  //5.거래처명(한글)
                        ",ISPH_NO:'" + this.ds_Master10.getColumn(mRow, "ISPH_NO") + "'"+  //6.제품사양번호
                        ",ITEM_CD:'" + this.ds_Master10.getColumn(mRow, "ITEM_CD") + "'"+  //7.제품코드
                        ",ITEM_NM:'" + this.ds_Master10.getColumn(mRow, "ITEM_NM") + "'"+  //8.제품명
                        ",ISPB_QT:'" + this.ds_Master10.getColumn(mRow, "ISPB_QT") + "'"+  //9.수량
                        ",HOGI_NM:'" + this.ds_Master10.getColumn(mRow, "HOGI_NM") + "'"+  //10.호기명
                        ",DEPM_CD:'" + this.ds_Master10.getColumn(mRow, "DEPM_CD") + "'"+  //11.조직코드
                        ",DEPM_NM:'" + this.ds_Master10.getColumn(mRow, "DEPM_NM") + "'"+  //12.조직명
                        ",SYST_ID:'" + this.ds_Master10.getColumn(mRow, "SYST_ID") + "'"+  //13.시스템사번
                        ",EMPM_CD:'" + this.ds_Master10.getColumn(mRow, "EMPM_CD") + "'"+  //14.사원코드
                        ",EMPM_NM:'" + this.ds_Master10.getColumn(mRow, "EMPM_NM") + "'"+  //15.성명
