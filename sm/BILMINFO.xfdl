this.fn_FormBizInit = function() 
{
	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	var objCondList = ["FA046|ds_Fa046"
	                  ,"FA804|ds_Fa804"
	                  ,"FA805|ds_Fa805|Y|ALL"  ];
	//공통코드 설정함수 호출
	this.gfn_GetCommCode(this, objCondList, "");
		
	//파라미터 값이 있는경우
	if (!this.gfn_IsNull(this.fvBilmNo)) {
		this.div_Search.form.edt_SearchText.set_value(this.fvBilmNo);
		
	}
	this.div_PopupTitle_btn_Search_onclick();//조회
	
	
	this.div_Search.form.cbo_UsedYn.set_index(0);
}

