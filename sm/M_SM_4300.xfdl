 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
			trace("MST_SEARCH ---> " + this.ds_Empm.rowcount);
			if(this.ds_Empm.rowcount == 1) {
				if(this.gfn_IsNull(this.ds_Empm.getColumn(0, "MTEL_NO"))) { alert("등록된 전화번호가 없습니다."); return; }
				var ch = this.ds_Master10.findRow("MTEL_NO", this.ds_Empm.getColumn(0, "MTEL_NO"));
				if(ch >= 0) { return; }
				var k = this.ds_Master10.addRow();
				this.ds_Master10.setColumn(k, "EMPM_NM", this.ds_Empm.getColumn(0, "EMPM_NM"));
				this.ds_Master10.setColumn(k, "MTEL_NO", this.ds_Empm.getColumn(0, "MTEL_NO"));
				this.ds_Master10.setColumn(k, "DEPM_NM", this.ds_Empm.getColumn(0, "DEPM_NM"));
				this.ds_Master10.setColumn(k, "POSI_NM", this.ds_Empm.getColumn(0, "POSI_NM"));
				this.ds_Master10.setColumn(k, "SYST_ID", this.ds_Empm.getColumn(0, "SYST_ID"));			
				
				this.div_Body.form.edt_RvemNm.set_value("");
			} else {
				this.div_Body_btn_CorhM2_01_onclick();
			}
			 
			break;
		case "SAVE":   //저장
			this.reload();
			break;			
	}
};

/***********************************************************************************************
 * 13.02. [팝업] CallBack 함수 처리
          strId : 팝업호출시 지정한 팝업ID
	      rtnValue : 팝업창에서 close시 지정한 반환값
          주의 : Open(popuptype=modeless)창은 팝업콜백을 지원하지 않음
 ***********************************************************************************************/ 
this.fn_PopupCallback = function(strId, rtnValue)
{
//trace(strId);
	if (strId == "M_SYST_ID_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			var k = this.ds_Master10.addRow();
			
			if (!this.gfn_IsNull(arrRtnVal.EMPM_CD)) {
				this.div_Body.form.edt_RvemNm.set_value(arrRtnVal.EMPM_NM);
				this.ds_Master10.setColumn(k, "EMPM_NM", arrRtnVal.EMPM_NM);
 				this.ds_Master10.setColumn(k, "MTEL_NO", arrRtnVal.MTEL_NO);
 				this.ds_Master10.setColumn(k, "DEPM_NM", arrRtnVal.DEPM_NM);
 				this.ds_Master10.setColumn(k, "POSI_NM", arrRtnVal.POSI_NM);
			}
		}
	} else if (strId == "M_SYST_ID_02") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if(this.gfn_IsNull(arrRtnVal.MTEL_NO)) {
				var arrMsgArg = ["SMS"];
				this.gfn_Alert("SM023", arrMsgArg); // 전화번호가 유효하지 않습니다.
				return false; 			
			}

			var ch = this.ds_Master10.findRow("MTEL_NO", arrRtnVal.MTEL_NO);
			if(ch >= 0) { return; }
			
			var k = this.ds_Master10.addRow();
			
			if (!this.gfn_IsNull(arrRtnVal.EMPM_CD)) {
				this.div_Body.form.edt_RvemNm.set_value(arrRtnVal.EMPM_NM);
				this.ds_Master10.setColumn(k, "EMPM_NM", arrRtnVal.EMPM_NM);
				this.ds_Master10.setColumn(k, "MTEL_NO", arrRtnVal.MTEL_NO);
				this.ds_Master10.setColumn(k, "DEPM_NM", arrRtnVal.DEPM_NM);
				this.ds_Master10.setColumn(k, "POSI_NM", arrRtnVal.POSI_NM);
				this.ds_Master10.setColumn(k, "SYST_ID", arrRtnVal.SYST_ID);
			}
		}
	}

};
 
