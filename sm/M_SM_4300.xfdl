this.div_Body_txt_SmssNm_onkeydown = function(obj:nexacro.TextArea,e:nexacro.KeyEventInfo)
{
	var iByteLength  = this.gfn_LengthByte(this.div_Body.form.txt_SmssNm.value) + 1;
	
	if(iByteLength > 4000)
	{
		var chkLength=this.div_Body.form.txt_SmssNm.value.length;
		this.div_Body.form.txt_SmssNm.set_maxlength(chkLength);
		if(e.keycode == 8)
		{
			this.div_Body.form.txt_SmssNm.set_maxlength("");
		}
	} else {
		this.div_Body.form.txt_SmssNm.set_maxlength(4000);
	}
};

/***********************************************************************************************
 * 15.2. [SMS내용] Key up 이벤트
 ***********************************************************************************************/ 
this.div_Body_txt_SmssNm_onkeyup = function(obj:nexacro.TextArea,e:nexacro.KeyEventInfo)
{
	var iByteLength  = this.gfn_LengthByte(this.div_Body.form.txt_SmssNm.value);
	if (iByteLength == -1) {	
		iByteLength = 0;
	}
	if (iByteLength > 4000) {
		this.div_Body.form.txt_SmssNm.set_value(this.gfn_SliceByByte(this.div_Body.form.txt_SmssNm.value, 4000));
		this.div_Body.form.mae_SmssCt.set_value(4000);
	} else {	
		this.div_Body.form.mae_SmssCt.set_value(iByteLength);
	}
};

