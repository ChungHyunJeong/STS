/*---------------------------------------------------------------------------------------------
 * 11.1.1. [데이터셋-ds_Master] Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	//ds_Master1(Tree)에서 Row 선택시 ds_Detail1 결과에서 필터 처리
	var sClssGu = this.ds_Master1.getColumn(this.ds_Master1.rowposition, "CLSS_GU");
	var sTreeCd = this.ds_Master1.getColumn(this.ds_Master1.rowposition, "TREE_CD");
	
	this.ds_Detail1.filter("");
	if (sClssGu == '2') {
		this.ds_Detail1.filter("MODM_CD == '"+ sTreeCd + "'");
	} else if (sClssGu == '3') {
		this.ds_Detail1.filter("TREE_CD == '"+ sTreeCd + " || CLSM_CD == '"+ sTreeCd + "'");				
	} else if (sClssGu == '4') {
		this.ds_Detail1.filter("PRGM_ID == '"+ sTreeCd + "'");
	}
};


