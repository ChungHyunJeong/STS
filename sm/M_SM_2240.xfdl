 ************************************************************************************************/
this.fvComPrgmId = "";//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
//this.fvMasterRowPosition = 0;//저장 후 현재 위치한 Dataset위치로 이동(M_SM_1110 참고)


/***********************************************************************************************
 * 3. Form 이벤트 영역
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 * 3.1. Form Onload 이벤트
  ---------------------------------------------------------------------------------------------*/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{	
	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 
}

/*---------------------------------------------------------------------------------------------
 * 3.2. Form [공통] 초기설정 함수 - 필수
  ---------------------------------------------------------------------------------------------*/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	this.fvComPrgmId = this.gfn_CommLoadFormPrgmId(this);//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
	this.gfn_SetGrid(this.div_TabView.form.tab_Master.tpg_01.form.div_MasterView.form.grd_Master);//[공통]그리드 이벤트 설정
	this.gfn_SetGrid(this.div_TabView.form.tab_Master.tpg_02.form.div_MasterView.form.grd_Master);//[공통]그리드 이벤트 설정
	this.gfn_SetGrid(this.div_TabView.form.tab_Master.tpg_02.form.div_DetailView.form.tab_Detail.tpg_01.form.grd_Detail);//[공통]그리드 이벤트 설정
	this.gfn_SetGrid(this.div_TabView.form.tab_Master.tpg_03.form.div_MasterView.form.grd_Master);//[공통]그리드 이벤트 설정

}

/*---------------------------------------------------------------------------------------------
 * 3.3. Form [업무] 초기설정 함수
  ---------------------------------------------------------------------------------------------*/
this.fn_FormBizInit = function() 
{	
	//조회조건 입력생성
	this.ds_Input.addRow();
	
	//[공통] 날짜설정nextMonth1
	var sMonthFirstDay = this.gfn_GetCommDate("monthFirstDay");	//이번달 첫째일자		
	var sNowDay = this.gfn_GetCommDate("toDay");				//현재일자		

	this.div_Search.form.cal_FromDd.set_value(sMonthFirstDay);
	this.div_Search.form.cal_ClosDd.set_value(sNowDay);
	
	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	var objCondList = ["HR201|ds_Duty"
	                  ,"HR202|ds_Pusi"
					  ];
	// 공통코드 설정함수 호출  --> 추후 공통코드 정의시 진행			   
	this.gfn_GetCommCode(this, objCondList, "");
	
	//[업무] 일반콤보조회 : 권한그룹
	this.fn_ComboSearch();
	
	//콤보 인덱스 설정
	this.div_Search.form.cbo_PgrmGu.set_index(0);	
}

/*---------------------------------------------------------------------------------------------
 * 3.4. Form OnClose 이벤트
  ---------------------------------------------------------------------------------------------*/
