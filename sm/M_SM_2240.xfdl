  ---------------------------------------------------------------------------------------------*/
this.fn_ProgAuthListSearch = function() 
{ 
	// Maste 그리드에서 새로운 Row가 발생하지 않으면 실행하지 않음
	//if (this.ds_Master.rowcount > 0) { //Master그리드에서 새로운 Row가 존재하는 확인
		
		// ds_Input 바인딩값에 우측에서 클릭한 값을 넣음
		this.ds_Input.setColumn(0, "DEPM_CD", this.div_Search.form.edt_DepmCd.value);	//부서
		this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_FromDd.value);	//조회기간(FROM)
		this.ds_Input.setColumn(0, "CLOS_DD", this.div_Search.form.cal_ClosDd.value);	//조회기간(TO)
		this.ds_Input.setColumn(0, "SESU_ID", this.div_Search.form.edt_SystId.value);	//사용자
		this.ds_Input.setColumn(0, "PGRM_CD", this.div_Search.form.cbo_PgrmCd.value);	//권한그룹
		this.ds_Input.setColumn(0, "UPGH_YN", this.div_Search.form.cbo_PgrmGu.value);	//권한구분
		
		//  우측상단 상세결과 조회
		//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
		this.ds_Detail1.clearData();

		//[공통]서비스정보 초기화
		this.gfn_InitComService(this);
		
		//[공통]서비스정보 추가
		var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
		svcAddObj.sSelectSql = "sm:M_SM_2240_SL01";
		this.gfn_AddComSearchService(this, svcAddObj);

		//[공통]서비스 호출 변수
		var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
		svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
		svcCallObj.sActionName = this.G_SV_SM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
		svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
		svcCallObj.sOutDatset = "ds_Detail1=ds_Detail";//출력 데이터셋
		svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

		//[공통]서비스 호출
		this.gfn_CallComService(this, svcCallObj);	
	//}
}


