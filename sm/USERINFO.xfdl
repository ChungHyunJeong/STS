this.fvComPrgmId = "";//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
this.fvAuthYn = "";//[부모창파라메타] 권한체크여부
this.fvDemhMm = "";//기준일자
this.fvEmpmNm = "";//[부모창파라메타] 사원명

/***********************************************************************************************
 * 3. Form 이벤트 영역
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 * 3.1. Form Onload 이벤트
  ---------------------------------------------------------------------------------------------*/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{	
	//부모창 파라메타 처리
	this.fvEmpmNm = this.getOwnerFrame().EmpmNm;//[부모창파라메타] 사원명
	this.fvComPrgmId = this.getOwnerFrame().PrgmId;//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
	this.fvAuthYn = this.getOwnerFrame().AuthYn;//[부모창파라메타] 권한체크여부
	
	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 
}

/*---------------------------------------------------------------------------------------------
 * 3.2. Form [공통] 초기설정 함수 - 필수
  ---------------------------------------------------------------------------------------------*/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	this.gfn_SetGrid(this.grd_Master10);//[공통]그리드 이벤트 설정 - 출력목록
	this.gfn_SetGrid(this.grd_Detail10);//[공통]그리드 이벤트 설정 - 출력목록
	
}

/*---------------------------------------------------------------------------------------------
 * 3.3. Form [업무] 초기설정 함수
  ---------------------------------------------------------------------------------------------*/
this.fn_FormBizInit = function() 
{
	//[공통] 날짜설정
	var sNowDay = this.gfn_GetCommDate("toDay");//현재일자		
	this.fvDemhMm = sNowDay;//기준일자
	
	//부모창에서 전달 받은 파라메터 세팅
	if (!this.gfn_IsNull(this.fvEmpmNm)) {
		this.div_Search.form.edt_SechVl.set_value(this.fvEmpmNm);
	}	

	//조직조회
	this.fn_Search();
}

/*---------------------------------------------------------------------------------------------
 * 3.4. Form OnClose 이벤트
  ---------------------------------------------------------------------------------------------*/
this.fn_FormClose = function(obj:nexacro.Form,e:nexacro.CloseEventInfo)
{
	return true;
};

/*---------------------------------------------------------------------------------------------
 * 3.4. Form OnSize 이벤트
  ---------------------------------------------------------------------------------------------*/
this.fn_FormOnsize = function(obj:nexacro.Form,e:nexacro.SizeEventInfo)
{ 

	if (obj.width > 1400) {
		this.grd_Detail10.set_autofittype("col");
	} else {
		this.grd_Detail10.set_autofittype("none");
	}

};

