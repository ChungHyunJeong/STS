	this.gfn_SetGrid(this.div_DetailView.form.tab_Detail.tpg_01.form.grd_Detail);//[공통]그리드 이벤트 설정
	this.gfn_SetGrid(this.div_UserView.form.tab_User.tpg_01.form.grd_Userlist);//[공통]그리드 이벤트 설정
