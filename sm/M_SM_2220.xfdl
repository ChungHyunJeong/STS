		// 데이터셋 Copy 후 Delete
		this.ds_Userlist.copyRow(iAddRow, this.ds_Detail, iFromRow);	
		this.ds_Detail.deleteRow(iFromRow);		
		*/
		
		if (this.ds_Detail.getCaseCount("s_chk == '1'") > 0) {

			var iAddRow = 0;	// 한줄추가
			var arrDeleteData = new Array();
			var iChkIndex = 0;//체크된 인덱스
			
			for (var iLoop=0; iLoop < this.ds_Detail.getRowCount(); iLoop++) {
				if (this.ds_Detail.getColumn(iLoop, "s_chk") == "1") {
					iAddRow = this.ds_Userlist.addRow();	// 한줄추가
					this.ds_Userlist.copyRow(iAddRow, this.ds_Detail, iLoop);
					arrDeleteData.push(iLoop);
				}
			}

			while (true) 
			{
				iChkIndex = arrDeleteData.pop();//체크된 인덱스
				this.ds_Detail.deleteRow(iChkIndex);
				
				if (arrDeleteData.length == 0) {
					break;
				}
			}
			
		}		

	}

};

