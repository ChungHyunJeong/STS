/*---------------------------------------------------------------------------------------------
 * 12.1.7. [사용자함수 - 서비스] 사용자목록 -> 그룹별관리자목록
  ---------------------------------------------------------------------------------------------*/
this.div_UserView_btn_UserIns_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if (this.ds_Userlist.rowposition > -1) { //User 그리드에서 새로운 Row가 존재하는 확인
	
		if (this.ds_Userlist.getCaseCount("s_chk == '1'") > 0) {

			var iAddRow = 0;	// 한줄추가
			var arrDeleteData = new Array();
			var iChkIndex = 0;//체크된 인덱스
			
			for (var iLoop=0; iLoop < this.ds_Userlist.getRowCount(); iLoop++) {
				if (this.ds_Userlist.getColumn(iLoop, "s_chk") == "1") {
					iAddRow = this.ds_Detail.addRow();	// 한줄추가
					this.ds_Detail.setColumn(iAddRow, "PGRM_CD", this.ds_Master.getColumn(this.ds_Master.rowposition, "PGRM_CD"));//권한그룹코드

					// 데이터셋 Copy 후 Delete배열에 담기
					this.ds_Detail.copyRow(iAddRow, this.ds_Userlist, iLoop);	
					//this.ds_Userlist.deleteRow(iLoop);	
					arrDeleteData.push(iLoop);
				}
			}

			while (true) 
			{
				iChkIndex = arrDeleteData.pop();//체크된 인덱스
				this.ds_Userlist.deleteRow(iChkIndex);
				
				if (arrDeleteData.length == 0) {
					break;
				}
			}
			
		}
	}
};

