 * 10.1. [사용자함수] 선택한 행 리턴
 ***********************************************************************************************/ 
this.fn_SelectRow = function(iRow)
{
	if (iRow > -1) {	
	
        var rtnValue = "({" +
                        ' SCHM_CD:"' + this.ds_Master10.getColumn(iRow, "SCHM_CD") + '"'+  //0.학교코드
                        ',SCHM_NM:"' + this.ds_Master10.getColumn(iRow, "SCHM_NM") + '"'+  //1.학교명
						',SCHM_GU:"' + this.ds_Master10.getColumn(iRow, "SCHM_GU") + '"'+  //2.학교구분
						
                "})";

					
		this.gfn_PopClose(rtnValue);
	} else {
		this.gfn_Alert("SM046");//선택된 내용이 없습니다.
		return;		
	}
}


