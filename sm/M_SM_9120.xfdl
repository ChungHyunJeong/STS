 * 8. [서비스] CallBack 함수 처리부분
 ***********************************************************************************************/
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "COMBO_SEARCH": //일반콤보 조회		
			this.ds_Modm.insertRow(0);
			this.ds_Modm.setColumn(0,"MOLM_NM","ALL");
			this.div_Search.form.cbo_ModmCd.set_index(0);
			break;
		case "MST_SEARCH": //조회	
			if(this.ds_Master.rowcount == 0){
				this.gfn_Alert("SM044"); //조회된 데이터가 없습니다.
			}
			break;
		
		case "SAVE":   //저장		
			break;	
		
		case "DELETE": //삭제		
			break;		
		case "fn_PopupCallback": //프로그램 이력
			break;		
			
	}
};

/***********************************************************************************************
 * 9. [팝업] CallBack 함수 처리부분
      strId : 팝업호출시 지정한 팝업ID
	  rtnValue : 팝업창에서 close시 지정한 반환값
      주의 : Open(popuptype=modeless)창은 팝업콜백을 지원하지 않음
 ***********************************************************************************************/
this.fn_PopupCallback = function(strId, rtnValue)
{
	//[조회영역]
	//사용자
	if (strId == "S_SYST_ID_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var obj = eval(rtnValue);
			this.div_Search.form.edt_EmpmCd.set_value(obj.EMPM_CD);	
			this.div_Search.form.edt_EmpmNm.set_value(obj.EMPM_NM);
			this.div_Search.form.edt_SystId.set_value(obj.SYST_ID);
		} else {
			//조회조건영역 - 자신의자료 권한여부 체크		
			if (this.gfn_CheckOwnAuth(this.fvComPrgmId)) {
				this.div_Search.form.edt_EmpmCd.set_value(this.gfn_GetSessionValue("EMPM_CD"));
				this.div_Search.form.edt_EmpmNm.set_value(this.gfn_GetSessionValue("EMPM_NM"));
				this.div_Search.form.edt_SystId.set_value(this.gfn_GetSessionValue("SYST_ID"));	
			} else {
				this.div_Search.form.edt_EmpmCd.set_value("");
				this.div_Search.form.edt_EmpmNm.set_value("");
				this.div_Search.form.edt_SystId.set_value("");				
			}
		}
	}else if(strId == "S_PRGM_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var obj = eval(rtnValue);
			this.div_Search.form.edt_PrgmId.set_value(obj.PRGM_ID);	
			this.div_Search.form.edt_PrgmNm.set_value(obj.PRGL_NM);
		} else {
			this.div_Search.form.edt_PrgmId.set_value("");	
			this.div_Search.form.edt_PrgmNm.set_value("");		
		}
	}

};
