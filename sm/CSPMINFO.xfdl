 ***********************************************************************************************/ 
this.div_PopupTitle_btn_Choice_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	//Multi Row 선택인경우
	if (!this.gfn_IsNull(this.fvMultiYn) && this.fvMultiYn == "Y") {
		if (this.ds_Master10.getCaseCount("s_chk == '1'") < 1) {
			this.gfn_Alert("SM046");//선택된 내용이 없습니다.
			return;
		} else {
			this.ds_Master10Temp.clearData();
			this.ds_Master10.filter("");
			this.ds_Master10.filter("s_chk == '1'");
			this.ds_Master10Temp.copyData(this.ds_Master10, true);
			
			this.gfn_PopClose(this.ds_Master10Temp.saveXML());
		}
	} else {
		var mRow = this.ds_Master10.rowposition;		
		this.fn_Choice(mRow);
	}
}

/***********************************************************************************************
 * 13.02. [사용자함수] 선택한 정보 부모창 리턴 함수 
 ***********************************************************************************************/ 
this.fn_Choice = function(iRow) {
	if (iRow > -1) {	
	
		rtnValue = "({" +
                        ' COMM_CD:"' + this.ds_Master10.getColumn(iRow, "COMM_CD") + '"'+  //0.회사코드
                        ',CUSM_CD:"' + this.ds_Master10.getColumn(iRow, "CUSM_CD") + '"'+  //1.거래처코드
                        ',CUSM_NM:"' + this.ds_Master10.getColumn(iRow, "CUSM_NM") + '"'+  //2.거래처명
                        ',CSPM_CD:"' + this.ds_Master10.getColumn(iRow, "CSPM_CD") + '"'+  //3.고객코드
                        ',CSPM_NM:"' + this.ds_Master10.getColumn(iRow, "CSPM_NM") + '"'+  //4.고객명
                        ',CSCM_ID:"' + this.gfn_Nvl(this.ds_Master10.getColumn(iRow, "CSCM_ID"), "") + '"'+  //5.SCM ID
                        ',DEPM_NM:"' + this.gfn_Nvl(this.ds_Master10.getColumn(iRow, "DEPM_NM"), "") + '"'+  //6.부서
                        ',RANK_NM:"' + this.gfn_Nvl(this.ds_Master10.getColumn(iRow, "RANK_NM"), "") + '"'+  //7.직급
                        ',WORK_NM:"' + this.gfn_Nvl(this.ds_Master10.getColumn(iRow, "WORK_NM"), "") + '"'+  //8.담당업무
						',MTEL_NO:"' + this.gfn_Nvl(this.ds_Master10.getColumn(iRow, "MTEL_NO"), "") + '"'+  //9.휴대전화                       
                        ',OTEL_NO:"' + this.gfn_Nvl(this.ds_Master10.getColumn(iRow, "OTEL_NO"), "") + '"'+  //10.사내전화
                        ',EMAL_NM:"' + this.gfn_Nvl(this.ds_Master10.getColumn(iRow, "EMAL_NM"), "") + '"'+  //11.이메일
                        ',USED_YN:"' + this.gfn_Nvl(this.ds_Master10.getColumn(iRow, "USED_YN"), "") + '"'+  //12.사용여부
                        ',OFAX_NO:"' + this.gfn_Nvl(this.ds_Master10.getColumn(iRow, "OFAX_NO"), "") + '"'+  //13.팩스번호
                        ',TAXR_YN:"' + this.gfn_Nvl(this.ds_Master10.getColumn(iRow, "TAXR_YN"), "") + '"'+  //14.세금계산서수신여부
                "})";		

		
	    rtnValue = rtnValue.replace( '\r\n' , '');  //엔터값 삭제(존재시 오류)
           
    //trace(rtnValue);
	
		this.gfn_PopClose(rtnValue);
	} else {
		this.gfn_Alert("SM046");//선택된 내용이 없습니다.
		return;		
	}
}

/***********************************************************************************************
 * 14. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 14.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회	
			//조회시 focus 이동
			this.grd_Master10.setFocus(0);	
			
			this.ds_Master10.addColumn("s_chk", "string");
			
			break;						
	}
};


/***********************************************************************************************
 * 14.02. [팝업] CallBack 함수 처리
          strId : 팝업호출시 지정한 팝업ID
	      rtnValue : 팝업창에서 close시 지정한 반환값
          주의 : Open(popuptype=modeless)창은 팝업콜백을 지원하지 않음
 ***********************************************************************************************/ 
this.fn_PopupCallback = function(strId, rtnValue)
{
	//[조회영역]
	//거래처
	if (strId == "S_CUSM_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);
			
			if (!this.gfn_IsNull(arrRtnVal.CUSM_CD)) {
				this.div_Search.form.edt_CusmCd.set_value(arrRtnVal.CUSM_CD);
				this.div_Search.form.edt_CusmNm.set_value(arrRtnVal.CUSM_NM);
			}
		} else {
			this.div_Search.form.edt_CusmCd.set_value("");
			this.div_Search.form.edt_CusmNm.set_value("");		
		}
	} 		
	
};
 
