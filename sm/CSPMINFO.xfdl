this.fvComPrgmId = "";//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
this.fvCspmNm = "";//[부모창파라메타] 고객명
this.fvCusmCd = "";//[부모창파라메타] 거래처코드
this.fvCusmNm = "";//[부모창파라메타] 거래처명
this.fvMultiYn = "";//[부모창파라메타] 멀티Row 선택여부

/***********************************************************************************************
