/***********************************************************************************************
 * 00.03. Form Onload 이벤트
 ***********************************************************************************************/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{	
	//부모창 파라메타 처리
	this.fvCspmNm = this.getOwnerFrame().CspmNm;//[부모창파라메타] 고객명
	this.fvCusmCd = this.getOwnerFrame().CusmCd;//[부모창파라메타] 거래처코드
	this.fvCusmNm = this.getOwnerFrame().CusmNm;//[부모창파라메타] 거래처명
	this.fvMultiYn = this.getOwnerFrame().MultiYn//[부모창파라메타] 멀티Row 선택여부
	this.fvComPrgmId = this.getOwnerFrame().PrgmId;//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
		 
	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 
}

/***********************************************************************************************
 * 00.04. Form [공통] 초기설정 함수 - 필수
 ***********************************************************************************************/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	this.gfn_SetGrid(this.grd_Master10);//[공통]그리드 이벤트 설정 - 프로젝트목록

}

/***********************************************************************************************
 * 00.05. Form [업무] 초기설정 함수
 ***********************************************************************************************/
this.fn_FormBizInit = function() 
{ 
	
	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	var objCondList = ["SM088|ds_Sm088"];
	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, "");
	
	//콤보 인덱스 설정
	this.div_Search.form.cbo_UsedYn.set_index(1);
	
	if (!this.gfn_IsNull(this.fvCusmCd)) {
		this.div_Search.form.edt_CusmCd.set_value(this.fvCusmCd);
		this.div_Search.form.edt_CusmCd.set_enable(false);
		this.div_Search.form.edt_CusmNm.set_enable(false);
		this.div_Search.form.btn_CusmCd.set_enable(false);		
	}	
	if (!this.gfn_IsNull(this.fvCusmNm)) {
		this.div_Search.form.edt_CusmNm.set_value(this.fvCusmNm);
	}		
	
	if (!this.gfn_IsNull(this.fvCspmNm)) {
		this.div_Search.form.edt_CspmNm.set_value(this.fvCspmNm);
	}

	if (!this.gfn_IsNull(this.fvCusmCd) || !this.gfn_IsNull(this.fvCspmNm)) {
		this.div_PopupTitle_btn_Search_onclick();//조회
	}	
	
	//Multi Row 선택인경우
	if (!this.gfn_IsNull(this.fvMultiYn) && this.fvMultiYn == "Y") {
		this.grd_Master10.setRealColSize("head", 1, 40);
	}

}

