 * 01.01. [공통버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.fn_ComBtnSearch = function()
{ 
	//저장후 재조회시-마스터 데이터셋 이벤트 비활성화
	if (this.fvSchKeyYn) {
		this.ds_Master10.set_enableevent(false);
	}
	
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Master10.clearData();
	this.ds_Input.clearData();
	
	var sSearGu = this.div_Search.form.cbo_SearGu.value;
	var sSqlId = "";
	var sOutDatset = "";
	
	//[업무] 조회조건 입력 : 거래처관리 대상(고객정보는 거래처 row 조회시)
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_FromDtFr.value);	//취득일자/운영일자
	this.ds_Input.setColumn(0, "CLOS_DD", this.div_Search.form.cal_ClosDtTo.value);	//취득일자/운영일자
	this.ds_Input.setColumn(0, "EDMM_NO", this.div_Search.form.edt_EdmmNo.value);	//자산번호
	this.ds_Input.setColumn(0, "EDCM_CD", this.div_Search.form.edt_EdcmCd.value);	//자산분류
	this.ds_Input.setColumn(0, "DEPM_CD", this.div_Search.form.edt_DepmCd.value);	//부서
	this.ds_Input.setColumn(0, "SESU_ID", this.div_Search.form.edt_SystId.value);	//사번
	this.ds_Input.setColumn(0, "SEAR_GU", this.div_Search.form.cbo_SearGu.value);	//조회구분
	if (sSearGu == '2') { 	//조회구분 운영 (재고:0, 반납대상:1, 운영:2)
		this.ds_Input.setColumn(0, "OPST_CD", this.div_Search.form.cbo_OpstCd.value);	//운영상태
		sSqlId = "sm:M_SM_6300_M_SL20";
		sOutDatset = "ds_Master10=ds_Master20";//출력 데이터셋
	} else {	//조회구분 재고/반납대상  (재고:0, 반납대상:1, 운영:2)
		sSqlId = "sm:M_SM_6300_M_SL10";	
		sOutDatset = "ds_Master10=ds_Master10";//출력 데이터셋
	}

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = sSqlId;
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MST10_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_SM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
}

