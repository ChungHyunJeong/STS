 ***********************************************************************************************/ 
this.fn_ComBtnAdd = function()
{
//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}

	var iPrevRow = this.ds_Master10.rowposition;		//이전 Row
	var sPrevEdahNm = this.ds_Master10.getColumn(iPrevRow, "EDMM_NO");//전자자산관리번호 (이전)

	if (iPrevRow > -1) {	//Row가 존재할 시 생성가능(사유 - 전자자산관리번호가 있어야 함)

		this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
		var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Master10);//crud_flag 변경-입력(I)
		var sToDay = this.gfn_GetCommDate("toDay");//현재일자

		this.ds_Master10.setColumn(iAddRow, "EDAH_DD", sToDay);//운영일자-오늘날자로 설정
		this.ds_Master10.setColumn(iAddRow, "EDMM_NO", sPrevEdahNm);//전자자산관리번호
		this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//운영부서코드	
		this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//운영부서명	
		this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//운영자사번	
		this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//운영자명		
		this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//운영자ID	
		this.div_DetailInput.form.tab_Detail.tpg_01.form.cbo_OpstCd.set_index(-1);			//운영상태 초기화
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FAIL_YN", "0");			//고장여부
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "USED_YN", "1");			//사용여부
		this.div_DetailInput.form.tab_Detail.tpg_01.form.cbo_WaloCd.set_index(-1);			//창고위치 초기화

		var crudType = this.ds_Master10.getColumn(iAddRow, "crudType");//crudType
		if (crudType == 'I') {
			//입력영역 활성화
			this.div_MasterInput.set_enable(true);
			this.div_DetailInput.form.tab_Detail.set_enable(true);
			
			//입력항목 활성화
			this.div_DetailInput.form.tab_Detail.tpg_01.form.cal_EdahDd.set_enable(true);	//운영일자
			this.div_DetailInput.form.tab_Detail.tpg_01.form.cbo_OpstCd.set_enable(true);	//운영상태
			this.div_DetailInput.form.tab_Detail.tpg_01.form.cal_RetuDd.set_enable(true);	//반납예정일
			this.div_DetailInput.form.tab_Detail.tpg_01.form.cbo_MultYn.set_enable(true);	//복수지급해제
			this.div_DetailInput.form.tab_Detail.tpg_01.form.txt_RemkNm.set_readonly(false);	//비고
			this.div_DetailInput.form.tab_Detail.tpg_01.form.edt_EmpmCd.set_readonly(false);
		
			//처음 입력 항목에 포커스
			this.div_DetailInput.form.tab_Detail.tpg_01.form.cal_EdahDd.setFocus(); //운영일자
		}
		
		this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
	}
	
}

