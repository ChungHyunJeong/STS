/***********************************************************************************************
 *    04. [조회구분]
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *	  [조회영역-Combo] 조회구분 선택시 항목 활성화/비활성화
  ---------------------------------------------------------------------------------------------*/
this.div_Search_cbo_SearGu_onitemchanged = function(obj:nexacro.Combo,e:nexacro.ItemChangeEventInfo)
{
	var sSearGu = this.div_Search.form.cbo_SearGu.value;
	//조회구분이 재고/반납대상 = 비활성화, 운영 = 활성화

	if (sSearGu == '2') {	//조회구분 운영 (재고:0, 반납대상:1, 운영:2)
		this.div_Search.form.cal_FromDtFr.set_enable(true);		//운영일자-시작
		this.div_Search.form.cal_ClosDtTo.set_enable(true);		//운영일자-종료
		this.div_Search.form.cbo_OpstCd.set_enable(true);		//운영상태_콤보
		this.div_Search.form.cbo_OpstCd.set_index(0);			//운영상태 초기화
	} else {
		this.div_Search.form.cal_FromDtFr.set_enable(false);		//운영일자-시작
