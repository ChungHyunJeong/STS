this.fn_FormOnsize = function(obj:nexacro.Form,e:nexacro.SizeEventInfo)
{
	if (obj.width > 1300) {
		this.grd_Master10.set_autofittype("col");
		this.grd_Detail10.set_autofittype("col");
		
	} else {
		this.grd_Master10.set_autofittype("none");
		this.grd_Detail10.set_autofittype("none");
	}
};
 
/***********************************************************************************************
 * 1. 조회 영역
 ***********************************************************************************************/
/***********************************************************************************************
 * 01.01. [팝업버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.div_PopupTitle_btn_Search_onclick = function()
{ 

	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;	
	
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Input.clearData();
	this.ds_Master10.clearData();
	this.ds_Detail10.clearData();
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();

	if (!this.gfn_IsNull(this.fvPjthNo)) {
		this.ds_Input.setColumn(0, "PJTH_NO", this.fvPjthNo);
		this.fvPjthNo = "";
	} else {
		this.ds_Input.setColumn(0, "PJTH_NM", this.div_Search.form.edt_PjthNm.value);//프로젝트명	
		this.ds_Input.setColumn(0, "CUSM_CD", this.div_Search.form.edt_CusmCd.value);//거래처
		this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_FromDd.value);
		this.ds_Input.setColumn(0, "CLOS_DD", this.div_Search.form.cal_ClosDd.value);
		this.ds_Input.setColumn(0, "DEPM_CD", this.div_Search.form.edt_DepmCd.value);
		this.ds_Input.setColumn(0, "SESU_ID", this.div_Search.form.edt_SystId.value);	
