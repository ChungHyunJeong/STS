this.fn_Choice = function(mRow, dRow) {
	if (mRow > -1) {	
	
        var rtnValue = "({" +
                        ' COMM_CD:"' + this.ds_Master10.getColumn(mRow, "COMM_CD") + '"'+  //0.회사코드
                        ',PJTH_NO:"' + this.ds_Master10.getColumn(mRow, "PJTH_NO") + '"'+  //1.프로젝트번호
