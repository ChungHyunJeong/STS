/***********************************************************************************************
 * 15.08. 메일발송
 ***********************************************************************************************/ 
this.fn_SendMail = function() {

	nApp.gds_MailResult.clearData();//메일전송결과
	this.ds_MailUserList.clearData();//메일 발/수신자 정보
	this.ds_Mail.clearData();//메일정보
	this.ds_MailAtchFile.clearData();//첨부파일 정보
	
	var iRow = this.ds_Master10.rowposition;
	
	//메일전송 데이터 설정
	//메일정보
	this.ds_Mail.addRow();	
	this.ds_Mail.setColumn(0, "SUBJ_NM", this.ds_Master10.getColumn(iRow, "TITL_NM"));//메일제목
	this.ds_Mail.setColumn(0, "CONT_NM", this.ds_Master10.getColumn(iRow, "MAIL_NM"));//메일내용
	this.ds_Mail.setColumn(0, "MAIL_TP", "");//메일타입	
	
	//메일 발/수신자 정보
	var sMAIL_FR = this.ds_Master10.getColumn(iRow, "SDMA_NM");//메일전송자
	var sRVMA_NM = "";//수신자메일주소
	var sMAIL_TO = "";//메일수신자
	var sMALB_GU = "";//발송구분
	var sMAIL_CC = "";//메일참조
	var sMAIL_BC = "";//메일숨은참조
	var sNOTI_TO = "";//수신확인을 리턴받을 메일주소
	var iAddRow = 0;
	
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		sRVMA_NM = this.ds_Detail10.getColumn(iLoop, "RVMA_NM");//수신자메일주소		
		sMALB_GU = this.ds_Detail10.getColumn(iLoop, "MALB_GU");//발송구분
		
		sMAIL_TO = "";//메일수신자
		sMAIL_CC = "";//메일참조
		sMAIL_BC = "";//메일숨은참조
		sNOTI_TO = "";//수신확인을 리턴받을 메일주소	
		
		//발송구분 : 0020 참조
		if (sMALB_GU == "0020") {
			sMAIL_CC = sRVMA_NM;//메일참조
		//발송구분 : 0030 숨은참조
		} else if (sMALB_GU == "0030") {
			sMAIL_BC = sRVMA_NM;//메일숨은참조
		} else {
			sMAIL_TO = sRVMA_NM;//메일수신자
		}

		iAddRow = this.ds_MailUserList.addRow();
		this.ds_MailUserList.setColumn(iAddRow, "MAIL_FR", sMAIL_FR);//메일전송자	
		this.ds_MailUserList.setColumn(iAddRow, "MAIL_TO", sMAIL_TO);//메일수신자	
		this.ds_MailUserList.setColumn(iAddRow, "MAIL_CC", sMAIL_CC);//메일참조	
		this.ds_MailUserList.setColumn(iAddRow, "MAIL_BC", sMAIL_BC);//메일숨은참조	
		this.ds_MailUserList.setColumn(iAddRow, "NOTI_TO", sNOTI_TO);//수신확인을 리턴받을 메일주소	
	}	
	
	var sPATH_NM = "";//파일경로	
	var sFILE_ID = "";//서버 저장된 파일명		
	var sFILE_NM = "";//원본파일명
		
	//첨부파일 정보
	if (this.fvFileDiv.ds_Smfilb.getRowCount() > 0) {

		for (var iLoop=0; iLoop<this.fvFileDiv.ds_Smfilb.getRowCount(); iLoop++) {

			sPATH_NM = this.fvFileDiv.ds_Smfilb.getColumn(iLoop, "PATH_NM");//파일경로		
			sFILE_ID = this.fvFileDiv.ds_Smfilb.getColumn(iLoop, "FILE_ID");//저장된 파일명
			sFILE_NM = this.fvFileDiv.ds_Smfilb.getColumn(iLoop, "FILE_NM");//원본파일명
			
			iAddRow = this.ds_MailAtchFile.addRow();
			this.ds_MailAtchFile.setColumn(iAddRow, "PATH_NM", sPATH_NM);//파일경로			
			this.ds_MailAtchFile.setColumn(iAddRow, "FILE_ID", sFILE_ID);//서버 저장된 파일명		
			//this.ds_MailAtchFile.setColumn(iAddRow, "FILE_NM", sFILE_NM);//원본파일명
			this.ds_MailAtchFile.setColumn(iAddRow, "FILE_NM", sFILE_ID);//원본파일명
		}
	}	
	
	//첨부파일 정보-업무화면
	if (this.fvBizFileDiv.ds_Detail20.getRowCount() > 0) {

		for (var iLoop=0; iLoop<this.fvBizFileDiv.ds_Detail20.getRowCount(); iLoop++) {

			sPATH_NM = this.fvBizFileDiv.ds_Detail20.getColumn(iLoop, "PATH_NM");//파일경로		
			sFILE_ID = this.fvBizFileDiv.ds_Detail20.getColumn(iLoop, "FILE_ID");//저장된 파일명
			sFILE_NM = this.fvBizFileDiv.ds_Detail20.getColumn(iLoop, "FILE_NM");//원본파일명
			
			iAddRow = this.ds_MailAtchFile.addRow();
			this.ds_MailAtchFile.setColumn(iAddRow, "PATH_NM", sPATH_NM);//파일경로			
			this.ds_MailAtchFile.setColumn(iAddRow, "FILE_ID", sFILE_ID);//서버 저장된 파일명		
			this.ds_MailAtchFile.setColumn(iAddRow, "FILE_NM", sFILE_NM);//원본파일명
		}
	}		
	
	//메일전송 유효성 체크
	if (!this.fn_MailSendValidation()) return;	

	nApp.gds_MailResult.clearData();	
	
   	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);	
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MAIL_SEND";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = "MailAction";//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sMethodName = "Send";// Action 안에서 호출해야 하는 메소드 명, 기본은 execute를 호출하도록 되어있음	
	svcCallObj.sInputDaset = "ds_Mail=ds_Mail ds_UserList=ds_MailUserList ds_MailAtchFile=ds_MailAtchFile" //입력 데이터셋
	svcCallObj.sOutDatset = "gds_MailResult=gds_MailResult";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명	
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
}

