/***********************************************************************************************
 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{   

		case "MST_SEARCH": //조회		

			if (this.ds_Master10.getRowCount() > 0) {
			
				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master10.findRowExpr("MALH_NO=='" + this.fvSchKey01 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
									
				}				
				
				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
				this.div_DetailInput.form.tab_Detail.set_enable(true);
					
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
				
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterView.form.grd_Master10);
				
			} else {
										
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterView.form.grd_Master10);
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			
				//입력영역 비활성화
				this.div_MasterInput.set_enable(false);
				this.div_DetailInput.form.tab_Detail.set_enable(false);
					
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = false;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);					
			}
		
			if (this.ds_Master10.getRowCount() > 0) {

				if (this.fvWebCompLoadYn) {					
					this.setTimer(998,500);
				}					
			} else {

				//내용 Web컴포넌트에 값 세팅
				var oWeb = this.div_DetailInput.form.tab_Detail.tpg_01.form.web_ContNm.getProperty("window");				                
				oWeb.callMethod("fn_setData", "");				
			}			
	
			this.fvDtlSearchYn = true;//상세조회여부			
			
			//저장후 재조회여부-true
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}			
	
			break;
			
		case "SAVE":   //저장		

			//첨부파일 추가건이 없으면
			if (!this.fvFileDiv.fn_CheckFileAddYn()) {
				this.fn_SendMail();//메일전송
			} else {
				this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);			
			}

			break;			
		
		case "MAIL_SEND": //메일전송
				trace(nApp.gds_MailResult.saveXML());
				if (nApp.gds_MailResult.getRowCount() > 0) {
				
					var sRSLT_CD = nApp.gds_MailResult.getColumn(0, "RSLT_CD");//메시지코드
					var sRSLT_MG = nApp.gds_MailResult.getColumn(0, "RSLT_MG");//메시지내용
					
					if (sRSLT_CD == "E") {
						var arrMsg = [sRSLT_MG];
						this.gfn_Alert("SM115", arrMsg);//이메일 발송중 오류가 발생했습니다.\n&ITM0
					} else {
						this.gfn_Alert("SM114");//이메일이 정상 발송되었습니다.
						this.fvSchKeyYn = true;//저장후 재조회여부
						this.fn_ComBtnSearch();//저장후 재조회
					}
				}

			break;
					
		case "DTL_SEARCH": //조회		
			this.ds_Detail10.addColumn("s_chk", "string");
			this.ds_Detail10.addColumn("crudType", "string");
			this.ds_Detail10.addColumn("MAIL_GU", "string");
			this.ds_Detail20.addColumn("S_CHK", "string");
			this.ds_Detail20.addColumn("crudType", "string");
			
			if (this.ds_Detail20.getRowCount() > 0) {
				this.fvBizFileDiv.ds_Detail20.copyData(this.ds_Detail20);
			}
			
			//첨부파일 정보 조회
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
			oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MALH_NO");//PK번호2-메일번호				
			
			this.fvFileDiv.fn_AtchFileSearch(oFileInfo);			
			
			if (this.ds_Detail10.getRowCount() > 0) {
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master11);
			} else {
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master11);			
			}
			break;			

		case "DELETE": //삭제		
			this.fn_ComBtnSearch();//조회
			break;	
			
	}
};

