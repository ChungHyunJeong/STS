		oWeb.callMethod("fn_setData", this.gfn_ReplaceWebEditorData(sContNm));	
		
	}
	
	//내용 Web컴포넌트에 값 세팅
	var oWeb = this.div_DetailInput.form.tab_Detail.tpg_01.form.web_ContNm.getProperty("window");
	var iHeight = this.div_DetailInput.form.tab_Detail.tpg_01.form.web_ContNm.getOffsetHeight()-60;
	oWeb.callMethod("fn_updateSize", iHeight);	
	
	this.fvWebCompLoadYn = true;//웹컴포넌트 로드 완료여부	
	
	//결재화면에서 팝업으로 호출시
	if (this.fvWfPopupMode) {	
		this.setTimer( 999, 500 );		
	}
	
};
  
/***********************************************************************************************
 * 02.DG0.08. [Detail Tab] 변경 이벤트
 ***********************************************************************************************/
this.div_DetailInput_tab_Detail_onchanged = function(obj:nexacro.Tab,e:nexacro.TabIndexChangeEventInfo)
{
	var iTabIndex = e.postindex;
	
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);

	//Tab-메일발송상세
	if (iTabIndex == 0) {
		this.div_DetailInput.form.div_ComGrdBtn.set_url("");
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);
	//Tab-첨부파일
