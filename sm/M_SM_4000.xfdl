		this.ds_Detail10.setColumn(iAddRow, "MALB_GU", this.gfn_Nvl(sMALB_GU,""));//메일발송구분(공통코드(SM400)-0010:수신,0020:참조,0030:숨은참조)
		this.ds_Detail10.setColumn(iAddRow, "REMK_NM", this.gfn_Nvl(sREMK_NM,""));//비고
		this.ds_Detail10.setColumn(iAddRow, "crudType", "I");//crudType
	}
	
	this.ds_Detail10.set_enableevent(true);
	
	if (this.ds_Detail10.getRowCount() > 0) {
		this.ds_Detail10.set_rowposition(0);
	}	
	
	//첨부파일 정보
	if (this.fvMailAtchFileList.length > 0) {
		var objMailAtchFile = new Object();//첨부파일 정보
		var sPATH_NM = "";//파일경로	
		var sFILE_ID = "";//서버 저장된 파일명		
		var sFILE_NM = "";//원본파일명
	
		for (var iLoop=0; iLoop<this.fvMailAtchFileList.length; iLoop++) {
			objMailAtchFile = this.fvMailAtchFileList[iLoop];

			sPATH_NM = objMailAtchFile.PATH_NM;//파일경로		
			sFILE_ID = objMailAtchFile.FILE_ID;//저장된 파일명
			sFILE_NM = objMailAtchFile.FILE_NM;//원본파일명
			
			iAddRow = this.fvBizFileDiv.ds_Detail20.addRow();
			
			this.fvBizFileDiv.ds_Detail20.setColumn(iAddRow, "crudType", "I");
			this.fvBizFileDiv.ds_Detail20.setColumn(iAddRow, "PATH_NM", sPATH_NM);//파일경로			
			this.fvBizFileDiv.ds_Detail20.setColumn(iAddRow, "FILE_ID", sFILE_ID);//서버 저장된 파일명		
			this.fvBizFileDiv.ds_Detail20.setColumn(iAddRow, "FILE_NM", sFILE_NM);//원본파일명
		}
	}	
	this.btn_SendMail.set_enable(true);//메일발송버튼
}]]></Script>
