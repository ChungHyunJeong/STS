this.fn_FormBizInit = function() 
{
	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	var objCondList = [
					  ];
	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, "");
	
	//[공통-Pivot] 그리드 검색/Pivot 설정
	this.div_Search.form.div_ComGridSearch.form.fn_SetCmmGridProc(this, this.div_MasterView.form.tab_Master);
	
	this.div_Search.form.chk_outEx.set_value("1");
	this.div_Search.form.chk_outProEx.set_value("1");

	//파라미터 값이 있는경우
	if (!this.gfn_IsNull(this.fvYm)) {
		this.div_Search.form.cal_Ym.set_value(this.fvYm);
		this.div_Search.form.mae_CnthNo.set_value(this.fvCnthNo);
		this.div_Search.form.edt_CnthNm.set_value(this.fvCnthNm);
		this.div_Search.form.edt_DepmCd.set_value(this.fvDepmCd);
		this.div_Search.form.edt_DepmNm.set_value(this.fvDepmNm);
		this.div_Search.form.edt_AcomCd.set_value(this.fvAcomCd);
		this.div_Search.form.edt_AcomNm.set_value(this.fvAcomNm);
		
		this.gfn_SetCnthNoMaskFormat(this.div_Search.form.mae_CnthNo);//계약번호 동적 마스킹 처리	
	}

	this.div_PopupTitle_btn_Search_onclick();//조회
}

