		var sInputDaset = "" //입력 데이터셋
		var sOutDatset = "";//출력 데이터셋
		var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object			
			
		//Master 변경건이 있으면 Master 저장
		if (this.gfn_DsIsUpdated(this.ds_Tpg01_Master)) {
			//[공통]서비스 정보 추가
			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
			svcAddObj.sInsertSql = "sm:M_SM_0300_IN01";//저장Sql
			svcAddObj.sUpdateSql = "sm:M_SM_0300_UP01";//수정Sql
			svcAddObj.sDeleteSql = "sm:M_SM_0300_DL01";//삭제Sql
			svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
									 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

			this.gfn_AddComSaveService(this, svcAddObj);
			sInputDaset = "ds_Master=ds_Tpg01_Master:U" //입력 데이터셋

		}
		
		//Detail 변경건이 있으면 Detail 저장
		if (this.gfn_DsIsUpdated(this.ds_Tpg01_Detail)) {
			trace(this.ds_Tpg01_Detail.saveXML());
			//[공통]서비스 정보 추가
			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
			svcAddObj.sInsertSql = "sm:M_SM_0300_IN02";//저장Sql
			svcAddObj.sUpdateSql = "sm:M_SM_0300_UP02";//수정Sql
			svcAddObj.sDeleteSql = "sm:M_SM_0300_DL02";//삭제Sql
			svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
									 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

			this.gfn_AddComSaveService(this, svcAddObj);
			if (this.gfn_IsNull(sInputDaset)) {
				sInputDaset = "ds_Detail=ds_Tpg01_Detail:U" //입력 데이터셋
			} else {
				sInputDaset = sInputDaset + " ds_Detail=ds_Tpg01_Detail:U" //입력 데이터셋
			}
		
		}
		
	}else if(this.div_MasterInput.form.tab_Master.tabindex==1){ //환경설정코드 추가
		//변경여부 체크
		if (!this.gfn_DsIsUpdated(this.ds_Tpg02_Master)) {
			this.gfn_Alert("SM036");//변경된 내용이 없습니다.
			return;
		}
		//저장전 유효성 체크
		//Master 데이터 유효성 체크
		if (this.gfn_DsIsUpdated(this.ds_Tpg02_Master)) {
			if (!this.fn_VaildTpg02MasterSave()) return;
		}
		//Detail 변경건이 있으면 Detail 저장
		if (this.gfn_DsIsUpdated(this.ds_Tpg02_Master)) {
			
			//[공통]서비스 정보 추가
			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
			svcAddObj.sInsertSql = "sm:M_SM_0300_IN00";//저장Sql
			svcAddObj.sUpdateSql = "sm:M_SM_0300_UP00";//수정Sql
			svcAddObj.sDeleteSql = "sm:M_SM_0300_DL00";//삭제Sql
			svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
									 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

			this.gfn_AddComSaveService(this, svcAddObj);
			if (this.gfn_IsNull(sInputDaset)) {
				sInputDaset = "ds_Master=ds_Tpg02_Master:U" //입력 데이터셋
			} else {
				sInputDaset = sInputDaset + " ds_Master=ds_Tpg02_Master:U" //입력 데이터셋
			}
		
		}
	}

	
	//[공통]서비스 호출 변수
	var svcCallObj 			= new Object();	//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid 		= "SAVE";		//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName 	= this.G_SV_SM;	//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset 	= sInputDaset 	//입력 데이터셋
	svcCallObj.sOutDatset 	= sOutDatset;	//출력 데이터셋
	svcCallObj.sOtherArg 	= sOtherArg;	//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명		
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	

}

