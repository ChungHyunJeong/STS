/***********************************************************************************************
 * 8. [서비스] CallBack 함수 처리부분
 ***********************************************************************************************/
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
	
		case "COMBO_SEARCH": //일반콤보 조회		
			this.div_Search.form.cbo_ModmCd.set_index(0);
			break;
			
		case "TOP01_MST_SEARCH": //환경설정 관리 조회		
			this.ds_Tpg01_Master.addColumn("s_chk", "string");
			this.ds_Tpg01_Master.addColumn("crudType", "string");
			if(this.ds_Tpg01_Master.rowcount == 0){
				this.gfn_Alert("SM044"); //조회된 데이터가 없습니다.
			}else{
			//	this.fn_DetailSearch();

			}
			break;
			
		case "TOP01_DTL_SEARCH": //상세조회		
			this.ds_Tpg01_Detail.addColumn("s_chk", "string");
			this.ds_Tpg01_Detail.addColumn("crudType", "string");
			if(this.ds_Tpg01_Detail.rowcount == 0){
				//this.gfn_Alert("SM044"); //조회된 데이터가 없습니다.
			}
			
			//기존 RowPosition 위치로 이동
			//if(this.fvMasterRowPosition  != 0 ){
				//this.ds_Tpg01_Master.set_rowposition(this.fvMasterRowPosition);
				//this.fvMasterRowPosition =0;
			//}
			break;
			
		case "TOP02_MST_SEARCH": //환경설정코드 조회		
			this.ds_Tpg02_Master.addColumn("s_chk", "string");
			this.ds_Tpg02_Master.addColumn("crudType", "string");
			if(this.ds_Tpg02_Master.rowcount == 0){
				//this.gfn_Alert("SM044"); //조회된 데이터가 없습니다.
			}
			
			break;
		
		case "SAVE":   //저장		
			if(this.div_MasterInput.form.tab_Master.tabindex==0){	//환경설정관리 추가
				//this.fvMasterRowPosition = this.ds_Tpg01_Master.rowposition;		
			}
			
			this.fn_ComBtnSearch();//조회
			break;	
		
		case "DELETE": //삭제		
			this.fn_ComBtnSearch();//조회
			break;							
	}
};

