		if (!this.gfn_DsIsUpdated(this.ds_Tpg01_Master) && !this.gfn_DsIsUpdated(this.ds_Tpg01_Detail)) {
			this.gfn_Alert("SM036");//변경된 내용이 없습니다.
			return;
		}
		
		//저장전 유효성 체크
		//Master 데이터 유효성 체크
		if (this.gfn_DsIsUpdated(this.ds_Tpg01_Master)) {
			if (!this.fn_VaildTpg01MasterSave()) return;
		}
		
		//저장전 유효성 체크
		//Master 데이터 유효성 체크
		if (this.gfn_DsIsUpdated(this.ds_Tpg01_Detail)) {
			if (!this.fn_VaildTpg01DetailSave()) return;
		}
		
		//[공통]서비스 정보 초기화
		this.gfn_InitComService(this);
		
		//[공통]서비스정보 추가
