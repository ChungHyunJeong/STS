this.fn_SvcDelete = function() 
{ 
	//삭제전 유효성 체크
	//Master 데이터 유효성 체크
	if (!this.fn_VaildDetailDelete()) return;
	
	//삭제키 정보 설정
	this.ds_DetailDelete.clearData();
	this.ds_DetailDelete.addRow();
	this.ds_DetailDelete.setColumn(0, "PGRM_CD", this.ds_Detail.getColumn(this.ds_Detail.rowposition, "PGRM_CD"));//프로그램ID
	this.ds_DetailDelete.setColumn(0, "PRGM_ID", this.ds_Detail.getColumn(this.ds_Detail.rowposition, "PRGM_ID"));//프로그램ID
	this.ds_DetailDelete.setColumn(0, "crudType", "D");//삭제-Flag
	
	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//Master 삭제
