 /*---------------------------------------------------------------------------------------------
 * 7.2.3. [Detail영역-그리드] HEAD 클릭 이벤트
  ---------------------------------------------------------------------------------------------*/
  
this.div_DetailView_tab_Detail_tpg_01_grd_Detail_onheadclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	//this.gfn_Grid_onheadclick(obj, e);
	
	if (e.col > 5) {	// 권한부여 이후 모든대상
		var sChk = obj.getCellProperty("head", e.cell, "text");
		
		if (this.gfn_IsNull(sChk) || sChk == '0' || sChk == '1') {	// 권한부여 Title이 아닌 체크박스 대상
			var sCheckValue = "0";//체크박스 값
			var sColumn = "";//클릭한 헤더와 관련된 컬럼ID
			
			sColumn = obj.getCellProperty("body", e.col, "text").replace("bind:", "");

			if (this.gfn_IsNull(sChk) || sChk == "0") {
				sCheckValue = "1";//체크박스 값
				obj.setCellProperty("head", e.cell, "text", sCheckValue);
			} else {
				sCheckValue = "0";//체크박스 값
				obj.setCellProperty("head", e.cell, "text", sCheckValue);
			}
			
			var sUSED_YN = "";//권한부여
			
			for (var iLoop=0; iLoop < this.ds_Detail.getRowCount(); iLoop++) {
			
				if (sColumn != "USED_YN") {
					sUSED_YN = this.ds_Detail.getColumn(iLoop, "USED_YN");//권한부여
					
					if (sUSED_YN == "1") {
						this.ds_Detail.setColumn(iLoop, sColumn, sCheckValue);
					}
				} else {
					this.ds_Detail.setColumn(iLoop, sColumn, sCheckValue);
				}
			}
		}
	}	
};


/***********************************************************************************************
 * 8. [서비스] CallBack 함수 처리부분
 ***********************************************************************************************/
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "COMBO_SEARCH": //조회		
			//콤보 인덱스 설정
			this.div_Search.form.cbo_PgrmCd.set_index(0);

			//화면Open시 자동조회
			this.fn_ComBtnSearch();

			break;
		case "MST_SEARCH": //조회		
			if (this.ds_Master.getRowCount() > 0) {
				this.div_MasterView.form.grd_Master.setTreeStatus(0, true);
			}
			break;
		case "DTL_SEARCH": //조회		
			this.ds_Detail.addColumn("crudType", "string");
			break;
		case "SAVE":   //저장		
			this.fn_ComBtnSearch();//조회
			break;	
		case "DELETE": //삭제		
			this.fn_ComBtnSearch();//조회
			break;							
	}
	
	
};

