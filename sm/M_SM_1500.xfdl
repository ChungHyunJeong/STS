		this.ds_Input.addRow();
		this.ds_Input.setColumn(0, "OBJ_ID", obj.getColumn(e.newrow, "OBJ_ID"));
		this.ds_Input.setColumn(0, "CATALOG", obj.getColumn(e.newrow, "CATALOG"));
		
		//[공통]서비스정보 초기화
		this.gfn_InitComService(this);
		//[공통]서비스정보 추가
