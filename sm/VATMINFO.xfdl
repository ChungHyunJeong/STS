this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{	
	//부모창 파라메타 처리
	this.fvVatmGu = this.getOwnerFrame().VatmGu;//[부모창파라메타] 매입매출구분
	this.fvVatmNm = this.getOwnerFrame().VatmNm;//[부모창파라메타] 부가세명	
	this.fvComPrgmId = this.getOwnerFrame().PrgmId;//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
	 
	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 
	
}

/***********************************************************************************************
 * 00.04. Form [공통] 초기설정 함수 - 필수
 ***********************************************************************************************/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	this.gfn_SetGrid(this.grd_Master10);//[공통]그리드 이벤트 설정 - 프로젝트목록

}

/***********************************************************************************************
 * 00.05. Form [업무] 초기설정 함수
 ***********************************************************************************************/
this.fn_FormBizInit = function() 
{ 
	
	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	// 2021.08.26 매입매출구분 공통코드 변경 FA060 -> FA072
	var objCondList = ["FA070|ds_Fa070","FA072|ds_Fa072"];
	
	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, "");
	
	//콤보 인덱스 설정
	this.div_Search.form.cbo_VatmGu.set_index(0);
	
	if (!this.gfn_IsNull(this.fvVatmGu)) {
		this.div_Search.form.cbo_VatmGu.set_value(this.fvVatmGu);
	}	

	if (!this.gfn_IsNull(this.fvVatmNm)) {
		this.div_Search.form.edt_VatmNm.set_value(this.fvVatmNm);
	}

	if (!this.gfn_IsNull(this.fvVatmGu) || !this.gfn_IsNull(this.fvVatmNm)) {
		this.div_PopupTitle_btn_Search_onclick();//조회
	}	

}

