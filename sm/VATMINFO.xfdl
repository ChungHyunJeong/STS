this.div_PopupTitle_btn_Close_onclick = function()
{ 
	this.gfn_PopClose();
}

/***********************************************************************************************
 * 13. 선택
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [팝업버튼] 선택 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.div_PopupTitle_btn_Choice_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var mRow = this.ds_Master10.rowposition;
	this.fn_Choice(mRow);
}

/***********************************************************************************************
 * 13.02. [사용자함수] 선택한 정보 부모창 리턴 함수 
 ***********************************************************************************************/ 
this.fn_Choice = function(iRow) {
	if (iRow > -1) {	
	
		rtnValue = "({" +
                        ' COMM_CD:"' + this.ds_Master10.getColumn(iRow, "COMM_CD") + '"'+  //0.회사코드
                        ',VATM_CD:"' + this.ds_Master10.getColumn(iRow, "VATM_CD") + '"'+  //1.부가세코드
                        ',VATM_NM:"' + this.ds_Master10.getColumn(iRow, "VATM_NM") + '"'+  //2.부가세명
                        ',VATM_RT:"' + this.ds_Master10.getColumn(iRow, "VATM_RT") + '"'+  //3.부가세율
                        ',VATY_CD:"' + this.ds_Master10.getColumn(iRow, "VATY_CD") + '"'+  //4.계산서종류(공통)
                        ',VATM_GU:"' + this.ds_Master10.getColumn(iRow, "VATM_GU") + '"'+  //5.매입매출구분(공통)
                "})";


					
		this.gfn_PopClose(rtnValue);
	} else {
		this.gfn_Alert("SM046");//선택된 내용이 없습니다.
		return;		
	}
}

