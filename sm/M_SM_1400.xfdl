/***********************************************************************************************
 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
	
			if (this.ds_Master10.getRowCount() > 0) {
			
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterView.form.grd_Master10);
				
			} else {
				
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterView.form.grd_Master10);
			}
			break;
		case "COMBO_SEARCH": //콤보조회-모듈
			if (this.ds_Modm.getRowCount() > 0) {
				this.ds_Modm.insertRow(0);
				this.ds_Modm.setColumn(0, "MOLM_NM", "ALL");
				//콤보 인덱스 설정
				this.div_Search.form.cbo_ModmCd.set_index(0);
				
				this.fn_ComBtnSearch();//조회
			}
		    break;
		case "SAVE":   //저장		
			this.fn_MenuReflash();//글로벌 메뉴정보 재조회
			this.fn_ComBtnSearch();//저장후 재조회

			break;	
		case "DTL_SEARCH": //조회		
			this.ds_Master11.addColumn("crudType", "string");
			
			if (this.ds_Master11.getRowCount() > 0) {
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master11);
			} else {
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master11);			
			}
			break;			
			
		case "DELETE": //삭제		
			this.fn_ComBtnSearch();//조회
			break;							
	}
};

/***********************************************************************************************
 * 13.02. [팝업] CallBack 함수 처리
          strId : 팝업호출시 지정한 팝업ID
	      rtnValue : 팝업창에서 close시 지정한 반환값
          주의 : Open(popuptype=modeless)창은 팝업콜백을 지원하지 않음
 ***********************************************************************************************/ 
this.fn_PopupCallback = function(strId, rtnValue)
{
	//부서
	if (strId == "M_DPCD_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.DEPM_CD)) {
				this.ds_Master11.setColumn(this.ds_Master11.rowposition, "DPCD_CD", arrRtnVal.DEPM_CD);
				this.ds_Master11.setColumn(this.ds_Master11.rowposition, "DEPM_NM", arrRtnVal.DEPM_NM);
			}
		}				
	} 
};
 
