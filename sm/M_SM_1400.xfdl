/***********************************************************************************************
 * 02.MG0.07. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
 this.ds_Master11_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	var iRow = e.row;
	var sPRGD_YN = this.ds_Master11.getColumn(iRow, "PRGD_YN");//개인설정여부
	var sSYST_ID = this.ds_Master11.getColumn(iRow, "SYST_ID");//시스템사번	
	//사용여부 체크시
	if (sPRGD_YN == "1") {
		//기등록건이 있으면
		if (!this.gfn_IsNull(sSYST_ID)) {
			this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
		} else {
			this.ds_Master11.setColumn(iRow, "crudType", "I");//저장
			this.ds_Master11.setColumn(iRow, "PRGM_ID", this.ds_Master11.getColumn(iRow, "MENU_ID"));
		}
		
		//출고부서
		if (e.columnid == "DPCD_CD") {
			this.gfn_GetCommDepmGrid(obj, "DPCD_CD", "DEPM_NM");//부서명 검색
		}	
	} else {
		//기등록건이 있으면
		if (!this.gfn_IsNull(sSYST_ID)) {
			this.ds_Master11.setColumn(iRow, "crudType", "D");//삭제
		} else {
			this.ds_Master11.setColumn(iRow, "crudType", "");
		}
	}
};


