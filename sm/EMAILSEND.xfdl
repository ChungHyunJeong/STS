this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MAIL_SEND": //전송	
			//gds_MailResult
			trace(nApp.gds_MailResult.saveXML());
			break;						
	}
};

/***********************************************************************************************
 * 14.02. [팝업] CallBack 함수 처리
          strId : 팝업호출시 지정한 팝업ID
	      rtnValue : 팝업창에서 close시 지정한 반환값
          주의 : Open(popuptype=modeless)창은 팝업콜백을 지원하지 않음
 ***********************************************************************************************/ 
this.fn_PopupCallback = function(strId, rtnValue)
{			
	//고객		
	if (strId == "M_CSPM_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			this.ds_Master10Temp.clearData();
			this.ds_Master10Temp.loadXML(rtnValue);
			//trace(this.ds_Master10Temp.saveXML());
			
			if (this.ds_Master10Temp.getRowCount() > 0) {
				var iAddRow = 0;
				for (var iLoop=0; iLoop < this.ds_Master10Temp.getRowCount(); iLoop++) {
					iAddRow = this.gfn_GridInsertProcess(this, this.ds_MailUserList);//crud_flag 변경-입력(I)

					this.ds_MailUserList.setColumn(iAddRow, "SNDR_ID", this.ds_MailUserList.getColumn(0, "SNDR_ID"));//전송자ID
					this.ds_MailUserList.setColumn(iAddRow, "SNDR_NM", this.ds_MailUserList.getColumn(0, "SNDR_NM"));//전송자명
					this.ds_MailUserList.setColumn(iAddRow, "MAIL_FR", this.ds_MailUserList.getColumn(0, "MAIL_FR"));//전송자메일
					
					this.ds_MailUserList.setColumn(iAddRow, "RESV_ID", this.ds_Master10Temp.getColumn(iLoop, "CSPM_CD"));//수신자ID
					this.ds_MailUserList.setColumn(iAddRow, "RESV_NM", this.ds_Master10Temp.getColumn(iLoop, "CSPM_NM"));//수신자명
					this.ds_MailUserList.setColumn(iAddRow, "MAIL_TO", this.ds_Master10Temp.getColumn(iLoop, "EMAL_NM"));//수신자메일
				}
			}
		}			
	}
};
 
