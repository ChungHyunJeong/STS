 ***********************************************************************************************/ 
this.div_PopupTitle_btn_MailSend_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{

	//내용 Web컴포넌트 정보값 세팅
	var oWeb = this.web_ContNm.getProperty("window");
	var sContNm = oWeb.callMethod("fn_getData");//내용
	
	this.ds_Mail.setColumn(0, "CONT_NM", sContNm);
	
	//메일전송 유효성 체크
	if (!this.fn_MailSendValidation()) return;	
	
	nApp.gds_MailResult.clearData();	
	
   	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);	
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MAIL_SEND";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = "MailAction";//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sMethodName = "Send";// Action 안에서 호출해야 하는 메소드 명, 기본은 execute를 호출하도록 되어있음	
	svcCallObj.sInputDaset = "ds_Mail=ds_Mail ds_UserList=ds_MailUserList ds_MailAtchFile=ds_MailAtchFile" //입력 데이터셋
	svcCallObj.sOutDatset = "gds_MailResult=gds_MailResult";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명	
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);		
}

/***********************************************************************************************
 * 13.02 메일전송 유효성체크
 ***********************************************************************************************/ 
this.fn_MailSendValidation = function() {

	//메일내용 체크
	//메일제목 빈값여부 체크
	if (this.gfn_IsNull(this.ds_Mail.getColumn(0, "SUBJ_NM"))) {
		this.edt_SubjNm.setFocus();
		var arrMsgArg = ["메일제목"];
		this.gfn_Alert("SM073", arrMsgArg);//&ITM0 정보가 유효하지 않습니다.
		return false;
	}
	
	//메일내용 빈값여부 체크
	if (this.gfn_IsNull(this.ds_Mail.getColumn(0, "CONT_NM"))) {
		var arrMsgArg = ["메일내용"];
		this.gfn_Alert("SM073", arrMsgArg);//&ITM0 정보가 유효하지 않습니다.	
		return false;
	}		

	//메일 발/수신자 정보 체크
	//[저장] Validation 설정
	var arrArgs = ["MAIL_FR^NULL"
	              ,"MAIL_TO^NULL"
			      ]; 
				  
	var sCheckGroupTitle = this.sta_MailUser.text;//체크그룹 타이틀
				  
	//Grid Vaild 체크
	if (!this.gfn_CheckGridValid(this.grd_MailUserList, arrArgs, sCheckGroupTitle) ) {
		return false;
	}
	
	if (this.ds_MailAtchFile.getRowCount() > 0) {
		//첨부파일 정보 체크
		//[저장] Validation 설정
		var arrArgs = ["PATH_NM^NULL"
					  ,"FILE_ID^NULL"		  
					  ]; 
					  
		var sCheckGroupTitle = this.sta_AtchFile.text;//체크그룹 타이틀
					  
		//Grid Vaild 체크
		if (!this.gfn_CheckGridValid(this.grd_AtchFile, arrArgs, sCheckGroupTitle) ) {
			return false;
		}	
	}
	
	return true;
}
 
 
