/***********************************************************************************************
 * 15.02. 메일전송 파라메타 설정
 ***********************************************************************************************/ 
this.fn_SetMailSendData = function()
{
	nApp.gds_MailResult.clearData();//메일전송결과
	this.ds_MailUserList.clearData();//메일 발/수신자 정보
	this.ds_Mail.clearData();//메일정보
	this.ds_MailAtchFile.clearData();//첨부파일 정보

	var objMailUser = new Object();//메일 발/수신자 정보
	var sMAIL_FR = "";//메일전송자
	var sMAIL_TO = "";//메일수신자
	
	//메일전송 데이터 설정
	//메일정보
	this.ds_Mail.addRow();	
	this.ds_Mail.setColumn(0, "SUBJ_NM", this.fvMail.SUBJ_NM);//메일제목
	this.ds_Mail.setColumn(0, "CONT_NM", this.fvMail.CONT_NM);//메일내용
	this.ds_Mail.setColumn(0, "MAIL_TP", this.fvMail.MAIL_TP);//메일타입
	
	this.fvContNm = this.fvMail.CONT_NM//메일내용
	
	//메일 발/수신자 정보
	var sMAIL_CC = "";//메일참조
	var sMAIL_BC = "";//메일숨은참조
	var sNOTI_TO = "";//수신확인을 리턴받을 메일주소
	var iAddRow = 0;
	
	for (var iLoop=0; iLoop<this.fvMailUserList.length; iLoop++) {
		objMailUser = this.fvMailUserList[iLoop];

		sMAIL_FR = objMailUser.MAIL_FR;//메일전송자
		sMAIL_TO = objMailUser.MAIL_TO;//메일수신자
		sMAIL_CC = objMailUser.MAIL_CC;//메일참조
		sMAIL_BC = objMailUser.MAIL_BC;//메일숨은참조
		sNOTI_TO = objMailUser.NOTI_TO;//수신확인을 리턴받을 메일주소	

		iAddRow = this.ds_MailUserList.addRow();
		this.ds_MailUserList.setColumn(iAddRow, "MAIL_FR", sMAIL_FR);//메일전송자	
		this.ds_MailUserList.setColumn(iAddRow, "MAIL_TO", sMAIL_TO);//메일수신자	
		this.ds_MailUserList.setColumn(iAddRow, "MAIL_CC", sMAIL_CC);//메일참조	
		this.ds_MailUserList.setColumn(iAddRow, "MAIL_BC", sMAIL_BC);//메일숨은참조	
		this.ds_MailUserList.setColumn(iAddRow, "NOTI_TO", sNOTI_TO);//수신확인을 리턴받을 메일주소	
	}
	
	//첨부파일 정보
	if (this.fvMailAtchFileList.length > 0) {
		var objMailAtchFile = new Object();//첨부파일 정보
		var sPATH_NM = "";//파일경로	
		var sFILE_ID = "";//서버 저장된 파일명		
		var sFILE_NM = "";//원본파일명
	
		for (var iLoop=0; iLoop<this.fvMailAtchFileList.length; iLoop++) {
			objMailAtchFile = this.fvMailAtchFileList[iLoop];

			sPATH_NM = objMailAtchFile.PATH_NM;//파일경로		
			sFILE_ID = objMailAtchFile.FILE_ID;//저장된 파일명
			sFILE_NM = objMailAtchFile.FILE_NM;//원본파일명
			
			iAddRow = this.ds_MailAtchFile.addRow();
			this.ds_MailAtchFile.setColumn(iAddRow, "PATH_NM", sPATH_NM);//파일경로			
			this.ds_MailAtchFile.setColumn(iAddRow, "FILE_ID", sFILE_ID);//서버 저장된 파일명		
			this.ds_MailAtchFile.setColumn(iAddRow, "FILE_NM", sFILE_NM);//원본파일명
		}
	}	

	//메일내용이있을경우
	if (this.fvWebCompLoadYn && !this.gfn_IsNull(this.fvContNm)) {
		//내용 Web컴포넌트에 값 세팅
		var oWeb = this.web_ContNm.getProperty("window");
		oWeb.callMethod("fn_setData", this.gfn_ReplaceWebEditorData(this.fvContNm));	
		
	}	
	
}

