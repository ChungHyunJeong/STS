  ---------------------------------------------------------------------------------------------*/
this.cbo_HcadCd_onitemchanged = function(val){
	
	
	trace("요청유형---->"+val);
	var nRow 	 = this.ds_SmHcad.findRowExpr("HCAD_CD == '" + val + "'");	
	trace("ROW번호"+nRow);
	var sTyinCd  = this.ds_SmHcad.getColumn(nRow, "TYIN_CD");	
