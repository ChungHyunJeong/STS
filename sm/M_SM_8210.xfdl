	var HcadRow 	 = this.ds_SmHcad.findRowExpr("HCAD_CD == '" + this.ds_Master10.getColumn(iRow, "HCAD_CD") + "'");	
	var sTyinCd  = this.ds_SmHcad.getColumn(HcadRow, "TYIN_CD");	
	//trace("ROW번호"+nRow);
	//trace("유형별선택값"+sTyinCd);
	
	/*  추후 사용 예정 - 권한관리자에게 결제 요청
	if(sTyinCd =="0040"){//권한관리
		var PgrmRow  = this.ds_Pgrm.findRowExpr("PGRM_CD == '" + this.ds_Master10.getColumn(iRow, "PGRM_CD") + "'");	
		var sSYST_ID  = this.ds_Pgrm.getColumn(PgrmRow, "SYST_ID");	
		
		sWfUser = sWfUser + "ⓐ" + sWFGU_CD + ";" + sSYST_ID;
		
	}
	*/
	

	var sPopupId = "C_WF_REQUEST";//팝업ID - 결재요청
	var oArg = { CommCd:this.gfn_GetSessionValue("COMM_CD")//회사코드
	           , ModuCd:"SM"//모듈구분
			   , WctmId:"WF_"+ this.fvComPrgmId +"_W1"//결재업무ID
			   , SystId:this.gfn_GetSessionValue("SYST_ID")//결재자_시스템사번
			   , WofhNm:"[HelpDesk처리:"+ this.gfn_StringFormat(this.ds_Master10.getColumn(iRow, "HPRH_NO"),"######-####") +"] " + this.ds_Master10.getColumn(iRow, "SMRY_NM") + " "//결재명
			   , WofhNo:this.ds_Master10.getColumn(iRow, "WOFH_NO")//결재PID
			   , InnrGb:"1"//내부결재구분(1:신규, 2:내부)
			   , CharSystId:""//담당자_시스템사번
			   , CharWfguCd:""//담당자 결재구분
			   , SworqVr:"3"//결재요청버전(1,2,3)
			   , WfamAmtr:""//금액
			   , WfamOpid:""//원천PIID
			   , WfPkvl:sPks//PK값 정보 - PK 값을 순서대로 전달, Row구분자는 ⓡ, 조건 구분자는 ⓐ
			   , WfUser:sWfUser//결재자구분
	           };//Argument처리
			  // trace("-------->"+oArg.WfUser);
	trace(oArg.toString());
	this.gfn_WfRequestPop(sPopupId, oArg);	
}

/***********************************************************************************************
 * 09.02. [공통버튼] 결재요청 유효성체크
 ***********************************************************************************************/  
this.fn_VaildWfDecide = function() {
	
	trace("처리구분"+this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PRST_CD"));
	//처리신청이 아니면 결재불가
	if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PRST_CD") != '0040'){
		var arrMsgArg = ["처리내용", "결재요청"];
		this.gfn_Alert("SM062", arrMsgArg);//[&ITM0]을(를) 추가 후 [&ITM1]가능 합니다.
		return false;
	}
	
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.fvFileDiv.fn_CheckFileAddYn()) {
		var sWOFH_ST = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST");//결재진행상태

		//결재상태에 따른 결재요청 가능여부 체크
		if (!this.gfn_CheckDecideWf(sWOFH_ST)) {
			return false;
		}
		
	} else {
		var arrMsgArg = ["결재요청"];
		this.gfn_Alert("SM064", arrMsgArg);//변경된 내용이 있습니다.\n저장 후 &ITM0하시기 바랍니다.
		return false;
	}
	
	//결재진행내역체크
	if (this.ds_Master10.getRowCount() < 1 || this.ds_Master10.rowposition < 0) {
		this.gfn_Alert("WF016", arrMsgArg);//결재내역을 선택한 후 결재요청하세요.
		return false;
	}	
	
	return true;
}

