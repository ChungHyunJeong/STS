{	
	//if (!this.gfn_CheckFromToDate(this.fvComPrgmId, this.div_Search.form.cal_PdrhDtFr, this.div_Search.form.cal_PdrhDtTo)) return false;//조회기간 범위 체크
	//조회조건 필수입력값 있으면 체크
	
	//[저장] Validation 설정
	var arrArgs = ["this.div_Search.form.edt_DepmCd^사용부서|NULL"
				  ]; 
				  
	var sCheckGroupTitle = "검색조건";//체크그룹 타이틀

	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
		return false;
	}
	
	return true;
}

