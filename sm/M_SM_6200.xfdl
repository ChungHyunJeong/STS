/***********************************************************************************************
 * 02.MF0.03. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master] 데이터 Row 변경 이벤트 - ds_Master10
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {

		//조회된 정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EDMM_NO"))) {
			//자산관리번호
			var iEdmmNoLen = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EDMM_NO").length;

			//프로젝트번호 format 변경 (10자리)
			if (iEdmmNoLen > 10) 
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_EdmmNo.set_format("@@@-@@@@@@-@@@");		
			 else 
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_EdmmNo.set_format("@@@@@@-@@@@");
				
			//첨부파일 정보 조회
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
			oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EDMM_NO");//PK번호2
			//oFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3

	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
			this.fvFileDiv.fn_AtchFileSearch(oFileInfo);	
			
	//첨부파일정보 권한설정
	var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
	oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
	this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);			
		
		}
		//고정자산관리번호(edt_FiasNo)
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FIAS_NO"))) {
			//고정자산관리번호
			var iFiasNoLen = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FIAS_NO").length;

			//프로젝트번호 format 변경 (10자리)
			if (iFiasNoLen > 10) 
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_FiasNo.set_format("@@@@@@@@@@@@-@@@@@@");		
			 else 
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_FiasNo.set_format("@@@@@@-@@@@");
				   
		}
		
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master10] Row 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Confirm("SM003", "", "CONF.MAST10_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	

	return true;
	
};  

