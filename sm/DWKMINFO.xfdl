/***********************************************************************************************
 * 13.02. [사용자함수] 선택한 정보 부모창 리턴 함수 
 ***********************************************************************************************/ 
this.fn_Choice = function(mRow) {
	if (mRow > -1) {	
	
        rtnValue = "({" +
                        ' COMM_CD:"' + this.ds_Master10.getColumn(mRow, "COMM_CD") + '"'+  //0.회사코드
						',WORK_GU:"' + this.ds_Master10.getColumn(mRow, "WORK_GU") + '"'+  //1.일용직구분
                        ',DEPM_CD:"' + this.ds_Master10.getColumn(mRow, "DEPM_CD") + '"'+  //2.부서코드
                        ',DEPM_NM:"' + this.ds_Master10.getColumn(mRow, "DEPM_NM") + '"'+  //3.부서명
                        ',EMPM_CD:"' + this.ds_Master10.getColumn(mRow, "EMPM_CD") + '"'+  //4.사번
                        ',SYST_ID:"' + this.ds_Master10.getColumn(mRow, "SYST_ID") + '"'+  //5.시스템사번
                        ',EMPM_NM:"' + this.ds_Master10.getColumn(mRow, "EMPM_NM") + '"'+  //6.성명
                        ',POSI_NM:"' + this.ds_Master10.getColumn(mRow, "POSI_NM") + '"'+  //7.직위
                        ',REMK_NM:"' + this.ds_Master10.getColumn(mRow, "REMK_NM") + '"'+  //8.비고
                "})";
					
		this.gfn_PopClose(rtnValue);
	} else {
		this.gfn_Alert("SM046");//선택된 내용이 없습니다.
		return;		
	}
}

