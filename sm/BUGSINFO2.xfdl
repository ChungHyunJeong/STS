 ***********************************************************************************************/
this.fn_FormCall = function(oParam)
{
	trace("fn_FormCall");
	//화면전달 파라메터 체크
	this.div_Search.form.cal_FromDd.set_value(oParam.FromDd);
	this.div_Search.form.cal_ClosDd.set_value(oParam.ClosDd);
	this.div_Search.form.edt_AcumCd.set_value(oParam.AcumCd);
	this.div_Search.form.edt_AcumNm.set_value(oParam.AcumNm);
	this.div_Search.form.edt_CusmCd.set_value(oParam.CusmCd);
	this.div_Search.form.edt_CusmNm.set_value(oParam.CusmNm);
	this.div_Search.form.cbo_CredYn.set_value(oParam.CredYn);

	var sCredYn = oParam.CredYn;
	this.ds_FaAcom.filter("CRED_YN=='" + oParam.CredYn + "'");
	var iAddRow = this.ds_FaAcom.addRow();
	this.ds_FaAcom.setColumn(iAddRow, "ACOM_NM", "ALL");
	this.ds_FaAcom.setColumn(iAddRow, "SORT_NO", "1");
	this.ds_FaAcom.set_keystring("S:+SORT_NO+ACOM_CD");
	//체크 콤보 초기화 함수 호출
	this.gfn_InitMultiCombo(this.div_Search.form.edt_AcomCd, this.objConfig);	

	//체크 콤보의 Code 값을 입력하는 함수 실행
	this.gfn_SetMultiComboCode(this.div_Search.form.edt_AcomCd, oParam.AcomCd);//전체	
	//조회
	this.fn_ComBtnSearch();
	
	
}

/***********************************************************************************************
 * 1. 조회 영역
 ***********************************************************************************************/
/***********************************************************************************************
 * 01.01. [팝업버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.div_PopupTitle_btn_Search_onclick = function()
{ 

	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;	
	
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Input.clearData();
	this.ds_Master10.clearData();
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "BUDG_MM", this.div_Search.form.cal_BugsYm.value.substr(0, 6));//예산년월
	this.ds_Input.setColumn(0, "BUDP_CD", this.div_Search.form.edt_DepmCd.value);//예산부서
	this.ds_Input.setColumn(0, "ACOM_CD", this.div_Search.form.edt_AcomCd.value);//계정코드
	
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "fa:M_FA_ACCH_M_SL10";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid 		= "MST_SEARCH";			//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName 	= this.G_SV_FA;			//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset 	= "ds_Input=ds_Input" 	//입력 데이터셋
	svcCallObj.sOutDatset 	= "ds_Master10=ds_Master10";//출력 데이터셋
	svcCallObj.sOtherArg 	= "";					//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";		//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);			
}

/***********************************************************************************************
 * 01.02. [Vaildation] 조회 필수값 체크
 ***********************************************************************************************/
this.fn_VaildSearch = function()
{
	//[검색]NULL 체크
	var arrArgs = ["this.div_Search.form.cal_BugsYm^" + this.div_Search.form.sta_BugsYm.text + "|NULL"
				  ,"this.div_Search.form.edt_DepmCd^" + this.div_Search.form.sta_DepmCd.text + "|NULL"
				  ,"this.div_Search.form.edt_AcomCd^" + this.div_Search.form.sta_AcomCd.text + "|NULL" 
				  ];
				  
	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs) ) {
		return false;
	}
	
	return true;
}

