	this.fvAcomNm = this.getOwnerFrame().AcomNm;//[부모창파라메타] 계정명
	this.fvDepmCd = this.getOwnerFrame().DepmCd;//[부모창파라메타] 부서코드
	this.fvDepmNm = this.getOwnerFrame().DepmNm;//[부모창파라메타] 부서명
	this.fvComPrgmId = this.getOwnerFrame().PrgmId;//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID

 	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
