this.fn_FormOnsize = function(obj:nexacro.Form,e:nexacro.SizeEventInfo)
{ 

	if (obj.width > 1400) {
		this.div_MasterView.form.tab_Master.tpg_01.form.grd_Master10.set_autofittype("col");
	} else {
		this.div_MasterView.form.tab_Master.tpg_01.form.grd_Master10.set_autofittype("none");
	}

};

/***********************************************************************************************
