this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "COMBO_SEARCH": //일반콤보 조회		
			break;
			
		case "MST_SEARCH": //조회		
			this.ds_Master.addColumn("s_chk", "string");
			this.ds_Master.addColumn("crudType", "string");
			if(this.ds_Master.rowcount == 0){
				this.gfn_Alert("SM044"); //조회된 데이터가 없습니다.
			}
			break;
		
		case "SAVE":   //저장		
			//this.gfn_Alert( );	//
			this.fn_ComBtnSearch();//조회
			break;	
		
		case "DELETE": //삭제		
			this.fn_ComBtnSearch();//조회
			break;							
	}
};

