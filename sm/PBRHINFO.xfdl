/***********************************************************************************************
 * 13.02. [사용자함수] 선택한 정보 부모창 리턴 함수 
 ***********************************************************************************************/ 
this.fn_Choice = function(iRow) {
	if (iRow > -1) {	
	
        rtnValue = "({" +
                        ' COMM_CD:"' + this.ds_Master10.getColumn(iRow, "COMM_CD") + '"'+  //0.회사코드
                        ',PBRH_NO:"' + this.ds_Master10.getColumn(iRow, "PBRH_NO") + '"'+  //1.발생번호
                        ',PBRH_NM:"' + this.ds_Master10.getColumn(iRow, "PBRH_NM") + '"'+  //2.보고제목
                        ',CNTH_NO:"' + this.ds_Master10.getColumn(iRow, "CNTH_NO") + '"'+  //3.계약번호
                        ',CNTH_NM:"' + this.gfn_Nvl(this.ds_Master10.getColumn(iRow, "CNTH_NM"),"") + '"'+  //4.계약명
                        ',CUSM_CD:"' + this.gfn_Nvl(this.ds_Master10.getColumn(iRow, "CUSM_CD"),"") + '"'+  //5.거래처
                        ',CUSM_NM:"' + this.gfn_Nvl(this.ds_Master10.getColumn(iRow, "CUSM_NM"),"") + '"'+  //6.거래처명
                        ',DEPM_CD:"' + this.ds_Master10.getColumn(iRow, "DEPM_CD") + '"'+  //7.등록부서
                        ',DEPM_NM:"' + this.ds_Master10.getColumn(iRow, "DEPM_NM") + '"'+  //8.등록부서명
                        ',SYST_ID:"' + this.ds_Master10.getColumn(iRow, "SYST_ID") + '"'+  //9.등록자ID
                        ',EMPM_NM:"' + this.ds_Master10.getColumn(iRow, "EMPM_NM") + '"'+  //10.등록자사번
                        ',EMPM_CD:"' + this.ds_Master10.getColumn(iRow, "EMPM_CD") + '"'+  //11.등록자명
                        ',OCCU_GU:"' + this.ds_Master10.getColumn(iRow, "OCCU_GU") + '"'+  //12.발생구분(공통)
                        ',PBRH_CD:"' + this.ds_Master10.getColumn(iRow, "PBRH_CD") + '"'+  //13.발생공정(공통)
                        ',AREA_CD:"' + this.ds_Master10.getColumn(iRow, "AREA_CD") + '"'+  //14.발생지역
                        ',BUSI_CD:"' + this.ds_Master10.getColumn(iRow, "BUSI_CD") + '"'+  //15.사업군
                        ',INOU_GU:"' + this.ds_Master10.getColumn(iRow, "INOU_GU") + '"'+  //16.사내/외
						',REMK_NM:"' + nexacro.replaceAll(nexacro.replaceAll(this.ds_Master10.getColumn(iRow, "REMK_NM"), "\n", "^@|"), String.fromCharCode(13), "^@|") + '"'+  //17.비고
                        ',ECRH_NO:"' + this.ds_Master10.getColumn(iRow, "ECRH_NO") + '"'+  //18.ECR 번호
                        ',PBRH_QT:"' + this.ds_Master10.getColumn(iRow, "PBRH_QT") + '"'+  //19.발생수량
                        ',PBQT_CD:"' + this.ds_Master10.getColumn(iRow, "PBQT_CD") + '"'+  //20.발생수량단위
                "})";

		this.gfn_PopClose(rtnValue);
	} else {
		this.gfn_Alert("SM046");//선택된 내용이 없습니다.
		return;		
	}
}

