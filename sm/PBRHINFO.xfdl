 ************************************************************************************************/
this.fvComPrgmId = "";//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
this.fvPbrhNo = "";//[부모창파라메타] 문제점발생번호

/***********************************************************************************************
 * 00.02. Form 변수 선언
 ***********************************************************************************************/
/***********************************************************************************************
 * 00.03. Form Onload 이벤트
 ***********************************************************************************************/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{	
	//부모창 파라메타 처리
	this.fvPbrhNo = this.getOwnerFrame().PbrhNo;//[부모창파라메타] 문제점발생번호
	this.fvOccuGu = this.getOwnerFrame().OccuGu;//[부모창파라메타] ECR 대상 조회구분 (발생구분 = 1)

 	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
 	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 
}

/***********************************************************************************************
 * 00.04. Form [공통] 초기설정 함수 - 필수
 ***********************************************************************************************/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	//this.fvComPrgmId = this.getOwnerFrame().PrgmId;//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
	this.fvComPrgmId = "M_QM_1100";//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
	this.gfn_SetGrid(this.grd_Master10);//[공통]그리드 이벤트 설정 - 프로젝트목록
	
}

/***********************************************************************************************
 * 00.05. Form [업무] 초기설정 함수
 ***********************************************************************************************/
this.fn_FormBizInit = function() 
{
	//[공통] 날짜정보 설정	
	this.gfn_SetCommFromToDate(this.fvComPrgmId, "MS", this.div_Search.form.cal_FromDd, this.div_Search.form.cal_ClosDd);//조회조건영역

	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	var objCondList = ["QM110|ds_Qm110|Y|ALL"
					  ,"QM115|ds_Qm115"
					  ,"QM140|ds_Qm140"
					  ,"QM145|ds_Qm145"
					  ,"QM160|ds_Qm160"
					  ];
	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, "");
		
	//파라미터 값이 있는경우
	if (!this.gfn_IsNull(this.fvPbrhNo)) {	
		this.div_Search.form.edt_PbrhNo.set_value(this.fvPbrhNo);
		this.div_PopupTitle_btn_Search_onclick();//조회
	}

	if(!this.gfn_IsNull(this.fvOccuGu)){
		//this.div_Search.form.cbo_OCCU_GU.set_value(this.ds_Qm110.findRow("CODM_CD", this.fvOccuGu));
		this.div_Search.form.cbo_OCCU_GU.set_value(this.fvOccuGu);
	}else{
		this.div_Search.form.cbo_OCCU_GU.set_index(0);
	}
	this.div_Search.form.edt_PbrhNo.setFocus();	//로딩시 포커스
}

