/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{	
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)	
	
	if (e.columnid == "INSB_YN") {
		if(e.newvalue == '1') {					
			var sToDay = this.gfn_GetCommDate("toDay");//현재일자			
			this.ds_Detail10.setColumn(e.row,	"INSB_DD", sToDay);
			this.ds_Detail10.setColumn(e.row,   "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//등록자명		
			this.ds_Detail10.setColumn(e.row,   "INSU_ID", this.gfn_GetSessionValue("SYST_ID"));//등록자ID				
		} else {				
			this.ds_Detail10.setColumn(e.row,	"INSB_DD", null);
			this.ds_Detail10.setColumn(e.row,   "EMPM_NM", null);//등록자명		
			this.ds_Detail10.setColumn(e.row,   "INSU_ID", null);//등록자ID	
		}	
		
		
		this.fn_InshDd_Enable();		
	}			

};


this.fn_InshDd_Enable = function(){
	var nChk   = this.ds_Detail10.getCaseCount("JUNG_YN == '1'");
	var nChkEq = this.ds_Detail10.getCaseCount("JUNG_YN == 1 && INSB_YN==JUNG_YN");	
	
	if(nChk==nChkEq){	
		var sToDay = this.gfn_GetCommDate("toDay");//현재일자
		
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PdrhDd.set_enable(true);
		
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "INSH_DD", sToDay);
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));
		
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));	
		
		this.fn_DateSet(sToDay);		
		
	}else{	
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "INSH_DD", null);
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DEPM_CD", null);
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DEPM_NM", null);
		
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "SYST_ID", null);
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EMPM_CD", null);
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EMPM_NM", null);
		
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", null);
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", null);
		
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PdrhDd.set_enable(false);	
	}	
	
		
	//if(this.fvInputChk == "Y" && this.gfn_DsIsUpdated(this.ds_Detail10) ){			
	if(this.ds_Detail10.getCaseCount("crudType=='I'") < 1) {
		this.ds_Master10.setColumn(this.ds_Master10.rowposition,"crudType","U");
	}
}


this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{	
	//선택	
	if (e.columnid == "s_chk") {
		var cnthNo = this.ds_Detail20.getColumn(e.row, "CNTH_NO");
		var value = "0";
		for (var iLoop = 0; iLoop < this.ds_Detail20.getRowCount(); iLoop++) {
			if (this.ds_Detail20.getColumn(iLoop, "s_conf") != "1") {
				value = (this.ds_Detail20.getColumn(iLoop, "CNTH_NO") == cnthNo) ? e.newvalue : "0";
				this.ds_Detail20.setColumn(iLoop, "s_chk", value);
			}
		}
	}
};

