 ***********************************************************************************************/
 this.div_MasterInput_tab_Master_tpg_01_cal_PdrhDd_onchanged = function(obj:nexacro.Calendar,e:nexacro.ChangeEventInfo)
{
	this.fn_DateSet(obj.value);
};
 
this.fn_DateSet = function(date){ 
	var warrCt = this.ds_Master10.getColumn(this.ds_Master10.rowposition,  "WARR_CT");	
	
	var sInshDd= this.ds_Master10.getColumn(this.ds_Master10.rowposition, "INSH_DD");
		
	if(this.gfn_IsNull(sInshDd) || sInshDd == "19000101"){	
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", null);
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", null);	
	}else{
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD",  this.gfn_AddDate(date, 1));
		
		if(warrCt == 0 || this.gfn_IsNull(warrCt)){		
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD",  this.gfn_AddDate(date, 1));
		} else {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD",  this.gfn_AddMonth(date, warrCt));
		}	
	
	}
} 
