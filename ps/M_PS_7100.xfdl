/***********************************************************************************************
 * 02.DG1.05. [Detail그리드2-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Choice = function()
{
	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {
		
			var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
			
			if(sChkRowType != 'I'){							
				this.gfn_Alert("PS001"); //입력일 경우만 적용 가능합니다.
				return;				
			} else {
				
				var nRow = this.ds_Detail20.findRowExpr("s_chk=='1'");
				
				//[공통]서비스정보 초기화
				this.gfn_InitComService(this);
				
				this.ds_Master15.clearData();			
				this.ds_Input.clearData();
				this.ds_Detail10.clearData();				
				this.ds_Input.addRow();
				this.ds_Input.setColumn(0, "CNTH_NO", this.ds_Detail20.getColumn(nRow, "CNTH_NO"));
				
				//[공통]서비스정보 추가
				var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
				svcAddObj.sSelectSql = "ps:M_PS_7100_M_SL15";
				this.gfn_AddComSearchService(this, svcAddObj);
				
				//[공통]서비스 호출 변수
				var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
				svcCallObj.sTrid = "MST_SEARCH15";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
				svcCallObj.sActionName = this.G_SV_PS;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조			
				svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
				svcCallObj.sOutDatset = "ds_Master15=ds_Master15";//출력 데이터셋
				svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
				svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
				svcCallObj.isSync = true;//동기식으로 설정할지 여부
				
				//[공통]서비스 호출
				this.gfn_CallComService(this, svcCallObj);
				
			
				if(this.ds_Master15.rowcount > 0){
					this.ds_Master10.copyRow(this.ds_Master10.rowposition, this.ds_Master15, 0);
				
					this.ds_Master10.setColumn(this.ds_Master10.rowposition,"crudType","U");
					
					//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
					this.ds_Detail10.clearData();
					this.ds_Input.clearData();
					
					this.ds_Input.addRow();
					this.ds_Input.setColumn(0, "CNTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO"));			
					this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));								
					
										
					//[공통]서비스정보 초기화
					this.gfn_InitComService(this);
					
					//[공통]서비스정보 추가
					var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
					svcAddObj.sSelectSql = "ps:M_PS_7100_D_SL10";
					this.gfn_AddComSearchService(this, svcAddObj);
					
					//[공통]서비스 호출 변수
					var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
					svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
					svcCallObj.sActionName = this.G_SV_PS;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조			
					svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
					svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10";//출력 데이터셋
					svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
					svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

					//[공통]서비스 호출
					this.gfn_CallComService(this, svcCallObj);	
					
					 this.fvInputChk = "Y";
				
				}else{
				
					this.fvInputChk = "N";
					
					this.ds_Master10.copyRow(this.ds_Master10.rowposition, this.ds_Detail20, nRow);					
					this.ds_Master10.setColumn(this.ds_Master10.rowposition,	"crudType",		"I");		
					
					//--준공단계 추가 ----------
					var iAddRow = 0;
					var sInsbCd = "";
					var sJungYn = "";
					
					var sChkRow = this.ds_Detail20.findRowExpr("s_chk=='1'");
					
					for(iLoop =0; iLoop < this.ds_Ps710.getRowCount(); iLoop++){				
						iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)					
						sInsbCd = this.ds_Ps710.getColumn(iLoop, "CODM_CD");
						sJungYn = this.ds_Ps710.getColumn(iLoop, "OP02_NM");
						
						sJungYn = this.gfn_Nvl(sJungYn,"0");						
						
						this.ds_Detail10.setColumn(iAddRow, "CNTH_NO", strCnthNo);
						this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", 1);
						this.ds_Detail10.setColumn(iAddRow, "INSB_CD", sInsbCd);
						this.ds_Detail10.setColumn(iAddRow, "INSB_YN", "0");
						this.ds_Detail10.setColumn(iAddRow, "JUNG_YN", sJungYn);

						//-- 어차피  Detail10의 Row와 같음
						
						if(sInsbCd=="0020"){
							var sSalbYn = this.ds_Detail20.getColumn(sChkRow, "SALB_YN");
							
							if(sSalbYn=="N"){
								this.ds_Detail10.setColumn(iAddRow, "CHEK_AB","A");
							}
							
						}else if(sInsbCd=="0040"){
							var sPcobYn = this.ds_Detail20.getColumn(sChkRow, "PCOB_YN");
							
							if(sPcobYn=="N"){
								this.ds_Detail10.setColumn(iAddRow, "CHEK_AB","B");
							}
						}
						
					}
					
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "INSH_DD",null);
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD",null);
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD",null);
				}							
			
				var strCnthNo = this.ds_Detail20.getColumn(nRow, "CNTH_NO");
				
				for(var i=0 ; i<this.ds_Detail20.getRowCount(); i++){
					var strCnthNoNext = this.ds_Detail20.getColumn(i, "CNTH_NO");
					
					if(strCnthNo != strCnthNoNext) this.ds_Detail20.setColumn(i,"s_conf", "0");
				}
			}
				
			this.fn_CnthNoFormat(this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PdrhNo);	
			
			//선택항목 비활성화처리			
			this.ds_Detail20.setColumn(nRow, "s_conf", "1");//준공대상계약검색 컬러 변경 구분자 업데이트						
			this.ds_Detail20.setColumn(nRow, "s_chk", "0");	//Uncheck로 변경
			
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PdrhDd.set_enable(false);

			this.ds_Detail20.set_rowposition(-1);
			//프로젝트상세로 Tab이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);
						
			//var inshDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "INSH_DD");			
			//this.fn_DateSet(inshDd);
			
		}
	}
};

