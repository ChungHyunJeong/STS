 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{	
		case "JOJIK_SEARCH" : // 조직도 조회			
			
			this.gfn_Confirm("PS003", "", "CONF.CREATE", "fn_MsgCallback");//일괄생성 하시겠습니까?			
			
			break;
			
		case "COMBO_SEARCH" : // 공통코드 조회
			
			break;
		case "MST_SEARCH": //조회		
	
			if (this.ds_Master10.getRowCount() > 0) {
				this.fn_CnthNoFormat(this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo);
				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master10.findRowExpr("PRCH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ == '" + this.fvSchKey02 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}
				
				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
				this.fn_SetGridInputButtonEnable();
				this.div_DetailInput.form.tab_Detail.set_enable(true);
				
				//이미지-첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
				this.fvImgFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
				
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
				
				this.div_DetailInput.form.btn_allCreate.set_enable(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_CnthNo.set_visible(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CnthNm.set_width(233);
				
			} else {
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			
				//입력영역 비활성화
				this.div_MasterInput.set_enable(false);
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);
				this.div_DetailInput.form.tab_Detail.set_enable(false);
				
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterView.form.grd_Master10);
				
				//이미지-첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = false;//권한 유무	
				this.fvImgFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);		
				
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = false;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);						
				
				this.div_DetailInput.form.btn_allCreate.set_enable(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_CnthNo.set_visible(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CnthNm.set_width(206);
			}
			
			//저장후 재조회여부-true
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}			
	
			break;
		case "SAVE":   //저장		

			//첨부파일 추가건이 없으면
			if (!this.fvImgFileDiv.fn_CheckFileAddYn() && !this.fvFileDiv.fn_CheckFileAddYn()) {
				this.fvSchKeyYn = true;//저장후 재조회여부
				this.fn_ComBtnSearch();//저장후 재조회
				
			//이미지 저장시	
			} else if (this.fvImgFileDiv.fn_CheckFileAddYn()) {
				this.fvImgFileDiv.fn_AtchFileSave(this, fvFileInfo);							
				
			//파일 저장시	
			} else if (this.fvFileDiv.fn_CheckFileAddYn()) {
				this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);			
			}
			
			this.div_DetailInput.form.btn_allCreate.set_enable(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_CnthNo.set_visible(false);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CnthNm.set_width(233);
			
			//this.fn_MrpSearch();
			
			break;	
		case "DTL_SEARCH": //조회		
			this.ds_Detail10.addColumn("s_chk", "string");
			this.ds_Detail10.addColumn("crudType", "string");			
			this.ds_Detail20.addColumn("s_chk", "string");
			this.ds_Detail20.addColumn("crudType", "string");						

			this.ds_Detail10.addColumn("ITEM_NM", "string");			
			
			//this.cbo_PdrhGu_onitemchanged(this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_PdrhGu);		
			
			//이미지-첨부파일 정보 조회
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.PICH_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PRCH_NO");//관리번호
			this.fvImgFileDiv.fn_AtchFileSearch(oFileInfo);			
						
			//첨부파일 정보 조회
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
			oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PRCH_NO");//PK번호2
			oFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3

			this.fvFileDiv.fn_AtchFileSearch(oFileInfo);
			this.fn_SetGridInputButtonEnable();
			
			/*
			if(this.ds_Master10.getRowCount() > 0) {
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_CnthNo.set_visible(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CnthNm.set_width(233);				
			} else {
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_CnthNo.set_visible(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CnthNm.set_width(206);				
			}
			*/
			this.fn_CnthNoFormat(this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo);
			break;			
			
		case "DTLS_SEARCH": //파트너사작업상세
