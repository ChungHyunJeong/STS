/***********************************************************************************************
 * 5. 저장
 ***********************************************************************************************/
/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.gfn_DsIsUpdated(this.ds_Detail20) && !this.fvImgFileDiv.fn_CheckFileAddYn() && !this.fvFileDiv.fn_CheckFileAddYn() ) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.		
		return;
	}
	
	//이미지-첨부파일정보 변경시
	if (this.fvImgFileDiv.fn_CheckFileAddYn()) {
		//이미지-첨부파일정보만 변경시
		if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && this.ds_Master10.getRowCount() > 0) {
			//첨부파일정보 저장
			fvImgFileInfo = new Object();//[공통] 첨부파일정보
			fvImgFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvImgFileInfo.CALLBACK_FUNC = "";//호출할 콜백함수
			fvImgFileInfo.PATH_NM = this.fvImgPathNm;//업무파일경로
			fvImgFileInfo.PICH_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PRCH_NO");//관리번호
			//첨부파일Div 화면에 Key세팅하게 변경
			this.fvImgFileDiv.fn_AtchFileSave(this, fvImgFileInfo);
			
			//첨부파일 저장건이 없으면
			if (!this.fvFileDiv.fn_CheckFileAddYn()) {
				return;	
			}
		}
	}	

	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		//첨부파일정보만 변경시
		if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && this.ds_Master10.getRowCount() > 0) {
			//첨부파일정보 저장
			fvFileInfo = new Object();//[공통] 첨부파일정보
			fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, B-상세(그리드Row), E-기타
			fvFileInfo.CALLBACK_FUNC = "";//호출할 콜백함수
			fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
			fvFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PRCH_NO");//PK번호2-관리번호
			fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
			//첨부파일Div 화면에 Key세팅하게 변경
			this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);
			return;	
		}
	}

	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		if (!this.fn_VaildMasterSave()) return;
	}
		
// 	if(this.ds_Detail10.rowcount < 1){
// 		var arrMsgArg = ["담당자작업상세"];
// 		this.gfn_Alert("SM045", arrMsgArg);//[담당자작업상세]는 1건 이상 존재해야 합니다.
// 		return;
// 	}
	
	//Detail 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail10) || this.gfn_DsIsUpdated(this.ds_Detail20)) {
		if (!this.fn_VaildDetailSave()) return;
	}	
	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	//Master 입력 상태이면
	if (sChkRowType == "I") {
	
		//채번-관리번호
		var sTemhNo = this.gfn_GetTbnmNo(this.fvComPrgmId, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PRCH_DD"));//관리번호
		if (this.gfn_IsNull(sTemhNo)) {
			//관리번호를 가져오는데 실패했습니다.
			return;
		}	
		var sChkTemhNo = "";//계약번호-체크용		
		var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position	
		
		//Master 데이터셋에 채번정보 입력-관리번호
		this.ds_Master10.setColumn(iMasterRowPos, "PRCH_NO", sTemhNo);		

		this.ds_Master10.setColumn(iMasterRowPos, "WOFH_SQ", 1);
		this.ds_Master10.setColumn(iMasterRowPos, "WOFH_YN", "1");	
		this.ds_Master10.setColumn(iMasterRowPos, "WOFH_ST", "0010");
				
		var sTemhNo = this.ds_Master10.getColumn(iMasterRowPos, "PRCH_NO");//관리번호
		var nWofhSq = this.ds_Master10.getColumn(iMasterRowPos, "WOFH_SQ");//차수
		
		//Detail 데이터셋에 기본정보 입력-회사코드/출고번호/결재차수
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			sChkTemhNo = this.ds_Detail10.getColumn(iLoop, "PRCH_NO");//관리번호					
			
			if (this.gfn_IsNull(sChkTemhNo)) {		
				this.ds_Detail10.setColumn(iLoop, "PRCH_NO", sTemhNo);
				this.ds_Detail10.setColumn(iLoop, "WOFH_SQ", nWofhSq);

			}
		}
		
		//이미지-첨부파일정보 변경시
		if (this.fvImgFileDiv.fn_CheckFileAddYn()) {
			fvImgFileInfo = new Object();//[공통] 첨부파일정보
			fvImgFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			//첨부파일정보 변경시
			if (this.fvImgFileDiv.fn_CheckFileAddYn()) {
				fvImgFileInfo.CALLBACK_FUNC = "fn_ImageCallback";//호출할 콜백함수
			} else {
				fvImgFileInfo.CALLBACK_FUNC = "fn_ComBtnSearch";//호출할 콜백함수
			}
