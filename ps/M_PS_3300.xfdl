/***********************************************************************************************
 * 02.DG1.05. [Detail그리드2-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Choice = function()
{
	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail10.getCaseCount("s_chk=='1'") > 0) {
		
			var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
			
			if(sChkRowType != "I"){							
				this.gfn_Alert("PS001"); //입력일 경우만 적용 가능합니다.
				return;				
			} else {
				
				var nRow = this.ds_Detail10.findRowExpr("s_chk=='1'");	
				
				this.ds_Master10.copyRow(this.ds_Master10.rowposition, this.ds_Detail10, nRow);
			
				var strCnthNo = this.ds_Detail10.getColumn(nRow, "CNTH_NO");
				for(var i=0 ; i<this.ds_Detail10.getRowCount(); i++){
					var strCnthNoNext = this.ds_Detail10.getColumn(i, "CNTH_NO");
					
					if(strCnthNo != strCnthNoNext) this.ds_Detail10.setColumn(i,"s_conf", "0");
				}
			}
			
			//선택항목 비활성화처리			
			this.ds_Detail10.setColumn(nRow, "s_conf", "1");//FAC계약검색 컬러 변경 구분자 업데이트						
			this.ds_Detail10.setColumn(nRow, "s_chk", "0");	//Uncheck로 변경			
			this.fn_CnthNoFormat(this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.value);
	
		}
	}
};

/***********************************************************************************************
 * 02.DG1.06. [Detail그리드2-버튼] 취소 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Cancel = function()
{
	this.ds_Detail10.reset();

	//기존 체크정보 초기화
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
	
