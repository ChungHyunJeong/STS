	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	this.div_Search.form.spn_FromYy.set_value(sToDay.substr(0, 4));
	
	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
