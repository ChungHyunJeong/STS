this.fn_VaildSearch = function()
{
	//[결재] 결재내역 조회가 아니면
	if (!this.fvWfSearchMode) {
		if (!this.gfn_CheckFromToDate(this.fvComPrgmId, this.div_Search.form.cal_MethDtFr, this.div_Search.form.cal_MethDtTo)) return false;//조회기간 범위 체크
		
		//[저장] Validation 설정
		var arrArgs = [   "this.div_Search.form.edt_DepmCd^등록부서|NULL"
					  ]; 
					  
		var sCheckGroupTitle = "검색조건";//체크그룹 타이틀

		//컴포넌트 Vaild 체크
		if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
			return false;
		}
	}
	
	return true;
}

