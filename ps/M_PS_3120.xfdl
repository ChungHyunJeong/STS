 /***********************************************************************************************
 * 15.00. [결재] 결재상태에 따른 컴포넌트 Enable 등 처리
 ***********************************************************************************************/ 
this.fn_SetWfCompEnable = function()
{ 	
	//[결재]결재내역 팝업화면으로 호출여부
	if (!this.fvWfPopupMode) {
		//[결재] 공통버튼 제어
		var bCommSaveButton = false;//저장버튼 활성화 여부-기본값은 false로 해야함. - 결재승인후 수정일 필요한 화면에서만 true로 설정해야함. 
		this.fvWofhStatus = this.gfn_SetCommButtonWf(this, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"), bCommSaveButton);//결재진행상태		
	}
	this.div_DetailInput.form.tab_Detail.tpg_01.form.web_DetaNm.set_url("");//웹에디터 Url 설정
	//[결재]결재내역 조회모드이면 Visble false
	if (this.fvWfSearchMode || this.fvWofhStatus) {
		//[결재]결재내역 팝업화면으로 호출여부
		if (this.fvWfPopupMode) {
			this.div_Search.set_enable(false);//조회영역 Enable처리
		}
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);
		//[결재] Div하위의 컴포넌트 Enable 처리
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
		//this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 입력	

		
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AddhNm.set_enable(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_RemkPop.set_enable(true);// 비고(파트너사)
		this.div_DetailInput.form.tab_Detail.tpg_01.form.web_DetaNm.set_url(this.gfn_GetServerUrl("View"));//웹에디터 Url 설정
	} else {
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(true);
		//[결재] Div하위의 컴포넌트 Enable 처리
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, true);//Master 입력 프리폼
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form, true);//Detail 입력	
		//[결재] Div하위의 컴포넌트 Enable 처리 제외할 컴포넌트설정
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_AddhNo.set_readonly(true);	//제안번호
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_AddhDd.set_enable(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_readonly(true);	//제안부서
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmNm.set_readonly(true);	//제안부서명
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_readonly(true);	//제안자
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm.set_readonly(true);	//제안자명
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CnthNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmNm.set_readonly(true);
		
		this.div_DetailInput.form.tab_Detail.tpg_01.form.web_DetaNm.set_url(this.gfn_GetServerUrl("Edit"));//웹에디터 Url 설정
	}
}

