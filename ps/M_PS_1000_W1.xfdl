/***********************************************************************************************
 * 15.10. [사용자함수] 집행실행내역 계산
 ***********************************************************************************************/
this.fn_Tpg02_Cal = function() {
	//계약금액
	var nCnthAm = nexacro.toNumber(this.ds_Detail12.getCaseSum("DETA_CD!='0050'","WONE_AM"));
	var nCnthRt = (nCnthAm>0?100:0);
	//***************** 영업계약보고 BEGIN **********************//
	var nAm01 = nexacro.toNumber(this.ds_Detail12.getCaseSum("ITEM_CD!='Z000000001' && DETA_CD!='0050'","TMA_AM"));
	var nAm02 = nexacro.toNumber(this.ds_Detail12.getCaseSum("DETA_CD!='0050'","TCOD01_AM"));
	var nAm03 = nexacro.toNumber(this.ds_Detail12.getCaseSum("DETA_CD!='0050'","TCOG01_AM"));
	var nAm04 = nexacro.toNumber(this.ds_Detail12.getCaseSum("DETA_CD!='0050'","TLA_AM"));
	var nAm05 = nexacro.toNumber(this.ds_Detail12.getCaseSum("ITEM_CD=='Z000000001' && DETA_CD!='0050'","TMA_AM+TOU_AM"));
	var nAmOU = nexacro.toNumber(this.ds_Detail12.getCaseSum("ITEM_CD!='Z000000001' && DETA_CD!='0050'","TOU_AM"));
	
	this.ds_Master20.setColumn(0,"CNTH01_AM", nCnthAm);
	this.ds_Master20.setColumn(0,"CNTH01_RT", nCnthRt);
	this.ds_Master20.setColumn(0,"AM01", nAm01);
	this.ds_Master20.setColumn(0,"AMOU", nAmOU);
	this.ds_Master20.setColumn(0,"AM02", nAm02);
	this.ds_Master20.setColumn(0,"AM03", nAm03);
	this.ds_Master20.setColumn(0,"AM04", nAm04);
	this.ds_Master20.setColumn(0,"AM05", nAm05);
	
	this.ds_Master20.setColumn(0,"RT01", nexacro.round((nAm01/nCnthAm) * 100,1));
	this.ds_Master20.setColumn(0,"RT02", nexacro.round((nAm02/nCnthAm) * 100,1));
	this.ds_Master20.setColumn(0,"RT03", nexacro.round((nAm03/nCnthAm) * 100,1));
	this.ds_Master20.setColumn(0,"RT04", nexacro.round((nAm04/nCnthAm) * 100,1));
	this.ds_Master20.setColumn(0,"RT05", nexacro.round((nAm05/nCnthAm) * 100,1));
	this.ds_Master20.setColumn(0,"RTOU", nexacro.round((nAmOU/nCnthAm) * 100,1));
	
	this.ds_Master20.setColumn(0,"TOT01_AM", nAm01+nAm02+nAm03+nAm04+nAm05+nAmOU);
	this.ds_Master20.setColumn(0,"TOT01_RT", nexacro.round((nAm01+nAm02+nAm03+nAm04+nAm05+nAmOU)/nCnthAm * 100,1));
	
	this.ds_Master20.setColumn(0,"PR01_AM", nCnthAm - (nAm01+nAm02+nAm03+nAm04+nAm05+nAmOU));
	this.ds_Master20.setColumn(0,"PR01_RT", nexacro.round((nCnthAm - (nAm01+nAm02+nAm03+nAm04+nAm05+nAmOU)) / nCnthAm * 100,1));
	
	
	var nAm06 = nexacro.toNumber(this.ds_Detail12.getCaseSum("DETA_CD!='0050'","SEXP01_AM"));
