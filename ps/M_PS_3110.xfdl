		trace("sPrchNo  : " + sPrchNo );
		trace("sWofhSq  : " + sWofhSq );
		var sPopupId = "M_PS_3100";
		var sPopupTitle = "프로젝트진척관리"; //Popup화면 타이틀
		var sPopupFormUrl  = "ps::M_PS_3100.xfdl"; //Popup화면 URL
