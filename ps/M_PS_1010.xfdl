this.ds_Master20_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	trace("e.reason --> " + e.reason);
	//if(e.reason == 52) { return; } // 헤드 정렬 클릭시 return
	if (e.newrow > -1 && !this.fvMasterSearchMode) {

		//조회된 정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master20.getColumn(this.ds_Master20.rowposition, "CNTH_NO"))) {
		
			this.fvMasterSearchMode = true;//마스터내역 조회중여부

			this.ds_Detail40.clearData();		//발주/입고상세
			this.ds_Detail50.clearData();		//입고/출고상세
			this.ds_Detail60.clearData();		//청구/발주상세
			this.ds_Input.clearData();
			
