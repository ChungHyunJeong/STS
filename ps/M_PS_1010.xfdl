this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	trace("e.reason --> " + e.reason);
	//if(e.reason == 52) { return; } // 헤드 정렬 클릭시 return
	if (e.newrow > -1 && !this.fvMasterSearchMode) {

		//조회된 정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO"))) {
		
			this.fvMasterSearchMode = true;//마스터내역 조회중여부

			this.ds_Detail10.clearData();		//발주/입고상세
			this.ds_Detail20.clearData();		//입고/출고상세
			this.ds_Detail30.clearData();		//청구/발주상세
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "CNTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO"));
			this.ds_Input.setColumn(0, "JUNGONGYN", this.div_Search.form.cbo_JungongYn.value); // 준공여부
					
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//var selectedTabIdx = this.div_MasterView.form.tab_Master.tabindex;
			var selectedTabIdx = 0;
			var callPageNumber = 10;
			
			switch(selectedTabIdx) {
				case 0: callPageNumber = "10"; break;
				case 1: callPageNumber = "20"; break;
				case 2: callPageNumber = "30"; break;
				case 3: callPageNumber = "40"; break;
				//case 4: callPageNumber = "50"; this.div_MasterView.form.tab_Master.tpg_05.form.grd_Master50.set_suppresslevel( "sameskip" ); break;
				default : break;
