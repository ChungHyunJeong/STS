this.div_MasterView_tab_Master_tpg_05_grd_Master50_onselectchanged = function(obj:nexacro.Grid,e:nexacro.GridSelectEventInfo)
{
	var selectedRow = e.row;
	var selectFrom = 0;
	var selectTo = 0;
	if(selectedRow % 3 == 0) { 
		selectFrom = nexacro.toNumber(selectedRow)+1;
		selectTo = nexacro.toNumber(selectedRow)+2; 
	} else if(selectedRow % 3 == 1) {
		selectFrom = nexacro.toNumber(selectedRow)-1;
		selectTo = nexacro.toNumber(selectedRow)+1; 
	} else if(selectedRow % 3 == 2) {
		selectFrom = nexacro.toNumber(selectedRow)-2;
		selectTo = nexacro.toNumber(selectedRow)-1; 
	}
	
	this.div_MasterView.form.tab_Master.tpg_05.form.grd_Master50.set_enableevent(false);
	
	this.div_MasterView.form.tab_Master.tpg_05.form.grd_Master50.selectRow(pre_selectedFrom, false);
	this.div_MasterView.form.tab_Master.tpg_05.form.grd_Master50.selectRow(pre_selected, false);
	this.div_MasterView.form.tab_Master.tpg_05.form.grd_Master50.selectRow(pre_selectedTo, false);
	
	this.div_MasterView.form.tab_Master.tpg_05.form.grd_Master50.selectRow(selectedRow, true);
	this.div_MasterView.form.tab_Master.tpg_05.form.grd_Master50.selectRow(selectFrom, true);
	this.div_MasterView.form.tab_Master.tpg_05.form.grd_Master50.selectRow(selectTo, true);
	
	pre_selectedFrom = selectFrom;
	pre_selected = selectedRow;
	pre_selectedTo = selectTo;
	
	this.div_MasterView.form.tab_Master.tpg_05.form.grd_Master50.set_enableevent(true);
	//this.ds_Master50.set_enableevent(true);
};
 
