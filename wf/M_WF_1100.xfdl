this.fn_ComBtnReject = function()
{
	var oWfParam = new Object();//[결재] 결재관련 파라메타 정보 Object
	
	if (this.gfn_IsNull(this.div_DetailInput.form.txt_MemoNm.value)) {
		this.gfn_Alert("WF009");//첨언은 필수입력 항목입니다.
		return;
	}
	
	//내부결재가 아닌경우 - 업무정보 호출
	if (!this.fvInnerWf) {
		//[결재] 결재버튼 비활성화-기각
		this.fn_SetComApprButtonAuth("REJECT", false);		
		
		//[기각] 시 업무영역 전처리 함수 호출-업무영역 처리 성공시
		if (this.fn_ComWfRejectizCall(oWfParam)) {
			var rtnValue = this.fn_WfApprStatusUpdate("0050");
			
			//처리실패시
			if (!rtnValue) {
				this.gfn_Alert("WF007");//결재상태 저장 시 오류가 발생했습니다.\nIT혁신팀에 문의하세요.
				//[결재] 결재버튼 비활성화-기각
				this.fn_SetComApprButtonAuth("REJECT", true);				
				return false;			
