this.fn_SetComApprButtonAuth = function(btnDst, enable) {
	//[공통버튼] 버튼 Enable 설정 
	var objComButton = new Object();

	if (btnDst == "APPR") {
		objComButton.APPR_Enable = enable;//[승인]버튼 Enable 설정
	} else if (btnDst == "REJECT") {
		objComButton.REJECT_Enable = enable;//[기각]버튼 Enable 설정
	} else if (btnDst == "CONFIRM") {
		objComButton.CONFIRM_Enable = enable;//[확인]버튼 Enable 설정
	} else if (btnDst == "CANCEL") {
		objComButton.CANCEL_Enable = enable;//[취소]버튼 Enable 설정		
	} else if (btnDst == "INNER") {
		objComButton.INNERWF_Enable = enable;//[내부결재]버튼 Enable 설정				
	} else {
		return;
	}
	
	//[공통버튼]버튼 Enable 설정 공통함수 호출
	this.gfn_SetWfComButtonEnable(objComButton, this);	
}

