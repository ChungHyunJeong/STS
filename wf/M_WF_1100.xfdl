/***********************************************************************************************
 * 11.01. [공통버튼] 승인 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnAppr = function()
{
	//내부결재가 아닌경우 - 업무정보 호출
	if (!this.fvInnerWf) {
		var sWFLO_CD  = this.ds_Detail13.getColumn(0, "WFLO_CD");//결재자위치(WF120)
		var sUPDT_NM = "";// 업데이트테이블명
		var sUPDC_NM = "";//업데이트칼럼명
		var sUPSC_NM = "";//업데이트상태칼럼명
		
		if (this.ds_Detail12.getRowCount() > 0) {
			sUPDT_NM = this.ds_Detail12.getColumn(0, "UPDT_NM");// 업데이트테이블명
			sUPDC_NM = this.ds_Detail12.getColumn(0, "UPDC_NM");//업데이트칼럼명
			sUPSC_NM = this.ds_Detail12.getColumn(0, "UPSC_NM");//업데이트상태칼럼명
		}
		
		//[결재] 결재버튼 비활성화-승인
		this.fn_SetComApprButtonAuth("APPR", false);

		var oWfParam = new Object();//[결재] 결재관련 파라메타 정보 Object
		
		// 결재재자 위치가 최종이면서, 신규결재일때만
		if (sWFLO_CD == "0030") {
			
			//최종결재 [승인] 시 업무영역 전처리 함수 호출-업무영역 처리 성공시
			if (this.fn_ComWfApprBizCall(oWfParam)) {
				var rtnValue = this.fn_WfApprStatusUpdate("0040");
				
				//처리실패시
				if (!rtnValue) {
					this.gfn_Alert("WF007");//결재상태 저장 시 오류가 발생했습니다.\nIT혁신팀에 문의하세요.
					//[결재] 결재버튼 비활성화-승인
					this.fn_SetComApprButtonAuth("APPR", true);
					return false;
				}
				
			} else {
				this.gfn_Alert("WF008");//[결재] 필수 입력 등록되지 않거나 프로시저 실행시 오류가 발생했습니다.
				//[결재] 결재버튼 비활성화-승인
				this.fn_SetComApprButtonAuth("APPR", true);			
				return false;
			}
			
		// 결재재자 위치가 중간이면서, 신규결재일때만
		} else if (sWFLO_CD == "0020") {
			//중간결재 [승인] 시 업무영역 전처리 함수 호출-업무영역 처리 성공시
			if (this.fn_ComWfMidApprBizCall(oWfParam)) {
				var rtnValue = this.fn_WfApprStatusUpdate("0030");
				
				//처리실패시
				if (!rtnValue) {
