	if (procCode == "0010") {
		this.fvWfMsg = "승인";//결재메시지
	} else if (procCode == "0020") {
		this.fvWfMsg = "기각";//결재메시지
	} else if (procCode == "0030") {
		this.fvWfMsg = "확인";//결재메시지
