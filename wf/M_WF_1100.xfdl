//서명이미지 정보 찾기
this.fn_FindImgUrl = function(sSYST_ID) 
{
	var rtnValue = "";
	
	if (this.ds_Detail15.getRowCount() > 0) {
		var iFindRow = this.ds_Detail15.findRow("SYST_ID", sSYST_ID);
		
		if (iFindRow > -1) {
			var sEMSI_ID = this.ds_Detail15.getColumn(iFindRow, "EMSI_ID");//저장파일명	
			var sFILE_UL = this.ds_Detail15.getColumn(iFindRow, "FILE_UL");//URL

			if (!this.gfn_IsNull(sEMSI_ID)) {
				var sEncode = encodeURI(sEMSI_ID.replace(/\+/,"%2B"));
				rtnValue = sFILE_UL + sEncode;
			}
		}
	} 
	
	return rtnValue;
}

