/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1 && !this.fvDtlSearchYn) {
	
		//결재PID정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_NO"))) {
			this.div_Appr.set_url("");
			//[결재버튼] Enable 설정
			this.fn_SetComApprButtonAuthAll(false);
			
			this.fvDtlSearchYn = true;//상세조회여부
	
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Detail11.clearData();
			this.ds_Detail12.clearData();
			this.ds_Detail13.clearData();
			this.ds_Detail15.clearData();
			this.ds_Input.clearData();
			this.fn_ClearApprData();//결재상세 내역데이터 초기화
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "WOFH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_NO"));
			this.ds_Input.setColumn(0, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//결재자ID
					
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "wf:M_WF_1100_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			//[공통]서비스정보 추가
			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "wf:M_WF_1100_D_SL11";
			this.gfn_AddComSearchService(this, svcAddObj);			
			
			//[공통]서비스정보 추가
			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "wf:M_WF_1100_D_SL12";
			this.gfn_AddComSearchService(this, svcAddObj);			

			//[공통]서비스정보 추가
			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "wf:M_WF_1100_D_SL13";
			this.gfn_AddComSearchService(this, svcAddObj);	
			
			//[공통]서비스정보 추가 - 서명이미지
			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "wf:M_WF_1100_D_SL15";
			this.gfn_AddComSearchService(this, svcAddObj);				
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_WF;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10 ds_Detail11=ds_Detail11 ds_Detail12=ds_Detail12 ds_Detail13=ds_Detail13 ds_Detail15=ds_Detail15";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);		
		}
	}
};

