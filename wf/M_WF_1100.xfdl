 * 1. 조회 영역
 ***********************************************************************************************/
/***********************************************************************************************
 * 01.01. [공통버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.fn_ComBtnSearch = function()
{ 

	var sQueryId = "";//쿼리ID
	this.fvInnerWf = false;//내부결재여부
	this.fvDtlSearchYn = false;//상세조회여부
	
	//결재화면 초기화
	this.div_Appr.set_url("");

	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Master10.clearData();
	this.ds_Detail10.clearData();
	this.ds_Detail11.clearData();
	this.ds_Detail12.clearData();
	this.ds_Detail13.clearData();	
	this.ds_Detail11Help.clearData();
	this.ds_Detail11Recv.clearData();
	this.ds_Input.clearData();
	this.fn_ClearApprData();//결재상세 내역데이터 초기화	
	
	//팝업창으로 호출했을경우
	if (!this.gfn_IsNull(this.fvWofhNo)) {
		sQueryId = "wf:M_WF_1100_M_SL18";//쿼리ID
		//[업무] 조회조건 입력
		this.ds_Input.addRow();
		this.ds_Input.setColumn(0, "WOFH_NO", this.fvWofhNo);		
	} else {
		//[결재버튼] Enable 설정
		this.fn_SetComApprButtonAuthAll(false);
		
		//조회전 유효성 체크
		if (!this.fn_VaildSearch()) return;	

		//[업무] 조회조건 입력
		this.ds_Input.addRow();
		this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_TemhDtFr.value);
		this.ds_Input.setColumn(0, "CLOS_DD", this.div_Search.form.cal_TemhDtTo.value);
		this.ds_Input.setColumn(0, "SESU_ID", this.div_Search.form.edt_SystId.value);
		this.ds_Input.setColumn(0, "MODU_CD", this.div_Search.form.cbo_ModmCd.value);
		this.ds_Input.setColumn(0, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//결재자ID
	
		var sWfDst = this.div_Search.form.rdo_Wfcm.value;//결재함구분
		
		//미결함
		if (sWfDst == "0010") {
			sQueryId = "wf:M_WF_1100_M_SL10";//쿼리ID
		//진행함
		} else if (sWfDst == "0020") {
			sQueryId = "wf:M_WF_1100_M_SL11";//쿼리ID
		//완료함
		} else if (sWfDst == "0030") {
			sQueryId = "wf:M_WF_1100_M_SL12";//쿼리ID		
		//반려함
		} else if (sWfDst == "0040") {
			sQueryId = "wf:M_WF_1100_M_SL13";//쿼리ID
		//결재함
		} else if (sWfDst == "0050") {
			sQueryId = "wf:M_WF_1100_M_SL14";//쿼리ID			
		//대행함
		} else if (sWfDst == "0060") {
			sQueryId = "wf:M_WF_1100_M_SL15";//쿼리ID		
		//취소함
		} else if (sWfDst == "0070") {
			sQueryId = "wf:M_WF_1100_M_SL17";//쿼리ID				
		} else {
			return;
		}
	}
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);	
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = sQueryId;
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MST_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_WF;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Master10=ds_Master10";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
}

