/***********************************************************************************************
 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
	
			if (this.ds_Master10.getRowCount() > 0) {			
				
				//입력영역 활성화
				
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterView.form.grd_Master10);
				
			} else {
				
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterView.form.grd_Master10);				
			}
			break;
		case "SAVE":   //저장		

			//첨부파일 추가건이 없으면
			if (!this.fvFileDiv.fn_CheckFileAddYn()) {
				this.fvSchKeyYn = true;//저장후 재조회여부
				this.fn_ComBtnSearch();//저장후 재조회
			} else {
				this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);			
			}
			break;	
		case "DTL_SEARCH": //상세조회		
			
			this.fvDtlSearchYn = false;//상세조회여부
			
			if (this.ds_Detail10.getRowCount() > 0) {
				this.ds_Detail10Appr.clearData();
				this.ds_Detail10.filter("");
				this.ds_Detail10.filter("WFGU_CD=='0010'");
				this.ds_Detail10Appr.copyData(this.ds_Detail10, true);	
				
				this.ds_Detail10ApprDir.clearData();
				this.ds_Detail10ApprDirTmp.clearData();
				this.ds_Detail10.filter("");
				this.ds_Detail10.filter("WFGU_CD=='0070'");
				//this.ds_Detail10ApprDir.copyData(this.ds_Detail10, true);	
				this.ds_Detail10ApprDirTmp.copyData(this.ds_Detail10, true);	
				this.ds_Detail10.filter("");
				
				if (this.ds_Detail10ApprDirTmp.getRowCount() > 0) {
					this.ds_Detail10ApprDir.addRow();
					var jIndex = 0;
					var sAPPR_CODE = "";
					for (var iLoop=0; iLoop < 11; iLoop++) {
						sAPPR_CODE = this.ds_Detail10ApprDirTmp.getColumn(0, "APPR_CODE" + iLoop);
						
						if (!this.gfn_IsNull(sAPPR_CODE)) {
							this.ds_Detail10ApprDir.setColumn(0, "APPR_NAME" + jIndex,  this.ds_Detail10ApprDirTmp.getColumn(0, "APPR_NAME" + iLoop));
							this.ds_Detail10ApprDir.setColumn(0, "APPR_JIKW" + jIndex,  this.ds_Detail10ApprDirTmp.getColumn(0, "APPR_JIKW" + iLoop));
							this.ds_Detail10ApprDir.setColumn(0, "APPR_DATE" + jIndex,  this.ds_Detail10ApprDirTmp.getColumn(0, "APPR_DATE" + iLoop));
							this.ds_Detail10ApprDir.setColumn(0, "APPR_TIME" + jIndex,  this.ds_Detail10ApprDirTmp.getColumn(0, "APPR_TIME" + iLoop));
							this.ds_Detail10ApprDir.setColumn(0, "APPR_CODE" + jIndex,  this.ds_Detail10ApprDirTmp.getColumn(0, "APPR_CODE" + iLoop));
							jIndex++;
						}
						
					}
				}
				
				var sImgUrl = "";//서명이미지URL
				var sApprCode = "";//결재자시스템사번
				var sFindWfreCd = "";//결재결과
				if (this.ds_Detail10Appr.getRowCount() > 0) {
					var iIndex = 0;
					
					//for (var iLoop=0; iLoop < 9; iLoop++) {
					for (var iLoop=0; iLoop < 11; iLoop++) {
						if (!this.gfn_IsNull(this.ds_Detail10Appr.getColumn(0, "APPR_NAME" + iLoop))) {
							if (iIndex < 7) {
								eval("this.div_DetailInput.form.div_WfSign.form.sta_ApprNm" + iIndex).set_text(this.ds_Detail10Appr.getColumn(0, "APPR_NAME" + iLoop) + "(" + this.ds_Detail10Appr.getColumn(0, "APPR_JIKW" + iLoop) +")");//성명
								//eval("this.div_DetailInput.form.div_WfSign.form.sta_ApprJikw" + iLoop).set_text(this.ds_Detail10Appr.getColumn(0, "APPR_JIKW" + iLoop));//직위
								eval("this.div_DetailInput.form.div_WfSign.form.sta_ApprDate" + iIndex).set_text(this.ds_Detail10Appr.getColumn(0, "APPR_DATE" + iLoop));//일자
								eval("this.div_DetailInput.form.div_WfSign.form.sta_ApprTime" + iIndex).set_text(this.ds_Detail10Appr.getColumn(0, "APPR_TIME" + iLoop));//시간
								sApprCode = this.ds_Detail10Appr.getColumn(0, "APPR_CODE" + iLoop);//결재자시스템사번
								sImgUrl = this.fn_FindImgUrl(sApprCode);//서명이미지URL							
								//eval("this.div_DetailInput.form.div_WfSign.form.img_ApprSign" + iLoop).set_image(sImgUrl);//서명
								eval("this.div_DetailInput.form.div_WfSign.form.img_ApprSign" + iIndex).set_image(sImgUrl);//서명
								
								sFindWfreCd = this.fn_FindWfreCd(sApprCode);//결재결과
								//결재결과 - 기각이면
								if (sFindWfreCd == "0020") {
									eval("this.div_DetailInput.form.div_WfSign.form.sta_ApprNm" + iIndex).set_background("#c19feb");
								}
								
								//iIndex++;
							}
							iIndex++;
						}
					}
				}
				
