/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 

	//내용 Web컴포넌트 정보값 세팅
	var oWeb = this.div_DetailInput.form.tab_Detail.tpg_01.form.web_ContNm.getProperty("window");
	var sOffhNm = oWeb.callMethod("fn_getData");//내용	

	if (this.ds_Master10.getRowCount() > 0 && this.ds_Master10.rowposition > -1) {
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "OFFH_NM", sOffhNm);//본문내용
	}
	
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}

	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		//첨부파일정보만 변경시
		if (!this.gfn_DsIsUpdated(this.ds_Master10) && this.ds_Master10.getRowCount() > 0) {
			//첨부파일정보 저장
			fvFileInfo = new Object();//[공통] 첨부파일정보
			fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, B-상세(그리드Row), E-기타
			fvFileInfo.CALLBACK_FUNC = "";//호출할 콜백함수
			fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
			fvFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OFFH_NO");//PK번호2-관리번호
			fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
			//첨부파일Div 화면에 Key세팅하게 변경
			this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);
			return;	
		}
	}

	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		if (!this.fn_VaildMasterSave()) return;
	}

	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	//Master 입력 상태이면
	if (sChkRowType == "I") {
	
		//채번-관리번호
		var sTemhNo = this.gfn_GetTbnmNo(this.fvComPrgmId, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OFFH_DD"));//관리번호
		if (this.gfn_IsNull(sTemhNo)) {
			//관리번호를 가져오는데 실패했습니다.
			return;
		}
		
		var sChkTemhNo = "";//출고번호-체크용
		var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position
		
		//Master 데이터셋에 채번정보 입력-관리번호
		this.ds_Master10.setColumn(iMasterRowPos, "OFFH_NO", sTemhNo);
		this.ds_Master10.setColumn(iMasterRowPos, "WOFH_SQ", "1");//결재차수
		this.ds_Master10.setColumn(iMasterRowPos, "WOFH_YN", "1");//최종여부
		
		//사외공문이 아닌경우 문서번호 채번
		if (this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_OffhGu.value != "0050") {
			if (this.fn_GetDocuNo()) {
				this.ds_Master10.setColumn(iMasterRowPos, "DOCU_NO", this.ds_Master12.getColumn(0, "DOCU_NO"));//문서번호
			} else {
				return;
			}
		}
	}	
	
	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		fvFileInfo = new Object();//[공통] 첨부파일정보
		fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
		fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
		fvFileInfo.CALLBACK_FUNC = "fn_ComBtnSearch";//호출할 콜백함수
		fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
		fvFileInfo.PK01_NO = nApp.gvCompany;//PK번호1-회사코드
		fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OFFH_NO");//PK번호2-관리번호
		fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수			
	}	
	
	//저장시 재조회 Key설정
	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OFFH_NO");//조회Key01
	this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//조회Key02

	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	//[공통]서비스 정보 추가
	svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
	svcAddObj.sInsertSql = "wf:M_WF_2100_M_IN10";//저장Sql
	svcAddObj.sUpdateSql = "wf:M_WF_2100_M_UP10";//수정Sql
	svcAddObj.sDeleteSql = "wf:M_WF_2100_M_DL10";//삭제Sql
	svcAddObj.sSaveFlagColumn = "";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
							 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

	this.gfn_AddComSaveService(this, svcAddObj);

	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "SAVE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_WF;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Master10=ds_Master10:U" //입력 데이터셋
	svcCallObj.sOutDatset = "";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명		
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	

}

/***********************************************************************************************
 * 05.02. [Vaildation] Master 저장 필수값 체크 - 입력항목 
 ***********************************************************************************************/ 
this.fn_VaildMasterSave = function()
{
	//[저장] Validation 설정
	var arrArgs = ["this.div_MasterInput.form.tab_Master.tpg_01.form.cal_OffhDd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_OffhDd.text + "|NULL" //등록일자
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_DepmCd.text + "|NULL" //등록부서
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SystId^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_EmpmCd.text + "|NULL" //등록자
				  ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_OffhGu^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_OffhGu.text + "|NULL" //문서구분
				  ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_TitlNm^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_TitlNm.text + "|NULL" //제목
				  //,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ReceNm^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_ReceNm.text + "|NULL" //수신
				  //,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_RefeNm^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_RefeNm.text + "|NULL" //참조
				  //,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_TeleNo^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_TeleNo.text + "|NULL" //전화번호
				  //,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_FaxtNo^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_FaxtNo.text + "|NULL" //FAX번호
				//  ,"this.div_MasterInput.form.tab_Master.tpg_01.form.mae_OffhAm^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_OffhAm.text + "|NULL" //기안금액				  
			      ]; 
			
	var sCheckGroupTitle = this.div_MasterInput.form.tab_Master.tpg_01.text;//체크그룹 타이틀
	
	var iRow = this.ds_Master10.rowposition;

	var sOFFH_GU = this.ds_Master10.getColumn(iRow, "OFFH_GU");//문서구분

	if (!this.gfn_IsNull(sOFFH_GU)) {
		var iFindRow = this.ds_Master13.findRow("OFFM_CD", sOFFH_GU);//문서구분

		if (iFindRow > -1) {
			var sOTEL_YN = this.ds_Master13.getColumn(iFindRow, "OTEL_YN");//전화번호여부
			var sOFAX_YN = this.ds_Master13.getColumn(iFindRow, "OFAX_YN");//팩스번호여부
			var sRCPT_YN = this.ds_Master13.getColumn(iFindRow, "RCPT_YN");//수신처여부
			var sCONF_YN = this.ds_Master13.getColumn(iFindRow, "CONF_YN");//참조처여부
			var sWFTI_CD = this.ds_Master13.getColumn(iFindRow, "WFTI_CD");//문서번호
			
			//문서번호-등록시
			if (sWFTI_CD == "0010") {
				//arrArgs.push("this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DocuNo^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_DocuNo.text + "|NULL");//문서번호
			}				

			if (sOTEL_YN == "1") {
				arrArgs.push("this.div_MasterInput.form.tab_Master.tpg_01.form.edt_TeleNo^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_TeleNo.text + "|NULL");//전화번호
			}
			
			if (sOFAX_YN == "1") {
				arrArgs.push("this.div_MasterInput.form.tab_Master.tpg_01.form.edt_FaxtNo^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_FaxtNo.text + "|NULL");////FAX번호
			}
			
			if (sRCPT_YN == "1") {
				arrArgs.push("this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ReceNm^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_ReceNm.text + "|NULL");//수신
			}
			
			if (sCONF_YN == "1") {
				arrArgs.push("this.div_MasterInput.form.tab_Master.tpg_01.form.edt_RefeNm^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_RefeNm.text + "|NULL");//참조
			}
					
		}
	}	

	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
		return false;
	}

	return true;
}

/***********************************************************************************************
 * 05.03. [문서번호 채번] 사외공문(0050) 이 아닌경우 저장시 문서번호 채번
 ***********************************************************************************************/ 
this.fn_GetDocuNo = function()
{
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Master12.clearData();
	this.ds_Input.clearData();

	//[업무] 조회조건 입력
