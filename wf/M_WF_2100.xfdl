/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {

		//[결재] 결재상태에 따른 컴포넌트 제어
		this.fn_SetWfCompEnable();		
		
		//관리번호 정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OFFH_NO"))) {
		
			//내용 Web컴포넌트에 값 세팅
			//웹컴포넌트가 로드 완료되었을경우							
			if (this.fvWebCompLoadYn) {			
				//var oWeb = this.div_DetailInput.form.tab_Detail.tpg_01.form.web_ContNm.getProperty("window");
				//var sContNm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OFFH_NM");//본문내용
				//oWeb.callMethod("fn_setData", this.gfn_ReplaceWebEditorData(sContNm));			
				
				this.setTimer(998,500);
				
			}
			
			//첨부파일 정보 조회
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
			oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OFFH_NO");//PK번호2-관리번호
			oFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수						
			
			this.fvFileDiv.fn_AtchFileSearch(oFileInfo);	
		}
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master10] Row 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10) || this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	

	return true;
};  

/*---------------------------------------------------------------------------------------------
 *    03. [데이터셋-ds_Master10] Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	if (e.columnid == "OFFH_GU") {
		var iRow = this.ds_Master10.rowposition;

		var sOFFH_GU = this.ds_Master10.getColumn(iRow, "OFFH_GU");//문서구분

		if (!this.gfn_IsNull(sOFFH_GU)) {
			var iFindRow = this.ds_Master13.findRow("OFFM_CD", sOFFH_GU);//문서구분

			if (iFindRow > -1) {
				var sOTEL_YN = this.ds_Master13.getColumn(iFindRow, "OTEL_YN");//전화번호여부
				var sOFAX_YN = this.ds_Master13.getColumn(iFindRow, "OFAX_YN");//팩스번호여부
				var sRCPT_YN = this.ds_Master13.getColumn(iFindRow, "RCPT_YN");//수신처여부
				var sCONF_YN = this.ds_Master13.getColumn(iFindRow, "CONF_YN");//참조처여부				
				var sWFTI_CD = this.ds_Master13.getColumn(iFindRow, "WFTI_CD");//문서번호

				//문서구분-등록시
				if (sWFTI_CD == "0010") {
					//this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DocuNo.set_cssclass("essential");
					//this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DocuNo.set_readonly(false);
					
				} else {
					//this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DocuNo.set_cssclass("");
					//this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DocuNo.set_readonly(true);
				}

				if (sOTEL_YN == "1") {
					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_TeleNo.set_cssclass("essential");
				} else {
					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_TeleNo.set_cssclass("");
				}
				
				if (sOFAX_YN == "1") {
					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_FaxtNo.set_cssclass("essential");
				} else {
					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_FaxtNo.set_cssclass("");
				}
				
				if (sRCPT_YN == "1") {
					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ReceNm.set_cssclass("essential");			
				} else {
					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ReceNm.set_cssclass("");			
				}
				
				if (sCONF_YN == "1") {
					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_RefeNm.set_cssclass("essential");	
				} else {
					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_RefeNm.set_cssclass("");
				}
						
				return;
			}
		}
		
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DocuNo.set_cssclass("");
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_TeleNo.set_cssclass("");
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_FaxtNo.set_cssclass("");
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ReceNm.set_cssclass("");	
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_RefeNm.set_cssclass("");	
	}
};

