{
	if (e.newrow > -1) {

		//첨언정보가 있으면 표시
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(e.newrow, "MEMO_NM"))) {
			this.txt_MemoNm.set_value(this.ds_Master10.getColumn(e.newrow, "MEMO_NM"));
		} else {
			this.txt_MemoNm.set_value("");
		}
	}	
};

 
