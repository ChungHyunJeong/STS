this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
			if (this.ds_Master10.getRowCount() > 0) {
				this.div_Search.form.edt_WofhNm.set_value(this.ds_Master10.getColumn(0, "WOFH_NM"));//결재명
			}
			break;						
	}
};


