/***********************************************************************************************
 * 05.02. [Vaildation] Master 저장 필수값 체크 - 입력항목 
 ***********************************************************************************************/ 
this.fn_VaildMasterSave = function()
{
	//[저장] Validation 설정
	var arrArgs = ["WCLD_SE^NULL"
	              ,"WFGU_CD^NULL"				  		  
			      ,"SYST_ID^NULL"
			      ]; 
				  
	var sCheckGroupTitle = this.div_DetailInput.form.tab_Detail.tpg_01.text;//체크그룹 타이틀
				  
	//Grid Vaild 체크
	if (!this.gfn_CheckGridValid(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10, arrArgs, sCheckGroupTitle) ) {
		return false;
	}
	
	if (this.ds_Master10.getRowCount() > 0) {
		var sWFGU_CD = "";//결재구분
		var iProvRow = 0;//결재/협조Row
		var iRecvRow = 0;//접수Row
		var iLastProvRow = 0;//마지막 결재자Row
		var sWAIT_GU = "";//결재대기여부

		for (var iLoop=0; iLoop < this.ds_Master10.getRowCount(); iLoop++) {
			sWFGU_CD = this.ds_Master10.getColumn(iLoop, "WFGU_CD");//결재구분
			
			//결재구분이 접수인경우
			if (sWFGU_CD == "0030") {
				iRecvRow = iLoop;//접수Row
				break;
			}			
		}
		
		for (var iLoop=0; iLoop < this.ds_Master10.getRowCount(); iLoop++) {
			sWFGU_CD = this.ds_Master10.getColumn(iLoop, "WFGU_CD");//결재구분
			
			//결재구분이 결재/협조/결재(주관부서)인경우
			if (sWFGU_CD == "0010" || sWFGU_CD == "0020" || sWFGU_CD == "0070") {
				iProvRow = iLoop;//결재/협조Row
				sWAIT_GU = this.ds_Master10.getColumn(iLoop, "WAIT_GU");//결재대기여부
				//결재대기여부-결재대기/결재완료가 아닌경우
				if (sWAIT_GU != "1" && sWAIT_GU != "2") {
					iLastProvRow = iLoop;//마지막 결재자Row
				}
				
			}			
		}	
		
		//마지막 결재자 정보가 없는경우
		if (iLastProvRow < 1) {
			var arrMsgArg = ["최종 결재자"];
			this.gfn_Alert("SM008", arrMsgArg); //는 필수 입력 항목입니다		
			return false;
		}

		//접수자가 결재/협조가 보다 결재라인에 먼저 있을경우 메세지
		if (iRecvRow > 0 && iRecvRow < iProvRow) {
			this.gfn_Alert("WF025");//결재구분[접수]는 결재라인에 마지막에 위치해야 합니다.
			return false;
		}
		
	}		
	
	return true;
}

