/*---------------------------------------------------------------------------------------------
 *    04. [Detail영역-버튼] ▶ 클릭 이벤트
  ---------------------------------------------------------------------------------------------*/
this.div_DetailInput_tab_Detail_tpg_01_btn_Add_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if (this.ds_Empm.getRowCount() > 0) {
		var iRowPos = this.ds_Empm.rowposition;
		
		if (iRowPos > -1) {
			var sWfliCd = this.div_DetailInput.form.tab_Detail.tpg_01.form.cbo_WfliCd.value;//결재구분
			
			if (this.gfn_IsNull(sWfliCd)) {
				var arrMsgArg = [this.div_DetailInput.form.tab_Detail.tpg_01.form.sta_WfliCd.text];
				this.gfn_Alert("SM040", arrMsgArg);//&ITM0을(를) 선택하세요.
				this.div_DetailInput.form.tab_Detail.tpg_01.form.cbo_WfliCd.setFocus();
				return;
			}

			var iAddRow = this.ds_Master10.addRow();
			
			var iUsrdSe = 0;
			if (this.ds_Master10.getMax("WCLD_SE") == undefined) {
				iUsrdSe = 1;
			} else {
				iUsrdSe = this.ds_Master10.getMax("WCLD_SE") + 1;
			}

			this.ds_Master10.setColumn(iAddRow, "WOFH_NO", this.ds_Master10.getColumn(0, "WOFH_NO"));//결재PID
			this.ds_Master10.setColumn(iAddRow, "WCLD_SE", iUsrdSe);//결재자순서
			this.ds_Master10.setColumn(iAddRow, "WFGU_CD", sWfliCd);//결재구분
			this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.ds_Empm.getColumn(iRowPos, "SYST_ID"));//결재자사번
			this.ds_Master10.setColumn(iAddRow, "WFCO_CD", this.ds_Empm.getColumn(iRowPos, "COMM_CD"));//회사코드
			this.ds_Master10.setColumn(iAddRow, "WFSU_ID", this.ds_Empm.getColumn(iRowPos, "SYST_ID"));//결재자사번
			this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.ds_Empm.getColumn(iRowPos, "EMPM_CD"));//사번
			this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.ds_Empm.getColumn(iRowPos, "EMPM_NM"));//성명
			this.ds_Master10.setColumn(iAddRow, "POSI_NM", this.ds_Empm.getColumn(iRowPos, "POSI_NM"));//직위
			this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.ds_Empm.getColumn(iRowPos, "DEPM_NM"));//부서명	
			this.ds_Master10.setColumn(iAddRow, "DUTY_NM", this.ds_Empm.getColumn(iRowPos, "DUTY_NM"));//직책명
			this.ds_Master10.setColumn(iAddRow, "RANK_NM", this.ds_Empm.getColumn(iRowPos, "RANK_NM"));//직급명
			this.ds_Master10.setColumn(iAddRow, "SORT_NO", iUsrdSe);//정렬순서
			this.ds_Master10.setColumn(iAddRow, "crudType", "I");//crudType
			this.ds_Master10.setColumn(iAddRow, "REQU_YN", "0");//필수여부
			
			//중복체크
			if (!this.fn_DupCheck()) {
				this.ds_Master10.deleteRow(iAddRow);
				return;
			}
			
			//결재순서 재설정
			for (var iLoop=0; iLoop < this.ds_Master10.getRowCount(); iLoop++) {
				this.ds_Master10.setColumn(iLoop, "WCLD_SE", iLoop+1);		
				this.ds_Master10.setColumn(iLoop, "SORT_NO", iLoop+1);//정렬순서
			}				
			
			//접수자 순서체크
			if (!this.fn_WfLiCd_DupCheck(iAddRow)) {
				return;
			}
		}
	}
};

/*---------------------------------------------------------------------------------------------
 *    05. [Detail영역-버튼] ◀ 클릭 이벤트
  ---------------------------------------------------------------------------------------------*/
this.div_DetailInput_tab_Detail_tpg_01_btn_Add00_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if (this.ds_Master10.getCaseCount("s_chk == '1'") > 0) {
		for (var iLoop=this.ds_Master10.getRowCount(); iLoop >=0; iLoop--) {
			var sChk = this.ds_Master10.getColumn(iLoop, "s_chk");
			
			if (sChk == "1") {
				this.ds_Master10.deleteRow(iLoop);
			}
			
		}
		
		//결재순서 재설정
		for (var iLoop=0; iLoop < this.ds_Master10.getRowCount(); iLoop++) {
			this.ds_Master10.setColumn(iLoop, "WCLD_SE", iLoop+1);		
			this.ds_Master10.setColumn(iLoop, "SORT_NO", iLoop+1);//정렬순서
		}	
			
	} else {
		this.gfn_Alert("SM006");//삭제할 대상을 선택하세요.
		return;
	}
};

/*---------------------------------------------------------------------------------------------
 *    06. [Detail영역-버튼] ▲ 클릭 이벤트
  ---------------------------------------------------------------------------------------------*/
this.div_DetailInput_tab_Detail_tpg_01_btn_Up_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var iRowNowPos = this.ds_Master10.rowposition;//현재Row
	var iRowMovePos = iRowNowPos - 1;//이동할Row
	
	if (iRowMovePos == 0) return;
	
	var sWFGU_CD = this.ds_Master10.getColumn(iRowNowPos, "WFGU_CD");//결재구분
	var sWAIT_GU = this.ds_Master10.getColumn(iRowMovePos, "WAIT_GU");//결재대기여부
	var sWFGU_CD_NOW = this.ds_Master10.getColumn(iRowNowPos, "WAIT_GU");//결재대기여부-현재Row

	if (sWFGU_CD_NOW == '1' || sWFGU_CD_NOW == '2') {
		return;
	}
	
	if (sWFGU_CD == "0060") {
		this.gfn_Alert("WF015");//기안자는 결재순서를 변경하실수 없습니다.
		return;
	}
	
	if (sWAIT_GU != '1' && sWAIT_GU != '2') {
		this.ds_Master10.moveRow(iRowNowPos, iRowMovePos);//행이동
		for (var iLoop=0; iLoop < this.ds_Master10.getRowCount(); iLoop++) {
			this.ds_Master10.setColumn(iLoop, "WCLD_SE", iLoop+1);		
			this.ds_Master10.setColumn(iLoop, "SORT_NO", iLoop+1);//정렬순서
		}	
	}
};

/*---------------------------------------------------------------------------------------------
 *    07. [Detail영역-버튼] ▼ 클릭 이벤트
  ---------------------------------------------------------------------------------------------*/
this.div_DetailInput_tab_Detail_tpg_01_btn_Down_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var iRowNowPos = this.ds_Master10.rowposition;//현재Row
	var iRowMovePos = iRowNowPos + 1;//이동할Row

	var sWFGU_CD = this.ds_Master10.getColumn(iRowNowPos, "WFGU_CD");//결재구분
	var sWFGU_CD_Next = this.ds_Master10.getColumn(iRowMovePos, "WFGU_CD");//결재구분-Next
	var sWAIT_GU = this.ds_Master10.getColumn(iRowMovePos, "WAIT_GU");//결재대기여부	
	var sWFGU_CD_NOW = this.ds_Master10.getColumn(iRowNowPos, "WAIT_GU");//결재대기여부-현재Row
	
	if (sWFGU_CD_NOW == '1' || sWFGU_CD_NOW == '2') {
		return;
	}	

	if (sWFGU_CD == "0060") {
		this.gfn_Alert("WF015");//기안자는 결재순서를 변경하실수 없습니다.
		return;
	}
	
	if (sWFGU_CD_Next == "0030") return;
	
	if (sWAIT_GU != '1' && sWAIT_GU != '2') {	
		this.ds_Master10.moveRow(iRowNowPos, iRowMovePos);//행이동
