 ***********************************************************************************************/
/***********************************************************************************************
 * 00.01. Form Include 선언
 ***********************************************************************************************/
//include "comLib::CommonUtil.xjs";//공통함수

/***********************************************************************************************
 * 00.02. Form 변수 선언
 ***********************************************************************************************/
this.fvComPrgmId = "";//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
this.fvWofhNo = "";//[부모창파라메타] 결재PID
this.fvWctmId = "";//[부모창파라메타] 결재업무ID
this.fvWfchYn = "";//[부모창파라메타] 결재선변경여부
this.fvWfabYn = "";//[부모창파라메타] 전결규정설정
this.fvSystId = "";//[부모창파라메타] 기안자

/***********************************************************************************************
 * 00.03. Form Onload 이벤트
 ***********************************************************************************************/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{
	this.fvWofhNo = this.getOwnerFrame().WofhNo;//[부모창파라메타] 결재PID
	this.fvComPrgmId = this.getOwnerFrame().PrgmId;//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
	//this.fvWctmId = this.getOwnerFrame().WctmId;//[부모창파라메타] 결재업무ID
	//this.fvWfchYn = this.getOwnerFrame().WfchYn;//[부모창파라메타] 결재선변경여부
	//this.fvWfabYn = this.getOwnerFrame().WfabYn;//[부모창파라메타] 전결규정설정
	//this.fvSystId = this.getOwnerFrame().SystId;//[부모창파라메타] 기안자

	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 
}

/***********************************************************************************************
 * 00.04. Form [공통] 초기설정 함수 - 필수
 ***********************************************************************************************/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	this.gfn_SetGrid(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Empm);//[공통]그리드 이벤트 설정 - 사원정보
	this.gfn_SetGrid(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);//[공통]그리드 이벤트 설정 - 결재라인 상세정보
}

/***********************************************************************************************
 * 00.05. Form [업무] 초기설정 함수
 ***********************************************************************************************/
this.fn_FormBizInit = function() 
{ 
	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	var objCondList = ["WF105|ds_Wf105Grd","WF105|ds_Wf105|Y|선택"];
	
	var objFilterList = ["NA"
					  ,"OP01_NM=='1'"
	                   ];	 	
	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, objFilterList);
	
	//콤보 인덱스 설정
	this.div_DetailInput.form.tab_Detail.tpg_01.form.cbo_WfliCd.set_index(0);

	if (!this.gfn_IsNull(this.fvWofhNo)) {
		this.fn_ComBtnSearch(this.fvWofhNo);//조회
	}
}

/***********************************************************************************************
 * 00.06. Form [업무] 일반콤보 조회 함수
 ***********************************************************************************************/
this.fn_ComboSearch = function() 
{
	
}

