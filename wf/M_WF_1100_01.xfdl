this.fn_DupCheck = function() 
{ 
	var iCnt = 0;
	
	for (var iLoop=0; iLoop < this.ds_Master10.getRowCount(); iLoop++) {
		for (var jLoop=0; jLoop < this.ds_Master10.getRowCount(); jLoop++) {
			if (iLoop != jLoop && this.ds_Master10.getColumn(iLoop, "SYST_ID") == this.ds_Master10.getColumn(jLoop, "SYST_ID")) {
				iCnt++;
				break;
			}
		}
	}
	
	if (iCnt > 0) {
		this.gfn_Alert("SM041");//중복된 데이터가 존재합니다.
		return false;
	}
	
	return true;

}

/***********************************************************************************************
 * 15.04. [사용자함수] 접수자 순서체크
 ***********************************************************************************************/ 
 this.fn_WfLiCd_DupCheck = function(nRow) 
{ 
	var iCnt = 0;
	
	for (var iLoop=0; iLoop < this.ds_Master10.getRowCount(); iLoop++) {
		//선택된 Row가 접수인 경우
		if (this.ds_Master10.getColumn(nRow, "WFLI_CD") == "0030") {
			if (this.ds_Master10.getColumn(iLoop, "WCLD_SE") > this.ds_Master10.getColumn(nRow, "WCLD_SE") && this.ds_Master10.getColumn(iLoop, "WFLI_CD") != "0030") {
				iCnt++;
			}
		} else {
			if (this.ds_Master10.getColumn(iLoop, "WCLD_SE") < this.ds_Master10.getColumn(nRow, "WCLD_SE") && this.ds_Master10.getColumn(iLoop, "WFLI_CD") == "0030") {
				iCnt++;
			}
		}
	}
	
	if (iCnt > 0) {
		var strMsg = "결재자 지정 시 결재구분 순서!! \n"
			   + "[순서] ①결재 or 협조 ②접수\n\n"
			   + "'결재구분' or '순서'";
		var arrMsgArg = [strMsg];
		this.gfn_Alert("SM042", arrMsgArg);//&ITM0을(를) 선택하세요.
		
		return false;
	}
	
	return true;
}

