}

/***********************************************************************************************
 * 05.03. [Vaildation] Detail 저장 필수값 체크 - 그리드항목
 ***********************************************************************************************/ 
this.fn_VaildDetailSave = function()
{
	//[저장] Validation 설정
	var arrArgs = ["USRD_SE^NULL"
	              ,"WFGU_CD^NULL"				  
	              ,"WFCR_CD^NULL"			  
			      ,"WFSU_ID^NULL"
			      ]; 
				  
	var sCheckGroupTitle = this.div_DetailInput.form.tab_Detail.tpg_01.text;//체크그룹 타이틀
				  
	//Grid Vaild 체크
	if (!this.gfn_CheckGridValid(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10, arrArgs, sCheckGroupTitle) ) {
		return false;
	}
	
	if (this.ds_Detail10.getRowCount() > 0) {
		var sWFCR_CD = "";//결재생성위치
		var sWFGU_CD = "";//결재구분
		var bCheckFlag = false;
		var sMsg = "";
		for (var iLoop=0; iLoop < this.ds_Detail10.getRowCount(); iLoop++) {
			sWFCR_CD = this.ds_Detail10.getColumn(iLoop, "WFCR_CD");//결재생성위치
			sWFGU_CD = this.ds_Detail10.getColumn(iLoop, "WFGU_CD");//결재구분
			
			//결재생성위치-자동생성전 이면서 결재구분이 접수인경우
			if (sWFCR_CD == "0010" && sWFGU_CD == "0030") {
				bCheckFlag = true;
				if (this.gfn_IsNull(sMsg)) {
					sMsg = iLoop + 1;
				} else {
					sMsg = sMsg + ", " + (iLoop + 1);
				}
			}
			
		}
		
		if (bCheckFlag) {
			var arrMsg = [sMsg];
			this.gfn_Alert("WF020", arrMsg);//&ITM0 행 입력 데이터 오류\n접수인경우에는 결재생성위치를 자동생성후로 등록해야 합니다.
			return false;
		}
		
	}		
	
	return true;
}
 
