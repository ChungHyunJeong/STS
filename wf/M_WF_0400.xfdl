 *    04. [Detail영역-버튼] ▶ 클릭 이벤트
  ---------------------------------------------------------------------------------------------*/
this.div_DetailInput_tab_Detail_tpg_01_btn_Add_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if (this.ds_Empm.getRowCount() > 0) {
	
		var iMstRowPos = this.ds_Master10.rowposition;
		
		//결재라인 정보 체크
		if (iMstRowPos < 0) {
			this.gfn_Alert("WF040");//결재라인을 선택하세요.
			return;
		}		
		
		var iRowPos = this.ds_Empm.rowposition;
		
		if (iRowPos > -1) {
			var sWfliCd = this.div_DetailInput.form.tab_Detail.tpg_01.form.cbo_WfliCd.value;//결재구분
			
			if (this.gfn_IsNull(sWfliCd)) {
				var arrMsgArg = [this.div_DetailInput.form.tab_Detail.tpg_01.form.sta_WfliCd.text];
				this.gfn_Alert("SM040", arrMsgArg);//&ITM0을(를) 선택하세요.
				this.div_DetailInput.form.tab_Detail.tpg_01.form.cbo_WfliCd.setFocus();
				return;
			}

			var iAddRow = this.ds_Detail10.addRow();
			
			var iUsrdSe = 0;
			if (this.ds_Detail10.getMax("USRD_SE") == undefined) {
				iUsrdSe = 1;
			} else {
				iUsrdSe = this.ds_Detail10.getMax("USRD_SE") + 1;
			}
	
			if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "USRM_NO"))) {
				this.ds_Detail10.setColumn(iAddRow, "USRM_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "USRM_NO"));//결재라인번호
			}
			this.ds_Detail10.setColumn(iAddRow, "USRD_SE", iUsrdSe);//결재자순서
			//this.ds_Detail10.setColumn(iAddRow, "WFLI_CD", sWfliCd);//결재라인입력구분
			this.ds_Detail10.setColumn(iAddRow, "WFGU_CD", sWfliCd);//결재구분
			this.ds_Detail10.setColumn(iAddRow, "WFSU_ID", this.ds_Empm.getColumn(iRowPos, "SYST_ID"));//결재자사번
			this.ds_Detail10.setColumn(iAddRow, "EMPM_CD", this.ds_Empm.getColumn(iRowPos, "EMPM_CD"));//사번
			this.ds_Detail10.setColumn(iAddRow, "EMPM_NM", this.ds_Empm.getColumn(iRowPos, "EMPM_NM"));//성명
			this.ds_Detail10.setColumn(iAddRow, "POSI_NM", this.ds_Empm.getColumn(iRowPos, "POSI_NM"));//직위
			this.ds_Detail10.setColumn(iAddRow, "DUTY_NM", this.ds_Empm.getColumn(iRowPos, "DUTY_NM"));//직책
			this.ds_Detail10.setColumn(iAddRow, "DEPM_NM", this.ds_Empm.getColumn(iRowPos, "DEPM_NM"));//부서명	
			
			this.ds_Detail10.setColumn(iAddRow, "ROWS_TP", "I");
			
			
			//중복체크
			if (!this.fn_DupCheck()) {
				this.ds_Detail10.deleteRow(iAddRow);
				return;
			}
			
			//접수자 순서체크
			if (!this.fn_WfLiCd_DupCheck(iAddRow)) {
				return;
			}
		}
	}
	
	trace(this.ds_Detail10.saveXML());
};

