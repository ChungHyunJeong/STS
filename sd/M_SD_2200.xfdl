 * 02.DG0.07. [Detail 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)

	//품목코드
	if (e.columnid == "ITEM_CD") {
		this.gfn_GetCommItemGrid(obj, "ITEM_CD", "ITEM_NM");//품명 검색			
				
	// 수량 입력(변경)시
	} else if (e.columnid == "QUTB_QT") {
		var iQutrQt = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUTB_QT");//수량
		var iWoneAm = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "WONE_AM");//원화금액 (상세합계)
		var iForeAm = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "FORE_AM");//외화금액 (상세합계)
		var iWonePr = "";//원화단가
		var iForePr = "";//외화단가

		//원화단가
		if (!this.gfn_IsNull(iWoneAm)) {	//원화금액 존재할 경우
			if (iWoneAm > 0) {	//원화금액 0보다 클 경우
				iWonePr = nexacro.floor(iWoneAm / iQutrQt, 2); 
			} else if (!this.gfn_IsNull(iQutrQt)) {	//수량이 존재할 경우
				iWonePr = 0; 
			
			}
		} 
		
		//외화단가
		if (!this.gfn_IsNull(iForeAm)) {	//외화금액 존재할 경우
			if (iForeAm > 0) {	//외화금액 0보다 클 경우
				iForePr = nexacro.floor(iForeAm / iQutrQt, 2); 
			} else if (!this.gfn_IsNull(iQutrQt)) {	//수량이 존재할 경우
				iForePr = 0; 
			
			}
		} 
			
		//그리드에 적용
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "WONE_PR", nexacro.floor(iWonePr,0));	//원화단가(10/18 저장시 소숫점 0 절사함)
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "FORE_PR", iForePr);	//외화단가
		
	// 원화금액 변경시
	} else if (e.columnid == "WONE_AM") {

		var iQutrQt = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUTB_QT");//수량
		var iWoneAm = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "WONE_AM");//원화금액 (상세합계)
		var iWonePr = "";//원화단가

		//원화단가
		if (!this.gfn_IsNull(iWoneAm)) {	//원화금액 존재할 경우
			if (iWoneAm > 0) {	//원화금액 0보다 클 경우
				iWonePr = nexacro.floor(iWoneAm / iQutrQt, 2); 
			} else if (!this.gfn_IsNull(iQutrQt)) {	//수량이 존재할 경우
				iWonePr = 0; 
			
			}
		} 		
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "WONE_PR", nexacro.floor(iWonePr,0));	//원화단가(10/18 저장시 소숫점 0 절사함)
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM",this.ds_Detail10.getCaseSum("crudType!='D'","WONE_AM"));	//Master에 원화금액 표기
		
	// 외화금액 변경시
	} else if (e.columnid == "FORE_AM") {
		var iQutrQt = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUTB_QT");//수량
		var iForeAm = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "FORE_AM");//외화금액 (상세합계)
		var iForePr = "";//외화단가
		
		//외화단가
		if (!this.gfn_IsNull(iForeAm)) {	//외화금액 존재할 경우
			if (iForeAm > 0) {	//외화금액 0보다 클 경우
				iForePr = nexacro.floor(iForeAm / iQutrQt, 2); 
			} else if (!this.gfn_IsNull(iQutrQt)) {	//수량이 존재할 경우
				iForePr = 0; 
			
			}
		} 
		
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "FORE_PR", iForePr);	//외화단가		
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_AM",this.ds_Detail10.getCaseSum("crudType!='D'","FORE_AM"));	//Master에 외화금액 표기

	// 체크박스 체크해제시 (계약관리는 행삭제 기능이 없음)
	} else if (e.columnid == "s_chk") {
		var codeKey = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "codeKey");
		var seqKey = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "seqKey");
		var sQutbSe = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUTB_SE");
		var s_chk = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "s_chk"); 
		var sDelCnt = this.ds_Detail11.getCaseCount("codeKey == '"+codeKey+"' && seqKey == '"+seqKey+"' && QUTB_SE == '"+sQutbSe+"' && crudType == 'D'");

		//s_chk가 변경 대상이고 crueType 이전값이 D의 있을 경우
		if (s_chk == '0') {

			//Detail11(견적상세내역) 대상 존재시
			if (sDelCnt > 0) {
				//Detail11 삭제
				for (var iLoop=this.ds_Detail11.getRowCount(); iLoop>=0; iLoop--) {	
					if (codeKey == this.ds_Detail11.getColumn(iLoop, "codeKey")
					 && seqKey == this.ds_Detail11.getColumn(iLoop, "seqKey")
					 && sQutbSe == this.ds_Detail11.getColumn(iLoop, "QUTB_SE")) {
						//crudType를 이전상태로 변경
						var crudType = this.ds_Detail11.getColumn(iLoop, "crudType");
						if (crudType == 'D') {
							this.ds_Detail11.setColumn(iLoop, "crudType", "");
							this.ds_Detail11.setColumn(iLoop, "s_chk", "0");
						}			
					}
				}
			}

			//crudType이 "D"를 제외하여 재계산
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM",this.ds_Detail10.getCaseSum("crudType!='D'","WONE_AM"));	//Master에 원화금액 표기
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_AM",this.ds_Detail10.getCaseSum("crudType!='D'","FORE_AM"));	//Master에 외화금액 표기	

		}
	}
	
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Detail20] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{

	//체크시 해당 프로그램 전체선택
	if (e.columnid == "s_chk") {	
		if (this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "s_chk") == '1') {		
			var sPjthNo = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "PJTH_NO");//프로젝트번호
			var sPjthSe = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "PJTB_SE");//프로젝트순번
			var sIsphNo = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "ISPH_NO");//제품사양번호

			//Detail10에 프로젝트에 대한 제품사양번호가 존재하는지에 대해 확인
			var iDupCnt = this.ds_Detail10.getCaseCount("PJTH_NO == '"+sPjthNo+"' && PJTB_SE == '"+sPjthSe+"' && ISPH_NO == '"+sIsphNo+"'"); 
			if(iDupCnt > 0 ){
				var arrMsgArg = ["제품사양번호"];
				this.gfn_Alert("SD008", arrMsgArg); //해당 제품사양번호가 존재합니다.
				return;
			}			

			//수량 체크 : 제품수량
			if(this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "CNTB_QT") == 0){
				this.gfn_Alert("SD009"); //수량이 존재하지 않습니다.
				return;
			}
			
			//프로젝트가 최초 1건만 체크됐을 경우에만 처리함(2건일 경우 return)
			var iChkCnt = this.ds_Detail20.getCaseCount("PJTH_NO == '"+sPjthNo+"' && s_chk == '1'"); //체크건수
			if (iChkCnt > 1) return;
			
			//최초 1건만 체크시 로직 처리
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				//Detail10 중복여부 체크
				iDupCnt = this.ds_Detail10.getCaseCount("PJTH_NO == '"+sPjthNo+"' && PJTB_SE == '"+sPjthSe+"' && ISPH_NO == '"+sIsphNo+"'"); //제품사양번호 존재확인
			
				//프로젝트번호,프로젝트순번이 같고 색상변경이 안되고, Detail10에 제품사양번호가 미존재 대상				
				if (this.ds_Detail20.getColumn(iLoop, "PJTH_NO") == sPjthNo 
				 && this.ds_Detail20.getColumn(iLoop, "PJTB_SE") == sPjthSe 
				 && iDupCnt == 0
				 && (this.gfn_IsNull(this.ds_Detail20.getColumn(iLoop, "s_conf")) || this.ds_Detail20.getColumn(iLoop, "s_conf") == '0')) {
								
					//체크박스 Check
					this.ds_Detail20.setColumn(iLoop, "s_chk", "1");//Check로 변경
					
				}
			}
		}
	}
	
};

