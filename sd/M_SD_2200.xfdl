this.fn_ComBtnAdd = function()
{
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10) || this.gfn_DsIsUpdated(this.ds_Detail10) || this.gfn_DsIsUpdated(this.ds_Detail11)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	} 

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무

	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.div_DetailInput.form.tab_Detail.set_enable(true);
	this.div_MasterInput.form.btn_Sd1200.set_enable(false); //수매수계획관리 팝업버튼 비활성화(12/22)

	//Master10 프로젝트 컬럼 활성화
	this.div_MasterInput.form.tab_Master.tpg_01.form.btn_PjthNo.set_enable(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PjthNo.set_format("@@@-@@@@");

	//Deatil20 TAB 검색어 Clear
    this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.mae_PjthNo.set_value("");
    this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_PjtbSe.set_value("");
    this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_PjtbNm.set_value("");
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.mae_PjthNo.set_format("@@@-@@@@");	
	
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.btn_PjthNo.set_enable(true);
	
	//Deatil20 TAB 날짜 초기화
	this.gfn_SetCommFromToDate(this.fvComPrgmId, "DS", this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_FromDd, this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_ClosDd);//조회조건영역	


	var iAddRow = this.ds_Master10.addRow();	

	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	this.fvMmCloseDay = sToDay;//[월마감] 마감일자

	//필수값 셋팅 : 화면존재 && NOT NULL 대상 (설계시 확인필요)
	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", "1");
	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");	
	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//영업부서코드	
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//영업부서명	
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//영업자사번	
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//영업자명		
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//영업자ID	
	this.ds_Master10.setColumn(iAddRow, "QUTH_DD", sToDay);//견적일자-오늘날자로 설정
	this.ds_Master10.setColumn(iAddRow, "CURR_CD", "0090");   // 화폐코드 Default "KRW"
	this.ds_Master10.setColumn(iAddRow, "EXCH_RT", "1.00");    // 환율 Default "1.00"	
	this.ds_Master10.setColumn(iAddRow, "DELE_YN", "0");   // 삭제여부-아니오(0)
	
	// 신규의 경우 화면ID로 임시 codeKey 발생 (회사코드 + 화면ID + 결재차수)
	this.ds_Master10.setColumn(iAddRow, "codeKey", this.gfn_GetSessionValue("COMM_CD")+this.fvComPrgmId+"0"); 

	//처음 입력 항목에 포커스
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_QttyNm.setFocus();
	this.div_DetailInput.form.tab_Detail.set_tabindex(1); //제품사양검색 Tab 이동 (12/21)
	
	this.ds_Detail10.clearData();//견적상세
	this.ds_Detail11.clearData();//견적상세팝업
	this.ds_Detail20.clearData();//제품사양검색
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화		
		
	//첨부파일정보 권한설정
	var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
	oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
	this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
	
	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
}

