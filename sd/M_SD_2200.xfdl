/***********************************************************************************************
 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST10_SEARCH": //조회		
	
			if (this.ds_Master10.getRowCount() > 0) {

				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master10.findRowExpr("QUTH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ=='" + this.fvSchKey02 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}				

				//국내외구분이 해외의 경우 가격조건 true
				this.fn_SetPricCdCssclass();						

				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
				this.div_DetailInput.form.tab_Detail.set_enable(true);

				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	

				//Master10 프로젝트 컬럼 비활성화
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_PjthNo.set_enable(false);

				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_MasterView.form.grd_Master10);
				
			} else {
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			
				//입력영역 비활성화
				this.div_MasterInput.set_enable(false);
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);
				this.div_DetailInput.form.tab_Detail.set_enable(false);
					
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = false;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);					
				
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_MasterView.form.grd_Master10);
			}		
		
			break;
		case "SAVE":   //저장		

			//첨부파일 추가건이 없으면
			if (!this.fvFileDiv.fn_CheckFileAddYn()) {
				this.fvSchKeyYn = true;//저장후 재조회여부
				this.fn_ComBtnSearch();//저장후 재조회
			} else {
				this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);			
			}
			break;	
		case "DTL10_SEARCH": //조회		
			this.ds_Detail10.addColumn("s_chk", "string");
			this.ds_Detail10.addColumn("crudType", "string");
			this.ds_Detail10.addColumn("s_conf", "string");
			
			this.ds_Detail11.addColumn("s_chk", "string");
			this.ds_Detail11.addColumn("crudType", "string");
			this.ds_Detail11.addColumn("crudPopup", "string");
			
			//상세내용 존재시
			if (this.ds_Detail10.getRowCount() > 0) {

				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Detail10.findRowExpr("QUTH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ=='" + this.fvSchKey02 + "' && QUTB_SE=='" + this.fvSchKey03 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Detail10.enableevent) {
						this.ds_Detail10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Detail10.set_rowposition(-1);
						} 
						this.ds_Detail10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Detail10.set_rowposition(0);
					}
				}		

				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);
				
			} else {
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);					
			}

			//견적내용에 프로젝트번호가 존재시 해당 프로젝트번호로 제품사양TAB(Detail20) 검색함
			this.ds_Detail20.clearData();//제품사양검색 초기화			
			var sPjthNo = this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PjthNo.value;
			var sPjthSe = this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PjtbSe.value;
			
			//프로젝트번호 format 변경 (9자리, 7자리)
			if (sPjthNo.length > 8) 
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.mae_PjthNo.set_format("@-@@@@-@@@@");		
			 else 
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.mae_PjthNo.set_format("@@@-@@@@");

			if (!this.gfn_IsNull(sPjthNo) && !this.gfn_IsNull(sPjthSe)) {
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.mae_PjthNo.set_value(this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PjthNo.value);
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_PjtbSe.set_value(this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PjtbSe.value);
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_PjtbNm.set_value(this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PjtbNm.value);		
				
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.btn_PjthNo.set_enable(false);
			
			} else {
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.mae_PjthNo.set_value("");
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_PjtbSe.set_value("");
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_PjtbNm.set_value("");		
				
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.btn_PjthNo.set_enable(true);
			}
			
			//첨부파일 정보 조회
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
			oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO");//PK번호2-견적번호
			oFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
			
			this.fvFileDiv.fn_AtchFileSearch(oFileInfo);			

			//저장후 재조회여부-true 
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}	
			
			break;			
			
		case "DTL20_SEARCH": //제품사양검색		
			this.ds_Detail20.addColumn("s_chk", "string");		
			this.ds_Detail20.addColumn("s_conf", "string");

			if (this.ds_Detail20.getRowCount() > 0) {
				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20);
			
			} else {
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20);
			
			}
			
			break;			

		case "SEQMAX_SEARCH": //견적순번 MAX	
			//1. 최대견적순번을 가져 온다음 견적순번과 추가대상의 순번(seqkey)를 조합하여 견적순번을 처리함
			//2. 견적순번을 처리하면 견적상세순번의 key값으로 저장함
			
			//Detail10, Detail11 견적순번 MAX값에 seqKey값을 더하여 견적순번(QUTB_SE) 데이터셋 값을 변경함
			var iMaxQutbSe = nexacro.toNumber(this.ds_MaxSeq.getColumn(0, "QUTB_SE"));
			
			//Detail10 데이터셋에 PK정보 입력
			for (var iLoop10=0; iLoop10<this.ds_Detail10.getRowCount(); iLoop10++) {
				var crudType = this.ds_Detail10.getColumn(iLoop10, "crudType");//신규대상만 처리함

				//Detail10이 신규이면 Detal11은 전체가 신규이므로 견적순번을 계산해서 처리함
				if (crudType=='I') { 
					//[임시가져오기[Detail10과 Detail11 비교를 하기 위한 기준 Key 가져오기
					var codeKey = this.ds_Detail10.getColumn(iLoop10, "codeKey"); //코드key값
					var seqKey = this.ds_Detail10.getColumn(iLoop10, "seqKey");   //임시순번
					var sQutbSe = this.ds_Detail10.getColumn(iLoop10, "QUTB_SE"); //견적순번
					
					//견적순번 = 최대순번 + 임시순번 ==> DB에 저장
					var iQutbSe = iMaxQutbSe + nexacro.toNumber(seqKey);	
					
					//[Detail10]DB에 저장해야 하는 견적순번 데이터셋 저장
					this.ds_Detail10.setColumn(iLoop10, "QUTB_SE", iQutbSe);

					//[비교후 처리]Detail11 데이터셋에 QUTB_SE 저장
					for (var iLoop11=0; iLoop11<this.ds_Detail11.getRowCount(); iLoop11++) {
						//Detail10과 Detail11 Key 비교하여 같은대상 견적순번 저장
						if (codeKey == this.ds_Detail11.getColumn(iLoop11, "codeKey")
						 && seqKey == this.ds_Detail11.getColumn(iLoop11, "seqKey")
						 && sQutbSe == this.ds_Detail11.getColumn(iLoop11, "QUTB_SE"))
						 
							//[Detail11]견적순번 데이터셋 저장
							this.ds_Detail11.setColumn(iLoop11, "QUTB_SE", iQutbSe);
					}
				}
			}

			//[공통]저장 처리 진행함수 호출
			this.fn_ComSaveProcess();
			
			break;			

		case "COMBO_SEARCH" : // 공통코드 조회
			this.ds_FaSaum.filter("SAUM_CD == '0010' || SAUM_CD == '0400'") 

			break;			

		case "WF_RECIDE": //재기안처리		
			if (nApp.gds_WfRst.getRowCount() > 0) {
				if (nApp.gds_WfRst.getColumn(0, "O_RET_CODE") == "TRUE") {
					this.gfn_Alert("WF012");//재기안 처리가 완료되었습니다.
					this.fvSchKeyYn = true;//저장후 재조회여부
					this.fn_ComBtnSearch();//재기안 재조회
				} else {
					var arrMsgArg = [nApp.gds_WfRst.getColumn(0, "O_RET_CMSG")];
					this.gfn_Alert("WF013", arrMsgArg);//재기안 처리시 오류가 발생하였습니다.\n&ITM0				
				}
			} else {
				var arrMsgArg = [""];
				this.gfn_Alert("WF013", arrMsgArg);//재기안 처리시 오류가 발생하였습니다.\n&ITM0
			}
			break;					

	}
};


