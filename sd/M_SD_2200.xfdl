 * 02.DG0.05. [Detail그리드-버튼] 취소 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd01Cancel = function()
{

	//검색값설정 - 컬럼명:검색값
	var objSearchList = ["PJTH_NO|" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PJTH_NO") //프로젝트번호
						,"PJTB_SE|" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PJTB_SE") //프로젝트순번
						,"ITEM_CD|" + this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "ITEM_CD") //품목번호
						,"ISPH_NO|" + this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "ISPH_NO") //제품사양번호
						];

	//[공통] 행삭제시 검색Tab 선택Row 해제 처리
	this.gfn_DeleteRowSchTabUncheck(this.ds_Detail20, objSearchList);

	//견적상세 데이터셋에 데이터 존재 확인
	if (this.ds_Detail11.rowcount > -1) {
		var sQutbSe = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUTB_SE");
		var codeKey = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "codeKey");
		var seqKey = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "seqKey");
		
		//Detail11 삭제
		for (var iLoop=this.ds_Detail11.getRowCount(); iLoop>=0; iLoop--) {	
			if (codeKey == this.ds_Detail11.getColumn(iLoop, "codeKey")
			 && seqKey == this.ds_Detail11.getColumn(iLoop, "seqKey")
			 && sQutbSe == this.ds_Detail11.getColumn(iLoop, "QUTB_SE")) {
				//crudType == 'I' 이면 삭제, 그외 crudType를 이전상태로 변경
				var crudType = this.ds_Detail11.getColumn(iLoop, "crudType");
				if (crudType == 'I') {
					//Detail11 데이터셋 삭제
					this.ds_Detail11.deleteRow(iLoop);				
					//this.gfn_UndoColumnDs(this.ds_Detail11, iLoop);	//공통에서 생성한것(사용이 안됨 - 확인 후 사용)
				} else {
					this.ds_Detail11.setColumn(iLoop, "crudType", "");
					this.ds_Detail11.setColumn(iLoop, "s_chk", "0");
					
					//이전값으로 복구
					this.ds_Detail11.setColumn(iLoop, "ITEM_CD", this.ds_Detail11.getOrgColumn(iLoop, "ITEM_CD"));
					this.ds_Detail11.setColumn(iLoop, "ITEM_NM", this.ds_Detail11.getOrgColumn(iLoop, "ITEM_NM"));
					this.ds_Detail11.setColumn(iLoop, "SPEC_NM", this.ds_Detail11.getOrgColumn(iLoop, "SPEC_NM"));
					this.ds_Detail11.setColumn(iLoop, "QUTB_QT", this.ds_Detail11.getOrgColumn(iLoop, "QUTB_QT"));
					this.ds_Detail11.setColumn(iLoop, "QUTB_PR", this.ds_Detail11.getOrgColumn(iLoop, "QUTB_PR"));
					this.ds_Detail11.setColumn(iLoop, "WONE_PR", this.ds_Detail11.getOrgColumn(iLoop, "WONE_PR"));
					this.ds_Detail11.setColumn(iLoop, "WONE_AM", this.ds_Detail11.getOrgColumn(iLoop, "WONE_AM"));
					this.ds_Detail11.setColumn(iLoop, "FORE_PR", this.ds_Detail11.getOrgColumn(iLoop, "FORE_PR"));
					this.ds_Detail11.setColumn(iLoop, "FORE_AM", this.ds_Detail11.getOrgColumn(iLoop, "FORE_AM"));
					this.ds_Detail11.setColumn(iLoop, "REMK_NM", this.ds_Detail11.getOrgColumn(iLoop, "REMK_NM"));
				}			
			}
		}
		
	}
	
	//Detail10 Undo
	this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);
	
	
	//견적번호가 없고 견적상세 내용이 미존재시 Master Clear
	if (this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition,"QUTH_NO")) && this.ds_Detail10.rowcount == 0) {
		this.fn_ClearMasterValue();
		
	//Master 원화금액, 외화금액 변경
	} else {
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM",this.ds_Detail10.getCaseSum("crudType!='D'","WONE_AM"));	//Master에 원화금액 표기
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_AM",this.ds_Detail10.getCaseSum("crudType!='D'","FORE_AM"));	//Master에 외화금액 표기	
	}
};
 
