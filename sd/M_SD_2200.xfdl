		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PjthNo.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PjtbSe.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PjtbNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_InexGu.set_enable(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PlceNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CspmNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CurrCd.set_enable(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_ExchRt.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_WoneAm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_ForeAm.set_readonly(true);

		//신규일때 프로젝트 돋보기 활성화 (10/18 요청사항)
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO"))) {
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_PjthNo.set_enable(false);
		}

		this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.mae_PjthNo.set_readonly(true);
		this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_PjtbSe.set_readonly(true);
		this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_PjtbNm.set_readonly(true);

	}
	
	//수매수계획관리 팝업버튼 활성화/비활성화
	if (this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO"))) {	//견적번호가 없을 시
		this.div_MasterInput.form.btn_Sd1200.set_enable(false); //비활성화
	} else {
		this.div_MasterInput.form.btn_Sd1200.set_enable(true);	//활성화
	}
}

