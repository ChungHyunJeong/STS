this.fn_ComBtnPrint = function()
{ 
	var iSelRow = this.ds_Master10.rowposition;//선택한Row

	if (this.ds_Master10.getRowCount() > 0 && iSelRow > -1) {
		var sINEX_GU = this.ds_Master10.getColumn(iSelRow, "INEX_GU");//국내외구분
		
		var sReportFile = "";//레포트파일ID
		
		//해외
		if (sINEX_GU == "0020") {
			sReportFile = "M_SD_2200_02";//레포트파일ID
		} else {
			sReportFile = "M_SD_2200_01";//레포트파일ID
		}

		//마스터출력할 Row 복사
		this.ds_Master10Rt.clearData();
		this.ds_Master10Rt.addRow();
		this.ds_Master10Rt.copyRow(0, this.ds_Master10, this.ds_Master10.rowposition);

		this.ds_Master10Rt.setColumn(0, "CURR_NM", this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CurrCd.text);	//화폐단위
		this.ds_Master10Rt.setColumn(0, "PAYM_NM", this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_PaymCd.text);	//대금결재

		//사업장 본사, 공장의 주소/전화/팩스 저장
		for (var iLoop=0; iLoop<this.ds_FaSaum.getRowCount(); iLoop++) {		
			if (this.ds_FaSaum.getColumn(iLoop, "SAUM_CD") == '0010') {	//본사(분당)
				this.ds_Master10Rt.setColumn(0, "OADD_NM", this.ds_FaSaum.getColumn(iLoop, "ADD1_NM"));	//사업장주소 (OFFICE)
				this.ds_Master10Rt.setColumn(0, "OTEL_NO", this.ds_FaSaum.getColumn(iLoop, "OTEL_NO"));	//사업장전화 (OFFICE)
				this.ds_Master10Rt.setColumn(0, "OFAX_NO", this.ds_FaSaum.getColumn(iLoop, "OFAX_NO"));	//사업장팩스 (OFFICE)
			} else if (this.ds_FaSaum.getColumn(iLoop, "SAUM_CD") == '0400')  {	//음성공장
				this.ds_Master10Rt.setColumn(0, "FADD_NM", this.ds_FaSaum.getColumn(iLoop, "ADD1_NM"));	//사업장주소 (FACTORY)
				this.ds_Master10Rt.setColumn(0, "FTEL_NO", this.ds_FaSaum.getColumn(iLoop, "OTEL_NO"));	//사업장전화 (FACTORY)
				this.ds_Master10Rt.setColumn(0, "FFAX_NO", this.ds_FaSaum.getColumn(iLoop, "OFAX_NO"));	//사업장팩스 (FACTORY)
			}			
		}

		var dsObj = new Object();//출력 데이터셋 정보
		dsObj.ds_Master10 = this.ds_Master10Rt;
		dsObj.ds_Detail10 = this.ds_Detail10;

		var paObj = new Object();//출력 파라메타 정보	
		paObj.LOGO_UL = this.gfn_GetReportImageUrl("LOGO_UL_01");//이미지-로고

		var oArg = {ProjectFolder:"SD"        //레포트저장 폴더
				  , ReportFile:sReportFile //레포트파일명
				  , ReportDs:dsObj            //레포트파라메타 - 데이터셋
				  , ReportParam:paObj         //레포트파라메타 - 변수
				   };                         //Popup화면 전달 파라메터
				   
		this.gfn_ReportPop("REPORT", oArg, "fn_PopupCallback");//레포트 팝업 호출	
			
	} else {
		this.gfn_Alert("SM083");//출력할 정보가 없습니다.
	}
}

