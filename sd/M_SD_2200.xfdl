/***********************************************************************************************
 * 02.DG1.05. [Detail그리드2-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Choice = function()
{

	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {
			
			//동일대상만 체크 대상 값 Flag
			var sIdntNoChk=""; //Key(프로젝트번호) 체크
			var nRatm = 0; //환율
			var seqKey
			
			//프로젝트번호 추가 체크
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					
					//1. Detail20 Tab과 Master Input 화면 항목 비교 : 프로젝트번호
					if(this.ds_Detail10.rowcount > 0 ){
						if(this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PjthNo.value != this.ds_Detail20.getColumn(iLoop,"PJTH_NO") ){
							var arrMsgArg = ["프로젝트번호"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 프로젝트번호만 적용 가능합니다.
							return;
						}
					}
					
					//2. Detail20 그리드내 체크된 대상 동일여부 비교 : 프로젝트번호
					if(!this.gfn_IsNull(sIdntNoChk) ){
						if(sIdntNoChk != this.ds_Detail20.getColumn(iLoop,"PJTH_NO") ){
							var arrMsgArg = ["프로젝트번호"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 프로젝트번호만 적용 가능합니다.
							return;
						}
					}
					sIdntNoChk = this.ds_Detail20.getColumn(iLoop,"PJTH_NO"); //for문 비교를 위한 이전값 저장
					
					//3. 수량 체크 : 제품수량
					if(this.ds_Detail20.getColumn(iLoop, "ISPB_QT") == 0){
						this.gfn_Alert("SD009"); //수량이 존재하지 않습니다 안내메시지
						return;
					}
				}
			}
			
			var arrCheckRow = [];//체크된 Row
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				//체크 대상여부 확인
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {

					//Detail10에 적용되어 있는지 체크
					var objSearchList = ["COMM_CD|" + this.ds_Detail20.getColumn(iLoop, "COMM_CD") //회사코드
										,"ITEM_CD|" + this.ds_Detail20.getColumn(iLoop, "ITEM_CD") //품목코드
										,"ISPH_NO|" + this.ds_Detail20.getColumn(iLoop, "ISPH_NO") //제품사양번호
										];

					//[공통] Detail10 중복건 체크후 해당건 Skip
					if (!this.gfn_DsSchDupcheck(this.ds_Detail10, objSearchList)) {
						//선택된 첫번째 프로젝트 정보를 Master10에 적용
						//프로젝트번호, 프로젝트순번, 거래처, 고객, 국내외구분, 납품장소 정보를 가져옴
						if(this.ds_Detail10.rowcount == 0 ){ 
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"PJTH_NO",this.ds_Detail20.getColumn(iLoop,"PJTH_NO"));	//프로젝트번호
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"PJTB_SE",this.ds_Detail20.getColumn(iLoop,"PJTB_SE"));	//프로젝트순번
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"PJTB_NM",this.ds_Detail20.getColumn(iLoop,"PJTB_NM"));	//프로젝트명(PJTH_NM -> PJTB_NM상세명으로 수정 9/26)
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"QUTH_NM",this.ds_Detail20.getColumn(iLoop,"PJTB_NM"));	//견적명
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"INEX_GU",this.ds_Detail20.getColumn(iLoop,"INEX_GU"));	//국내외구분
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"CUSM_CD",this.ds_Detail20.getColumn(iLoop,"CUSM_CD"));	//거래처코드
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"CUSM_NM",this.ds_Detail20.getColumn(iLoop,"CUSM_NM"));	//거래처명
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"CSPM_CD",this.ds_Detail20.getColumn(iLoop,"CSPM_CD"));	//고객코드
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"CSPM_NM",this.ds_Detail20.getColumn(iLoop,"CSPM_NM"));	//고객명
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"CURR_CD",this.ds_Detail20.getColumn(iLoop,"CURR_CD"));	//화폐코드
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"PLAC_CD",this.ds_Detail20.getColumn(iLoop,"CUSM_CD"));	//납품처코드
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"PLCE_NM",this.ds_Detail20.getColumn(iLoop,"CUSM_NM"));	//납품처명
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"PLAC_NM",this.ds_Detail20.getColumn(iLoop,"PLAC_NM"));	//납품장소

							//견적일자 환율 자동입력
							nRatm = this.gfn_GetCommRatm(this.ds_Master10.getColumn(this.ds_Master10.rowposition,"QUTH_DD"), this.ds_Detail20.getColumn(iLoop,"CURR_CD"));//환율정보
							this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EXCH_RT", nRatm);	//환율정보

							//Master10 프로젝트 컬럼 활성화
							this.div_MasterInput.form.tab_Master.tpg_01.form.btn_PjthNo.set_enable(false);

						}
										
						// 라인추가 후 Detail20 -> Detail10으로 적용
						var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)
						
						//선택항목 비활성화처리 (색상변경 및 체크박스 Lock)
						this.ds_Detail20.setColumn(iLoop, "s_conf", "1");
						this.ds_Detail20.setColumn(iLoop, "s_chk", "0");//Uncheck로 변경

						//제품사양검색(Detail20) -> 견적상세(Detail10) Copy : 컬럼명 동일
						this.ds_Detail10.copyRow(iAddRow, this.ds_Detail20, iLoop);		
						
						//제품사양검색(Detail20) -> 견적상세(Detail10) Copy : 컬럼명 다름
						this.ds_Detail10.setColumn(iAddRow, "QUTB_QT", this.ds_Detail20.getColumn(iLoop,"ISPB_QT"));
						
						//견적상세 Master에서 Key값 적용
						this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
						this.ds_Detail10.setColumn(iAddRow, "QUTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO")); //견적번호
						this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ")); //결재번호
						this.ds_Detail10.setColumn(iAddRow, "QUTB_SE", "1"); //견적순번 (신규의 경우 1으로 셋팅)
						this.ds_Detail10.setColumn(iAddRow, "s_chk", "0");
						
						//Key 순번 (견적상세 체크시 필요)
						if (iAddRow == 0) {
							var seqKey = "1"; //최초값 
						} else {
							var seqKey = nexacro.toNumber(this.ds_Detail10.getMax("seqKey"));
							seqKey = seqKey + 1;
							
						}
						this.ds_Detail10.setColumn(iAddRow, "codeKey", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "codeKey"));	
						this.ds_Detail10.setColumn(iAddRow, "seqKey", seqKey);

						arrCheckRow.push(iLoop );						
					}

				}
			}
					
			this.ds_Detail20.set_rowposition(-1);
			
			//적용 후 견적상세 Tab이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);
		}
	}
};


