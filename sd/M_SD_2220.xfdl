 			} else {
				if (this.ds_Master10.getRowCount() > 0) {
					//내용 Web컴포넌트에 값 세팅
					var oWeb = this.div_DetailInput.form.tab_Detail.tpg_01.form.web_ContNm.getProperty("window");
					var sContNm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PJDT_NM");
					oWeb.callMethod("fn_setData", this.gfn_ReplaceWebEditorData(sContNm));	
 				
 				} else {
					//내용 Web컴포넌트에 값 세팅
					var oWeb = this.div_DetailInput.form.tab_Detail.tpg_01.form.web_ContNm.getProperty("window");				                
					oWb.callMethod("fn_setData", "");				
 				}			
 			}
			
			this.fvDtlSearchYn = true;//상세조회여부
			
			//상세내용 존재시
			if (this.ds_Detail20.getRowCount() > 0) {
