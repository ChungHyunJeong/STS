/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 
 	//내용 Web컴포넌트 정보값 세팅
 	var oWeb = this.div_DetailInput.form.tab_Detail.tpg_01.form.web_ContNm.getProperty("window");
 	var sContNm = oWeb.callMethod("fn_getData");//내용	

	if (this.ds_Master10.getRowCount() > 0) {
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PJDT_NM", sContNm);
	} else {
		this.ds_Master10.addRow();
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PJDT_NM", sContNm);
	}
	
	var sChkCommCd = "";
	//ds_Detail20 crudType 임의로 변경
	for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
		sChkCommCd = this.ds_Detail20.getColumn(iLoop, "COMM_CD");
		if (this.gfn_IsNull(sChkCommCd)) {
			this.ds_Detail20.setColumn(iLoop, "crudType", "I");

