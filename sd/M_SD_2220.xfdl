				this.gfn_SetGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_03.form.grd_Detail30);				
			}
			
			break;			
			
		case "DELETE": //삭제		
			this.fn_ComBtnSearch();//조회
			break;
			
		case "WF_RECIDE": //재기안처리		
			if (nApp.gds_WfRst.getRowCount() > 0) {
