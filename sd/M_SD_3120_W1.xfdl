			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "ITEM_CD", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "ITEM_NM", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "SPEC_NM", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "UNIT_CD", "");
		}		
		
	//[Detail영역-검색]
	}
	
};
 
