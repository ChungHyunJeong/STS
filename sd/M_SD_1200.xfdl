/***********************************************************************************************
 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST10_SEARCH": //조회	
		
			this.ds_Master10.addColumn("crudType", "string");

			if (this.ds_Master10.getRowCount() > 0) {

				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master10.findRowExpr("PJTH_NO=='" + this.fvSchKey01 + "' && PJTB_SE=='" + this.fvSchKey02 + "' && PJTB_DD=='" + this.fvSchKey03 + "' && PLAH_DD=='" + this.fvSchKey04 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}
				
				// 프로젝트별수매수현황에서 넘긴 데이터
				if(!this.gfn_IsNull(this.getOwnerFrame().paramPjthNo)) {
					var FromDd = this.getOwnerFrame().paramFromDd;
					var sSelectPlahDd = this.ds_Master10.getCaseMax("PLAH_DD <= '"+FromDd+"'", "PLAH_DD");
					var iFvFindRow = this.ds_Master10.findRowExpr("PLAH_DD=='" + sSelectPlahDd + "'");//업무Key Row 찾기

					if(iFvFindRow == -1) {
						sSelectPlahDd = this.ds_Master10.getMin("PLAH_DD");
					}

					iFvFindRow = this.ds_Master10.findRowExpr("PLAH_DD=='" + sSelectPlahDd + "'");//업무Key Row 찾기
					this.fvPopupYn = false;//팝업으로 호출여부

					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1); // 찾은 인덱스가 0일경우 비정상 동작하여 임의로 이동시킴
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}

					if(this.getOwnerFrame().paramAbcGubun == 'A') { // 수주
						this.div_DetailInput.form.tab_Detail.set_tabindex(0);
					} else if(this.getOwnerFrame().paramAbcGubun == 'B') { // 매출
						this.div_DetailInput.form.tab_Detail.set_tabindex(1);
					} else if(this.getOwnerFrame().paramAbcGubun == 'C') { // 수금
						this.div_DetailInput.form.tab_Detail.set_tabindex(2);
					}
				}
				
				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
				this.div_DetailInput.form.tab_Detail.set_enable(true);

				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_MasterView.form.grd_Master10);
				
			} else {
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			
				//입력영역 비활성화
				this.div_MasterInput.set_enable(false);
				this.div_DetailInput.form.tab_Detail.set_enable(false);
					
				//계획변경 버튼 비활성화 (조회결과 프로젝트 미존재시 계획변경 불가)
				this.div_MasterInput.form.btn_PlanChange.set_enable(false);
				
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_MasterView.form.grd_Master10);
			}		
		
			break;
		case "SAVE":   //저장		

			this.fvSchKeyYn = true;//저장후 재조회여부			
			this.fn_ComBtnSearch();//저장후 재조회
			
			break;	
		case "DTL10_SEARCH": //조회		
			this.ds_Detail10.addColumn("crudType", "string");
			this.ds_Detail20.addColumn("crudType", "string");
			this.ds_Detail30.addColumn("crudType", "string");
			this.ds_Detail10.addColumn("changeYn", "string");
			this.ds_Detail20.addColumn("changeYn", "string");
			this.ds_Detail30.addColumn("changeYn", "string");

			//Detail10, Detail20, Detail30그리드 변경가능여부 셋팅 (10/18 설계자 요청)
			var sPlahDd  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PLAH_DD");//변경일자
			var sWofhYn  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_YN");//최종여부
			var sToDay = this.gfn_GetCommDate("toDay");//현재일자		

			//수주상세 - 계획 존재시
			if (this.ds_Detail10.getRowCount() > 0) {

				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Detail10.findRowExpr("PJTH_NO=='" + this.fvSchKey01 + "' && PJTB_SE=='" + this.fvSchKey02 + "' && PLAH_DD=='" + this.fvSchKey04 + "' && PLAB_SE=='" + this.fvSchKey05 + "'");//업무Key Row 찾기

					//Detail10 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Detail10.enableevent) {
						this.ds_Detail10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Detail10.set_rowposition(-1);
						} 
						this.ds_Detail10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Detail10.set_rowposition(0);
					}
				}		

				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);

				//변경일자가 당일이고 최종의 경우 그리드 변경가능여부 셋팅 호출 (10/18 설계자 요청)
				if (sPlahDd == sToDay && sWofhYn == '1') {
					this.fn_SetGridDetail10(); 	//Detail10 그리드 변경가능여부 호출
				}
				
			} else {
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);					
			}

			//수주상세 - 실적(계약) 존재시
			if (this.ds_Detail10.getRowCount() > 0) {
				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail11);				
			} else {
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail11);					
			}


			//매출상세 - 계획 존재시
			if (this.ds_Detail20.getRowCount() > 0) {

				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Detail20.findRowExpr("PJTH_NO=='" + this.fvSchKey01 + "' && PJTB_SE=='" + this.fvSchKey02 + "' && PLAH_DD=='" + this.fvSchKey04 + "' && PLAC_SE=='" + this.fvSchKey06 + "'");//업무Key Row 찾기

					//Detail20 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Detail20.enableevent) {
						this.ds_Detail20.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Detail20.set_rowposition(-1);
						} 
						this.ds_Detail20.set_rowposition(iFvFindRow);
					} else {
						this.ds_Detail20.set_rowposition(0);
					}
				}		

				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20);

				//변경일자가 당일이고 최종의 경우 그리드 변경가능여부 셋팅 호출 (10/18 설계자 요청)
				if (sPlahDd == sToDay && sWofhYn == '1') {
					this.fn_SetGridDetail20(); 	//Detail20 그리드 변경가능여부 호출
				}

			} else {
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20);					
			}			

			//매출상세 - 실적 존재시
			if (this.ds_Detail10.getRowCount() > 0) {
				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail21);				
			} else {
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail21);					
			}
			
			
			//수금상세 - 계획 존재시
			if (this.ds_Detail30.getRowCount() > 0) {

				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Detail30.findRowExpr("PJTH_NO=='" + this.fvSchKey01 + "' && PJTB_SE=='" + this.fvSchKey02 + "' && PLAH_DD=='" + this.fvSchKey04 + "' && PLAD_SE=='" + this.fvSchKey07 + "'");//업무Key Row 찾기

					//Detail30 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Detail30.enableevent) {
						this.ds_Detail30.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Detail30.set_rowposition(-1);
						} 
