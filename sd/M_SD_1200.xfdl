/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Detail20] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{

	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)

	// 매출상세 예상일자 or 계획금액 변경
	if (e.columnid == "PLAC_DD" || e.columnid == "PLAC_AM") {
		var sCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐
		var sPlacDd = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "PLAC_DD");//예상일자
		var iPlacAm = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "PLAC_AM");//계획금액
		var iExchRt = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "EXCH_RT");//환율
		var sCurrNm = this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CurrCd.text;//화폐명
		var iBecuAm = 0;	//전월평균환율적용금액

		var iWoneAm = nexacro.floor(iPlacAm * iExchRt, 0);	//원화금액 = 계획금액 * 환율 (10/18 소숫점없음 절삭처리)
		var iForeAm = 0 //외화금액

		if (sCurrCd != '0090') {	//KRW(원화)가 아닌 경우
			iForeAm = iPlacAm;	//외화금액 : KRW시 0원, KRW가 아닌경우 계획금액
		} 

		//그리드에 적용
		this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "WONE_AM", iWoneAm);	//원화금액
		this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "FORE_AM", iForeAm);	//외화금액
		this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "REGI_ID", this.gfn_GetSessionValue("SYST_ID"));//등록자ID	
		this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "REGI_NM", this.gfn_GetSessionValue("USEM_NM"));//등록자명		
		this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "REGI_DT", this.gfn_GetCommDate("toDayFull"));	//등록일시	

		//합계, 차액 계산
		this.fn_DetailAmountSum();
		
		//전월평균환율(2022-04-25 변경)	- Detail 예상일자 -> Master 조회시
		//전월평균환율적용금액 (2022-04-11 추가개발)
		//환율정보가 존재하고, 원화(KRW)가 아니고, 계획금액이 존재할 경우
		if (!this.gfn_IsNull(sCurrNm) && sCurrNm != 'KRW' && (!this.gfn_IsNull(iPlacAm) || iPlacAm > 0)) {
			var iLmebAm = this.ds_MaxLmebAm.getColumn(0, "LMEB_AM");	//전월평균환율적용금액 호출 (2022-04-25 변경)
			//var iLmebAm = this.fn_MaxExchangeRateSearch(sCurrNm, sPlacDd);	//전월평균환율적용금액 호출 (환율, 예상일자)
			
			//조회된 값이 존재하면
			if (!this.gfn_IsNull(iLmebAm)) {
				iBecuAm = nexacro.floor(iPlacAm * iLmebAm, 0);				//전월평균환율적용금액 = 원자재시세 * 예상금액
			}

		//금액이 존재할 경우 (환율이 없으면 Default 1, 원화일 경우 Default 1)
		} else if (!this.gfn_IsNull(iPlacAm)) {
			iBecuAm = iPlacAm;		//전월평균환율적용금액 = 계획금액
		} else {
			iBecuAm = "";			//전월평균환율적용금액 = 초기화
		}

		//전월평균환율 및 전월평균환율적용금액 처리
		this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "BFCU_RT", iLmebAm);	
		this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "BFCU_AM", iBecuAm);			

	}
	
};


