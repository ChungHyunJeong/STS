/***********************************************************************************************
 * 02.MG0.07. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
		
	// 수주확율 또는 비고가 변경이 있으면
	if (e.columnid == "ORRA_CD" || e.columnid == "REMK_NM") {
		//수매수계획에 INSERT, UPDATE 대상 여부 확인
		
		var sOrraCd    = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ORRA_CD");//수주확율
		var sOrraCdOrg = this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "ORRA_CD");//수주확율(이전)
		var sRemkNm    = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "REMK_NM");//비고
		var sRemkNmOrg = this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "REMK_NM");//비고(이전)
	
		var sWofhYn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_YN"); //최종여부

		//계획변경이거나 수매수계획 데이터 없음(최종여부가 NULL)일 경우 'I'
		//수매수계획이 최종인 경우 'U'
		if (!this.fvPlanChange && sOrraCd == sOrraCdOrg && sRemkNm == sRemkNmOrg) {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "crudType", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "crudType"));	//이전상태값	
		} else if (!this.fvPlanCheck && sWofhYn == '1') {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "crudType", "U");
		} else if (this.fvPlanCheck || this.gfn_IsNull(sWofhYn)) {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "crudType", "I");
		}		
		
	}
};


