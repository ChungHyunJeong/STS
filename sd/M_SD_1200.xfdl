/***********************************************************************************************
 * 15.10. [Max] 원자재시세정보 기준 전월 최종금액
 ***********************************************************************************************/ 
this.fn_MaxExchangeRateSearch = function(sCurrNm, sPlanDd)
{
	//[최종]2022-04-25 변경사항 - 예상일자 기준이 아닌 최초 조회일자 기준으로 변경
	//[최종]로딩시 조회되는 일자(당일)보다 작은 최종월 환율 가져옴
	//[최종]Detail 예상일자 oncolumnchanged에서 -> Master 화폐정보 가져오는 onrowposchanged에서 호출함
	
	//전월평균환율적용금액 - 예상일자 기준으로 최종월 환율 가져오기	(2022-04-11)
	
	this.ds_MaxLmebAm.clearData();		//최종월 환율
	this.ds_Input.clearData();
	
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "PLAN_DD", sPlanDd); //예상일자 -> 당일로 변경
	this.ds_Input.setColumn(0, "CURR_NM", sCurrNm); //화폐단위명

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "sd:M_SD_1200_D_SL90";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MAX_RATE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_SD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_MaxLmebAm=ds_Detail90";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.isSync = true;//동기식으로 설정할지 여부

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);			

	//조회된 값이 존재하면
	if (this.ds_MaxLmebAm.getRowCount() > 0) {
		return true;
		//return this.ds_MaxLmebAm.getColumn(0, "LMEB_AM");	//원자재시세
	} else {
		return false;
	
		//return null;
	}

};
