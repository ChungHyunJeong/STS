/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master] 데이터 Row 변경 이벤트 - ds_Master10
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	//조회시 Detail10, Detail20, Detail30 변경사유 필수값 해제
	this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 9, "cssclass", "");
	this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 10, "cssclass", "");
	this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.setCellProperty("head", 9, "cssclass", "");
	this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.setCellProperty("head", 10, "cssclass", "");
	this.div_DetailInput.form.tab_Detail.tpg_03.form.grd_Detail30.setCellProperty("head", 9, "cssclass", "");
	this.div_DetailInput.form.tab_Detail.tpg_03.form.grd_Detail30.setCellProperty("head", 10, "cssclass", "");

	if (e.newrow > -1) {
	
		if (this.fvPopupYn) return; //팝업으로 호출여부
	
		//조회된 정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PJTH_NO"))) {
		
			//프로젝트번호 format 변경 (9자리, 7자리)
			var iPjthLen = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PJTH_NO").length;
			if (iPjthLen > 8) 
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PjthNo.set_format("@-@@@@-@@@@");		
			 else 
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PjthNo.set_format("@@@-@@@@");

			//계약번호 format 변경 (9자리, 7자리)
			if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO"))) {
				var iCnthLen = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO").length;
				if (iCnthLen > 8) 
					this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_format("@@@@-@-@@@@");		
				else 
					this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_format("@@@-@@@@");
			}

			//계획변경 버튼 활성화/비활성화 : 당일 변경대상
			this.fvPlanChange = false;	//계획변경 상태여부
			this.fvPlanCheck = false; //계획변경 버튼 클릭여부 해제
			var sToDay = this.gfn_GetCommDate("toDay");//현재일자			
			var iPlahDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PLAH_DD");
			var iWofhYn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_YN");

			if(iPlahDd < sToDay && iWofhYn == '1') {	      //변경일자가 오늘보다 크고, 최종상태가 1인 경우 계획버튼 활성화, 수정불가
				this.div_MasterInput.form.btn_PlanChange.set_enable(true); //계획버튼
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_OrraCd.set_enable(false); //수주확율
				this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(true); //비고		
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(false); //Detail 행추가/행삭제

			} else if(iPlahDd == sToDay && iWofhYn == '1') { //변경일자가 오늘이고, 최종상태가 1인 경우 계획버튼 비활성화, 수정가능
				this.div_MasterInput.form.btn_PlanChange.set_enable(false); //계획버튼
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_OrraCd.set_enable(true); //수주확율
				this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(false); //비고			
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(true); //Detail 행추가/행삭제

				//계획변경 시 행추가/저장 및 그리드 필수값 처리
				this.fvPlanChange = true;	//계획변경 상태여부
				//계획변경 시 Detail10, Detail20, Detail30 변경사유 필수값 처리
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 9, "cssclass", "essential");
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 10, "cssclass", "grdInput");
				this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.setCellProperty("head", 9, "cssclass", "essential");
				this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.setCellProperty("head", 10, "cssclass", "grdInput");
				this.div_DetailInput.form.tab_Detail.tpg_03.form.grd_Detail30.setCellProperty("head", 9, "cssclass", "essential");
				this.div_DetailInput.form.tab_Detail.tpg_03.form.grd_Detail30.setCellProperty("head", 10, "cssclass", "grdInput");
			} else if(iWofhYn == '0') {						//최종상태가 0의 경우 계획버튼 비활성화, 수정불가
				this.div_MasterInput.form.btn_PlanChange.set_enable(false); //계획버튼
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_OrraCd.set_enable(false); //수주확율
				this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(true); //비고
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(false); //Detail 행추가/행삭제				
			} else {										//최종상태 null의 경우 계획버튼 비활성화, 수정가능
				this.div_MasterInput.form.btn_PlanChange.set_enable(false); //계획버튼
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_OrraCd.set_enable(true); //수주확율
				this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(false); //비고			
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(true); //Detail 행추가/행삭제				
			}

			//화계내용(CURR_NM) 검색 - 2022-04-11 추가
			var iFindRow = this.ds_Sm020.findRowExpr("CODM_CD == '"+this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD")+"'");//화폐코드
			var sCurrNm =  this.ds_Sm020.getColumn(iFindRow, "CODM_NM");	//화폐명
			
			//최종월 전월평균환율 조회(2022-04-25 Detail 예상일자 조회시 변경함)
			var sMaxRate = this.fn_MaxExchangeRateSearch(sCurrNm, sToDay);
			
			//Detail Tab으로 이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(this.fvTabIndex);			

			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();		//수주상세
			this.ds_Detail11.clearData();		//수주상세 - 실적
			this.ds_Detail20.clearData();		//매출상세
			this.ds_Detail21.clearData();		//매출상세 - 실적
			this.ds_Detail30.clearData();		//수금상세
			this.ds_Detail31 .clearData();		//수금상세 - 실적
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "PJTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PJTH_NO")); 
			this.ds_Input.setColumn(0, "PJTB_SE", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PJTB_SE"));
			this.ds_Input.setColumn(0, "PLAH_DD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PLAH_DD"));
			this.ds_Input.setColumn(0, "CNTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO"));
			this.ds_Input.setColumn(0, "CURR_NM", sCurrNm);
					
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "sd:M_SD_1200_D_SL10";	//수주상세
			this.gfn_AddComSearchService(this, svcAddObj);

			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "sd:M_SD_1200_D_SL11";	//매출상세 - 실적
			this.gfn_AddComSearchService(this, svcAddObj);

			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "sd:M_SD_1200_D_SL20";	//매출상세
			this.gfn_AddComSearchService(this, svcAddObj);

			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "sd:M_SD_1200_D_SL21";	//매출상세 - 실적
			this.gfn_AddComSearchService(this, svcAddObj);

			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "sd:M_SD_1200_D_SL30";	//수금상세
			this.gfn_AddComSearchService(this, svcAddObj);
			
			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "sd:M_SD_1200_D_SL31";	//수금상세 - 실적
			this.gfn_AddComSearchService(this, svcAddObj);


			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL10_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_SD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10 ds_Detail11=ds_Detail11 ds_Detail20=ds_Detail20 ds_Detail21=ds_Detail21 ds_Detail30=ds_Detail30 ds_Detail31=ds_Detail31";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);		
		}
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master] Row 변경전 이벤트 - ds_Master10
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10) || this.gfn_DsIsUpdated(this.ds_Detail10) || this.gfn_DsIsUpdated(this.ds_Detail20) || this.gfn_DsIsUpdated(this.ds_Detail30)) {
		this.gfn_Confirm("SM003", "", "CONF.MAST10_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	

	return true;
};  


