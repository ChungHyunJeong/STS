/***********************************************************************************************
 * 00.03. Form Onload 이벤트
 ***********************************************************************************************/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{
	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 
	
	//현황에서도 넘기기 때문에 이 문구 추가.
	if(this.gfn_IsNull(this.getOwnerFrame().paramPjthNo)) {
		//화면전달 파라메터 체크	
	   this.fvCmScrnCallFunc = this.getOwnerFrame().arguments["scrnCallFunc"];//화면호출변수명
	   trace("fvCmScrnCallFunc = " + this.fvCmScrnCallFunc);
	   if (!this.gfn_IsNull(this.fvCmScrnCallFunc)) {
		  this.fvCmScrnCallArgs = this.getOwnerFrame().arguments["srnCallArgs"];//화면전달파라메타

		  //프로젝트번호 존재시 초기화
		  if (this.gfn_IsNull(this.fvCmScrnCallArgs.PjthNo)) {
			  this.div_Search.form.cal_FromDd.set_value(this.fvCmScrnCallArgs.fromDd);
			  this.div_Search.form.cal_ClosDd.set_value(this.fvCmScrnCallArgs.closDd);
		  } else {
			this.gfn_SetCommFromToDate(this.fvComPrgmId, "MS", this.div_Search.form.cal_FromDd, this.div_Search.form.cal_ClosDd);//조회조건 초기화
		  }
		  
		  this.div_Search.form.mae_PjthNo.set_value(this.fvCmScrnCallArgs.PjthNo);
		  this.div_Search.form.edt_PjthNm.set_value(this.fvCmScrnCallArgs.PjthNm);
		  this.div_Search.form.edt_DepmCd.set_value(this.fvCmScrnCallArgs.DepmCd);
		  this.div_Search.form.edt_DepmNm.set_value(this.fvCmScrnCallArgs.DepmNm);		
		  this.div_Search.form.edt_EmpmCd.set_value(this.fvCmScrnCallArgs.EmpmCd);
		  this.div_Search.form.edt_EmpmNm.set_value(this.fvCmScrnCallArgs.EmpmNm);
		  this.div_Search.form.edt_SystId.set_value(this.fvCmScrnCallArgs.SystId);		
		  this.div_Search.form.edt_CusmCd.set_value(this.fvCmScrnCallArgs.CusmCd);
		  this.div_Search.form.edt_CusmNm.set_value(this.fvCmScrnCallArgs.CusmNm);
		  
		  //조회
		  this.fn_ComBtnSearch();
		 }
      
   }   
}

