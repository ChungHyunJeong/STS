/***********************************************************************************************
 * 02.DG0.02. [Detail그리드-버튼] 행추가 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd01AddRow = function()
{
	var sMonthLastday = this.gfn_GetCommDate("monthLastday");//당월말일
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	var iCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐코드
	var nRatm = this.gfn_GetCommRatm(sToDay, iCurrCd);//환율정보

	//Detail Tab위치 확인
	var iTabIndex = this.div_DetailInput.form.tab_Detail.tabindex;

	if (iTabIndex == 0) {			//수주상세 Tab
		//예상일자, 계획금액 변경여부 확인
		var changeCount = this.ds_Detail10.getCaseCount("changeYn == '1' && (PLAB_AM == null || (CHAN_NM == null && crudType == 'U'))"); //변경대상 건수
		var changeYn = "";	//변경대상여부
		var iPlanAm = "";	//계획금액
		var sChanNm = "";	//변경사유
		var crudType = "";	//crudType
		
		if (changeCount > 0) { //변경대상 존재시
			for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
				changeYn = this.ds_Detail10.getColumn(iLoop, "changeYn"); //변경대상여부
				iPlanAm = this.ds_Detail10.getColumn(iLoop, "PLAB_AM"); //계획금액
				sChanNm = this.ds_Detail10.getColumn(iLoop, "CHAN_NM"); //변경사유
				crudType = this.ds_Detail10.getColumn(iLoop, "crudType"); //crudType

				if (changeYn == "1" && (this.gfn_IsNull(iPlanAm) || (iPlanAm == "0" && sChanNm != '0030'))) {	//변경대상이고, 계획금액이 미존재시(취소시 계획금액 0 제외)
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(3, iLoop);
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();
				
					var arrMsgArg = ["수주계획상세 계획금액"];
					this.gfn_Alert("SM008", arrMsgArg); //계획금액은 필수입력 항목입니다.

					break;
					return false;
