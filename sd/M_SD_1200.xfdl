/***********************************************************************************************
 * 02.DG0.07. [Detail 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)

	//수주상세 예상일자 or 계획금액 변경
	if (e.columnid == "PLAB_DD" || e.columnid == "PLAB_AM") {
		var sCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐
		var sPlabDd = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "PLAB_DD");//예상일자
		var iPlabAm = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "PLAB_AM");//계획금액
		var iExchRt = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "EXCH_RT");//환율
		var sCurrNm = this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CurrCd.text;//화폐명
		var iBecuAm = 0;	//전월평균환율적용금액

		var iWoneAm = nexacro.floor(iPlabAm * iExchRt, 0);	//원화금액 = 계획금액 * 환율 (10/18 소숫점없음 절삭처리)
		var iForeAm = 0 //외화금액

		if (sCurrCd != '0090') {	//KRW(원화)가 아닌 경우
			iForeAm = iPlabAm;	//외화금액 : KRW시 0원, KRW가 아닌경우 계획금액
		}
	
		//그리드에 적용
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "WONE_AM", iWoneAm);	//원화금액
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "FORE_AM", iForeAm);	//외화금액
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "REGI_ID", this.gfn_GetSessionValue("SYST_ID"));//등록자ID	
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "REGI_NM", this.gfn_GetSessionValue("USEM_NM"));//등록자명		
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "REGI_DT", this.gfn_GetCommDate("toDayFull"));	//등록일시	

		//합계, 차액 계산
		this.fn_DetailAmountSum();

		//전월평균환율(2022-04-25 변경)	- Detail 예상일자 -> Master 조회시
		//전월평균환율적용금액 (2022-04-11 추가개발)
		//환율정보가 존재하고, 원화(KRW)가 아니고, 계획금액이 존재할 경우
		if (!this.gfn_IsNull(sCurrNm) && sCurrNm != 'KRW' && (!this.gfn_IsNull(iPlabAm) || iPlabAm > 0)) {
			var iLmebAm = this.ds_MaxLmebAm.getColumn(0, "LMEB_AM");	//전월평균환율적용금액 호출 (2022-04-25 변경)
			//var iLmebAm = this.fn_MaxExchangeRateSearch(sCurrNm, sPlabDd);	//전월평균환율적용금액 호출 (환율, 예상일자) 
			
			//조회된 값이 존재하면
			if (!this.gfn_IsNull(iLmebAm)) {
				iBecuAm = nexacro.floor(iPlabAm * iLmebAm, 0);				//전월평균환율적용금액 = 원자재시세 * 예상금액
			}

		//금액이 존재할 경우 (환율이 없으면 Default 1, 원화일 경우 Default 1)
		} else if (!this.gfn_IsNull(iPlabAm)) {
			iBecuAm = iPlabAm;		//전월평균환율적용금액 = 계획금액
		} else {
			iBecuAm = "";			//전월평균환율적용금액 = 초기화
		}

