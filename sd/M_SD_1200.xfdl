	this.div_Search.form.edt_DepmCd.set_value(oParam.DepmCd);
	this.div_Search.form.edt_DepmNm.set_value(oParam.DepmNm);		
	this.div_Search.form.edt_EmpmCd.set_value(oParam.EmpmCd);
	this.div_Search.form.edt_EmpmNm.set_value(oParam.EmpmNm);
	this.div_Search.form.edt_SystId.set_value(oParam.SystId);		
	this.div_Search.form.edt_CusmCd.set_value(oParam.CusmCd);
	this.div_Search.form.edt_CusmNm.set_value(oParam.CusmNm);
		
	//조회
	this.fn_ComBtnSearch()
	
}

