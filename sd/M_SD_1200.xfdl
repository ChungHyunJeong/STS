 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.gfn_DsIsUpdated(this.ds_Detail20) && !this.gfn_DsIsUpdated(this.ds_Detail30)) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}	
	
	//저장전 유효성 체크
	//Master 데이터 유효성 체크 ( 변경내용이 존재 or 최초 저장시(PLAH_DD가 NULL))
	if (this.gfn_DsIsUpdated(this.ds_Master10) || this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PLAH_DD"))) {
		if (!this.fn_VaildMasterSave()) return;
	}

	//Detail20(매출계획상세) Row가 없을시 (12/20 추가)
	if (this.ds_Detail20.rowcount == 0) {			
		this.div_DetailInput.form.tab_Detail.set_tabindex(1); //매출금액 Tab 이동
		var arrMsgArg = ["매출계획상세내역"];
		this.gfn_Alert("HR001", arrMsgArg); //매출계획상세내역 데이터가 없습니다.
		
		return;
	}
	
	//Detail30(수금계획상세) Row가 없을시 (12/20 추가)
	if (this.ds_Detail30.rowcount == 0) {			
		this.div_DetailInput.form.tab_Detail.set_tabindex(2); //수금금액 Tab 이동		
		var arrMsgArg = ["수금계획상세내역"];
		this.gfn_Alert("HR001", arrMsgArg); //매출계획상세내역 데이터가 없습니다.
		return;
	}

	//Detail10 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		if (!this.fn_VaildDetail10Save()) return;	
	}	

	//Detail20 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail20)) {
		if (!this.fn_VaildDetail20Save()) return;
	}	
	
	//Detail30 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail30)) {
		if (!this.fn_VaildDetail30Save()) return;		
	}

	//Master12수주금액대비 차액 발생시 (차액이 0원보다 큰 경우)
	if (this.gfn_DsIsUpdated(this.ds_Master12)) {
		var iPldbAm = nexacro.toNumber(this.ds_Master12.getColumn(0,"PLDB_AM")); //수주금액(기준)
		var iPldcAm = Math.abs(nexacro.toNumber(this.ds_Master12.getColumn(0,"PLDC_AM"))); //매출금액 차액
		var iPlddAm = Math.abs(nexacro.toNumber(this.ds_Master12.getColumn(0,"PLDD_AM"))); //수금금액 차액
		
		//수주금액 == 0 or 수주금액 < 0
		if (iPldbAm == 0 || iPldbAm < 0) {
			this.div_DetailInput.form.tab_Detail.set_tabindex(0); //수주금액 Tab 이동

			this.gfn_Alert("SD016"); //수주금액 실적이 존재하지 않습니다.
			return;
		} else if (this.ds_Master12.getColumn(0,"CURR_CD") == "0090") {
			//매출금액차액 != 0
			//if (iPldcAm != 0) {
			if (iPldcAm > 1000) {
				this.div_DetailInput.form.tab_Detail.set_tabindex(1); //매출금액 Tab 이동

				var arrMsgArg = ["매출"];
				this.gfn_Alert("SD015", arrMsgArg); //수주금액 기준 매출금액 차이가 발생합니다.
				return;
				
			//수금금액차액 != 0
			//} else if (iPlddAm != 0) {
			} else if (iPlddAm > 1000) {
				this.div_DetailInput.form.tab_Detail.set_tabindex(2); //수금금액 Tab 이동
				
				var arrMsgArg = ["수금"];
				this.gfn_Alert("SD015", arrMsgArg); //수주금액 기준 수금금액 차이가 발생합니다.
				return;
			}
		} else if (this.ds_Master12.getColumn(0,"CURR_CD") != "0090") {
			//매출금액차액 != 0
			//if (iPldcAm != 0) {
			if (iPldcAm > 10) {
				this.div_DetailInput.form.tab_Detail.set_tabindex(1); //매출금액 Tab 이동

				var arrMsgArg = ["매출"];
				this.gfn_Alert("SD015", arrMsgArg); //수주금액 기준 매출금액 차이가 발생합니다.
				return;
				
			//수금금액차액 != 0
			//} else if (iPlddAm != 0) {
			} else if (iPlddAm > 10) {
				this.div_DetailInput.form.tab_Detail.set_tabindex(2); //수금금액 Tab 이동
				
				var arrMsgArg = ["수금"];
				this.gfn_Alert("SD015", arrMsgArg); //수주금액 기준 수금금액 차이가 발생합니다.
				return;
			}
		}
	}
	
	//계획변경 상태이고(this.fvPlanChange = true), 계획변경 버튼 클릭시(this.fvPlanCheck = true) 신규로 Insert 처리
	if (this.fvPlanChange && this.fvPlanCheck) {
		//수주상세 신규등록
		if (this.ds_Detail10.rowcount > 0) {
			for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {	
				this.ds_Detail10.setColumn(iLoop, "crudType", "I");	    //신규
			}
		}
	
		//매출상세 신규등록
		if (this.ds_Detail20.rowcount > 0) {
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {	
				this.ds_Detail20.setColumn(iLoop, "crudType", "I");	    //신규
			}
		}
		
		//수금상세 신규등록
		if (this.ds_Detail30.rowcount > 0) {
			for (var iLoop=0; iLoop<this.ds_Detail30.getRowCount(); iLoop++) {	
				this.ds_Detail30.setColumn(iLoop, "crudType", "I");	    //신규
			}
		}		
	}
	
	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	//[공통]서비스 호출 변수
	var sInputDaset = "" //입력 데이터셋
	var sOutDatset = "";//출력 데이터셋
	var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2

	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	var crudTypeMaster = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "crudType"); //Master 신규여부
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자

	//Master 계획변경 상태이고(this.fvPlanChange = true), , 계획변경 버튼 클릭시(this.fvPlanCheck = true) 이전대상 최종상태를 0으로 변경
	if (this.fvPlanChange && this.fvPlanCheck) {
	
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
		svcAddObj.sInsertSql = "";//저장Sql
