/***********************************************************************************************
 * 15.06. [계산]수주금액, 매출금액, 수금금액 합계/차액
 ***********************************************************************************************/ 
this.fn_DetailAmountSum = function()
{
	var sCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐
	var sInexGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "INEX_GU");//국내외
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자

	if (sCurrCd == '0090') {	//KRW(원화)
		//원화계획 합계
		var iPlabAm10 = this.ds_Detail10.getCaseSum("PLAB_DD > '"+sToDay+"'", "WONE_AM")	//수주상세(계획) - 원화금액
		var iPlacAm20 = this.ds_Detail20.getCaseSum("PLAC_DD > '"+sToDay+"'", "WONE_AM")	//매출상세(계획) - 원화금액
		var iPladAm30 = this.ds_Detail30.getCaseSum("PLAD_DD > '"+sToDay+"'", "WONE_AM")	//수금상세(계획) - 원화금액

		//원화실적 합계
		var iPlabAm11 = this.ds_Detail11.getSum("WONE_AM")	//수주상세(실적) - 원화금액
		var iPlacAm21 = this.ds_Detail21.getSum("WONE_AM")	//매출상세(실적) - 원화금액
		var iPladAm31 = this.ds_Detail31.getSum("WONE_AM")	//수금상세(실적) - 원화금액
		
	} else {
		//외화계획 합계
		var iPlabAm10 = this.ds_Detail10.getCaseSum("PLAB_DD > '"+sToDay+"'", "FORE_AM")	//수주상세(계획) - 외화금액
		var iPlacAm20 = this.ds_Detail20.getCaseSum("PLAC_DD > '"+sToDay+"'", "FORE_AM")	//매출상세(계획) - 외화금액
		var iPladAm30 = this.ds_Detail30.getCaseSum("PLAD_DD > '"+sToDay+"'", "FORE_AM")	//수금상세(계획) - 외화금액

		//외화실적 합계
		var iPlabAm11 = this.ds_Detail11.getSum("FORE_AM")	//수주상세(실적) - 외화금액
		var iPlacAm21 = this.ds_Detail21.getSum("FORE_AM")	//매출상세(실적) - 외화금액
		var iPladAm31 = this.ds_Detail31.getSum("FORE_AM")	//수금상세(실적) - 외화금액	
	}

	//Master12 차액 그리드 계산 (10/19 합계로 계산)
	var iPlabSum10 = nexacro.floor((iPlabAm10 + iPlabAm11).toFixed(8), 2); //수주금액(기준) (수주계획+수주실적 합계)
	var iPlacSum20 = nexacro.floor((iPlacAm20 + iPlacAm21).toFixed(8), 2); //매출금액(실적) (매출계획+매출실적 합계)
	var iPladSum30 = nexacro.floor((iPladAm30 + iPladAm31).toFixed(8), 2); //수금금액(기준) (수금계획+수금실적 합계)
	
	var iPlanDf20 = nexacro.floor(iPlabSum10 - iPlacSum20, 2); //매출금액(차액)
	//2021-09-24 수정사항
	if (sInexGu == '0010') {	//국내외 구분이 국내인 경우 (해외, 국내영세율 제외)
		var iPlanDf30 = nexacro.floor(iPlabSum10*1.1 - iPladSum30, 2); //수주금액(차액) - (수정) 부가세를 추가해서 계산
	} else {
		var iPlanDf30 = nexacro.floor(iPlabSum10 - iPladSum30, 2); //수주금액(차액)
	}

	//this.alert(iPladAm30 + "    " + iPladAm31);

	//Master11 그리드 적용
	this.ds_Master11.clearData();
	this.ds_Master11.addRow();
	this.ds_Master11.setColumn(0, "EXPH_GU", "실적");		//구분(실적)
	this.ds_Master11.setColumn(0, "EXPB_AM", iPlabAm11);	//계획 수주금액
	this.ds_Master11.setColumn(0, "EXPC_AM", iPlacAm21);	//계획 매출금액
	this.ds_Master11.setColumn(0, "EXPD_AM", iPladAm31);	//계획 수금금액
	this.ds_Master11.setColumn(0, "PLAH_GU", "계획");		//구분(계획)
	this.ds_Master11.setColumn(0, "PLAB_AM", iPlabAm10);	//계획 수주금액
	this.ds_Master11.setColumn(0, "PLAC_AM", iPlacAm20);	//계획 매출금액
	this.ds_Master11.setColumn(0, "PLAD_AM", iPladAm30);	//계획 수금금액

	//Master12 그리드 적용
	this.ds_Master12.clearData();
	this.ds_Master12.addRow();
	this.ds_Master12.setColumn(0, "PLDB_GU", "수주금액(기준)");		//구분(수주)
	this.ds_Master12.setColumn(0, "PLDB_AM", iPlabSum10);				//수주금액(기준)
	this.ds_Master12.setColumn(0, "PLDC_GU", "매출금액(차액)");		//구분(매출)
	this.ds_Master12.setColumn(0, "PLDC_AM", iPlanDf20);				//매출금액(차액)
	this.ds_Master12.setColumn(0, "PLDD_GU", "수금금액(차액)");		//구분(수금)
	this.ds_Master12.setColumn(0, "PLDD_AM", iPlanDf30);				//수금금액(차액)

};


/***********************************************************************************************
 * 15.07. [Change]Detail10 그리드 수정상태 설정
 ***********************************************************************************************/ 
this.fn_SetGridDetail10 = function()
{
	var sCurrCd  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐코드		
	var sPlabDd  = "";	//예상일자
	var iWoneAm  = 0;	//원화금액
	var iForeAm  = 0;	//외화금액
	var iCount = 0;	//Detail10과 Detail11 비교 건수

	//Detail10 그리드 셋팅
	for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
		sPlabDd  = this.ds_Detail10.getColumn(iLoop, "PLAB_DD");//예상일자
		iWoneAm  = this.ds_Detail10.getColumn(iLoop, "WONE_AM");//원화금액
		iForeAm  = this.ds_Detail10.getColumn(iLoop, "FORE_AM");//외화금액
		
		//화폐코드에 따른 원화금액/외화금액 비교
		if (sCurrCd == '0090') {	//KRW(원화)
			iCount = this.ds_Detail11.getCaseCount("PURC_DD == '"+sPlabDd+"' && WONE_AM == '"+iWoneAm+"'");	//원화금액 비교
