				//변경일자가 당일이고 최종의 경우 그리드 변경가능여부 셋팅 호출 (10/18 설계자 요청)
				if (sPlahDd == sToDay && sWofhYn == '1') {
					this.fn_SetGridDetail30(); 	//Detail30 그리드 변경가능여부 호출
				}

			} else {
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_03.form.grd_Detail30);					
			}	

			//수금상세 - 실적 존재시
