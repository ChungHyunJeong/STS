	//[결재]결재내역 조회모드이면 Visble false || 결재상태가 "등록"이 아닐경우
	if (this.fvWfSearchMode || this.fvWofhStatus) {
		//[결재]결재내역 팝업화면으로 호출여부
		if (this.fvWfPopupMode) {
			this.div_Search.set_enable(false);//조회영역 Enable처리
		}
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);
		//[결재] Div하위의 컴포넌트 Enable 처리
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 입력			
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_RemkPop.set_enable(true); //비고
		
		//마이너스 매출인 아닌경우만 활성화
		if( this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTY_CD") != "4"){
			if( this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MINS_CN") != 1){	//기존 마이너스 매출이 없으면..
				if( this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WONE_AM") > 0){	//원화금액이 마이너스가 아니면..
					this.div_MasterInput.form.btn_CorhM1.set_enable(true);
				}
			}
		}
		
		//[전표보기]
		//결재상태-승인인경우 => [전표보기] 버튼 활성화
		if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST") == "0040") {
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(true);
		} else {
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(false);
		}
	} else {
		//[결재]결재내역 팝업화면으로 호출여부
		if (this.fvWfPopupMode) {	
			this.div_Search.set_enable(true);//조회영역 Enable처리
		}
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(true);
		//[결재] Div하위의 컴포넌트 Enable 처리
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, true);//Master 입력 프리폼
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form, true);//Detail 입력	
		//[결재] Div하위의 컴포넌트 Enable 처리 제외할 컴포넌트설정
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_ColhNo.set_readonly(true);
		//////기능사용중지 김형수 부장요청)
		//this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ColhDd.set_enable(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_readonly(true);	//부서
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_readonly(true);	//사용자
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_readonly(true);	//거래처
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmNm.set_readonly(true);	
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_WoneAm.set_readonly(true);	
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_ForeAmt.set_readonly(true);	
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CurrCd.set_enable(false);	
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_ExchRt.set_readonly(true);	
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_AcumCd.set_enable(false);	
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_BfcoNo.set_readonly(true);	
		
		this.div_MasterInput.form.btn_CorhM1.set_enable(false);//마이너스수금
	}
};
 

