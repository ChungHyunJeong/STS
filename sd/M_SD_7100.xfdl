		//ds10CurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");
		ds10CurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
