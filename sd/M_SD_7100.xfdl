		this.ds_Detail10.setColumn(iLoop, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
		this.ds_Detail10.setColumn(iLoop, "COLH_NO", "");
		this.ds_Detail10.setColumn(iLoop, "WONE_AM", this.ds_Detail10.getColumn(iLoop, "WONE_AM")*-1);	//원화금액
		this.ds_Detail10.setColumn(iLoop, "FORE_AM", this.ds_Detail10.getColumn(iLoop, "FORE_AM")*-1);	//외화금액
		this.ds_Detail10.setColumn(iLoop, "DEWO_AM", this.ds_Detail10.getColumn(iLoop, "DEWO_AM")*-1);	//공제원화금액
		this.ds_Detail10.setColumn(iLoop, "DEFO_AM", this.ds_Detail10.getColumn(iLoop, "DEFO_AM")*-1);	//공제외화금액
		
		//this.ds_Detail10.setColumn(iLoop, "ADDCHK", "Y");
	}
	//trace(this.ds_Detail10.saveXML());
	this.ds_Detail10.set_enableevent(true);
	
	// 	//마이너스수금시 원화, 외화 활성화 (20221019)	
	var sCotyCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTY_CD"); //수금구분  	
	var sColhNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COLH_NO"); //수금번호   // 원수금번호(sBfcoNo)

	//수금번호 Not Null & 수금구분(SD710) 마이너스수금('4')이면 금액, 수금금액 수정가능 (20221019)
 	if ( !this.gfn_IsNull(sBfcoNo) && ( sCotyCd == '4')) {	
	
		for (var iLoop=0; iLoop < this.ds_Detail10.rowcount; iLoop++) {		
	
			this.ds_Detail10.setColumn(iLoop, "ADDCHK", "Y");
		
		}	
	}
	
	
	//화폐에 따라 원화, 외화 비활성화(20221019)		     
	var sCurrCd = this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CurrCd.value; //화폐 //20221019
	if(sCurrCd == '0090'){
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 9, "edittype", "mask");		
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 11, "edittype", "none");	
	} else {
		
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 9, "edittype", "none");		
	    this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 11, "edittype", "mask");
	}
	
	
	//입금상세
	this.ds_Detail30.set_enableevent(false);
	for (var iLoop=0; iLoop < this.ds_Detail30.rowcount; iLoop++) {
		this.ds_Detail30.setColumn(iLoop, "crudType", "I");
		this.ds_Detail30.setColumn(iLoop, "COMM_CD", this.ds_Detail30.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
		this.ds_Detail30.setColumn(iLoop, "COLH_NO", "");
		
		this.ds_Detail30.setColumn(iLoop, "WONE_AM", this.ds_Detail30.getColumn(iLoop, "WONE_AM")*-1);	//원화
		this.ds_Detail30.setColumn(iLoop, "FORE_AM", this.ds_Detail30.getColumn(iLoop, "FORE_AM")*-1);	//외화

