			}else{
				iAm = nexacro.toNumber(this.ds_Detail10.getColumn(iLoop, "FORE_AM"))+nexacro.toNumber(this.ds_Detail10.getColumn(iLoop, "DEFO_AM"));
			}
			
			if(iAm > nexacro.toNumber(this.ds_Detail10.getColumn(iLoop, "CNTB_AM")) + 10 ){//원단위 오류 무시 요청. 염은주 과장 221011
				//var arrMsgArg = ["수금반영금액("+this.ds_Detail10.getColumn(iLoop, "COLB_AM") +")", "계약금액("+this.ds_Detail10.getColumn(iLoop, "CNTB_AM") +")"];
