		var AcchNo = this.ds_Master10.getColumn(iRow, "ACCH_NO");//발의번호
		var WofnSq = "1";//결재차수-고정

		var sPopupId = "WF_M_FA_1000";//팝업ID - 계정코드
		var oArg = {paramTitle:"전표등록", AcchNo:AcchNo, WofnSq:WofnSq};//Argument처리
		this.gfn_AcchPop(sPopupId, oArg); 
	}
};

this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//수금구분 변경(미입금수금: 8 )
	if (e.columnid == "COTY_CD") {
		if(obj.getColumn(e.row, "COTY_CD") == "8") {//미입금수금이면 입금 선택 초기화
			//alert(this.ds_Detail30.getRowCount());
			this.ds_Detail30.set_enableevent(false);
			for (var iLoop=0; iLoop < this.ds_Detail30.getRowCount(); iLoop++) {
				this.ds_Detail30.setColumn(iLoop, "s_chk", "0");
				this.ds_Detail30.setColumn(iLoop, "crudType", "");
			}
			this.ds_Detail30.set_enableevent(true);
		}
		
	//환율변경시 재계산
	}else if(e.columnid == "EXCH_RT") {
		var nRatm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");	
		var sCotyCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTY_CD"); //수금구분
		var ds30SelectRow = this.ds_Detail30.findRow("s_chk","1");
		
		for (var iLoop=0; iLoop < this.ds_Detail20.getRowCount(); iLoop++) {
			s_chk = this.ds_Detail20.getColumn(iLoop, "s_chk");
			
			//체크
			if (s_chk == "1") {
				if(sCurrCd !="0090"){
					if (sCotyCd == 8){//미수금수금이면
						this.ds_Detail20.setColumn(iLoop, "ADWN_AM",  nexacro.floor((this.ds_Detail20.getColumn(iLoop, "ADFR_AM") * nRatm).toFixed(8)));	//수금원화
						this.ds_Detail20.setColumn(iLoop, "DEWO_AM",  nexacro.floor((this.ds_Detail20.getColumn(iLoop, "DEFO_AM") * nRatm).toFixed(8)));	//공제원화
						//if(obj.getColumn(iLoop, "CURR_CD")
					}else{
						this.ds_Detail20.setColumn(iLoop, "ADWN_AM",  nexacro.floor((this.ds_Detail20.getColumn(iLoop, "UNCL_AM") * this.ds_Detail30.getColumn(ds30SelectRow, "EXCH_RT")).toFixed(8)));	//수금원화
						this.ds_Detail20.setColumn(iLoop, "ADFR_AM",  this.ds_Detail20.getColumn(iLoop, "UNCL_AM"));	//수금외화-> 미수금액자동 입력
					}
				}
				
				var objDate = new nexacro.Date();
				var objOldDate = new nexacro.Date(objDate.getYear(objDate.addYear(-1)),11,31);
					
				var iOldYear = nexacro.toNumber(this.ds_Detail20.getColumn(iLoop,"OLD_YEAR"));	//이전년도 수
				var nOldRatm = this.gfn_GetCommRatm(objOldDate, this.ds_Detail20.getColumn(iLoop, "CURR_CD"));//전년도 마지막날 환율정보
			
				var iAdfrAm = this.ds_Detail20.getColumn(iLoop,"ADFR_AM"); //수금외화금액
				var iAdwnAm = this.ds_Detail20.getColumn(iLoop,"ADWN_AM"); //수금원화금액
				var iDefoAm = this.ds_Detail20.getColumn(iLoop,"DEFO_AM"); //공제외화금액
				var iDewoAm = this.ds_Detail20.getColumn(iLoop,"DEWO_AM"); //공제원화금액
				var iExchAm = this.ds_Detail20.getColumn(iLoop,"EXCH_RT"); //환율
				//전년도 매출 처리
				if(iOldYear != 0 && sCurrCd !="0090"){
					// 매출원화 변경  = 전년도말일자환율 * (수금외화금액 + 공제외화금액)
					this.ds_Detail20.setColumn(iLoop, "WONE_AM", nexacro.floor(nexacro.toNumber((nOldRatm * (iAdfrAm + iDefoAm)).toFixed(8))));
					
					//환자손 = (전년도말일자환율 * (수금외화금액 + 공제외화금액)) - 수금원화금액 - 수금공제금액
					this.ds_Detail20.setColumn(iLoop, "FELS_AM", (nexacro.floor(nexacro.toNumber((nOldRatm * (iAdfrAm + iDefoAm)).toFixed(8))) - iAdwnAm - iDewoAm));
				}else{
					this.ds_Detail20.setColumn(iLoop, "FELS_AM", nexacro.floor(nexacro.toNumber(((iAdfrAm + iDefoAm) * iExchAm).toFixed(8))) - iAdwnAm - iDewoAm);
				}
			}
		}
	}
	
	//var nRatm = 
	//if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTY_CD")=="1"){
		//var nRatm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");	//현재환율
	//}
};

