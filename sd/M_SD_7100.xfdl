this.fn_DetailGrd01DeleteRow = function()
{
	var sTabIndex = this.div_DetailInput.form.tab_Detail.tabindex ;
	if(sTabIndex == 0){//수금내역
		var s_chk = "";//체크
		//미출고검색 내역 체크 가능하게 변경
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			s_chk = this.ds_Detail10.getColumn(iLoop, "s_chk");
			
			//체크
			if (s_chk == "1") {
			
				//검색값설정 - 컬럼명:검색값
				var objSearchList = ["SALH_NO|" + this.ds_Detail10.getColumn(iLoop, "SALH_NO") //매출번호
									,"SALH_SQ|" + this.ds_Detail10.getColumn(iLoop, "SALH_SQ") //매출결재차수
									,"SALB_SE|" + this.ds_Detail10.getColumn(iLoop, "SALB_SE") //매출순번
									];

				//[공통] 행삭제시 검색Tab 선택Row 해제 처리
				this.gfn_DeleteRowSchTabUncheck(this.ds_Detail20, objSearchList);
			}
		}
	
		this.gfn_GridDelProcess(this, this.ds_Detail10);//crud_flag 변경-삭제(D)			
	}else if(sTabIndex == 2){//입금상세
		//this.gfn_GridDelProcess(this, this.ds_Detail30);//crud_flag 변경-삭제(D)			
	}else if(sTabIndex == 3){//공제내역상세
		this.gfn_GridDelProcess(this, this.ds_Detail40);//crud_flag 변경-삭제(D)			
	}else if(sTabIndex == 4){//수수료상세
		this.gfn_GridDelProcess(this, this.ds_Detail50);//crud_flag 변경-삭제(D)			
	}
	
	
	
};


