/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Detail20] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//trace(e.columnid);
	
	var sCotyCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTY_CD"); //수금구분

	//환율계산 - 수금반영정보 (외화,원화)수정시
	if (e.columnid == "ADWN_AM" || e.columnid == "ADFR_AM") {
		this.fn_EXCHcalculate(obj, e.row);
	}
	
	//미수금입금이면 매출연계에서 화폐 처리.
	if (e.columnid == "s_chk" && sCotyCd == 8){
		var ds20ChkCount = obj.getCaseCount( "s_chk == '1'" );//선택된 수

		if(ds20ChkCount == 1){
			 this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CURR_CD", obj.getColumn(obj.findRow("s_chk","1"), "CURR_CD"));	
			 this.div_MasterInput_tab_Master_tpg_01_cbo_CurrCd_onitemchanged(this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CurrCd);
		}
	}
	
	// 선택시 - 수금반영정보 자동적용(원화,외화)
	if (e.columnid == "s_chk" && obj.getColumn(e.row, "s_chk") == "1") {
	
		var ds30SelectRow = this.ds_Detail30.findRow("s_chk","1");		
		
		if(obj.getColumn(e.row, "CURR_CD") == "0090"){ 
			//수금원화 입력금액 없으면 미수금으로 (20220912)
			var sAdwnAm = obj.getColumn(e.row, "ADWN_AM")	 //수금원화	
			
			if ( this.gfn_IsNull(obj.getColumn(e.row, "ADWN_AM")) || sAdwnAm == 0 ) {						
				obj.setColumn(e.row, "ADWN_AM", obj.getColumn(e.row, "UNCL_AM"));	//수금원화-> 미수금으로 자동입력	
			}		
			
			//org
			//obj.setColumn(e.row, "ADWN_AM", obj.getColumn(e.row, "UNCL_AM"));	//원화 미수금으로 자동입력	
			

			//여기
			obj.setColumn(e.row, "ADFR_AM",  0);	//수금외화-> 미수금액자동 입력
			
		//외화	
		}else{ 		
		//this.alert('외화선택');
			var nRatm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");	
			
			var sCotyCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTY_CD"); //수금구분
			
			if (sCotyCd == 8){//미수금수금이면
				obj.setColumn(e.row, "ADWN_AM",  nexacro.floor((obj.getColumn(e.row, "UNCL_AM") * nRatm).toFixed(8)));	//수금원화
				//if(obj.getColumn(e.row, "CURR_CD")
			}else{
				obj.setColumn(e.row, "ADWN_AM",  nexacro.floor((obj.getColumn(e.row, "UNCL_AM") * this.ds_Detail30.getColumn(ds30SelectRow, "EXCH_RT")).toFixed(8)));	//수금원화
			}
			
			//수금외화 입력금액 없으면 미수금으로 (20220912)
			var sAdfrAm = obj.getColumn(e.row, "ADFR_AM")		
			
			if ( this.gfn_IsNull(obj.getColumn(e.row, "ADFR_AM")) || sAdfrAm == 0 ) {						
				obj.setColumn(e.row, "ADFR_AM",  obj.getColumn(e.row, "UNCL_AM"));	//수금외화-> 미수금액자동 입력
			}
			
// // 			//org
// 			obj.setColumn(e.row, "ADFR_AM",  obj.getColumn(e.row, "UNCL_AM"));	//수금외화-> 미수금액자동 입력
			
			var objDate = new nexacro.Date();
			var objOldDate = new nexacro.Date(objDate.getYear(objDate.addYear(-1)),11,31);
				
			var iOldYear = nexacro.toNumber(obj.getColumn(e.row,"OLD_YEAR"));	//이전년도 수
			var nOldRatm = this.gfn_GetCommRatm(objOldDate, obj.getColumn(e.row, "CURR_CD"));//전년도 마지막날 환율정보
		
			var iAdfrAm = obj.getColumn(e.row,"ADFR_AM"); //수금외화금액
			var iAdwnAm = obj.getColumn(e.row,"ADWN_AM"); //수금원화금액
			var iDefoAm = obj.getColumn(e.row,"DEFO_AM"); //공제외화금액
			var iDewoAm = obj.getColumn(e.row,"DEWO_AM"); //공제원화금액
			var iExchAm = obj.getColumn(e.row,"EXCH_RT"); //환율
			//전년도 매출 처리
			if(iOldYear != 0 && sCurrCd !="0090"){
				// 매출원화 변경  = 전년도말일자환율 * (수금외화금액 + 공제외화금액)
				obj.setColumn(e.row, "WONE_AM", nexacro.floor(nexacro.toNumber((nOldRatm * (iAdfrAm + iDefoAm)).toFixed(8))));
				
				//환자손 = (전년도말일자환율 * (수금외화금액 + 공제외화금액)) - 수금원화금액 - 수금공제금액
				obj.setColumn(e.row, "FELS_AM", (nexacro.floor(nexacro.toNumber((nOldRatm * (iAdfrAm + iDefoAm)).toFixed(8))) - iAdwnAm - iDewoAm));
			}else{
				obj.setColumn(e.row, "FELS_AM", nexacro.floor(nexacro.toNumber(((iAdfrAm + iDefoAm) * iExchAm).toFixed(8))) - iAdwnAm - iDewoAm);
			}
		}
		
	}else if (e.columnid == "s_chk" && obj.getColumn(e.row, "s_chk") == "0") {
//this.alert('미선택');
		obj.setColumn(e.row, "ADWN_AM", 0);	//수금원화
		obj.setColumn(e.row, "ADFR_AM", 0);	//수금외화
		obj.setColumn(e.row, "DEWO_AM", 0);	//공제외화
		obj.setColumn(e.row, "DEFO_AM", 0);	//공제외화
		obj.setColumn(e.row, "FELS_AM", 0);	//환차손
	
	//공제 외화입력시
	} else if (e.columnid == "DEFO_AM") {
		
		this.fn_DEFOcalculate(obj, e.row);
	}
	
};

/*---------------------------------------------------------------------------------------------
 *    02.1 환율계산 - 수금반영정보 (외화,원화)수정시
  ---------------------------------------------------------------------------------------------*/  
this.fn_EXCHcalculate  = function(obj,eRow)
{
	trace("환율코드--->"+obj.getColumn(eRow, "CURR_CD"));
	var sCotyCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTY_CD"); //수금구분	
	//nRatm =1000; //테스트용
	if(obj.getColumn(eRow, "CURR_CD") != "0090"){
		var objDate = new nexacro.Date();
		var objOldDate = new nexacro.Date(objDate.getYear(objDate.addYear(-1)),11,31);
		var iOldYear = nexacro.toNumber(obj.getColumn(eRow,"OLD_YEAR"));	//이전년도 수
		
		var nOldRatm = this.gfn_GetCommRatm(objOldDate, obj.getColumn(eRow, "CURR_CD"));//전년도 마지막날 환율정보
		var iAdfrAm = obj.getColumn(eRow,"ADFR_AM"); //수금외화금액
		var iDefoAm = obj.getColumn(eRow,"DEFO_AM"); //공제외화금액
		var iDewoAm = obj.getColumn(eRow,"DEWO_AM"); //공제원화금액
		var iExchRt = obj.getColumn(eRow,"EXCH_RT"); //환율
		var sCurrCd = obj.getColumn(eRow,"CURR_CD") // 화폐
		
		
		//전년도 매출 처리
		if(iOldYear != 0 && sCurrCd !="0090"){
			var nRatm = 0;
			nRatm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");	
			/*
			if (sCotyCd == 8){//미수금수금이면
				nRatm = obj.getColumn(eRow, "EXCH_RT");	
			}else{
				nRatm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");	
			}	
			*/
			// 매출원화 변경  = 전년도말일자환율 * (수금외화금액 + 공제외화금액)
			obj.setColumn(eRow, "WONE_AM", nexacro.floor((nOldRatm * (iAdfrAm + iDefoAm)).toFixed(8)));
			
			obj.setColumn(eRow, "ADWN_AM", nexacro.floor((nRatm * iAdfrAm).toFixed(8)));	//수금반영 원화
			var iAdwnAm = obj.getColumn(eRow,"ADWN_AM"); //수금원화금액
		
			//환자손 = (전년도말일자환율 * (수금외화금액 + 공제외화금액)) - 수금원화금액 - 수금공제금액
			obj.setColumn(eRow, "FELS_AM", (nexacro.floor((nOldRatm * nexacro.toNumber((iAdfrAm + iDefoAm)).toFixed(8)) - iAdwnAm - iDewoAm)));
		}else{
			var nRatm = 0;
			nRatm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");	
			 
			/*
			if (sCotyCd == 8){//미수금수금이면
				nRatm = obj.getColumn(eRow, "EXCH_RT");	
			}else{
				nRatm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");	
			}	
			*/
			trace(nRatm);
			trace(iAdfrAm);
			obj.setColumn(eRow, "ADWN_AM", nexacro.floor((nRatm * iAdfrAm).toFixed(8)));	//수금반영 원화
			
			var iAdwnAm = obj.getColumn(eRow,"ADWN_AM"); //수금원화금액
		
			//환자손 = (매출환율*외화금액)+(매출환율*외화공제금액) - (수금환율*외화금액) - (수금환율*외화공제금액)
			obj.setColumn(eRow, "FELS_AM", nexacro.floor(nexacro.toNumber((iAdfrAm * iExchRt).toFixed(8))) + nexacro.floor(nexacro.toNumber((iDefoAm * iExchRt).toFixed(8)) - iAdwnAm - iDewoAm));
		}

	}
}


/*---------------------------------------------------------------------------------------------
 *    02.2  환율계산 - 공제 외화입력시 
  ---------------------------------------------------------------------------------------------*/  
this.fn_DEFOcalculate  = function(obj,eRow)
{
	var objDate = new nexacro.Date();
	var objOldDate = new nexacro.Date(objDate.getYear(objDate.addYear(-1)),11,31);
	var sCotyCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTY_CD"); //수금구분

	var iOldYear = nexacro.toNumber(obj.getColumn(eRow,"OLD_YEAR"));	//이전년도 수
	var nOldRatm = this.gfn_GetCommRatm(objOldDate, obj.getColumn(eRow, "CURR_CD"));//전년도 마지막날 환율정보
	var iAdfrAm = obj.getColumn(eRow,"ADFR_AM"); //수금외화금액
	var iAdwnAm = obj.getColumn(eRow,"ADWN_AM"); //수금원화금액
	var iDefoAm = obj.getColumn(eRow,"DEFO_AM"); //공제외화금액
	var iDewoAm = obj.getColumn(eRow,"DEWO_AM"); //공제원화금액
	var iExchRt = obj.getColumn(eRow,"EXCH_RT"); //환율
	var sCurrCd = obj.getColumn(eRow,"CURR_CD") // 화폐
	//전년도 매출 처리
	if(iOldYear != 0 && sCurrCd !="0090"){
		// 매출원화 변경  = 전년도말일자환율 * (수금외화금액 + 공제외화금액)
		obj.setColumn(eRow, "WONE_AM", nexacro.floor(nexacro.toNumber((nOldRatm * (iAdfrAm + iDefoAm)).toFixed(8))));
		
		var nRatm = 0;
		nRatm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");	
		/*
		if (sCotyCd == 8){//미수금수금이면
			nRatm = obj.getColumn(eRow, "EXCH_RT");	
		}else{
			nRatm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");	
		}	
		*/

		obj.setColumn(obj.rowposition,"DEWO_AM", nexacro.floor(nexacro.toNumber((nRatm * obj.getColumn(obj.rowposition,"DEFO_AM")).toFixed(8)))) ;//공제원화
		
			
		//환자손 = (전년도말일자환율 * (수금외화금액 + 공제외화금액)) - 수금원화금액 - 수금공제금액
		obj.setColumn(eRow, "FELS_AM", (nexacro.floor(nexacro.toNumber((nOldRatm * (iAdfrAm + iDefoAm)).toFixed(8))) - iAdwnAm - iDewoAm));
	}else{
		var nRatm = 0;
		nRatm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");	
		/*
		if (sCotyCd == 8){//미수금수금이면
			nRatm = obj.getColumn(eRow, "EXCH_RT");	
		}else{
			nRatm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");	
		}
		*/
			
		obj.setColumn(obj.rowposition,"DEWO_AM", nexacro.floor(nexacro.toNumber((nRatm * obj.getColumn(obj.rowposition,"DEFO_AM")).toFixed(8)))) ;//공제원화
		//환자손 = (매출환율*외화금액)+(매출환율*외화공제금액) - (수금환율*외화금액) - (수금환율*외화공제금액)
		obj.setColumn(eRow, "FELS_AM", nexacro.floor(nexacro.toNumber((iAdfrAm * iExchRt).toFixed(8))) + nexacro.floor(nexacro.toNumber((iDefoAm * iExchRt).toFixed(8))) - iAdwnAm - iDewoAm);
	}
}
