this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//선수금일경우 계산???  일반일땐?? 마이너스일떈??
	
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	//선수금일 경우
	if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTY_CD")=="1"){
		var nRatm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");	//현재환율
		//계약번호
		if (e.columnid == "CNTH_NO") {
			this.gfn_GetCommCnthGrid(obj, "CNTH_NO", "CNTH_NM");//계정명 검색
		//수금원화
		}else if(e.columnid == "WONE_AM"){
		    
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM",obj.getCaseSum("crudType!='D'","WONE_AM")+ obj.getCaseSum("crudType!='D'","DEWO_AM"));
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_AM", 0);
		//수금외화
		}else if(e.columnid == "FORE_AM"){		    			
			
			 obj.setColumn(e.row, "WONE_AM", obj.getColumn(e.row, "FORE_AM") * nRatm);	//수금반영 원화
			//obj.setColumn(e.row, "WONE_AM", nexacro.round(obj.getColumn(e.row, "FORE_AM") * nRatm, 0));	//수금반영 원화
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM",((obj.getCaseSum("crudType!='D'","FORE_AM")+ obj.getCaseSum("crudType!='D'","FORE_AM")) * nRatm).toFixed(0));
			//this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM",((obj.getCaseSum("crudType!='D'","FORE_AM")+ obj.getCaseSum("crudType!='D'","FORE_AM")) * nRatm).toFixed(8)); //org
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_AM",obj.getCaseSum("crudType!='D'","FORE_AM")+ obj.getCaseSum("crudType!='D'","DEFO_AM"));
		
			
		//공제원화
		}else if(e.columnid == "DEWO_AM"){
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM",obj.getCaseSum("crudType!='D'","WONE_AM")+ obj.getCaseSum("crudType!='D'","DEWO_AM"));
		//공제외화
		}else if(e.columnid == "DEFO_AM"){
			obj.setColumn(e.row, "DEWO_AM", obj.getColumn(e.row, "DEFO_AM") * nRatm);	//공제반영 원화
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM",nexacro.floor(((obj.getCaseSum("crudType!='D'","FORE_AM")+ obj.getCaseSum("crudType!='D'","FORE_AM")) * nRatm).toFixed(8))); //소수점이하 버림(공제상세내역과 일치요청 20220912)
			//this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM",((obj.getCaseSum("crudType!='D'","FORE_AM")+ obj.getCaseSum("crudType!='D'","FORE_AM")) * nRatm).toFixed(8));
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_AM",obj.getCaseSum("crudType!='D'","FORE_AM")+ obj.getCaseSum("crudType!='D'","DEFO_AM"));
		}
	 
	}  
/*********마이너스매출 START**********/

// 	 //마이너스수금일 경우(20221019)
	if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTY_CD")=="4"){ 
	
		 //원화 경우
