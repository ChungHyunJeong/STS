			sSum = e.newvalue + obj.getColumn(e.row, "DEFO_AM");
		}else if( e.columnid == "DEWO_AM") {
			sSum = e.newvalue + obj.getColumn(e.row, "ADWN_AM");
		}else if( e.columnid == "DEFO_AM") {
			sSum = e.newvalue + obj.getColumn(e.row, "ADFR_AM");
		}
	}
	var sCotyCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTY_CD"); //수금구분
	
	// 선택시 - 수금반영정보 선택체크
