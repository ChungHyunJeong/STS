/***********************************************************************************************
 * 02.DG5.03. [자동전표상세 그리드-버튼] 자동전표 미리보기 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd01Atob = function() {
	var iCurRow = this.ds_Master10.rowposition;//현재 선택된 행
	var comm_cd = this.ds_Master10.getColumn(iCurRow, "COMM_CD")
	var coty_cd = this.ds_Master10.getColumn(iCurRow, "COTY_CD"); // 수금구분

	var autm_cd	= ""; // 자동전표호출할 프로그램 ID
	var kye_01	= this.ds_Master10.getColumn(iCurRow, "COLH_NO");
	var kye_02	= this.ds_Master10.getColumn(iCurRow, "WOFH_SQ");
	
	if(coty_cd == '1' || coty_cd == '3') { // 선수금
		switch(this.ds_Detail10.getColumn(0, "INEX_GU")) { // 국내, 국외
			case '0010': // 선수금이면서 국내계약
				autm_cd = "SD_7100_20";
			break;
			case '0020': // 선수금이면서 해외계약
				autm_cd = "SD_7100_30";
			break;
			case '0030': // 선수금이면서 국내계약
				autm_cd = "SD_7100_20";
			break;
		}
	}else if (coty_cd == '4'){
		switch(this.ds_Detail10.getColumn(0, "INEX_GU")) { // 국내, 국외
			case '0010': // 마이너스수금이면서 국내계약
				autm_cd = "SD_7100_80";
			break;
			case '0020': // 마이너스수금이면서 해외계약
				autm_cd = "SD_7100_90";
			break;
			case '0030': // 마이너스수금이면서 국내계약
				autm_cd = "SD_7100_80";
			break;
		}
	}else if (coty_cd == '8'){
		switch(this.ds_Detail10.getColumn(0, "INEX_GU")) { // 국내, 국외
			case '0010': // 미입금이면서 국내계약
				autm_cd = "SD_7100_25";
			break;
			case '0020': // 미입금이면서 해외계약
				autm_cd = "SD_7100_50";
			break;
			case '0030': // 미입금이면서 국내계약
				autm_cd = "SD_7100_25";
			break;
		}
	} else {
		switch(this.ds_Detail10.getColumn(0, "INEX_GU")) { // 국내, 국외
			case '0010': // 선수금이 아니면서 국내계약
				autm_cd = "SD_7100_10";
			break;
			case '0020': // 선수금이 아니면서 해외계약
				autm_cd = "SD_7100_40";
			break;
			case '0030': // 선수금이 아니면서 국내계약
				autm_cd = "SD_7100_10";
			break;
		}
	}
	
	if (iCurRow > -1) {
		var atobObj = new Object();//[공통] 자동전표 파라메타 정보 Object
		atobObj.COMM_CD = comm_cd;//회사코드
		atobObj.AUTM_CD = autm_cd;//분류코드-T_FA_AUTM(자동전표환경설정) 테이블 내용 참조
		atobObj.PRGM_ID = this.fvComPrgmId;//프로그램ID
		atobObj.MANA_NO = kye_01;//관리번호-업무Key
		atobObj.WOFH_SQ = kye_02;//결재차수
		atobObj.REGI_ID = this.gfn_GetSessionValue("SYST_ID");//등록자
		atobObj.REGI_NM = this.gfn_GetSessionValue("USEM_NM");//등록자명
		atobObj.KEY_01 = "@COMM_CD=" + comm_cd;//T_FA_AUTD-자동전표환경설정-D 테이블의 쿼리에 사용하는 파라메타 Key-Value정보
		atobObj.KEY_02 = "@COLH_NO=" + kye_01;
		atobObj.KEY_03 = "@WOFH_SQ=" + kye_02;
		atobObj.KEY_04 = "";
		atobObj.KEY_05 = "";
		atobObj.KEY_06 = "";
		atobObj.KEY_07 = "";
		atobObj.KEY_08 = "";
		atobObj.KEY_09 = "";
		atobObj.KEY_10 = "";
		
		eval(this.fvAtobDiv).fn_DetailGrd01Atob(atobObj);//자동전표 미리보기
	}
}
