this.fn_ComBtnCopy = function()
{ 
	this.gfn_SetCompGroupEnable(this.div_DetailInput.form, true);//Detail 상위단 입력 프리폼 활성화
	
	//선택된 Row
	//var arrRows = this.div_MasterView.form.grd_Master10.getSelectedRows();
