		return;
	} 
	
	//수수료 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail50)) {
		if (!this.fn_VaildDetail50Save()) return;
	}
	
	//수수료 상세 차대변 체크
	if(this.ds_Detail50.rowcount > 0){
		var ds50CtedAm = nexacro.toNumber(this.ds_Detail50.getCaseSum("crudType!='D'","CRED_AM"));
		//trace(ds50CtedAm);
		if(ds50CtedAm == 0 ){
			var arrMsgArg = ["차변금액","1","금액"];
			this.gfn_Alert("SM009", arrMsgArg); //&ITM0은(는) &ITM1 이상의 &ITM2만 입력 가능합니다.
			return;
		}
		
		var ds50DebiAm = nexacro.toNumber(this.ds_Detail50.getCaseSum("crudType!='D'","DEBI_AM"));
		
		if(ds50DebiAm == 0){
			var arrMsgArg = ["대변금액","1","금액"];
			this.gfn_Alert("SM009", arrMsgArg); //&ITM0은(는) &ITM1 이상의 &ITM2만 입력 가능합니다.
			return;
		}
		
		//if(!ds50CtedAm.isEqual(ds50DebiAm)){
		if(ds50CtedAm != ds50DebiAm){
			var arrMsgArg = ["차변금액","대변금액"];
			this.gfn_Alert("SM063", arrMsgArg); //[차변금액]은 [대변금액]과 일치 해야 합니다.
			return;
		}
	}
	return true;
}

