 * 02.DG1.05. [Detail그리드2-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Choice = function()
{
	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {
			
			
			var ds30SelectRow = 0;
			var ds30CurrCd = 0;
			var sCotyCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COTY_CD"); //수금구분
			if(sCotyCd != 8){//미수금입금이면 체크안함
				//입금내역 체크여부
				ds30SelectRow = this.ds_Detail30.findRow("s_chk","1");
				
				if(ds30SelectRow == -1){
					var arrMsgArg = ["입금내역"];
					this.gfn_Alert("SM040", arrMsgArg); //[입금내역]을(를) 선택하세요.
					return;
				}
				//임금원화
				ds30CurrCd = this.ds_Detail30.getColumn(ds30SelectRow,"CURR_CD");
			}
			
			
			var sCnthNoChk;
			var sAddnAm;
			//동일화폐 추가 체크
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					
					if(sCotyCd != 8){//미수금입금이면 체크안함
						//입금내역 화폐 체크
						if(ds30CurrCd != this.ds_Detail20.getColumn(iLoop,"CURR_CD") ){
							var arrMsgArg = ["입금내역 화폐", "화폐"];
							this.gfn_Alert("SM058", arrMsgArg); //[입금내역 화폐]와(과) 동일한 [화폐]만 적용 가능합니다.
							return;
						} 
						
						//매출연계 탭 화폐 체크
						if(this.ds_Detail10.rowcount > 0 ){
							if(this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CurrCd.value != this.ds_Detail20.getColumn(iLoop,"CURR_CD") ){
								var arrMsgArg = ["화폐"];
								this.gfn_Alert("SM057", arrMsgArg); //동일한 [화폐]만 적용 가능합니다.
								return;
							}
						}
					}
					//그리드내 화폐 체크
					if(!this.gfn_IsNull(sCnthNoChk) ){
						if(sCnthNoChk != this.ds_Detail20.getColumn(iLoop,"CURR_CD") ){
							var arrMsgArg = ["화폐"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 [화폐]만 적용 가능합니다.
							return;
						}
					}
					sCnthNoChk = this.ds_Detail20.getColumn(iLoop,"CURR_CD");
					
					
					
					//수금반영금액 체크 
					if(sCnthNoChk == "0090"){ //원화
						sAddnAm = nexacro.toNumber(this.ds_Detail20.getColumn(iLoop, "ADWN_AM"));
						sAdddAm = nexacro.toNumber(this.ds_Detail20.getColumn(iLoop, "DEWO_AM"));
					}else{
						sAddnAm = nexacro.toNumber(this.ds_Detail20.getColumn(iLoop, "ADFR_AM"));
						sAdddAm = nexacro.toNumber(this.ds_Detail20.getColumn(iLoop, "DEFO_AM"));
					}
					if(sCotyCd != 8){//미수금입금이면 체크안함
						if(sAddnAm == 0){
							var arrMsgArg = ["수금반영금액","1"];
							this.gfn_Alert("SM055", arrMsgArg); //[수금반영금액]을(를) 1 이상 입력해 주세요.
							return;
						}
					}
					if((sAddnAm+sAdddAm) > nexacro.toNumber(this.ds_Detail20.getColumn(iLoop, "UNCL_AM")) ){
						var arrMsgArg = ["수금반영금액, 공제금액 합계", "미수금액"];
						this.gfn_Alert("SM056", arrMsgArg); //[수금반영금액]이(가) [미수금액]을(를) 초과할 수 없습니다.
						return;
					}
					/*
					if(this.ds_Detail20.getColumn(iLoop, "COLB_AM") > this.ds_Detail20.getColumn(iLoop, "CNTB_AM") ){
						var arrMsgArg = ["수금반영금액", "계약금액"];
						this.gfn_Alert("SM056", arrMsgArg); //[수금반영금액]이(가) [계약금액]을(를) 초과할 수 없습니다.
						return;
					}
					*/
				}
			 }
			
			
			
			//매출연계검색 금액 제어
			var ds30SelectRow = this.ds_Detail30.findRow("s_chk","1");
			var ds30CurrCd = this.ds_Detail30.getColumn(ds30SelectRow,"CURR_CD");
			var ds30CmshDd = "";	//입금일자
			var ds30Amt =0;
			var ds20Amt =0;
			var ds20DeAm =0;
			//var ds40DeAm =0;
			if(ds30CurrCd == "0090" ){//원화
				ds30Amt = nexacro.toNumber(this.ds_Detail30.getColumn(ds30SelectRow,"WONE_AM"));
				//ds20Amt = this.ds_Detail20.getCaseSum("s_chk == '1'","ADWN_AM") + this.ds_Detail20.getCaseSum("s_chk == '1'","DEWO_AM");
				ds20Amt = nexacro.toNumber(this.ds_Detail20.getCaseSum("s_chk == '1'","ADWN_AM"));
				ds20DeAm = nexacro.toNumber(this.ds_Detail20.getCaseSum("s_chk == '1'","DEWO_AM"));
				//ds40DeAm = this.ds_Detail40.getCaseSum("crudType!='D'","WONE_AM");
			}else{
				ds30Amt = nexacro.toNumber(this.ds_Detail30.getColumn(ds30SelectRow,"FORE_AM"));
				//ds20Amt = this.ds_Detail20.getCaseSum("s_chk == '1'","ADFR_AM") + this.ds_Detail20.getCaseSum("s_chk == '1'","DEFO_AM");
				ds20Amt = nexacro.toNumber(this.ds_Detail20.getCaseSum("s_chk == '1'","ADFR_AM"));
				ds20DeAm = nexacro.toNumber(this.ds_Detail20.getCaseSum("s_chk == '1'","DEFO_AM"));
				//ds40DeAm = this.ds_Detail40.getCaseSum("crudType!='D'","FORE_AM");
			}
			
			if(sCotyCd != 8){//미수금입금이면 체크안함
