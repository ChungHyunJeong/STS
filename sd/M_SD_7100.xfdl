	//trace(this.ds_Detail30.saveXML());
	this.ds_Detail30.set_enableevent(true);
	
	//공제내역
	this.ds_Detail40.set_enableevent(false);
	for (var iLoop=0; iLoop < this.ds_Detail40.rowcount; iLoop++) {
		this.ds_Detail40.setColumn(iLoop, "crudType", "I");
		this.ds_Detail40.setColumn(iLoop, "COMM_CD", this.ds_Detail40.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
		this.ds_Detail40.setColumn(iLoop, "COLH_NO", "");

		this.ds_Detail40.setColumn(iLoop, "WONE_AM", this.ds_Detail40.getColumn(iLoop, "WONE_AM")*-1);	//원화금액
		this.ds_Detail40.setColumn(iLoop, "FORE_AM", this.ds_Detail40.getColumn(iLoop, "FORE_AM")*-1);	//외화금액
		
	}
	//trace(this.ds_Detail40.saveXML());
	this.ds_Detail40.set_enableevent(true);
	
	//수수료상세
	this.ds_Detail50.set_enableevent(false);
	for (var iLoop=0; iLoop < this.ds_Detail50.rowcount; iLoop++) {
		this.ds_Detail50.setColumn(iLoop, "crudType", "I");
		this.ds_Detail50.setColumn(iLoop, "COMM_CD", this.ds_Detail50.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
		this.ds_Detail50.setColumn(iLoop, "COLH_NO", "");

		this.ds_Detail50.setColumn(iLoop, "CRED_AM", this.ds_Detail50.getColumn(iLoop, "CRED_AM")*-1);	//차변금액
		this.ds_Detail50.setColumn(iLoop, "DEBI_AM", this.ds_Detail50.getColumn(iLoop, "DEBI_AM")*-1);	//대변금액
		
	}
	//trace(this.ds_Detail50.saveXML());
	this.ds_Detail50.set_enableevent(true);
	
	//자동전표 초기화
	eval(this.fvAtobDiv).fn_ClearAtobData();//자동전표 정보 초기화
	
	//활성화
	//////기능사용중지 김형수 부장요청)- 이성수 차장 요청 2022-06-09 마이너스 수금일경우 활성 그외 비활성
	this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ColhDd.set_enable(true);
	
	
	this.fvMinusCheck = true;	//마이너스체크여부
	
	//this.fn_ComBtnSave();//저장(자동저장 기능 사용안함: 김형수 부장요청)
		
