			ds10DeAmt = nexacro.toNumber(this.ds_Detail10.getCaseSum("crudType!='D'","DEFO_AM"));	// 공제액 외화 //add
			
			ds10SumAmtWon = nexacro.toNumber(this.ds_Detail10.getCaseSum("crudType!='D'","WONE_AM"));		//매출원화
