 * 02.DG0.02. [Detail그리드-버튼] 행추가 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd01AddRow = function()
{
	var sDs;
	var sTabIndex = this.div_DetailInput.form.tab_Detail.tabindex ;
	//trace(sTabIndex);
	
	if(sTabIndex == 0){//수금내역
		/*
		//화페입력 체크
		if(this.gfn_IsNull(this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CurrCd.value)){
			var arrMsgArg = ["화폐"];
			this.gfn_Alert("SM040", arrMsgArg); //&ITM0을(를) 선택하세요.
			return;
		}
		*/
		sDs = this.ds_Detail10;
	}else if(sTabIndex == 2){//입금상세
		sDs = this.ds_Detail30;
	}else if(sTabIndex == 3){//공제내역상세
		//수금내역 체크
		if(this.ds_Detail10.getRowCount() == 0){
			var arrMsgArg = ["수금내역","입력"];
			this.gfn_Alert("SM062", arrMsgArg); //[&ITM0]을(를) 추가 후 [&ITM1]가능 합니다.
			return;
		}
		sDs = this.ds_Detail40;
	}else if(sTabIndex == 4){//수수료상세
		sDs = this.ds_Detail50;
	}
	
	var iAddRow = this.gfn_GridInsertProcess(this, sDs );//crud_flag 변경-입력(I)

	sDs.set_enableevent(false);
	sDs.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));	//회사코드
	sDs.setColumn(iAddRow, "COLH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COLH_NO"));	//수금번호
	sDs.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));	//결재차수
	
	if(sTabIndex == 0){//수금내역
		
		
		sDs.setColumn(iAddRow, "WONE_AM", 0);	//수금원화
		sDs.setColumn(iAddRow, "FORE_AM", 0);	//수금외화
		sDs.setColumn(iAddRow, "DEWO_AM", 0);	//공제원화
		sDs.setColumn(iAddRow, "DEFO_AM", 0);	//공제외화
		//수금내역 원화 외화 제어
		this.fn_CurrValue(iAddRow);
	}else if(sTabIndex == 3){//공제내역상세	
		sDs.setColumn(iAddRow, "DECR_GU", "D");	//차변고정 
		sDs.setColumn(iAddRow, "CURR_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD"));	//화폐
		sDs.setColumn(iAddRow, "EXCH_RT", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT"));	//환율
		
		
	}else if(sTabIndex == 4){//수수료상세
		sDs.setColumn(iAddRow, "CURR_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD"));	//화폐
		sDs.setColumn(iAddRow, "EXCH_RT", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT"));	//환율
	}
	 sDs.set_enableevent(true);
	//sDs.setColumn(iAddRow, "CNTB_SE", "*");	//계약순번
	//sDs.setColumn(iAddRow, "CNTC_SE", "*");	//계약상세순번
	
	
};

