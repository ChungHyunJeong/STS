this.popCallSd1200 = function(dateSET, paramRow) {
	var sPrgmId = "M_SD_1200";//호출프로그램ID  총계정원장
    var sCallFunc = "fn_FormCall";//호출함수
	
	//[공통]특정화면 호출 파라메타 정보 Object
	var oParam = new Object();
	oParam.commCd = dateSET.getColumn(paramRow, "COMM_CD");
	oParam.fromDd = dateSET.getColumn(paramRow, "PJTH_DD");
	oParam.closDd = dateSET.getColumn(paramRow, "PLAH_DD");
	oParam.PjthNo = dateSET.getColumn(paramRow, "PJTH_NO");
	oParam.PjthNm = dateSET.getColumn(paramRow, "PJTH_NM");			
	oParam.DepmCd = dateSET.getColumn(paramRow, "DEPM_CD");
	oParam.DepmNm = dateSET.getColumn(paramRow, "DEPM_NM");			
	oParam.EmpmCd = dateSET.getColumn(paramRow, "EMPM_CD");
	oParam.EmpmNm = dateSET.getColumn(paramRow, "EMPM_NM");
	oParam.SystId = dateSET.getColumn(paramRow, "SYST_ID");			
	oParam.CusmCd = dateSET.getColumn(paramRow, "CUSM_CD");
	oParam.CusmNm = dateSET.getColumn(paramRow, "CUSM_NM");
	this.gfn_CallScrn(sPrgmId, sCallFunc, oParam, "");
}

