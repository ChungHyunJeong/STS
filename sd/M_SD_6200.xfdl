/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master10] 표준변경버튼 클릭
  ---------------------------------------------------------------------------------------------*/

this.div_MasterInput_btn_change_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var bSucc;
	var currow = this.ds_Master10.rowposition;
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	
	
	//입력활성화
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.div_MasterInput.form.tab_Master.tpg_01.set_enable(true);
	this.div_MasterInput.form.btn_change.set_enable( false );
	//등록부서
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_enable(false);
	this.div_MasterInput.form.tab_Master.tpg_01.form.btn_DepmCd.set_enable(false);
		//등록자
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_enable(false);
	this.div_MasterInput.form.tab_Master.tpg_01.form.btn_SystId.set_enable(false);
		//대분류,중분류
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItlmCd.set_enable(false);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItmmCd.set_enable(false);
		//등록일자
	this.div_MasterInput.form.tab_Master.tpg_01.form.cal_StahDd.set_enable(false);
	//비고
	this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_enable(true);
	//detail 
	this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.set_enable(true);
	
	
	var iAddRow = this.ds_Master10.addRow();
	bSucc = this.ds_Master10.copyRow(iAddRow, this.ds_Master10, currow);
	this.ds_Master10.setColumn(iAddRow, "STAH_DD", sToDay);//등록일자-오늘날자로 설정
	this.ds_Master10.setColumn(iAddRow, "CHG_YN", "Y");//등록일자-오늘날자로 설정
	this.ds_Master10.setColumn(iAddRow, "REGI_ID", "");//등록자사번
	this.ds_Master10.setColumn(iAddRow, "REGI_DT", "");//등록일
	this.ds_Master10.setColumn(iAddRow, "REGI_NM", "");//등록자
	this.ds_Master10.setColumn(iAddRow, "MODI_ID", "");//수정자사번
	this.ds_Master10.setColumn(iAddRow, "MODI_DT", "");//수정일
	this.ds_Master10.setColumn(iAddRow, "MODI_NM", "");//수정자  
	
	for (var iLoop=0; iLoop < this.ds_Detail10.getRowCount(); iLoop++) {
		this.ds_Detail10.setColumn(iLoop, "crudType", "I");
		this.ds_Detail10.setColumn(iLoop, "STAH_DD", sToDay);
		this.ds_Detail10.setColumn(iLoop, "CHG_YN", 'Y');
		
		this.ds_Detail10.setColumn(iLoop, "BFSP_CD", this.ds_Detail10.getColumn(iLoop, "SPEB_CD"));
		this.ds_Detail10.setColumn(iLoop, "BFSP_NM", this.ds_Detail10.getColumn(iLoop, "SPEB_NM"));
		this.ds_Detail10.setColumn(iLoop, "SPEB_CD", "");
		this.ds_Detail10.setColumn(iLoop, "SPEB_NM", "");
	}
	
	
	
	
};
