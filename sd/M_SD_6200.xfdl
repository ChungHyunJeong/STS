	this.ds_Input.setColumn(0, "ITMM_CD", this.div_Search.form.cbo_ItmmCd.value);
	this.ds_Input.setColumn(0, "DEPM_CD", this.div_Search.form.edt_DepmCd.value);
	this.ds_Input.setColumn(0, "SESU_ID", this.div_Search.form.edt_SystId.value);
	this.ds_Input.setColumn(0, "EMPM_CD", this.div_Search.form.edt_EmpmCd.value);
	this.ds_Input.setColumn(0, "WOFH_YN", this.div_Search.form.chk_WofhYn.value);
	//최종이력체크
	if (this.div_Search.form.chk_WofhYn.value) {
		this.ds_Input.setColumn(0, "WOFH_YN", "0");
	} else {
		this.ds_Input.setColumn(0, "WOFH_YN", "1");
	}

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "sd:M_SD_6200_M_SL10";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
