/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}

	//첨부파일정보 변경시
	
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		//첨부파일정보만 변경시
		if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && this.ds_Master10.getRowCount() > 0) {
			//첨부파일정보 저장
			fvFileInfo = new Object();//[공통] 첨부파일정보
			fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, B-상세(그리드Row), E-기타
			fvFileInfo.CALLBACK_FUNC = "";//호출할 콜백함수
			fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
			fvFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITML_CD");//PK번호2-대분류
			fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITMM_CD");//PK번호3-중분류
			fvFileInfo.PK04_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "STAH_GU");//PK번호3-표준타입
			fvFileInfo.PK05_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "STAH_DD");//PK번호3-등록일자
			//첨부파일Div 화면에 Key세팅하게 변경
			this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);
			return;	
		}
	}
	
	
	
	
	// 저장시 체크 함수
	if(!this.fn_check()){
		//trace("저장 체크 오류");
		return;
	}
	
	


	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	//Master 입력 상태이면
	if (sChkRowType == "I") {
	

	}	
	
	
	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		fvFileInfo = new Object();//[공통] 첨부파일정보
		fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
		fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
		fvFileInfo.CALLBACK_FUNC = "fn_ComBtnSearch";//호출할 콜백함수
		fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
		fvFileInfo.PK01_NO = nApp.gvCompany;//PK번호1-회사코드
		fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITML_CD");//PK번호2-대분류
		fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITMM_CD");//PK번호3-중분류	
		fvFileInfo.PK04_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "STAH_GU");//PK번호3-중분류	
		fvFileInfo.PK05_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "STAH_DD");//PK번호3-중분류			
	}	
	
	
	
	//저장시 재조회 Key설정
	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITLM_CD");//조회Key01
 	this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITMM_CD");//조회Key02
 	this.fvSchKey03 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "STAH_GU");//조회Key03
 	this.fvSchKey04 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "STAH_DD");//조회Key04



	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	//[공통]서비스 호출 변수
	var sInputDaset = "" //입력 데이터셋
	var sOutDatset = "";//출력 데이터셋
	var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		
	//Master 변경건이 있으면 Master 저장
	
	
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
	
		//trace("gfn_DsIsUpdated 2");
		this.ds_Master10Upt.clearData();
		this.ds_Master10Upt.addRow();
		this.ds_Master10Upt.copyRow(0, this.ds_Master10, this.ds_Master10.rowposition);
		this.ds_Master10Upt.setColumn(0, "crudType", "U");
		
		//trace("gfn_DsIsUpdated 3");
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
		svcAddObj.sInsertSql = "";//저장Sql
		svcAddObj.sUpdateSql = "sd:M_SD_6200_M_UP11";//수정Sql
		svcAddObj.sDeleteSql = "";//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

		this.gfn_AddComSaveService(this, svcAddObj);
	
		sInputDaset = "ds_Master11=ds_Master10Upt:U" //입력 데이터셋
		
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
		svcAddObj.sInsertSql = "sd:M_SD_6200_M_IN10";//저장Sql
		svcAddObj.sUpdateSql = "sd:M_SD_6200_M_UP10";//수정Sql
		svcAddObj.sDeleteSql = "";//삭제Sql
		svcAddObj.sSaveFlagColumn = "";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

		this.gfn_AddComSaveService(this, svcAddObj);
		
		sInputDaset = sInputDaset + " ds_Master10=ds_Master10:U" //입력 데이터셋		
		
	}
	
	//Detail 변경건이 있으면 Detail 저장
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object	
		svcAddObj.sInsertSql = "sd:M_SD_6200_D_IN10";//저장Sql
		svcAddObj.sUpdateSql = "sd:M_SD_6200_D_UP10";//수정Sql
		svcAddObj.sDeleteSql = "sd:M_SD_6200_D_DL10";//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		
		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = "ds_Detail10=ds_Detail10:U" //입력 데이터셋
		} else {
			sInputDaset = sInputDaset + " ds_Detail10=ds_Detail10:U" //입력 데이터셋
		}

	}		
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "SAVE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_SD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = sInputDaset //입력 데이터셋
	svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
	svcCallObj.sOtherArg = sOtherArg;//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명		
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	

}

/***********************************************************************************************
 * 05.02-1. [저장체크]
 ***********************************************************************************************/ 
this.fn_check = function()
{
	// ds_Detail 입력건 체크
	if(this.ds_Detail10.rowcount <= 0){
		this.gfn_Alert("SD105");  //사양상세가 입력 되지 않았습니다. 입력하세요.
		return;
	}
	
	this.ds_Check10.clearData();
	this.ds_Input.clearData();
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "ITLM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITLM_CD"));
	this.ds_Input.setColumn(0, "ITMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITMM_CD"));
	this.ds_Input.setColumn(0, "STAH_GU", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "STAH_GU"));
	this.ds_Input.setColumn(0, "STAH_DD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "STAH_DD"));
	
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	
	var svcAddObj = new Object();
	svcAddObj.sSelectSql = "sd:M_SD_6200_M_SL12"; //+ this.fvComPrgmId + "_C_SL10"; 
	this.gfn_AddComSearchService(this, svcAddObj);
	
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MST_CHECK";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_SD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Check10=ds_Check10";//출력 데이터셋
	svcCallObj.sOtherArg ="";// "key=" + this.gfn_GetSessionValue("COMM_CD");//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.isSync = true;//동기식으로 설정할지 여부
	
	//[공통]서비스 호출
	
	this.gfn_CallComService(this, svcCallObj);	
	
	
	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	if (sChkRowType == "I"){
		// INSERT 체크
		var insertCheckCnt = this.ds_Check10.getColumn(0, "CHECK_CNT");
		
		// 중복 데이타 처리
		if(insertCheckCnt != "0"){
			
			this.gfn_Alert("SM041");//중복된 데이터가 존재합니다.
			return false;
			
		}
		
		
	}
