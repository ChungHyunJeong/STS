/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {
		//사양정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITLM_CD"))
		    && !this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITMM_CD"))
			&& !this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "STAH_GU"))
			&& !this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "STAH_DD"))) {
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Input.clearData();
			
			  
			this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
			this.div_MasterInput.form.tab_Master.tpg_01.set_enable(true);
			this.div_MasterInput.form.btn_change.set_enable( true );  
			
			//조회시 enable = false
				//등록부서
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_enable(false);
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_DepmCd.set_enable(false);
				//등록자
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_enable(false);
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_SystId.set_enable(false);
				//대분류,중분류
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItlmCd.set_enable(false);
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItmmCd.set_enable(false);
				//등록일자
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_StahDd.set_enable(false);
			//표준타입
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_stahGu.set_enable(false);
			
			//최종인경우
			if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_YN") == "1") {
				// 수정가능, 등록일자가 현재일인경우
				if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CHG_YN")  == "Y"){
					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_StahNm.set_enable(true);
					this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_enable(true);
					this.div_MasterInput.form.tab_Master.tpg_01.form.chk_UsedYn.set_enable(true);
				}else{
					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_StahNm.set_enable(false);
					this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_enable(false);
					this.div_MasterInput.form.tab_Master.tpg_01.form.chk_UsedYn.set_enable(false);
				}
			}else{
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_StahNm.set_enable(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_enable(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.chk_UsedYn.set_enable(false);
			}
			
			
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "ITLM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITLM_CD"));
			this.ds_Input.setColumn(0, "ITMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITMM_CD"));
			this.ds_Input.setColumn(0, "STAH_GU", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "STAH_GU"));
			this.ds_Input.setColumn(0, "STAH_DD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "STAH_DD"));
			//this.ds_Input.setColumn(0, "PRGM_ID", this.fvComPrgmId);
					
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "sd:M_SD_6200_D_SL11";
			this.gfn_AddComSearchService(this, svcAddObj);
			//M_SD_6200_C_SL10
			
			svcAddObj.sSelectSql = "sd:M_SD_6200_C_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			
			// 표준변경버튼 VISIBLE
			svcAddObj.sSelectSql = "sd:M_SD_6200_M_SL11";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_SD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail10=ds_Detail11 ds_Speb=ds_Speb ds_Master11=ds_Master11";//";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);	
			
			
			
			
		}
	}
};



