this.fn_ComBtnAdd = function()
{
	
	//데이터 변경여부 체크
	if (this.fn_IsUpdateDataset(this.ds_Master10)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	
	var iAddRow = this.ds_Master10.addRow();
	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");	
	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//부서코드	
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//부서명	
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//등록자사번	
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//등록자명		
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//등록자ID	
	
	this.ds_Master10.setColumn(iAddRow, "STAH_DD", sToDay);//등록일자-오늘날자로 설정
	this.ds_Master10.setColumn(iAddRow, "CHG_YN", "Y");	
	this.ds_Master10.setColumn(iAddRow, "USED_YN", "1");	

	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	this.div_MasterInput.form.tab_Master.tpg_01.set_enable(true);
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.div_DetailInput.form.tab_Detail.set_enable(true);
	this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.set_enable(true);
	
	//사양변경 버튼
	this.div_MasterInput.form.btn_change.set_enable(false);

	//등록부서
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_enable(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.btn_DepmCd.set_enable(true);
	//등록자
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_enable(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.btn_SystId.set_enable(true);
	//대분류,중분류
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItlmCd.set_enable(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItmmCd.set_enable(true);
	//등록일자
	this.div_MasterInput.form.tab_Master.tpg_01.form.cal_StahDd.set_enable(true);
	//비고
	this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_enable(true);
	//표준타입
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_stahGu.set_enable(true);
	//표준타입설명
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_StahNm.set_enable(true);
	
	//표준상세 enable
	this.div_DetailInput.form.tab_Detail.set_enable(true);
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	
	//처음 입력 항목에 포커스
	
	this.ds_Detail10.clearData();//상세정보 초기화
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
	//첨부파일정보 권한설정
 	var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
 	oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
 	this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	
	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
}

