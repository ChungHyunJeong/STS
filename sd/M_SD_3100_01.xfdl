				//단가적용
				else{
					//nTMatgAm = nTMatdAm;
					nTLabgAm = nTLabdAm;
					//nTCosgAm = nTCosdAm;
					//nOutsAm = nOutsAm;
				}
			}
			// 경비 - 간접
			var fRow = this.ds_Cosm.findRow("FACT_CD", "0030");
			if(fRow >= 0){		
				//임율적용
				if(this.ds_Cosm.getColumn(fRow, "PRIC_GU")=="0010"){
					if(this.gfn_Nvl(this.ds_Cosm.getColumn(fRow, "COSM_RT"),0) > 0){
					nTCosgAm = (this.ds_Cosm.getColumn(fRow, "MATD_YN")=='1'?nTMatdAm:0
								+ this.ds_Cosm.getColumn(fRow, "LABD_YN")=='1'?nTLabdAm:0
