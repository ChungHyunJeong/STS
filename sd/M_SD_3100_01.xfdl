/***********************************************************************************************
 * 12.2. [사용자함수] 실행단가, 판관비 계산.
 ***********************************************************************************************/
this.fn_CalcPR = function(nRow) {
	if(this.gfn_Nvl(this.ds_Master10.getColumn(nRow, "PRIC_AM"),0) != 0){
		return;
	}
	var nQt = this.ds_Master10.getColumn(nRow, "CNTC_QT");	//수량
	var nCnthPr = this.ds_Master10.getColumn(nRow, "WONE_PR");	//계약단가
	
	if (this.fvCurrCd != '0090') {	//외화
		//nCnthAm = this.ds_Master10.getColumn(nRow, "FORE_AM");
	}
	
	//실행단가선언
	//재료비
	var nMatdAm = 0
	var nMatgAm = 0
	//노무비
	var nLabdAm = 0
	var nLabgAm = 0
	//경비
	var nCosdAm = 0
	var nCosgAm = 0
	//외주비
	var nOutsAm = 0		
	
	//원가 요율이 있을때..
	if(this.ds_Cosm.getRowCount() > 0){	
		//직접비계산
		/* 재료비 - 직접*/
		var fRow = this.ds_Cosm.findRowExpr("FACT_CD=='0010' && DIRE_GU=='0010'");
		if(fRow >= 0){			
			//임율적용
			if(this.gfn_Nvl(this.ds_Cosm.getColumn(fRow, "COSM_RT"),0) > 0){
			nMatdAm = nexacro.round(nCnthPr * (this.ds_Cosm.getColumn(fRow, "COSM_RT")/100),0);
			}
		}
		/* 재료비 - 간접*/
		fRow = this.ds_Cosm.findRowExpr("FACT_CD=='0010' && DIRE_GU=='0020'");
		if(fRow >= 0){			
			//임율적용
			if(this.gfn_Nvl(this.ds_Cosm.getColumn(fRow, "COSM_RT"),0) > 0){
			nMatgAm = nexacro.round(nCnthPr * (this.ds_Cosm.getColumn(fRow, "COSM_RT")/100),0);
			}
		}
		/* 노무비 - 직접*/
		fRow = this.ds_Cosm.findRowExpr("FACT_CD=='0020' && DIRE_GU=='0010'");
		if(fRow >= 0){		
			//임율적용
			if(this.gfn_Nvl(this.ds_Cosm.getColumn(fRow, "COSM_RT"),0) > 0){
			nLabdAm = nexacro.round(nCnthPr * (this.ds_Cosm.getColumn(fRow, "COSM_RT")/100),0);
			}
		}
		/* 노무비 - 간접*/
		fRow = this.ds_Cosm.findRowExpr("FACT_CD=='0020' && DIRE_GU=='0020'");
		if(fRow >= 0){		
			//임율적용
			if(this.gfn_Nvl(this.ds_Cosm.getColumn(fRow, "COSM_RT"),0) > 0){
			nLabgAm = nexacro.round(nCnthPr * (this.ds_Cosm.getColumn(fRow, "COSM_RT")/100),0);
			}
		}
		/* 경비 - 직접*/
		fRow = this.ds_Cosm.findRowExpr("FACT_CD=='0030' && DIRE_GU=='0010'");
		if(fRow >= 0){		
			//임율적용
			if(this.gfn_Nvl(this.ds_Cosm.getColumn(fRow, "COSM_RT"),0) > 0){
			nCosdAm = nexacro.round(nCnthPr * (this.ds_Cosm.getColumn(fRow, "COSM_RT")/100),0);
			}
		}
		/* 경비 - 간접*/
		fRow = this.ds_Cosm.findRowExpr("FACT_CD=='0030' && DIRE_GU=='0020'");
		if(fRow >= 0){		
			//임율적용
			if(this.gfn_Nvl(this.ds_Cosm.getColumn(fRow, "COSM_RT"),0) > 0){
			nCosgAm = nexacro.round(nCnthPr * (this.ds_Cosm.getColumn(fRow, "COSM_RT")/100),0);
			}
		}
		/* 외주비 - 간접*/
		/*
		var fRow = this.ds_Cosm.findRow("FACT_CD", "0040");
		if(fRow >= 0){		
			//임율적용
			if(this.ds_Cosm.getColumn(fRow, "PRIC_GU")=="0010"){
				if(this.gfn_Nvl(this.ds_Cosm.getColumn(fRow, "COSM_RT"),0) > 0){
				nOutsAm = nCnthPr * (this.ds_Cosm.getColumn(fRow, "COSM_RT")/100);
				}
			}
		}
		*/
	}
	
	this.ds_Master10.setColumn(nRow, "MATD_AM", nMatdAm);
	this.ds_Master10.setColumn(nRow, "LABD_AM", nLabdAm);
	this.ds_Master10.setColumn(nRow, "COSD_AM", nCosdAm);
	this.ds_Master10.setColumn(nRow, "OUTS_AM", nOutsAm);
	
	this.ds_Master10.setColumn(nRow, "MATG_AM", nMatgAm);
	this.ds_Master10.setColumn(nRow, "LABG_AM", nLabgAm);
	this.ds_Master10.setColumn(nRow, "COSG_AM", nCosgAm);
	
	//실행단가-원가계 자동입력
	var nPricAm = nMatdAm + nMatgAm + nLabdAm + nLabgAm + nCosdAm + nCosgAm + nOutsAm;	
	this.ds_Master10.setColumn(nRow, "PRIC_AM", nPricAm);
	
	//실행원가 자동계산
	this.fn_CalcAM(nRow);
}]]></Script>
