					}
				}
				//단가적용
				else{
					nTMatgAm = nTMatdAm;
					//nLabgAm = nTLabdAm;
					//nTCosgAm = nTCosdAm;
					//nOutsAm = nOutsAm;
				}
			}
			//노무비 - 간접
			var fRow = this.ds_Cosm.findRow("FACT_CD", "0020");
