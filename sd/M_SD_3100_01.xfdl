 ***********************************************************************************************/
this.fn_MasterGrd01AddRow = function()
{
	var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Master10);//crud_flag 변경-입력(I)	

	var sToDay = this.gfn_GetCommDate("toDay");//현재일자

	//Key 순번 (계약상세 체크시 필요)	
	if (iAddRow == 0) {
		var descKey = "001"; //01 --> 1로 변경(요청)
	} else {
		var descKey = nexacro.toNumber(this.ds_Master10.getMax("descKey"));
		descKey = descKey + 1;
		//descKey = descKey.toString().padLeft(2,'0');
	}
	
	this.ds_Master10.setColumn(iAddRow, "CNTH_NO", this.fvCnthNo);
	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", this.fvWofhSq);
	this.ds_Master10.setColumn(iAddRow, "CNTB_SE", this.fvCntbSe);
	this.ds_Master10.setColumn(iAddRow, "DETA_CD", '0020');//진행상태
	this.ds_Master10.setColumn(iAddRow, "PURC_DD", sToDay);//PO일자 - 오늘날자로 설정
	this.ds_Master10.setColumn(iAddRow, "PALM_CD", this.fvPlamCd);
	this.ds_Master10.setColumn(iAddRow, "ITEM_GU", this.fvItemGu);
	this.ds_Master10.setColumn(iAddRow, "SAUM_CD", this.fvSaumCd);
	this.ds_Master10.setColumn(iAddRow, "codeKey", this.fvcodeKey);	
	this.ds_Master10.setColumn(iAddRow, "seqKey", this.fvseqKey);	
	this.ds_Master10.setColumn(iAddRow, "descKey", descKey);	
	this.ds_Master10.setColumn(iAddRow, "crudPopup", "A");	//팝업 "ADD"

};

