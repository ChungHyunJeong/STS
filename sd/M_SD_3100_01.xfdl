  ---------------------------------------------------------------------------------------------*/
 this.fn_Search = function()
{ 

	this.ds_Input.clearData();
	
	//[업무] 조회조건 입력 : 거래처관리 대상(고객정보는 거래처 row 조회시)
	this.ds_Input.addRow();
	
	this.ds_Input.setColumn(0, "PALM_CD", this.fvPlamCd );
		
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "sd:M_SD_3100_D_SL13";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "sd:M_SD_3100_D_SL14";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "COSM_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_CA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Cosm=ds_Cosm ds_Pgrm=ds_Pgrm";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.isSync = true;//동기식으로 설정할지 여부
	svcCallObj.sCallBackFnc = "_gfn_Callback_ComFunc";//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);
		
	var objCommDs = this.ds_Cosm;
	var iRowCnt = objCommDs.getRowCount();
	
	if (iRowCnt < 0) {
		/* ds_Cosm, ds_Pgrm row 가 없음 */
	}
	
	//상세화면에서 조회하지 않고 부모창에서 조회한것을 가져와서 처리함
	//2021-05-12 설계자 변경
	this.ds_Master10.loadXML(this.fvDataset);
	this.ds_Master10.set_enableevent(false);
	for(var i=0;i<this.ds_Master10.rowcount;i++){
		if(this.ds_Master10.getColumn(i, "CNTC_PR") == 0 && this.ds_Master10.getColumn(i, "WONE_PR") != 0){
			this.ds_Master10.setColumn(i, "CNTC_PR", this.ds_Master10.getColumn(i, "WONE_PR"));
			//실행단가 자동계산
			this.fn_CalcPR(i);
		}
	}
	this.ds_Master10.set_enableevent(true);
	this.ds_Master10.filter("");		
	this.ds_Master10.filter("codeKey == '"+this.fvcodeKey+"' && seqKey == '"+this.fvseqKey+"' && CNTB_SE == '"+this.fvCntbSe+"'");

} 

