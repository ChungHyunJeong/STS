 ***********************************************************************************************/

/***********************************************************************************************
 * 11.1. [Master그리드-버튼] 복사 클릭 이벤트
 ***********************************************************************************************/
this.fn_MasterGrd01Copy = function()
{
	var iPreRow = this.ds_Master10.rowposition;

	if (iPreRow > -1) {	//선택시 복사함	
		var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Master10);//crud_flag 변경-입력(I)
		this.ds_Master10.copyRow(iAddRow, this.ds_Master10, iPreRow);
		this.ds_Master10.setColumn(iAddRow, "s_chk", "0");	    //선택Default 0
		this.ds_Master10.setColumn(iAddRow, "crudType", "I");	//신규 I
		this.ds_Master10.setColumn(iAddRow, "crudPopup", "A");	//팝업 "ADD"

		this.ds_Master10.setColumn(iPreRow, "s_chk", "0");	    //복사 Row 선택해제
	}

};

/***********************************************************************************************
 * 11.2. [Master그리드-버튼] 행추가 클릭 이벤트
 ***********************************************************************************************/
this.fn_MasterGrd01AddRow = function()
{
	var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Master10);//crud_flag 변경-입력(I)	

	//Key 순번 (견적상세 체크시 필요)	
	if (iAddRow == 0) {
		var descKey = "1"; 
	} else {
		var descKey = nexacro.toNumber(this.ds_Master10.getMax("descKey"));
		descKey = descKey + 1;
	}
	
	this.ds_Master10.setColumn(iAddRow, "QUTH_NO", this.fvQuthNo);
	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", this.fvWofhSq);
	this.ds_Master10.setColumn(iAddRow, "QUTB_SE", this.fvQutbSe);
	this.ds_Master10.setColumn(iAddRow, "QUTB_QT", 1); //수량 Default 1
	this.ds_Master10.setColumn(iAddRow, "codeKey", this.fvcodeKey);	
	this.ds_Master10.setColumn(iAddRow, "seqKey", this.fvseqKey);	
	this.ds_Master10.setColumn(iAddRow, "descKey", descKey);	
	this.ds_Master10.setColumn(iAddRow, "crudPopup", "A");	//팝업 "ADD"
	
};

/***********************************************************************************************
 * 11.3. [Master그리드-버튼] 행삽입 클릭 이벤트
 ***********************************************************************************************/
this.fn_MasterGrd01InsertRow = function()
{
};

/***********************************************************************************************
 * 11.4. [Master그리드-버튼] 행삭제 클릭 이벤트
 ***********************************************************************************************/
this.fn_MasterGrd01DeleteRow = function()
{	
	for (var iLoop=0; iLoop<this.ds_Master10.getRowCount();iLoop++) {
		var s_chk  = this.ds_Master10.getColumn(iLoop, "s_chk"); //s_chk
		var crudType  = this.ds_Master10.getColumn(iLoop, "crudType"); //crudType
		var crudPopup = this.ds_Master10.getColumn(iLoop, "crudPopup"); //팝업 대상 추가 "ADD" 확인
		if (s_chk == '1' && crudType == 'I' && crudPopup != "A") { // 삭제시 팝업 ADD인 경우 부모의 Detail11에서 삭제

			for (var iDelLoop=0; iDelLoop<this.fvOwnerMaxCount; iDelLoop++) {
				if (this.getOwnerFrame().form.opener.ds_Detail11.getColumn(iDelLoop, "codeKey") == this.ds_Master10.getColumn(iLoop, "codeKey")
				 && this.getOwnerFrame().form.opener.ds_Detail11.getColumn(iDelLoop, "seqKey")  == this.ds_Master10.getColumn(iLoop, "seqKey")
				 && this.getOwnerFrame().form.opener.ds_Detail11.getColumn(iDelLoop, "descKey") == this.ds_Master10.getColumn(iLoop, "descKey")
				 && this.getOwnerFrame().form.opener.ds_Detail11.getColumn(iDelLoop, "QUTB_SE") == this.ds_Master10.getColumn(iLoop, "QUTB_SE")
				 && this.getOwnerFrame().form.opener.ds_Detail11.getColumn(iDelLoop, "QUTC_SE") == this.ds_Master10.getColumn(iLoop, "QUTC_SE") ) {

					//부모 Detail11 삭제					
					this.getOwnerFrame().form.opener.ds_Detail11.deleteRow(iDelLoop);
				}
			}
			
			if (iLoop > this.fvOwnerMaxCount) break;			
		}	
	}

	this.gfn_GridDelProcess(this, this.ds_Master10);//crud_flag 변경-삭제(D)	

};

