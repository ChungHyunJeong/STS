this.fn_SelectRow = function()
{
	if (this.ds_Master10.rowcount > -1) {	
        var rtnValue = "({" +
                        " QUTB_QT:'" + this.ds_Master10.getSum("QUTB_QT") + "'"+  //1.수량
                        ",WONE_AM:'" + this.ds_Master10.getCaseSum("crudType!='D'","WONE_AM") + "'"+  //2.원화합계
                        ",FORE_AM:'" + this.ds_Master10.getCaseSum("crudType!='D'","FORE_AM") + "'"+  //3.외화합계
                "})";
				
		//this.getOwnerFrame().form.opener.ds_Detail11.copyData(this.ds_Master10); //취소가 되지 않아 사용불가

		//팝업 데이터셋에서 부모 데이터셋으로 복사		
		for (var iLoop=0; iLoop<this.ds_Master10.getRowCount(); iLoop++) {
			var crudType = this.ds_Master10.getColumn(iLoop, "crudType"); //crudType
			var crudPopup = this.ds_Master10.getColumn(iLoop, "crudPopup"); //팝업 대상 추가 "ADD"
			if (crudPopup != "A") { // 팝업 ADD가 아님 (신규(I) OR 수정(U) OR 삭제(D))
				for (var iRow=0; iRow<this.fvOwnerMaxCount;iRow++) {
					if (this.getOwnerFrame().form.opener.ds_Detail11.getColumn(iRow, "codeKey") == this.ds_Master10.getColumn(iLoop, "codeKey")
					 && this.getOwnerFrame().form.opener.ds_Detail11.getColumn(iRow, "seqKey")  == this.ds_Master10.getColumn(iLoop, "seqKey")
					 && this.getOwnerFrame().form.opener.ds_Detail11.getColumn(iRow, "descKey") == this.ds_Master10.getColumn(iLoop, "descKey")
					 && this.getOwnerFrame().form.opener.ds_Detail11.getColumn(iRow, "QUTB_SE") == this.ds_Master10.getColumn(iLoop, "QUTB_SE")
					 && this.getOwnerFrame().form.opener.ds_Detail11.getColumn(iRow, "QUTC_SE") == this.ds_Master10.getColumn(iLoop, "QUTC_SE") ) {
				
						this.getOwnerFrame().form.opener.ds_Detail11.setColumn(iRow, "ITEM_CD", this.ds_Master10.getColumn(iLoop, "ITEM_CD"));
						this.getOwnerFrame().form.opener.ds_Detail11.setColumn(iRow, "ITEM_NM", this.ds_Master10.getColumn(iLoop, "ITEM_NM"));
						this.getOwnerFrame().form.opener.ds_Detail11.setColumn(iRow, "QUTB_QT", this.ds_Master10.getColumn(iLoop, "QUTB_QT"));
						this.getOwnerFrame().form.opener.ds_Detail11.setColumn(iRow, "QUTB_PR", this.ds_Master10.getColumn(iLoop, "QUTB_PR"));
						this.getOwnerFrame().form.opener.ds_Detail11.setColumn(iRow, "WONE_PR", this.ds_Master10.getColumn(iLoop, "WONE_PR"));
						this.getOwnerFrame().form.opener.ds_Detail11.setColumn(iRow, "WONE_AM", this.ds_Master10.getColumn(iLoop, "WONE_AM"));
						this.getOwnerFrame().form.opener.ds_Detail11.setColumn(iRow, "FORE_PR", this.ds_Master10.getColumn(iLoop, "FORE_PR"));
						this.getOwnerFrame().form.opener.ds_Detail11.setColumn(iRow, "FORE_AM", this.ds_Master10.getColumn(iLoop, "FORE_AM"));
						this.getOwnerFrame().form.opener.ds_Detail11.setColumn(iRow, "REMK_NM", this.ds_Master10.getColumn(iLoop, "REMK_NM"));
						this.getOwnerFrame().form.opener.ds_Detail11.setColumn(iRow, "s_chk", this.ds_Master10.getColumn(iLoop, "s_chk"));
						this.getOwnerFrame().form.opener.ds_Detail11.setColumn(iRow, "crudType", this.ds_Master10.getColumn(iLoop, "crudType"));

						break;
					}
				}

			} else { //신규이면서 팝업ADD이면
				// 행추가 Row 복사
				var iAddRow = this.getOwnerFrame().form.opener.ds_Detail11.addRow();
				this.getOwnerFrame().form.opener.ds_Detail11.copyRow(iAddRow, this.ds_Master10, iLoop);
				this.getOwnerFrame().form.opener.ds_Detail11.setColumn(iAddRow, "crudPopup", ""); //팝업 "ADD"상태 해제

			}
		}
		
		
		//창닫고 값 넘기기
		this.gfn_PopClose(rtnValue);
		
	} else {
		this.gfn_Alert("SM046");//선택된 내용이 없습니다.
		return;		
	}
}


