{	

	//부모창 파라메타 처리
	this.fvQuthNo = this.getOwnerFrame().paramQuthNo;
	this.fvWofhSq = this.getOwnerFrame().paramWofhSq;
	this.fvQutbSe = this.getOwnerFrame().paramQutbSe;
	this.fvCurrCd = this.getOwnerFrame().paramCurrCd;
	this.fvExchRt = this.getOwnerFrame().paramExchRt;
	this.fvcodeKey = this.getOwnerFrame().paramcodeKey;
	this.fvseqKey = this.getOwnerFrame().paramseqKey;
	this.fvOwnerMaxCount = this.getOwnerFrame().paramOwnerMaxCount;
	this.fvDataset = this.getOwnerFrame().paramDataset;
	this.fvWfSearchMode = this.getOwnerFrame().paramWfSearchMode;//[결재]결재내역 조회모드
	this.fvWofhStatus = this.getOwnerFrame().paramWofhStatus;//[결재]결재상태
