this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	
	// 그리드에서 오른쪽 마우스 버튼 사용
	this.gfn_SetGrid(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10);//[공통]그리드 이벤트 설정 - 견적상세내역

	//[결재]결재내역 조회모드가 아니면-등록 && 결재상태가 "등록"인 경우
	if (!this.fvWfSearchMode && !this.fvWofhStatus) {
		this.div_MasterInput.form.div_ComGrdBtn.set_visible(true);
		this.div_PopupTitle.form.btn_Choice.set_visible(true);
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, true);//Master 입력 프리폼
				
		//권한체크 할 컴포넌트 : 컴포넌트 Full Path
		var objCompCheckList = ["this.div_MasterInput.form.div_ComGrdBtn"];		
		if (this.gfn_CommCompAuthCheck(this, objCompCheckList))
		{
			this.fvFormComBtnAuthYn = true;//그리드 공통버튼 권한 유무
		}
		
		this.fn_SetGridInputButton();//[공통] 그리드 입력 버튼 설정
	} else {
		this.div_MasterInput.form.div_ComGrdBtn.set_visible(false);
		this.div_PopupTitle.form.btn_Choice.set_visible(false);
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
		
	}
	
}

