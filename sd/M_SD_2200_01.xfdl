/***********************************************************************************************
 * 02.MG0.07. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)

	//품목코드
	if (e.columnid == "ITEM_CD") {
		this.gfn_GetCommItemGrid(obj, "ITEM_CD", "ITEM_NM");//품명 검색			
		
	// 수량 입력(변경)시
	} else if (e.columnid == "QUTB_QT") {
		var iQutrQt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTB_QT");//수량
		var iQutbPr = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTB_PR");//견적단가

		if (!this.gfn_IsNull(iQutbPr)) {	//견적단가 존재시			
			var iWonePr = nexacro.floor(this.fvExchRt * iQutbPr, 2);          	//원화단가 = 환율 * 견적단가 (10/18 계산시 소숫점 2자리까지)
			var iWoneAm = nexacro.floor(this.fvExchRt * iQutrQt * iQutbPr, 0);	//원화금액 = 환율 * 수량 * 견적단가 (10/18 계산완료시 소숫점 0 절사함)

			if (this.fvCurrCd == '0090') {	//KRW(원화)
				var iForePr = 0;	// 외화단가 = 0원
				var iForeAm = 0;	// 외화금액 = 0원
			} else {
				var iForePr = iQutbPr;          	//외화단가 = 견적단가
				var iForeAm = iQutrQt * iQutbPr;	//외화금액 = 수량 * 견적단가
			}
		
			//그리드에 적용
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_PR", nexacro.floor(iWonePr,0));	//원화단가(10/18 저장시 소숫점 0 절사함)
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM", iWoneAm);	//원화금액
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_PR", iForePr);	//외화단가
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_AM", iForeAm);	//외화금액
			
		}
		
	// 견적단가 입력(변경)시
	} else if (e.columnid == "QUTB_PR") {
		var iQutrQt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTB_QT");//수량
		var iQutbPr = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTB_PR");//견적단가
		
		if (!this.gfn_IsNull(iQutrQt)) {	//수량 존재시			
			var iWonePr = nexacro.floor(this.fvExchRt * iQutbPr, 2);          	//원화단가 = 환율 * 견적단가 (10/18 계산시 소숫점 2자리까지)
			var iWoneAm = nexacro.floor(this.fvExchRt * iQutrQt * iQutbPr, 0);	//원화금액 = 환율 * 수량 * 견적단가 (10/18 계산완료시 소숫점 0 절사함)
			
			if (this.fvCurrCd == '0090') {	//KRW(원화)
				var iForePr = 0;	// 외화단가 = 0원
				var iForeAm = 0;	// 외화금액 = 0원
			} else {
				var iForePr = iQutbPr;          	//외화단가 = 견적단가
				var iForeAm = iQutrQt * iQutbPr;	//외화금액 = 수량 * 견적단가
			}
		
		
			//그리드에 적용
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_PR", nexacro.floor(iWonePr,0));	//원화단가(10/18 저장시 소숫점 0 절사함)
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM", iWoneAm);	//원화금액
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_PR", iForePr);	//외화단가
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_AM", iForeAm);	//외화금액
			
		}	
	}

};





