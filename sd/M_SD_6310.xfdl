/***********************************************************************************************
 * 02.MF0.03. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{

	var chgYn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CHG_YN");
	
	if (e.newrow > -1) {
		//[결재] 결재상태에 따른 컴포넌트 제어			
		this.fn_SetWfCompEnable();	
		
		//출고No정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ISSH_NO"))) {
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "ISSH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ISSH_NO"));
			this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
			this.ds_Input.setColumn(0, "ITLM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITLM_CD"));
			this.ds_Input.setColumn(0, "ITMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITMM_CD"));
			this.ds_Input.setColumn(0, "PRGM_ID", this.fvComPrgmId);
					
			
	
			/*
				CHG_YN = 'Y' 등록일이 오늘인경우 수정가능함
			*/
			
			//버튼 enable = true
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_DepmCd.set_enable(false);
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_SystId.set_enable(false);
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_PjthNo.set_enable(false);
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_CusmCd.set_enable(false);
			
			
			//edit readonly = false
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_IsshNo.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_HogiNm.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PjthNo.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PjtbSe.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_readonly(true);
		
			if(chgYn == "Y"){
				
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_WofhDt.set_readonly(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_IsphGu.set_readonly(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(false);
				
				//그리드 readonly
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.set_readonly(false);
				
				//변경버튼
				this.div_MasterInput.form.btn_change.set_enable(false);
				
				
			}else{
		
				//변경버튼
				this.div_MasterInput.form.btn_change.set_enable(true);
		
		
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_WofhDt.set_readonly(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_IsphGu.set_readonly(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(true);
				
				//그리드 readonly
 				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.set_readonly(true);
				
			}
					
			
					
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "sd:M_SD_6310_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			
			
			svcAddObj.sSelectSql = "sd:M_SD_6200_C_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_SD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10 ds_Speb=ds_Speb";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);		
		}
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master10] Row 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무
	
	//데이터 변경여부 체크
	if (this.fn_IsUpdateDataset(this.ds_Master10) || this.fn_IsUpdateDataset(this.ds_Detail10)) {
		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	

	return true;
};  

