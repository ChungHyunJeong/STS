			this.gfn_Alert("SM056", arrMsgArg); //[매출반영금액]이(가) [미매출금액]을(를) 초과할 수 없습니다.
			
			return false;
		}
		//선수금 저장타입변경
		this.ds_Detail30.setColumn(0, "dType", "D"); //삭제		
	}
	
	//선수금존재시 발행조건(선수금:0040) 여부 체크
	if(AmAddnYn){
		if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "TATY_CD") !="0040"){
			var AddnAm = this.ds_Detail30.getSum("ADDN_AM");
			var AdfoAm = this.ds_Detail30.getSum("ADFO_AM");
			
			AddnAm = nexacro.toNumber(AddnAm);
			AdfoAm = nexacro.toNumber(AdfoAm);
			
			if(this.gfn_IsNull(AddnAm)){AddnAm = 0;}
			if(this.gfn_IsNull(AdfoAm)){AdfoAm = 0;}			
		
			if(AddnAm + AdfoAm > 0){		
				var arrMsgArg = [""];
				this.gfn_Alert("SD113", arrMsgArg); //선수금 등록시 발행조건도 선수금이어야 합니다.
				return false;
			}
		}
	}	
	
	return true;
}
  
