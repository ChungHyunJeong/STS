/***********************************************************************************************
 * 05.04. [Vaildation] 매출일자 제어 체크 - 그리드항목
 ***********************************************************************************************/ 
this.fn_VaildSalhDdSave = function()
{
	var sTARE_GU = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "TARE_GU");
	var sTAXB_GU = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "TAXB_GU");
	var sSALH_DD = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SALH_DD");
	//신고구분이 세금계산서, 수정세금계산서 이고,
	if(sTARE_GU =="0010" || sTARE_GU =="0060"){
		//발행구분이 정발행시 매출일자를 금일 이후로 등록 불가
		if(sTAXB_GU=="0"){
			var sToDay = this.gfn_GetCommDate("toDay");//현재일자
			trace("----------발행구분이 정발행시 체크------------");
			trace(sToDay);
			trace(sSALH_DD);
			trace(sToDay <= sSALH_DD);
			if(sToDay < sSALH_DD){
				var arrMsgArg = ["매출일자"];
				this.gfn_Alert("SD114", arrMsgArg);//매출일자(을)를 금일 이후로 등록 불가 합니다.
				return false;
			}
		}
	}
	
	//계약일자 이전 등록 불가
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		sCNTH_DD = this.ds_Detail10.getColumn(iLoop, "CNTH_DD");//계약일자
		trace("----------계약일자 이전 등록 불가 체크------------");
		trace(sCNTH_DD);
		trace(sSALH_DD);
		trace(sCNTH_DD > sSALH_DD);
		if(sCNTH_DD > sSALH_DD){
			this.gfn_Alert("SD115"); //계약일자 이전으로 등록 불가 합니다.
			return false;
		}
		
	}
	
	//발행구분 정/역 값이 N 일경우 첨부파일 필수 등록
	var iFvFindRow = this.ds_Master10.findRowExpr("TAXB_GU=='" + sTAXB_GU + "'");//업무Key Row 찾기
	
	if(this.ds_Sd510.getColumn(iFvFindRow,"OP01_NM")=="N"){
		if (this.fvFileDiv.fn_GetFileCount() < 1 ) {
			var arrMsgArg = ["첨부파일"];
			this.gfn_Alert("SM045", arrMsgArg);//[ &ITM0 ]은(는) 1건 이상 입력하여야 합니다.
			return false;
		}
	}
	
	
	
	return true;
}
 
