/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{

	if (e.newrow > -1) {
		//멀티선택여부
		if (this.fvChkMultiWfYn) return;
		this.ds_SdPjth.filter("");
		//[결재] 결재상태에 따른 컴포넌트 제어
		this.fn_SetWfCompEnable();		
		
 
		
		//결재진행상태-승인인경우
		if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST") == "0040") {
			//전자계약서 정보가 있으면 버튼 활성화
			if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "TAXS_NO")) && this.ds_Master10.getColumn(this.ds_Master10.rowposition, "TAXS_NO").length > 1) {
				this.div_MasterInput.form.btn_EdiCancel.set_enable(true);
			} else {
				this.div_MasterInput.form.btn_EdiCancel.set_enable(false);
			}		
		} else {
			this.div_MasterInput.form.btn_EdiCancel.set_enable(false);
		}
		
		//매출정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SALH_NO"))) {
		
			//[월마감제어-버튼/상세그리드]
			var sBUDG_MM = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SALH_DD"), "");//마감년월
			
			//마감여부 체크
			var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
			var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
			paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
			paramObj.MODM_CD = "SD";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
			paramObj.ACUM_CD = "";//[선택] 회계단위
			paramObj.BUSM_CD  = "";//[선택] 사업부문
			paramObj.SAUM_CD  = "";//[선택] 사업장코드
			paramObj.DEPM_CD  = "";//[선택] 부서코드
			paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
			paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
			
			var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		

			//월마감된 경우
			if (bCheck) {		
				this.fvMmCloseYn = true;//월마감여부-Tab버튼 제어용
			} else {
				this.fvMmCloseYn = false;//월마감여부-Tab버튼 제어용
			}		
			
			//매출코드 포멧 설정 변경 (기존데이터용)
			var strSalhNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SALH_NO")
			//trace(strSalhNo.length);
			//trace(strSalhNo.substr(0,1)=="S");
			if(strSalhNo.length == 10){
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SalhNo.set_format("######-####");
			}else{
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SalhNo.set_format("########-###");
			}
			
			//발의번호 포맷 설정 변경 (기존데이터용)
			var strAcchNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ACCH_NO")
			if(!this.gfn_IsNull(strAcchNo)){
				if(strAcchNo.length == 12){
					this.div_MasterInput.form.tab_Master.tpg_01.form.mae_AcchNo.set_format("########-####");
				}else{
					this.div_MasterInput.form.tab_Master.tpg_01.form.mae_AcchNo.set_format("######-####");
				}			
			}
			
			//입력영역 버튼 비활성화
			//this.fn_SetGridInputButtonEnable();
			
			//[결재]결재내역 팝업화면으로 호출여부
			if (!this.fvWfPopupMode) {
				//직수출, 수출실적 명세 일경우 수출신고번호 필수 입력
				//if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "TARE_GU") == "0020" || this.ds_Master10.getColumn(this.ds_Master10.rowposition, "TARE_GU") == "0050"){
				if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "TARE_GU") == "0020"){
					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SingNo.set_cssclass("essential");
					
				}else{
					this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SingNo.set_cssclass("");
				}
	
				//신고구분이 직수출인경우
				if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "TARE_GU") == "0020" || this.ds_Master10.getColumn(this.ds_Master10.rowposition, "TARE_GU") == "0050" || this.fvWofhStatus){
					//this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_EtaxGu.set_enable(false);	//전자/수기 
					this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_VatmCd.set_enable(false);	//부가세구분
					this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_TaxbGu.set_enable(false);	//발행구분
				}else{
					//this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_EtaxGu.set_enable(true);	//전자/수기 
					this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_VatmCd.set_enable(true);	//부가세구분
					this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_TaxbGu.set_enable(true);	//발행구분
				}
			}
			
			//이성수차장 요청 2022-06-13 활성화로 변경
			//this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_TareGu.set_enable(false);	//신고구분
			
			
			//컴포넌트 비활성화
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_readonly(true);	//거래처
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_CusmCd.set_enable(false);	//거래처버튼
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CurrCd.set_enable(false);	//화폐
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_AcumCd.set_enable(false); 	//회계단위
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_BfsaNo.set_readonly(true);//원매출번호

			this.div_MasterInput.form.btn_Specifications.set_enable(true);	//거래명세서 팝업버튼 활성화(2022-03-25)

			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "SALH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SALH_NO"));
			this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
			this.ds_Input.setColumn(0, "SALH_DD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SALH_DD"));	//매출일자
			
			//this.ds_Input.setColumn(0, "PRGM_ID", this.fvComPrgmId);
					
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "sd:M_SD_5100_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			svcAddObj.sSelectSql = "sd:M_SD_5100_D_SL40";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_SD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10 ds_Detail40=ds_Detail40";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);		
		}
	}
};

