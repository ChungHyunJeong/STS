			var sRowOrgWoneAm  = this.ds_Detail10.getOrgColumn(this.ds_Detail10.rowposition, "WONE_AM") * -1 ; //변경전금액 	
			var sRowNewWoneAm  =  this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "WONE_AM"); //변경 금액 			
			var sSumWoneAm     =  this.ds_Detail10.getCaseSum("crudType!='D'","WONE_AM") ; //변경후 금액 집계//  			
		 
