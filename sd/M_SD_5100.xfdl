	if (e.columnid == "PLAC_CD") {
		iSdPjthRow = this.ds_SdPjth.findRowExpr("PLAC_CD=='"+ obj.getColumn(e.row,"PLAC_CD") +"'")
		obj.setColumn(e.row, "PJTH_NO", this.ds_SdPjth.getColumn(iSdPjthRow,"PJTH_NO"));
		obj.setColumn(e.row, "PJTB_SE", this.ds_SdPjth.getColumn(iSdPjthRow,"PJTB_SE"));
		//obj.setColumn(e.row, "PLAH_DD", this.ds_SdPjth.getColumn(iSdPjthRow,"PLAC_DD")); 
		obj.setColumn(e.row, "PLAH_DD", this.ds_SdPjth.getColumn(iSdPjthRow,"PLAH_DD")); 
		obj.setColumn(e.row, "PLAC_SE", this.ds_SdPjth.getColumn(iSdPjthRow,"PLAC_SE"));
		
		trace(this.ds_SdPjth.getColumn(iSdPjthRow,"PLAC_NM"));
	}
	
	//증감
	if (e.columnid == "WONE_AD") {
		
		//증감 금액 추가 
		//this.ds_Detail10.set_enableevent(false);
		//for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			
		if(obj.getColumn(e.row,"CURR_CD") != "0090"){//외화인 경우만
			//obj.setColumn(e.row, "SALB_AM", nexacro.toNumber(obj.getOrgColumn(e.row,"SALB_AM")) + nexacro.toNumber(e.newvalue));// 매출금액
			obj.setColumn(e.row, "WONE_AM", nexacro.toNumber(obj.getOrgColumn(e.row,"WONE_AM")) + nexacro.toNumber(e.newvalue));// 매출금액
			//var nLossAm = nexacro.floor(obj.getColumn(e.row, "SALB_AM")) - nexacro.floor(obj.getColumn(e.row, "WONE_AM")) - nexacro.floor(obj.getColumn(e.row, "DEWO_AM"));
			//obj.setColumn(e.row, "LOSS_AM", nLossAm);	//환차손익
		}
		//this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM", obj.getCaseSum("crudType!='D'","WONE_AM") + obj.getCaseSum("crudType!='D'","DEWO_AM"));
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM", obj.getCaseSum("crudType!='D'","WONE_AM") );		
		
		
		trace("증감+금액-->"+obj.getColumn(e.row, "WONE_AM"));
		//}
		//this.ds_Detail10.set_enableevent(true);
	}

	/*********마이너스매출 START**********/

	//마이너스매출일 경우
	if ( !this.gfn_IsNull(sBfsaNo) && ( sTareGu == '0080')) {	
		//원화금액(20221007)
		if (e.columnid == "WONE_AM") { 
