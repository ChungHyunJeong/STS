				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM", obj.getCaseSum("crudType!='D'","WONE_AM"));		
				return; 
			} 								
		
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "WONE_VA", sRowNewWoneAm * 0.1) ; //원화부가세
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "VTAX_AM", nexacro.floor(obj.getCaseSum("crudType!='D'","WONE_VA").toFixed(8), 1 ));	
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM", obj.getCaseSum("crudType!='D'","WONE_AM"));
				
	//원화금액 END	
	//원화 부가세	(20221007)
	} else if (e.columnid == "WONE_VA") {		

			var iFvFindRow = this.ds_Master10.findRowExpr("SALH_NO=='" + this.fvSalhNo + "'") ;//원매출번호 Row 찾기  						   
			var sWoneVa = this.ds_Master10.getColumn(iFvFindRow, "VTAX_AM" ); //원매출부가세금액( sWoneVa  > 0)
			var sRowOrgWoneVa  =  this.ds_Detail10.getOrgColumn(this.ds_Detail10.rowposition, "WONE_VA") * -1 ; //변경금액 	
			var sRowNewWoneVa  =  this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "WONE_VA"); //변경금액 				
			var sSumWoneVa 	   =  this.ds_Detail10.getCaseSum("crudType!='D'","WONE_VA") ; //변경후 금액 집계 

		
	         if((sWoneVa * -1)  > sSumWoneVa) {				
				var arrMsgArg = ["[부가세]"];
				this.gfn_Alert("SD117", arrMsgArg); //원매출 마이너스금액보다 적을 수는 없습니다.
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "WONE_VA", sRowOrgWoneVa) ; //원화부가세 
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "VTAX_AM", obj.getCaseSum("crudType!='D'","WONE_VA"));	
				return; 
			} 
		 
			if( 0 <=  sRowNewWoneVa)  {
				var arrMsgArg = ["[부가세]"];
				this.gfn_Alert("SD118", arrMsgArg); //0 이상은  입력 불가능 합니다.			
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "WONE_VA", sRowOrgWoneVa) ; //원화부가세
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "VTAX_AM", obj.getCaseSum("crudType!='D'","WONE_VA"));	
				return; 
			} 	
	   	
	   this.div_MasterInput.form.tab_Master.tpg_01.form.mae_VtaxAm.set_value(nexacro.floor(obj.getCaseSum("crudType!='D'","WONE_VA").toFixed(8)));
	   
	  //원화부가세 END   
	  //외화금액(20221007)
	} else if( e.columnid == "FORE_AM" ) {

			var iFvFindRow = this.ds_Master10.findRowExpr("SALH_NO=='" + this.fvSalhNo + "'") ;//원매출번호 Row 찾기 		 						 	
			var sForeAm = this.ds_Master10.getColumn(iFvFindRow, "FORE_AM" );//원매출외화금액 						
 			var sRowOrgWoneAm  =  this.ds_Detail10.getOrgColumn(this.ds_Detail10.rowposition, "WONE_AM") * -1 ; //변경전금액 (20221020) 	
			var sRowOrgForeAm  =  this.ds_Detail10.getOrgColumn(this.ds_Detail10.rowposition, "FORE_AM") * -1 ; //변경금액 	
			var sRowNewForeAm  =  this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "FORE_AM"); //변경금액 				
			var sNewForeAm =  this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "FORE_AM"); //변경금액 							
			var sSumForeAm =  this.ds_Detail10.getCaseSum("crudType!='D'","FORE_AM") ; //변경후 금액 집계				
					
	        if((sForeAm * -1)  > sSumForeAm) {					
				var arrMsgArg = ["[외화금액]"];
				this.gfn_Alert("SD117", arrMsgArg); //원매출 마이너스금액보다 적을 수는 없습니다.
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "FORE_AM", sRowOrgForeAm) ; //외화금액 
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "WONE_AM", sRowOrgWoneAm) ; //원화금액 //20221020
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_AM", obj.getCaseSum("crudType!='D'","FORE_AM") );
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM", obj.getCaseSum("crudType!='D'","WONE_AM") );
				return; 
			} 
			
		    //원외화금액< 변경외화  
			if( 0 <= sNewForeAm)  {
				var arrMsgArg = ["[외화금액]"];
				this.gfn_Alert("SD118", arrMsgArg); //0 이상은  입력 불가능 합니다.		
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "FORE_AM", sRowOrgForeAm) ; //외화금액 
				return; 
			} 				
		
			 var nRatm = this.div_MasterInput.form.tab_Master.tpg_01.form.mae_ExchRt.value ; //환율	
			 obj.setColumn(obj.rowposition,"WONE_AM", nexacro.floor(nexacro.toNumber((nRatm * obj.getColumn(obj.rowposition,"FORE_AM")).toFixed(8)))) ;
	
	
	} //외화금액

	 this.div_MasterInput.form.tab_Master.tpg_01.form.mae_ForeAm.set_value(obj.getCaseSum("crudType!='D'","FORE_AM"));
	 this.div_MasterInput.form.tab_Master.tpg_01.form.mae_WoneAm.set_value(obj.getCaseSum("crudType!='D'","WONE_AM"));	

	} //마이너스매출 END
}; 
