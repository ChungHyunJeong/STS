		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			iForeAm = this.ds_Detail10.getColumn(iLoop, "FORE_AM");
//			this.ds_Detail10.setColumn(iLoop, "WONE_AM", nexacro.floor((nRatm * iForeAm).toFixed(8)));	//금액
			this.ds_Detail10.setColumn(iLoop, "WONE_AM", nexacro.floor((nRatm * iForeAm).toFixed(0)));	//금액
			this.ds_Detail10.setColumn(iLoop, "FORE_AM", iForeAm);	//외화단가		
			
		}
	}
	
	//금액계산
	this.fn_Reckoning();
	
	
	
};


