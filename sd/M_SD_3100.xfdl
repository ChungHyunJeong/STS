/***********************************************************************************************
 * 02.DG1.05. [Detail그리드2-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Choice = function()
{
	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {
			
			//동일대상만 체크 대상 값 Flag
			var sIdntNoChk=""; //Key(견적번호) 체크
			var nRatm = 0; //환율
			var sToDay = this.gfn_GetCommDate("toDay");//현재일자
			
			//견적번호 추가 체크 (1. 최초 입력일 경우 동일한 견적번호여부 확인, 추가 입력일 경우 제품번호확인)
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					
					//1. 견적검색 Tab과 입력화면 항목 비교 
					if(this.ds_Detail10.rowcount > 0 ){
						// 프로젝트번호 비교 : 크로스체크(중복비교)
						if(this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PjthNo.value != this.ds_Detail20.getColumn(iLoop,"PJTH_NO") ){
							var arrMsgArg = ["프로젝트번호"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 프로젝트번호만 적용 가능합니다.
							return;
						}  
						// 최초저장시(입력모드 true) 견적번호 비교
						if(this.fvInserMode && this.div_MasterInput.form.tab_Master.tpg_01.form.mae_QuthNo.value != this.ds_Detail20.getColumn(iLoop,"QUTH_NO")){
							var arrMsgArg = ["견적번호"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 견적번호만 적용 가능합니다.
							return;
						}
					}
					
					//2. 그리드내 견적번호 체크 - 최초저장의 경우(입력모드 true)만 체크
					if( !this.gfn_IsNull(sIdntNoChk) ){
						if(this.fvInserMode && sIdntNoChk != this.ds_Detail20.getColumn(iLoop,"QUTH_NO") ){
							var arrMsgArg = ["견적번호"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 견적번호만 적용 가능합니다.
							return;
						}
					}
					sIdntNoChk = this.ds_Detail20.getColumn(iLoop,"QUTH_NO");//견적번호
					
					//3. 수량 체크 : 제품수량
					if(this.ds_Detail20.getColumn(iLoop, "CNTB_QT") == 0){
						this.gfn_Alert("SD009"); //수량이 존재하지 않습니다.
						return;
					}
					
				}
			}
			
			var arrCheckRow = [];//체크된 Row
			
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				//체크 대상여부 확인			
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {

					//Detail10에 적용되어 있는지 체크
					var objSearchList = ["COMM_CD|" + this.ds_Detail20.getColumn(iLoop, "COMM_CD") //회사코드
										,"QUTH_NO|" + this.ds_Detail20.getColumn(iLoop, "QUTH_NO") //견적번호
										,"QUTH_SQ|" + this.ds_Detail20.getColumn(iLoop, "QUTH_SQ") //견적차수
										,"QUTB_SE|" + this.ds_Detail20.getColumn(iLoop, "QUTB_SE") //견적순번
										,"ITEM_CD|" + this.ds_Detail20.getColumn(iLoop, "ITEM_CD") //품목코드
										,"ISPH_NO|" + this.ds_Detail20.getColumn(iLoop, "ISPH_NO") //제품사양번호
										];

					//[공통] Detail10 중복건 체크후 해당건 Skip
					if (!this.gfn_DsSchDupcheck(this.ds_Detail10, objSearchList)) {
						//선택된 첫번째 프로젝트 정보를 Master10에 적용
						//프로젝트번호, 프로젝트순번, 거래처, 고객, 국내외구분, 납품장소 정보를 가져옴
						if(this.ds_Detail10.rowcount == 0 ){ 
							//프로젝트번호, 견적번호 자리수 확인
							var iPjthLen = this.ds_Detail20.getColumn(iLoop,"PJTH_NO").length;
