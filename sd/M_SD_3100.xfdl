		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_AM",this.ds_Detail10.getSum("FORE_AM"));	//Master에 외화금액 표기
		
	} 
	// 영업실행 변경시
	else if (e.columnid == "BSAC_AM") {
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BSAC_AM",this.ds_Detail10.getSum("BSAC_AM"));	//Master에 영업실행 표기
		
	} 
	// 영업실행율 변경시
	else if (e.columnid == "BSAC_RT") {
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BSAC_RT",this.ds_Detail10.getSum("BSAC_RT"));	//Master에 PRAC_RT 표기
	}
	// 상태 변경시
	else if (e.columnid == "DETA_CD") {
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM",this.ds_Detail10.getCaseSum("DETA_CD!='0050'","WONE_AM"));	//Master에 원화금액 표기
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_AM",this.ds_Detail10.getCaseSum("DETA_CD!='0050'","FORE_AM"));	//Master에 외화금액 표기
		//this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BSAC_AM",this.ds_Detail10.getCaseSum("DETA_CD!='0050'","BSAC_AM"));	//Master에 영업실행 표기
		//this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BSAC_RT",this.ds_Detail10.getCaseSum("DETA_CD!='0050'","BSAC_RT"));	//Master에 영업실행율 표기
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BSAC_AM",this.ds_Detail10.getCaseSum("DETA_CD!='0050'","STOT_AM"));	//Master에 영업실행 표기
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BSAC_RT",nexacro.toNumber(nexacro.round(this.ds_Detail10.getCaseSum("DETA_CD!='0050'","STOT_AM")/this.ds_Detail10.getCaseSum("DETA_CD!='0050'","WONE_AM")*100 ,2),0));

	}
	//수량 계약금액 변경시
	if(e.columnid == "CNTB_QT"
		|| e.columnid == "WONE_AM"
		|| e.columnid == "FORE_AM"){
		//간접비 계산.
		//실행원가 재계산.
		this.fn_CalcAM(e.row);
	}
	//재료비, 실행단가 변경시 실행원가 재계산.
	if(e.columnid == "TMAD_AM"
		|| e.columnid == "TLAD_AM"
		|| e.columnid == "TOUT_AM"
		|| e.columnid == "TCOD_AM"
		|| e.columnid == "SEXP_AM"
		|| e.columnid == "SFEE_AM"
		|| e.columnid == "SETC_AM"
		|| e.columnid == "MANA_AM"
		|| e.columnid == "DEVE_AM"){
		//이익율 계산.
		this.fn_CalcRt(e.row);
	}
	
	// crudType이 D이고 Detail10 체크박스 체크해제시 Detail11 crueType 해제방법 (견적관리는 행삭제 기능 참고하여 가져옴)
	//영업계산내역탭 재계산
	this.fn_Tpg04_Cal();
};

