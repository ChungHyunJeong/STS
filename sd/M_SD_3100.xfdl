/*---------------------------------------------------------------------------------------------
 *    03. [데이터셋-ds_Detail20] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{

	//체크시 해당 프로그램 전체선택
	if (e.columnid == "s_chk") {	
	
		if (this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "s_chk") == '1') {			
			var sQuthNo = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "QUTH_NO");//견적번호
			var sQutbSe = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "QUTB_SE");//견적순번
			var sIsphNo = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "ISPH_NO");//제품사양번호

			//Detail10에 견적번호/견적순번 or 제품사양번호가 존재하는지에 대해 확인
			var iDupCnt  = this.ds_Detail10.getCaseCount("QUTH_NO == '"+sQuthNo+"' && QUTB_SE == '"+sQutbSe+"'"); //견적번호,견적순번
			var iDupIsph = this.ds_Detail10.getCaseCount("ISPH_NO == '"+sIsphNo+"'"); //제품사양번호 (견적번호 무관)			
			//견적번호, 견적순번 존재유무
			if(iDupCnt > 0) {
				var arrMsgArg = ["견적번호/견적순번"];
				this.gfn_Alert("SD008", arrMsgArg); //해당 견적번호/견적순번이 이미 존재합니다.					
				this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "s_chk","0");
				return;
			}
			//제품사양번호 존재유무
			if(!this.gfn_IsNull(sIsphNo) && iDupIsph > 0 ){
				var arrMsgArg = ["제품사양번호"];
				this.gfn_Alert("SD008", arrMsgArg); //해당 제품사양번호가 이미 존재합니다.					
				this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "s_chk","0");
				return;
			}			

			//수량 체크 : 제품수량
			if(this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "CNTB_QT") == 0){
				this.gfn_Alert("SD009"); //수량이 존재하지 않습니다.
				this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "s_chk","0");
				return;
			}
			
			//견적번호가 최초 1건만 체크됐을 경우에만 처리함(2건일 경우 return)
			var iChkCnt = this.ds_Detail20.getCaseCount("QUTH_NO == '"+sQuthNo+"' && s_chk == '1'"); //체크건수
			if (iChkCnt > 1) return;

			//최초 1건만 체크시 로직 처리
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				//Detail10 중복여부 체크
				sQutbSe = this.ds_Detail20.getColumn(iLoop, "QUTB_SE");//견적순번
				sIsphNo = this.ds_Detail20.getColumn(iLoop, "ISPH_NO");//제품사양번호
				//존재유무 확인
				iDupCnt  = this.ds_Detail10.getCaseCount("QUTH_NO == '"+sQuthNo+"' && QUTB_SE == '"+sQutbSe+"'"); //견적번호,견적순번
				iDupIsph = this.ds_Detail10.getCaseCount("ISPH_NO == '"+sIsphNo+"'"); //제품사양번호 (견적번호 무관)			
				
				//견적번호,견적순번이 같고 색상변경이 안된 대상
				if( this.ds_Detail20.getColumn(iLoop, "QUTH_NO") == sQuthNo 
				 && iDupCnt == 0
				 && iDupCnt == 0
				 && ((this.gfn_IsNull(this.ds_Detail20.getColumn(iLoop, "s_conf")) || this.ds_Detail20.getColumn(iLoop, "s_conf") == '0')) ) {
								
					//체크박스 Check
					this.ds_Detail20.setColumn(iLoop, "s_chk", "1");//Check로 변경
					
				}
			}
			
		}
	}
	
};

