/***********************************************************************************************
 * 02.DG0.04. [Detail그리드-버튼] 행삭제 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd01DeleteRow = function()
{
	var s_chk = "";//체크
	//견적검색에서 체크 가능하게 변경
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		s_chk = this.ds_Detail10.getColumn(iLoop, "s_chk");
		
		//체크
		if (s_chk == "1") {
		
			//검색값설정 - 컬럼명:검색값
			var objSearchList = ["PJTH_NO|" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PJTH_NO") //프로젝트번호
								,"PJTB_SE|" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PJTB_SE") //프로젝트순번
								,"QUTH_NO|" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO") //견적번호
								,"QUTB_SE|" + this.ds_Detail10.getColumn(iLoop, "QUTB_SE") //견적순번
				                ,"ITEM_CD|" + this.ds_Detail10.getColumn(iLoop, "ITEM_CD") //품목번호
							    ,"ISPH_NO|" + this.ds_Detail10.getColumn(iLoop, "ISPH_NO") //제품사양번호
								];

			//[공통] 행삭제시 검색Tab 선택Row 해제 처리
			this.gfn_DeleteRowSchTabUncheck(this.ds_Detail20, objSearchList);

			//계약상세 데이터셋에 데이터 존재 확인
			if (this.ds_Detail11.rowcount > -1) {
				var sCntbSe = this.ds_Detail10.getColumn(iLoop, "CNTB_SE");
				var codeKey = this.ds_Detail10.getColumn(iLoop, "codeKey");
				var seqKey = this.ds_Detail10.getColumn(iLoop, "seqKey");
				//Detail11 삭제처리
				for (var iLp=0; iLp<this.ds_Detail11.getRowCount(); iLp++) {	
					if (codeKey == this.ds_Detail11.getColumn(iLp, "codeKey")
					 && seqKey == this.ds_Detail11.getColumn(iLp, "seqKey")
					 && sCntbSe == this.ds_Detail11.getColumn(iLp, "CNTB_SE")) {
						//crudType == 'I' 이면 삭제, 그외 crudType == 'D'로 변경
						var crudType = this.ds_Detail11.getColumn(iLp, "crudType");
						if (crudType == 'I') {
							this.ds_Detail11.deleteRow(iLp);				
						} else {
							this.ds_Detail11.setColumn(iLp, "crudType", "D");
							this.ds_Detail11.setColumn(iLp, "s_chk", "1");
						}
					}
				}
				
			}	
		}
	}
	
	this.gfn_GridDelProcess(this, this.ds_Detail10);//crud_flag 변경-삭제(D)	

	//계약번호가 없고 계약상세 내용이 미존재시 Master Clear
	if (this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition,"CNTH_NO")) && this.ds_Detail10.rowcount == 0) {
		this.fn_ClearMasterValue();
	}
	
};

/***********************************************************************************************
 * 02.DG0.05. [Detail그리드-버튼] 취소 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd01Cancel = function()
{

	//검색값설정 - 컬럼명:검색값
	var objSearchList = ["PJTH_NO|" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PJTH_NO") //프로젝트번호
	                    ,"PJTB_SE|" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PJTB_SE") //프로젝트순번
	                    ,"QUTH_NO|" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO") //견적번호
						,"QUTB_SE|" + this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUTB_SE") //견적순번
						,"ITEM_CD|" + this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "ITEM_CD") //품목번호
						,"ISPH_NO|" + this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "ISPH_NO") //제품사양번호
						];
	//[공통] 행삭제시 검색Tab 선택Row 해제 처리
	this.gfn_DeleteRowSchTabUncheck(this.ds_Detail20, objSearchList);

	//계약상세 데이터셋에 데이터 존재 확인
	if (this.ds_Detail11.rowcount > -1) {
		var sCntbSe = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "CNTB_SE");
		var codeKey = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "codeKey");
		var seqKey = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "seqKey");
		//Detail11 삭제
		for (var iLoop=this.ds_Detail11.getRowCount(); iLoop>=0; iLoop--) {	
			if (codeKey == this.ds_Detail11.getColumn(iLoop, "codeKey")
			 && seqKey == this.ds_Detail11.getColumn(iLoop, "seqKey")
			 && sCntbSe == this.ds_Detail11.getColumn(iLoop, "CNTB_SE")) {
				//crudType == 'I' 이면 삭제, 그외 crudType를 이전상태로 변경
				var crudType = this.ds_Detail11.getColumn(iLoop, "crudType");
				if (crudType == 'I') {
					//Detail11 데이터셋 삭제
					this.ds_Detail11.deleteRow(iLoop);				
					//this.gfn_UndoColumnDs(this.ds_Detail11, iLoop);	//공통에서 생성한것(확인 후 사용)
				} else {
					this.ds_Detail11.setColumn(iLoop, "crudType", "");
