	//this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.mae_PjthNo.set_readonly(false);
	//this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_PjtbSe.set_readonly(false);
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.btn_PjthNo.set_enable(true);

	//Deatil20 TAB 날짜 초기화
	this.gfn_SetCommFromToDate(this.fvComPrgmId, "DS", this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_FromDd, this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_ClosDd);//조회조건영역	

