	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	
	this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
	this.ds_Detail10.setColumn(iAddRow, "CNTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO"));
	this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
	this.ds_Detail10.setColumn(iAddRow, "DETA_CD", '0020');//진행상태
	this.ds_Detail10.setColumn(iAddRow, "PURC_DD", sToDay);//PO일자 - 오늘날자로 설정
	this.ds_Detail10.setColumn(iAddRow, "CNTB_SE", "000"); //계약순번 (신규의 경우 000으로 셋팅)

	//Key 순번 (견적상세 체크시 필요)	
	if (iAddRow == 0) {
		var seqKey = "001"; //01 --> 1로 변경(요청)
	} else {
		var seqKey = nexacro.toNumber(this.ds_Detail10.getMax("seqKey"));
		seqKey = seqKey + 1;
		seqKey = seqKey.toString().padLeft(3,'0');
	}
	
	this.ds_Detail10.setColumn(iAddRow, "codeKey", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "codeKey"));	
	this.ds_Detail10.setColumn(iAddRow, "seqKey", seqKey);	
	this.ds_Detail10.setColumn(iAddRow, "s_conf", "0");

};

