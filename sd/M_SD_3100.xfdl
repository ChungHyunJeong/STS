					, paramPlamCd:this.ds_Master10.getColumn(this.ds_Master10.rowposition,"PALM_CD")		//손익코드					
					, paramItemGu:this.ds_Master10.getColumn(this.ds_Master10.rowposition,"ITEM_GU")		//손익구분			
					, paramSaumCd:this.ds_Master10.getColumn(this.ds_Master10.rowposition,"SAUM_CD")		//사업장								
					, paramCurrCd:this.ds_Master10.getColumn(this.ds_Master10.rowposition,"CURR_CD")		//화폐코드
					, paramExchRt:this.ds_Master10.getColumn(this.ds_Master10.rowposition,"EXCH_RT")		//환율							
