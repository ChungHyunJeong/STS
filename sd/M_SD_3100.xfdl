{
	if (!this.gfn_CheckFromToDate(this.fvComPrgmId, this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_FromDd, this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_ClosDd)) return false;//검색기간 범위 체크
	//검색조건 필수입력값 있으면 체크
	//[검색]NULL 체크
	var arrArgs = ["this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.mae_PjthNo^프로젝트번호|NULL"
	              ,"this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_PjtbSe^프로젝트순번|NULL"
	              ]; 
	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs) ) {
		return false;
	}
	
	return true;
}


