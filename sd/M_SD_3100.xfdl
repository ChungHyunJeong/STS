							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"QUTH_NM",this.ds_Detail20.getColumn(iLoop,"QUTH_NM"));	//견적명
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"PJTH_NO",this.ds_Detail20.getColumn(iLoop,"PJTH_NO"));	//프로젝트번호
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"PJTB_SE",this.ds_Detail20.getColumn(iLoop,"PJTB_SE"));	//프로젝트순번
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"PJTH_NM",this.ds_Detail20.getColumn(iLoop,"PJTB_NM"));	//프로젝트명
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"CNTH_NM",this.ds_Detail20.getColumn(iLoop,"PJTB_NM"));	//계약명
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"INEX_GU",this.ds_Detail20.getColumn(iLoop,"INEX_GU"));	//국내외구분
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"CUSM_CD",this.ds_Detail20.getColumn(iLoop,"CUSM_CD"));	//거래처코드
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"CUSM_NM",this.ds_Detail20.getColumn(iLoop,"CUSM_NM"));	//거래처명
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"CSPM_CD",this.ds_Detail20.getColumn(iLoop,"CSPM_CD"));	//고객담당자코드
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"CSPM_NM",this.ds_Detail20.getColumn(iLoop,"CSPM_NM"));	//고객담당자명
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"PLAC_CD",this.ds_Detail20.getColumn(iLoop,"PLAC_CD"));	//납품처코드
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"PLCE_NM",this.ds_Detail20.getColumn(iLoop,"PLCE_NM"));	//납품처명
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"CURR_CD",this.ds_Detail20.getColumn(iLoop,"CURR_CD"));	//화폐코드
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"PAYM_CD",this.ds_Detail20.getColumn(iLoop,"PAYM_CD"));	//대금결제코드
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"PLAC_NM",this.ds_Detail20.getColumn(iLoop,"PLAC_NM"));	//납품장소
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"TYMA_CD",this.ds_Detail20.getColumn(iLoop,"TYMA_CD"));	//수주형태 Main
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"TYSU_CD",this.ds_Detail20.getColumn(iLoop,"TYSU_CD"));	//수주형태 Sub

							/* 회계단위, 진행상태, 계약일자, 환율 : Default 처리 */
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"ACUM_CD",'1000');//회계단위 - Default : 1000
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"PROC_ST",'0020');//진행상태 - Default : 수주
							//계약일자 환율 자동입력
							nRatm = this.gfn_GetCommRatm(this.ds_Master10.getColumn(this.ds_Master10.rowposition,"CNTH_DD"), this.ds_Detail20.getColumn(iLoop,"CURR_CD"));//환율정보
							this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EXCH_RT", nRatm);	//환율정보
							
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"PALM_CD",this.ds_Detail20.getColumn(iLoop,"PALM_CD"));
							//this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_FaPalm.set_value(this.ds_Detail20.getColumn(iLoop,"PALM_CD"));
							//trace(this.ds_Master10.getColumn(this.ds_Master10.rowposition,"PALM_CD"));
						}

						// 라인추가 후 Detail20 -> Detail10으로 적용
						var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10); //crud_flag 변경-입력(I)

						//선택항목 비활성화처리 (색상변경 및 체크박스 Lock)
						this.ds_Detail20.setColumn(iLoop, "s_conf", "1");
						this.ds_Detail20.setColumn(iLoop, "s_chk", "0");//Uncheck로 변경

						//견적검색(Detail20) -> 계약상세(Detail10) Copy
						this.ds_Detail10.copyRow(iAddRow, this.ds_Detail20, iLoop);

						//견적검색(Detail20) -> 계약상세(Detail10) Copy : 컬럼명 다름
						this.ds_Detail10.setColumn(iAddRow, "CNTB_QT", this.ds_Detail20.getColumn(iLoop,"QUTB_QT")); //수량
						this.ds_Detail10.setColumn(iAddRow, "DETA_CD", '0020'); //상세상태 : Default 0020
						this.ds_Detail10.setColumn(iAddRow, "PURC_DD", sToDay); //PO일자 : Default  당일
						this.ds_Detail10.setColumn(iAddRow, "WONE_PR",this.ds_Detail20.getColumn(iLoop,"WONE_PR"));
						this.ds_Detail10.setColumn(iAddRow, "WONE_AM",this.ds_Detail20.getColumn(iLoop,"WONE_AM"));
						this.ds_Detail10.setColumn(iAddRow, "FORE_PR",this.ds_Detail20.getColumn(iLoop,"FORE_PR"));
						this.ds_Detail10.setColumn(iAddRow, "FORE_AM",this.ds_Detail20.getColumn(iLoop,"FORE_AM"));						
						this.ds_Detail10.setColumn(iAddRow, "CNTB_PR",this.ds_Detail20.getColumn(iLoop,"QUTB_PR"));
						if(this.gfn_Nvl(this.ds_Detail10.getColumn(iAddRow,"CNTB_PR"),0)==0){
							//원화인경우.
							if(this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition,"CURR_CD"),"")=="0090"){
								this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "CNTB_PR", this.ds_Detail10.getColumn(iAddRow, "WONE_PR"));
							}else{
								this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "CNTB_PR", this.ds_Detail10.getColumn(iAddRow, "FORE_PR"));
							}
						}
						
						//영업실행(BSAC_AM), 영업실행율(BSAC_RT) 계산 --> 설계자 문의해야 함

						//견적상세 Master에서 Key값 적용
						this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
						this.ds_Detail10.setColumn(iAddRow, "CNTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO")); //계약번호
						this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ")); //결재차수
						this.ds_Detail10.setColumn(iAddRow, "CNTB_SE", "000"); //견적순번 (신규의 경우 000으로 셋팅)
						this.ds_Detail10.setColumn(iAddRow, "s_chk", "0");

						//Key 순번 (결제상세 체크시 필요)
						if (iAddRow == 0) {
							var seqKey = "001"; //01 --> 1로 변경(요청)
						} else {
							var seqKey = nexacro.toNumber(this.ds_Detail10.getMax("seqKey"));
							seqKey = seqKey + 1;
							seqKey = seqKey.toString().padLeft(3,'0');
						}
						this.ds_Detail10.setColumn(iAddRow, "codeKey", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "codeKey"));	
						this.ds_Detail10.setColumn(iAddRow, "seqKey", seqKey);

						//견적상세(Detail21) -> 계약상세(Detail11)
						var sChkKey = this.ds_Detail20.getColumn(iLoop, "COMM_CD") + this.ds_Detail20.getColumn(iLoop, "QUTH_NO") + this.ds_Detail20.getColumn(iLoop, "QUTH_SQ") + this.ds_Detail20.getColumn(iLoop, "QUTB_SE"); //조건비교 key

						//계약상세에서 계약상세로 복사
						for (var iLp=0; iLp<this.ds_Detail21.getRowCount(); iLp++) {
							var sCmpKey = this.ds_Detail21.getColumn(iLp, "COMM_CD") + this.ds_Detail21.getColumn(iLp, "QUTH_NO") + this.ds_Detail21.getColumn(iLp, "QUTH_SQ") + this.ds_Detail21.getColumn(iLp, "QUTB_SE"); //조건비교 key
							
							//견적검색 key로 견적상세 key와 비교
							if ( sChkKey == sCmpKey) {
								var iAddRowD = this.gfn_GridInsertProcess(this, this.ds_Detail11); //crud_flag 변경-입력(I)
								
								//견적검색(Detail21) -> 계약상세(Detail11) Copy
								this.ds_Detail11.copyRow(iAddRowD, this.ds_Detail21, iLp);
								this.ds_Detail11.setColumn(iAddRowD, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
								this.ds_Detail11.setColumn(iAddRowD, "CNTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO")); //계약번호
								this.ds_Detail11.setColumn(iAddRowD, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ")); //결재차수
								
								this.ds_Detail11.setColumn(iAddRowD, "CNTB_SE", "000"); //견적순번 (신규의 경우 00으로 셋팅)
								this.ds_Detail11.setColumn(iAddRowD, "DETA_CD", '0020');//진행상태
								this.ds_Detail11.setColumn(iAddRowD, "PURC_DD", sToDay);//PO일자 - 오늘날자로 설정

								this.ds_Detail11.setColumn(iAddRowD, "codeKey", this.ds_Detail10.getColumn(iAddRow, "codeKey"));//임시 Key
								this.ds_Detail11.setColumn(iAddRowD, "seqKey", this.ds_Detail10.getColumn(iAddRow, "seqKey"));//임시순번 Key
								this.ds_Detail11.setColumn(iAddRowD, "descKey", iLp);//임시순번 Key
								this.ds_Detail11.setColumn(iAddRowD, "crudType", "I");//입력(I)							 
							}
						}

				
						arrCheckRow.push(iLoop );
					}
				}
			}
			// 환율에 따른 단가/금액 재계산			
			this.fn_RatmChange();
			
			// 원화금액, 외화금액, 영업실행, 영업실행율 합계 Master에 적용
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM",this.ds_Detail10.getCaseSum("DETA_CD!='0050'","WONE_AM"));	//Master에 원화금액 표기
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_AM",this.ds_Detail10.getCaseSum("DETA_CD!='0050'","FORE_AM"));	//Master에 외화금액 표기
			//this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BSAC_AM",this.ds_Detail10.getCaseSum("DETA_CD!='0050'","BSAC_AM"));	//Master에 영업실행 표기
			//this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BSAC_RT",this.ds_Detail10.getCaseSum("DETA_CD!='0050'","BSAC_RT"));	//Master에 영업실행율 표기
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BSAC_AM",this.ds_Detail10.getCaseSum("DETA_CD!='0050'","STOT_AM"));	//Master에 영업실행 표기
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BSAC_RT",nexacro.round(this.ds_Detail10.getCaseSum("DETA_CD!='0050'","STOT_AM")/this.ds_Detail10.getCaseSum("DETA_CD!='0050'","WONE_AM")*100 ,2));

					
			this.ds_Detail20.set_rowposition(-1);
			
			//적용 후 계약상세 Tab이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);
						
			this.fn_SetWfCompEnable();
			
			//표준원가조회 밑 재계산 실행.
			this.fn_GetCosmData(true);
		}
	}
};

