/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 
	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		//첨부파일정보만 변경시
		if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && this.ds_Master10.getRowCount() > 0) {
			//첨부파일정보 저장
			fvFileInfo = new Object();//[공통] 첨부파일정보
			fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, B-상세(그리드Row), E-기타
			fvFileInfo.CALLBACK_FUNC = "";//호출할 콜백함수
			fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
			fvFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO");//PK번호2-계약번호
			fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
			//첨부파일Div 화면에 Key세팅하게 변경
			this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);
			return;	
		}
	}
	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		if (!this.fn_VaildMasterSave()) return;
	}
	
	//Detail 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		if (!this.fn_VaildDetailSave()) return;
	}	
	
	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	//Master 입력 상태이면
	if (sChkRowType == "I") {
	
		//채번
		var sTbnmNo = this.gfn_GetTbnmNo(this.fvComPrgmId, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_DD"));//채번값, 기준일자
		if (this.gfn_IsNull(sTbnmNo)) {
			//채번을 가져오는데 실패했습니다.
			return;
		}
		
		var sChkTbnmNo = "";//채번-체크용
		var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position
		var iDetailRowPos = this.ds_Detail10.rowposition;//Detail 현재 Row Position
		
		//Master 데이터셋에 채번정보 입력
		this.ds_Master10.setColumn(iMasterRowPos, "CNTH_NO", sTbnmNo);	//계약번호

		//PK값 정의[필수]
		var sCommCd = this.ds_Master10.getColumn(iMasterRowPos, "COMM_CD");//회사코드
		var sCnthNo = this.ds_Master10.getColumn(iMasterRowPos, "CNTH_NO");//계약번호
		var sWofhSq = this.ds_Master10.getColumn(iMasterRowPos, "WOFH_SQ");//결재차수
		
		//Detail10 데이터셋에 PK정보 입력
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			sChkTbnmNo = this.ds_Detail10.getColumn(iLoop, "CNTH_NO");//계약번호
			
			if (this.gfn_IsNull(sChkTbnmNo)) {
				this.ds_Detail10.setColumn(iLoop, "COMM_CD", sCommCd);
				this.ds_Detail10.setColumn(iLoop, "CNTH_NO", sCnthNo);
				this.ds_Detail10.setColumn(iLoop, "WOFH_SQ", sWofhSq);
				this.ds_Detail10.setColumn(iLoop, "CNTB_SE", this.ds_Detail10.getColumn(iLoop, "seqKey"));
			}
		}

		//Detail11 데이터셋에 PK정보 입력
		for (var iLoop=0; iLoop<this.ds_Detail11.getRowCount(); iLoop++) {
			sChkTbnmNo = this.ds_Detail11.getColumn(iLoop, "CNTH_NO");//계약번호
			
			if (this.gfn_IsNull(sChkTbnmNo)) {
				this.ds_Detail11.setColumn(iLoop, "COMM_CD", sCommCd);
				this.ds_Detail11.setColumn(iLoop, "CNTH_NO", sCnthNo);
				this.ds_Detail11.setColumn(iLoop, "WOFH_SQ", sWofhSq);
				this.ds_Detail11.setColumn(iLoop, "CNTB_SE", this.ds_Detail11.getColumn(iLoop, "seqKey"));
			}
		}
		
		//첨부파일정보 변경시
		if (this.fvFileDiv.fn_CheckFileAddYn()) {
			fvFileInfo = new Object();//[공통] 첨부파일정보
			fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
			fvFileInfo.CALLBACK_FUNC = "fn_ComBtnSearch";//호출할 콜백함수
			fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
			fvFileInfo.PK01_NO = nApp.gvCompany;//PK번호1-회사코드
			fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO");//PK번호2-계약번호
			fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
		}		
		
	} else {
		//1. 계약상세 계약순번 MAX값 가져오기 (PK이므로 DELE_YN 조건없이 가져옴)
		//Max Seq 가져오기
		this.fn_SeqMaxSearch();
		

		//2. Detail10, Detail11 계약순번 MAX값에 seqKey값을 더하여 계약순번(CNTB_SE) 데이터셋 값을 변경함
		var iMaxCntbSe = nexacro.toNumber(this.ds_MaxSeq.getColumn(0, "CNTB_SE"));

		//Detail10 데이터셋에 PK정보 입력
		for (var iLoop10=0; iLoop10<this.ds_Detail10.getRowCount(); iLoop10++) {
			var crudType = this.ds_Detail10.getColumn(iLoop10, "crudType");//신규여부			
			if (crudType=='I') { 
				//Detail10과 Detail11 비교 Key
				var codeKey = this.ds_Detail10.getColumn(iLoop10, "codeKey");//
				var seqKey = this.ds_Detail10.getColumn(iLoop10, "seqKey");//
				var sCntbSe = this.ds_Detail10.getColumn(iLoop10, "CNTB_SE");//
				
				// Max값 기준으로 순번 변경
				var seqKey = nexacro.toNumber(this.ds_Detail10.getColumn(iLoop10, "seqKey"));//임시순번			
				var iCntbSe = iMaxCntbSe + seqKey;
				iCntbSe = iCntbSe.toString().padLeft(3, '0');
				
				//순번 Detail10 데이터셋 저장
				//this.ds_Detail10.setColumn(iLoop10, "CNTB_SE", iLoop10);
				this.ds_Detail10.setColumn(iLoop10, "CNTB_SE", iCntbSe);

				//Detail11 데이터셋에 QUTB_SE 저장
				for (var iLoop11=0; iLoop11<this.ds_Detail11.getRowCount(); iLoop11++) {
					var crudType = this.ds_Detail11.getColumn(iLoop11, "crudType");//신규대상
					
					if (crudType=='I') { 
						//Detail11 Key 비교
						if (codeKey == this.ds_Detail11.getColumn(iLoop11, "codeKey")
						 && seqKey == this.ds_Detail11.getColumn(iLoop11, "seqKey")
						 && sCntbSe == this.ds_Detail11.getColumn(iLoop11, "CNTB_SE"))
							//순번 Detail11 데이터셋 저장
							this.ds_Detail11.setColumn(iLoop11, "CNTB_SE", iCntbSe);
							
					}		
				}

			}
		}

	}
	
	
	var sPalmCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PALM_CD");
	//Detail10 데이터셋에 PK정보 입력
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		this.ds_Detail10.setColumn(iLoop, "PALM_CD", sPalmCd);
	}

	//Detail11 데이터셋에 PK정보 입력
	for (var iLoop=0; iLoop<this.ds_Detail11.getRowCount(); iLoop++) {
		this.ds_Detail11.setColumn(iLoop, "PALM_CD", sPalmCd);
	}
		
	//저장시 재조회 Key설정
	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO");//조회Key01
	this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//조회Key02
	
	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	//[공통]서비스 호출 변수
	var sInputDaset = "" //입력 데이터셋
	var sOutDatset = "";//출력 데이터셋
	var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		
	//Master 변경건이 있으면 Master 저장
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
		svcAddObj.sInsertSql = "sd:M_SD_3100_M_IN10";//저장Sql
		svcAddObj.sUpdateSql = "sd:M_SD_3100_M_UP10";//수정Sql
		svcAddObj.sDeleteSql = "sd:M_SD_3100_M_DL10";//삭제Sql
		svcAddObj.sSaveFlagColumn = "";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

		this.gfn_AddComSaveService(this, svcAddObj);
		
		sInputDaset = "ds_Master10=ds_Master10:U" //입력 데이터셋

	}
	
	//Detail10 변경건이 있으면 Detail10 저장
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object	
		svcAddObj.sInsertSql = "sd:M_SD_3100_D_IN10";//저장Sql
		svcAddObj.sUpdateSql = "sd:M_SD_3100_D_UP10";//수정Sql
		svcAddObj.sDeleteSql = "sd:M_SD_3100_D_DL10";//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		
		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = "ds_Detail10=ds_Detail10:U" //입력 데이터셋
		} else {
			sInputDaset = sInputDaset + " ds_Detail10=ds_Detail10:U" //입력 데이터셋
		}

		//저장시 재조회 Key설정
		this.fvSchKey03 = this.ds_Detail10.getColumn(this.ds_Master10.rowposition, "CNTB_SE");//조회Key03
	}		

	//Detail11 변경건이 있으면 Detail11 저장 - 견적세부상세
	if (this.gfn_DsIsUpdated(this.ds_Detail11)) {
	
		//복사한것은 변경여부에 대한 체크가 되지 않아 강제로 변경된것으로 인식하게 함
		var sCrudType = "";
		for (var iLoop=0; iLoop<this.ds_Detail11.getRowCount(); iLoop++) {
			sCrudType = this.ds_Detail11.getColumn(iLoop, "crudType");
			
			if (!this.gfn_IsNull(sCrudType)) {
				this.ds_Detail11.setColumn(iLoop, "crudPopup", sCrudType);
			}
		}
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
		svcAddObj.sInsertSql = "sd:M_SD_3100_D_IN11";//저장Sql
		svcAddObj.sUpdateSql = "sd:M_SD_3100_D_UP11";//수정Sql
		svcAddObj.sDeleteSql = "sd:M_SD_3100_D_DL11";//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		
		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = "ds_Detail11=ds_Detail11:U" //입력 데이터셋
		} else {
			sInputDaset = sInputDaset + " ds_Detail11=ds_Detail11:U" //입력 데이터셋
		}
	
	}
	
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.gfn_DsIsUpdated(this.ds_Detail11) && !this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}

	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "SAVE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_SD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = sInputDaset //입력 데이터셋
	svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
	svcCallObj.sOtherArg = sOtherArg;//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명		
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	

}

