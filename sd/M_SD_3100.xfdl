/***********************************************************************************************
 * 12.1. [사용자함수] 판관비 계산.
 ***********************************************************************************************/
this.fn_CalcAM = function(nRow) {
	
	this.ds_Detail10.set_enableevent(false);
		
	var nQt = this.ds_Detail10.getColumn(nRow, "CNTB_QT");	//수량
	var nCnthAm = this.ds_Detail10.getColumn(nRow, "WONE_AM");	//계약금액
	
	if (this.fvCurrCd != '0090') {	//외화
		//nCnthAm = this.ds_Detail10.getColumn(nRow, "FORE_AM");
	}
	
	//실행원가 선언
	//재료비
	var nTMatdAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "TMAD_AM"),0);	//직접
	var nTMatgAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "TMAG_AM"),0);	//간접
	//노무비
	var nTLabdAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "TLAD_AM"),0);
	var nTLabgAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "TLAG_AM"),0);
	//경비
	var nTCosdAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "TCOD_AM"),0);
	var nTCosgAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "TCOG_AM"),0);	
	//외주비
	var nTOutsAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "TOUT_AM"),0);

	//원가 요율이 있을때..
	if(this.ds_Cosm.getRowCount() > 0){	
		//직접비계산
		/* 재료비 - 직접*/
		// 재료비 직접은 제외
		/*
		var fRow = this.ds_Cosm.findRowExpr("FACT_CD=='0010' && DIRE_GU=='0010'");
		if(fRow >= 0){			
			//임율적용
			if(this.gfn_Nvl(this.ds_Cosm.getColumn(fRow, "COSM_RT"),0) > 0){
			nTMatdAm = nexacro.round(nCnthAm * (this.ds_Cosm.getColumn(fRow, "COSM_RT")/100),0);
			}
		}
		*/
		/* 재료비 - 간접*/
		fRow = this.ds_Cosm.findRowExpr("FACT_CD=='0010' && DIRE_GU=='0020' && PRIC_GU=='0030'");
		if(fRow >= 0){			
			//계약금액적용
			if(this.gfn_Nvl(this.ds_Cosm.getColumn(fRow, "COSM_RT"),0) > 0){
			nTMatgAm = nexacro.round(nCnthAm * (this.ds_Cosm.getColumn(fRow, "COSM_RT")/100),0);
			}
		}
		/* 노무비 - 직접*/
		fRow = this.ds_Cosm.findRowExpr("FACT_CD=='0020' && DIRE_GU=='0010'");
		if(fRow >= 0){		
			//임율적용
			if(this.gfn_Nvl(this.ds_Cosm.getColumn(fRow, "COSM_RT"),0) > 0){
			nTLabdAm = nexacro.round(nCnthAm * (this.ds_Cosm.getColumn(fRow, "COSM_RT")/100),0);
			}
		}
		/* 노무비 - 간접*/
		fRow = this.ds_Cosm.findRowExpr("FACT_CD=='0020' && DIRE_GU=='0020' && PRIC_GU=='0030'");
		if(fRow >= 0){		
			//계약금액적용
			if(this.gfn_Nvl(this.ds_Cosm.getColumn(fRow, "COSM_RT"),0) > 0){
			nTLabgAm = nexacro.round(nCnthAm * (this.ds_Cosm.getColumn(fRow, "COSM_RT")/100),0);
			}
		}
		/* 경비 - 직접*/
		fRow = this.ds_Cosm.findRowExpr("FACT_CD=='0030' && DIRE_GU=='0010'");
		if(fRow >= 0){		
			//임율적용
			if(this.gfn_Nvl(this.ds_Cosm.getColumn(fRow, "COSM_RT"),0) > 0){
			nTCosdAm = nexacro.round(nCnthAm * (this.ds_Cosm.getColumn(fRow, "COSM_RT")/100),0);
			}
		}
		/* 경비 - 간접*/
		var fRow = this.ds_Cosm.findRowExpr("FACT_CD=='0030' && DIRE_GU=='0020' && PRIC_GU=='0030'");
		if(fRow >= 0){	
			//계약금액적용
			if(this.gfn_Nvl(this.ds_Cosm.getColumn(fRow, "COSM_RT"),0) > 0){
			nTCosgAm = nexacro.round(nCnthAm * (this.ds_Cosm.getColumn(fRow, "COSM_RT")/100),0);
			}
		}
	}
	
	this.ds_Detail10.setColumn(nRow, "TMAD_AM", nTMatdAm);
	this.ds_Detail10.setColumn(nRow, "TLAD_AM", nTLabdAm);
	this.ds_Detail10.setColumn(nRow, "TCOD_AM", nTCosdAm);		
	this.ds_Detail10.setColumn(nRow, "TMAG_AM", nTMatgAm);
	this.ds_Detail10.setColumn(nRow, "TLAG_AM", nTLabgAm);
	this.ds_Detail10.setColumn(nRow, "TCOG_AM", nTCosgAm);	
	
	//판관비 선언
	var nExpAm = 0;
	var nFeeAm = 0;
	var nEtcAm = 0;
	var nManaAm = 0;
	var nDeveAm = 0;
		
	//판관비 요율이 있을때..
	if(this.ds_Pgrm.getRowCount() > 0){	
		// 0010 계약금액 0020 직접원가
		//계약금액으로만 계산
		nExpAm = nexacro.round(nCnthAm * (this.ds_Pgrm.getColumn(0,"SEXP_RT")/100),0);
		nFeeAm = nexacro.round(nCnthAm * (this.ds_Pgrm.getColumn(0,"SFEE_RT")/100),0);
		nEtcAm = nexacro.round(nCnthAm * (this.ds_Pgrm.getColumn(0,"SETC_RT")/100),0);
		nManaAm = nexacro.round(nCnthAm * (this.ds_Pgrm.getColumn(0,"MANA_RT")/100),0);
		nDeveAm = nexacro.round(nCnthAm * (this.ds_Pgrm.getColumn(0,"DEVE_RT")/100),0);
		
		
	}
	
	this.ds_Detail10.setColumn(nRow, "SEXP_AM", nExpAm);
	this.ds_Detail10.setColumn(nRow, "SFEE_AM", nFeeAm);
	this.ds_Detail10.setColumn(nRow, "SETC_AM", nEtcAm);
	this.ds_Detail10.setColumn(nRow, "MANA_AM", nManaAm);
	this.ds_Detail10.setColumn(nRow, "DEVE_AM", nDeveAm);	
		
	this.ds_Detail10.set_enableevent(true);
	
	//원가,이익,실행 율 계산.
	this.fn_CalcRt(nRow);
}

/***********************************************************************************************
 * 12.1. [사용자함수] 원가,이익,실행 율 계산.
 ***********************************************************************************************/
this.fn_CalcRt = function(nRow) {
	this.ds_Detail10.set_enableevent(false);
	
	var nQt = this.ds_Detail10.getColumn(nRow, "CNTB_QT");	//수량
	var nCnthAm = this.ds_Detail10.getColumn(nRow, "WONE_AM");	//계약금액
	
	if (this.fvCurrCd != '0090') {	//외화
		//nCnthAm = this.ds_Detail10.getColumn(nRow, "FORE_AM");
	}
	//계약금액이 0 원 이상일때..
	if(nCnthAm > 0){
		//실행원가 선언
		//재료비
		var nTMatdAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "TMAD_AM"),0);	//직접
		var nTMatgAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "TMAG_AM"),0);	//간접
		//노무비
		var nTLabdAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "TLAD_AM"),0);
		var nTLabgAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "TLAG_AM"),0);
		//경비
		var nTCosdAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "TCOD_AM"),0);
		var nTCosgAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "TCOG_AM"),0);	
		//외주비
		var nTOutsAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "TOUT_AM"),0);
			
		/* 실행단가 */		
		var nMatdAm = nTMatdAm/nQt;	//직접
		var nMatgAm = nTMatgAm/nQt;	//간접
		var nLabdAm = nTLabdAm/nQt;
		var nLabgAm = nTLabgAm/nQt;
		var nCosdAm = nTCosdAm/nQt;
		var nCosgAm = nTCosgAm/nQt;
		var nOutsAm = nTOutsAm/nQt;
		
		this.ds_Detail10.setColumn(nRow, "MATD_AM", nTMatdAm/nQt);
		this.ds_Detail10.setColumn(nRow, "MATG_AM", nTMatgAm/nQt);
		this.ds_Detail10.setColumn(nRow, "LABD_AM", nTLabdAm/nQt);
		this.ds_Detail10.setColumn(nRow, "LABG_AM", nTLabgAm/nQt);
		this.ds_Detail10.setColumn(nRow, "COSD_AM", nTCosdAm/nQt);
		this.ds_Detail10.setColumn(nRow, "COSG_AM", nTCosgAm/nQt);
		this.ds_Detail10.setColumn(nRow, "OUTS_AM", nTOutsAm/nQt);
		var nPricAm = nMatdAm + nMatgAm + nLabdAm + nLabgAm + nCosdAm + nCosgAm + nOutsAm;	
		this.ds_Detail10.setColumn(nRow, "PRIC_AM", nPricAm);
	
		//판관비 선언	
		var nExpAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "SEXP_AM"),0);
		var nFeeAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "SFEE_AM"),0);
		var nEtcAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "SETC_AM"),0);
		var nManaAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "MANA_AM"),0);
		var nDeveAm = this.gfn_Nvl(this.ds_Detail10.getColumn(nRow, "DEVE_AM"),0);
			
		
		//원가계
		var nTotaAm = nTMatdAm + nTMatgAm + nTLabdAm + nTLabgAm + nTOutsAm + nTCosdAm + nTCosgAm;
		this.ds_Detail10.setColumn(nRow, "TOTA_AM", nTotaAm);
		this.ds_Detail10.setColumn(nRow, "TOTA_RT", nexacro.round((nTotaAm / nCnthAm * 100),2));
		
		//영업이익
		var nSatAm = nExpAm + nFeeAm + nEtcAm + nManaAm + nDeveAm;	//판관비합
		this.ds_Detail10.setColumn(nRow, "PROF_AM", nCnthAm - (nTotaAm + nSatAm));
		this.ds_Detail10.setColumn(nRow, "PROF_RT", nexacro.round((nCnthAm - (nTotaAm + nSatAm)) / nCnthAm * 100,2));
		
		this.ds_Detail10.setColumn(nRow, "STOT_AM", (nTotaAm + nSatAm));
		this.ds_Detail10.setColumn(nRow, "STOT_RT", nexacro.round((nTotaAm + nSatAm) / nCnthAm * 100,2));
	}else{
		this.ds_Detail10.setColumn(nRow, "TOTA_AM", 0);
		this.ds_Detail10.setColumn(nRow, "TOTA_RT", 0);
		this.ds_Detail10.setColumn(nRow, "PROF_AM", 0);
		this.ds_Detail10.setColumn(nRow, "PROF_RT", 0);
		this.ds_Detail10.setColumn(nRow, "STOT_AM", 0);
		this.ds_Detail10.setColumn(nRow, "STOT_RT", 0);
	}
	
	this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BSAC_AM",this.ds_Detail10.getCaseSum("DETA_CD!='0050'","STOT_AM"));	//Master에 영업실행 표기
	this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BSAC_RT",nexacro.toNumber(nexacro.round(this.ds_Detail10.getCaseSum("DETA_CD!='0050'","STOT_AM")/this.ds_Detail10.getCaseSum("DETA_CD!='0050'","WONE_AM")*100 ,2),0));

	this.ds_Detail10.set_enableevent(true);
}]]></Script>
