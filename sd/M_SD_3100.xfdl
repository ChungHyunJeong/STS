/***********************************************************************************************
 * 15.10. [사용자함수] 영업실행내역 계산
 ***********************************************************************************************/
this.fn_Tpg04_Cal = function() {
	//계약금액
	var nCnthAm = nexacro.toNumber(this.ds_Detail10.getCaseSum("DETA_CD!='0050'","WONE_AM"));
	var nCnthRt = (nCnthAm>0?100:0);
	var nAm01 = nexacro.toNumber(this.ds_Detail10.getCaseSum("ITEM_CD!='Z000000001' && DETA_CD!='0050'","TMAD_AM + TMAG_AM"));
	var nAm02 = nexacro.toNumber(this.ds_Detail10.getCaseSum("ITEM_CD!='Z000000001' && DETA_CD!='0050'","TCOD_AM"));
	var nAm03 = nexacro.toNumber(this.ds_Detail10.getCaseSum("ITEM_CD!='Z000000001' && DETA_CD!='0050'","TCOG_AM"));
	var nAm04 = nexacro.toNumber(this.ds_Detail10.getCaseSum("ITEM_CD!='Z000000001' && DETA_CD!='0050'","TLAD_AM + TLAG_AM"));
	var nAm05 = nexacro.toNumber(this.ds_Detail10.getCaseSum("ITEM_CD=='Z000000001' && DETA_CD!='0050'","WONE_AM"));
	var nAmOU = nexacro.toNumber(this.ds_Detail10.getCaseSum("ITEM_CD!='Z000000001' && DETA_CD!='0050'","TOUT_AM"));
	
	this.ds_Master20.setColumn(0,"CNTH_AM", nCnthAm);
	this.ds_Master20.setColumn(0,"CNTH_RT", nCnthRt);
	this.ds_Master20.setColumn(0,"AM01", nAm01);
	this.ds_Master20.setColumn(0,"AM02", nAm02);
	this.ds_Master20.setColumn(0,"AM03", nAm03);
	this.ds_Master20.setColumn(0,"AM04", nAm04);
	this.ds_Master20.setColumn(0,"AM05", nAm05);
	this.ds_Master20.setColumn(0,"AMOU", nAmOU);
	
	this.ds_Master20.setColumn(0,"RT01", nexacro.round((nAm01/nCnthAm) * 100,1));
	this.ds_Master20.setColumn(0,"RT02", nexacro.round((nAm02/nCnthAm) * 100,1));
	this.ds_Master20.setColumn(0,"RT03", nexacro.round((nAm03/nCnthAm) * 100,1));
	this.ds_Master20.setColumn(0,"RT04", nexacro.round((nAm04/nCnthAm) * 100,1));
	this.ds_Master20.setColumn(0,"RT05", nexacro.round((nAm05/nCnthAm) * 100,1));
	this.ds_Master20.setColumn(0,"RTOU", nexacro.round((nAmOU/nCnthAm) * 100,1));
	
	this.ds_Master20.setColumn(0,"TOT01_AM", nAm01+nAm02+nAm03+nAm04+nAm05+nAmOU);
	this.ds_Master20.setColumn(0,"TOT01_RT", nexacro.round((nAm01+nAm02+nAm03+nAm04+nAm05+nAmOU)/nCnthAm * 100,1));
	
	this.ds_Master20.setColumn(0,"PR01_AM", nCnthAm - (nAm01+nAm02+nAm03+nAm04+nAm05+nAmOU));
	this.ds_Master20.setColumn(0,"PR01_RT", nexacro.round((nCnthAm - (nAm01+nAm02+nAm03+nAm04+nAm05+nAmOU)) / nCnthAm * 100,1));
	
	
	var nAm06 = nexacro.toNumber(this.ds_Detail10.getCaseSum("DETA_CD!='0050'","SEXP_AM"));
	var nAm07 = nexacro.toNumber(this.ds_Detail10.getCaseSum("DETA_CD!='0050'","SFEE_AM"));
	var nAm08 = nexacro.toNumber(this.ds_Detail10.getCaseSum("DETA_CD!='0050'","SETC_AM+MANA_AM+DEVE_AM"));
	
	this.ds_Master20.setColumn(0,"AM06", nAm06);
	this.ds_Master20.setColumn(0,"AM07", nAm07);
	this.ds_Master20.setColumn(0,"AM08", nAm08);
	
	this.ds_Master20.setColumn(0,"RT06", nexacro.round((nAm06/nCnthAm) * 100,1));
	this.ds_Master20.setColumn(0,"RT07", nexacro.round((nAm07/nCnthAm) * 100,1));
	this.ds_Master20.setColumn(0,"RT08", nexacro.round((nAm08/nCnthAm) * 100,1));
	
	var nPr01Am = nexacro.toNumber(this.ds_Master20.getColumn(this.ds_Master20.rowposition,"PR01_AM"));
	
	this.ds_Master20.setColumn(0,"TOT02_AM", nAm06+nAm07+nAm08);
	this.ds_Master20.setColumn(0,"TOT02_RT", nexacro.round((nAm06+nAm07+nAm08)/nCnthAm * 100,1));
	
	this.ds_Master20.setColumn(0,"PR02_AM", nexacro.toNumber(this.ds_Master20.getColumn(0,"PR01_AM")) - nexacro.toNumber(this.ds_Master20.getColumn(0,"TOT02_AM"))) ;
	this.ds_Master20.setColumn(0,"PR02_RT", nexacro.toNumber(this.ds_Master20.getColumn(0,"PR01_RT")) - nexacro.toNumber(this.ds_Master20.getColumn(0,"TOT02_RT"))) ;
}


/***********************************************************************************************
 * 15.10. [사용자함수] 표준원가정보조회
 ***********************************************************************************************/
this.fn_GetCosmData = function(bFlag) {
	if(this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PALM_CD"))) return;
	
	this.ds_Input.clearData();
	this.ds_Pgrm.clearData();
	this.ds_Cosm.clearData();
	
	//[업무] 조회조건 입력 : 거래처관리 대상(고객정보는 거래처 row 조회시)
