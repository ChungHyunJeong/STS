/***********************************************************************************************
 * 15.08. [포멧] 회사형태/국가 변경시 사업자번호/주민번호 포멧변경 : (2022-03-04 추가기능)
 ***********************************************************************************************/ 
this.fn_SetFormatChange = function()
{
	var sCutyCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUTY_CD"); //회사형태
	var sNatmCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "NATM_CD"); //국가

	//사업자번호 활성화/비활성화
	if (sNatmCd == "KR") {
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.set_cssclass("essential");
		
		//[결재]결재내역이 아닌경우 (2022-03-07 결재기능 추가)
		if (!this.fvWfSearchMode && !this.fvWofhStatus) {	
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.set_readonly(false);
		}
	
		//회사형태에 따른 포멧
		if (sCutyCd == '0030') {
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.set_format("######-#######");
		} else {
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.set_format("###-##-#####");
		}

		//사업자번호 이전값으로 복원
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "SAUM_NO", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "SAUM_NO"));
		
	} else {
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.set_cssclass("");	
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.set_readonly(true);
		
		//사업자번호 거래처정보로 저장
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.set_format("########");		
		if (!this.gfn_IsNull(sNatmCd)) {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "SAUM_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD"));
		}
	}

	//회사형태에 따른 타이틀 제목
	if (sCutyCd == '0030') {
		this.div_MasterInput.form.tab_Master.tpg_01.form.sta_SaumNo.set_text("주민등록번호");
	} else {
		this.div_MasterInput.form.tab_Master.tpg_01.form.sta_SaumNo.set_text("사업자번호");
	}
};

