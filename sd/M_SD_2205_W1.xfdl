			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CspmCd.set_value("");		
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CspmNm.set_value("");		
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PlacNm.set_value("");		
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_InexGu.set_value("");		
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CurrCd.set_value("");		
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_QuthNm.set_value("");		
		}
		
	//고객		
	} else if (strId == "M_CSPM_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);
			
			if (!this.gfn_IsNull(arrRtnVal.CSPM_CD)) {
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CspmCd.set_value(arrRtnVal.CSPM_CD);
