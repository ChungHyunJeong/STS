/***********************************************************************************************
 * 02.DG0.07. [Detail 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)

	//품목코드
	if (e.columnid == "ITEM_CD") {
		this.gfn_GetCommItemGrid(obj, "ITEM_CD", "ITEM_NM");//품명 검색			
				
	// 수량 입력(변경)시
	} else if (e.columnid == "QUTB_QT") {
		var iQutrQt = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUTB_QT");//수량
		var iWoneAm = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "WONE_AM");//원화금액 (상세합계)
		var iForeAm = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "FORE_AM");//외화금액 (상세합계)
		var iWonePr = "";//원화단가
		var iForePr = "";//외화단가

		//원화단가
		if (!this.gfn_IsNull(iWoneAm)) {	//원화금액 존재할 경우
			if (iWoneAm > 0) {	//원화금액 0보다 클 경우
				iWonePr = nexacro.floor(iWoneAm / iQutrQt, 2); 
			} else if (!this.gfn_IsNull(iQutrQt)) {	//수량이 존재할 경우
				iWonePr = 0; 
			
			}
		} 
		
		//외화단가
		if (!this.gfn_IsNull(iForeAm)) {	//외화금액 존재할 경우
			if (iForeAm > 0) {	//외화금액 0보다 클 경우
				iForePr = nexacro.floor(iForeAm / iQutrQt, 2); 
			} else if (!this.gfn_IsNull(iQutrQt)) {	//수량이 존재할 경우
				iForePr = 0; 
			
			}
		} 
			
		//그리드에 적용
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "WONE_PR", nexacro.floor(iWonePr,0));	//원화단가(10/18 저장시 소숫점 0 절사함)
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "FORE_PR", iForePr);	//외화단가
		
	// 원화금액 변경시
	} else if (e.columnid == "WONE_AM") {

		var iQutrQt = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUTB_QT");//수량
		var iWoneAm = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "WONE_AM");//원화금액 (상세합계)
		var iWonePr = "";//원화단가

		//원화단가
		if (!this.gfn_IsNull(iWoneAm)) {	//원화금액 존재할 경우
			if (iWoneAm > 0) {	//원화금액 0보다 클 경우
				iWonePr = nexacro.floor(iWoneAm / iQutrQt, 2); 
			} else if (!this.gfn_IsNull(iQutrQt)) {	//수량이 존재할 경우
				iWonePr = 0; 
			
			}
		} 		
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "WONE_PR", nexacro.floor(iWonePr,0));	//원화단가(10/18 저장시 소숫점 0 절사함)
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM",this.ds_Detail10.getCaseSum("crudType!='D'","WONE_AM"));	//Master에 원화금액 표기
		
	// 외화금액 변경시
	} else if (e.columnid == "FORE_AM") {
		var iQutrQt = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "QUTB_QT");//수량
		var iForeAm = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "FORE_AM");//외화금액 (상세합계)
		var iForePr = "";//외화단가
		
		//외화단가
		if (!this.gfn_IsNull(iForeAm)) {	//외화금액 존재할 경우
			if (iForeAm > 0) {	//외화금액 0보다 클 경우
				iForePr = nexacro.floor(iForeAm / iQutrQt, 2); 
			} else if (!this.gfn_IsNull(iQutrQt)) {	//수량이 존재할 경우
				iForePr = 0; 
			
			}
		} 
		
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "FORE_PR", iForePr);	//외화단가		
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_AM",this.ds_Detail10.getCaseSum("crudType!='D'","FORE_AM"));	//Master에 외화금액 표기


	}
	
};


