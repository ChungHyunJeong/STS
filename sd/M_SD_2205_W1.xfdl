/***********************************************************************************************
 * 15.06. [데이터변경] 환율정보 변경시
 ***********************************************************************************************/ 
this.fn_RatmChange  = function()
{
	//Detail11 -> Detal10 -> Master10 순서로 원화단가/원화금액, 외화단가/외화금액 적용

	var iCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐코드
	var iExchRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");//환율정보

	var iQutrQt = 0; 	//견적세부상세 수량
	var iQutbPr = 0; 	//견적세부상세 견적단가
	var iWonePr = 0;	//견적세부상세 원화단가
	var iWoneAm = 0;	//견적세부상세 원화금액
	var iForePr = 0;	//견적세부상세 외화단가
	var iForeAm = 0;	//견적세부상세 외화금액
	
	var sCommCd = "";   //회사코드
	var sQuthNo = "";   //견적번호
	var iWofhSq = 0;    //결재차수
	var sQutbSe = "";   //견적순번	

	//2. Detail10 데이터셋에 원화금액, 외화금액 변경
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		codeKey = this.ds_Detail10.getColumn(iLoop, "codeKey");//임시Key
		seqKey = this.ds_Detail10.getColumn(iLoop, "seqKey");//임시seqKey
		sQutbSe = this.ds_Detail10.getColumn(iLoop, "QUTB_SE");//견적순번
		
		//Detail11에서 조건 합계
		iWoneAm = this.ds_Detail11.getCaseSum("codeKey == '"+codeKey+"' && seqKey == '"+seqKey+"' && QUTB_SE == '"+sQutbSe+"'", "WONE_AM");//원화금액 (상세합계)
		iForeAm = this.ds_Detail11.getCaseSum("codeKey == '"+codeKey+"' && seqKey == '"+seqKey+"' && QUTB_SE == '"+sQutbSe+"'", "FORE_AM");//외화금액 (상세합계)
	
		//원화금액, 외화금액을 변경하면 "ds_Detail10_oncolumnchanged"에서 원화단가,외화단가가 변경됨
		//이중로직을 타지 않기 위해 단가 계산부분은 제외함
		this.ds_Detail10.setColumn(iLoop, "WONE_AM", iWoneAm);	//원화금액
		this.ds_Detail10.setColumn(iLoop, "FORE_AM", iForeAm);	//외화금액
		
		//Master10 적용의 경우 "ds_Detail10_oncolumnchanged"에서 처리함
	}	

}		


/***********************************************************************************************
 * 15.07. [조회] 순번 Max 가져오기 - 견적순번
 ***********************************************************************************************/ 
this.fn_SeqMaxSearch  = function()
{
	this.ds_Input.clearData();
	this.ds_MaxSeq.clearData();

	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "QUTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO"));
	this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "sd:M_SD_2200_D_SL12";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "SEQMAX_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_SD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_MaxSeq=ds_MaxSeq";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.isSync = true;//동기식으로 설정할지 여부
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);			
	
};

