/***********************************************************************************************
 * 02.MG0.07. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
			
	//  견적일자 입력(변경)시
	if (e.columnid == "QUTH_DD") {
		var iCurrCd  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐코드
		var iQuthDd  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_DD");//견적일자
		var nOrgRatm = this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "EXCH_RT");//이전 환율정보

		//화폐코드
		if (!this.gfn_IsNull(iCurrCd)) {	//화폐코드 존재할 경우
			var nRatm = this.gfn_GetCommRatm(iQuthDd, iCurrCd);//환율정보
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EXCH_RT", nRatm);	//환율정보

			//이전환율정보와 재조회된 환율정보가 서로 다를경우 Detail10, Detail11 원화, 외화 데이터 변경
			if (!this.gfn_IsNull(nOrgRatm) && nOrgRatm != nRatm) {
				this.fn_RatmChange();
			}
		} 
		
		
	// 화폐코드 변경시
	} else if (e.columnid == "CURR_CD") {
		var iCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐코드
		var iQuthDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_DD");//견적일자
		var nOrgRatm = this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "EXCH_RT");//이전 환율정보

		//견적일자
		if (!this.gfn_IsNull(iQuthDd)) {	//견적일자 존재할 경우
			var nRatm = this.gfn_GetCommRatm(iQuthDd, iCurrCd);//환율정보
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EXCH_RT", nRatm);	//환율정보

			//이전환율정보와 재조회된 환율정보가 서로 다를경우 Detail10, Detail11 원화, 외화 데이터 변경
			if (!this.gfn_IsNull(nOrgRatm) && nOrgRatm != nRatm) {
				this.fn_RatmChange();
			}
		} 		
		
	// 국내외 구분시 Option값 확인하여 2일 경우 가격조건 입력가능 및 필수값 표기
	} else if (e.columnid == "INEX_GU") {
		
		//국내외구분이 해외의 경우 가격조건 true
		this.fn_SetPricCdCssclass();		
	}
};


