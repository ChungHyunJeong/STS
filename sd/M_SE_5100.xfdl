 ***********************************************************************************************/ 
this.fn_ComBtnAdd = function()
{
	
	//데이터 변경여부 체크
	if (this.fn_IsUpdateDataset(this.ds_Master10)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}

	this.div_MasterInput.form.tab_Master.set_tabindex(0);
		
	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	
	var iAddRow = this.ds_Master10.addRow();
	
	this.fvMmCloseDay = sToDay;//[월마감] 마감일자	
	
	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//INVOICE부서코드	
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//INVOICE부서명	
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//INVOICE사번	
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//INVOICE자명		
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//INVOICE자ID	
	this.ds_Master10.setColumn(iAddRow, "INVH_DD", sToDay);//INVOICE일자-오늘날자로 설정
	
	
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.set_readonly(false);
	this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.set_readonly(false);
	this.fvFileDiv.fn_SetCopmGridAuth(true);
	this.fvMmCloseYn = false;//월마감여부-Tab버튼 제어용
	this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PdrhDd.set_readonly(false);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cal_BolhDd.set_readonly(false);
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_LochNo.set_readonly(false);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_StarCd.set_enable(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ArriCd.set_enable(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_VessNm.set_readonly(false);
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_TradNm.set_readonly(false);
	this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(false);	

	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.fn_SetGridInputButtonEnable();
	this.div_DetailInput.form.tab_Detail.set_enable(true);	
	
	this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PdrhDd.set_enable(true);
	
	//처음 입력 항목에 포커스
	this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PdrhDd.setFocus();
	
	this.ds_Detail10.clearData();//상세정보 초기화
	this.ds_Detail20.clearData();//검색정보 초기화
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
	//첨부파일정보 권한설정
	var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
	oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
	this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
	
	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
	this.div_DetailInput.form.tab_Detail.set_tabindex(1);
	
}

