/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {	
		
		//INVOICENo정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "INVH_NO"))) {
			//[월마감제어-버튼/상세그리드]
			var sBUDG_MM = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "INVH_DD"), "");//마감년월
			
			//마감여부 체크
			var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
			var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
			paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
			paramObj.MODM_CD = "SD";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
			paramObj.ACUM_CD = "";//[선택] 회계단위
			paramObj.BUSM_CD  = "";//[선택] 사업부문
			paramObj.SAUM_CD  = "";//[선택] 사업장코드
			paramObj.DEPM_CD  = "";//[선택] 부서코드
			paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
			paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
			
			var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		

			//월마감된 경우
			if (bCheck) {
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.set_readonly(true);
				this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.set_readonly(true);
				this.fvFileDiv.fn_SetCopmGridAuth(false);			
				this.fvMmCloseYn = true;//월마감여부-Tab버튼 제어용
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PdrhDd.set_readonly(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_BolhDd.set_readonly(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_LochNo.set_readonly(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_StarCd.set_enable(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ArriCd.set_enable(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_VessNm.set_readonly(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_TradNm.set_readonly(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(true);				
			} else {
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.set_readonly(false);
				this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.set_readonly(false);
				this.fvFileDiv.fn_SetCopmGridAuth(true);
				this.fvMmCloseYn = false;//월마감여부-Tab버튼 제어용
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PdrhDd.set_readonly(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_BolhDd.set_readonly(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_LochNo.set_readonly(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_StarCd.set_enable(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ArriCd.set_enable(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_VessNm.set_readonly(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_TradNm.set_readonly(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(false);		
			}		
			
			//전화번호 포멧설정
			//this.gfn_TelFormat(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ATEL_NO"),this.div_MasterInput.form.tab_Master.tpg_01.form.mae_AtelNo);		
			
			//입력영역 버튼 비활성화
			//this.fn_SetGridInputButtonEnable();
			
			//INVOICE일자, INVOICE창고 비활성화
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PdrhDd.set_enable(false);			
			
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "INVH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "INVH_NO"));			
								
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "sd:M_SE_5100_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);
			
//			this.alert(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "INVH_NO"));
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_SD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			//svcCallObj.sActionName = "";//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조			
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);	
			
		}	
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master10] Row 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10) || this.gfn_DsIsUpdated(this.ds_Detail10)) {
		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	

	return true;
};  


/*---------------------------------------------------------------------------------------------
 *    03. [데이터셋-ds_Master10] 컬럼 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/

this.ds_Master10_cancolumnchange = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//[월마감제어-등록/수정]
	if (e.columnid == "INVH_DD") {
		var sBUDG_MM = "";//마감년월
		
		if (!this.gfn_IsNull(this.fvMmCloseDay)) {
			sBUDG_MM = this.gfn_Nvl(this.fvMmCloseDay, "");//마감년월
		} else {
			sBUDG_MM = this.gfn_Nvl(e.newvalue, "");//마감년월
		}
		
		this.fvMmCloseDay = "";

		//마감여부 체크
		var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
		var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
		paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
		paramObj.MODM_CD = "SD";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
		paramObj.ACUM_CD = "";//[선택] 회계단위
		paramObj.BUSM_CD  = "";//[선택] 사업부문
		paramObj.SAUM_CD  = "";//[선택] 사업장코드
		paramObj.DEPM_CD  = "";//[선택] 부서코드
		paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
		paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
		
		var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		

		//월마감된 경우
		if (bCheck) {
			this.gfn_Alert("CA025");//해당월의 정보가 마감이 되어서 등록/수정 할 수 없습니다.
			
			var sOrgDate = this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "INVH_DD");//변경전값
			
			//변경전값이 없을경우
			if (this.gfn_IsNull(sOrgDate)) {				
				var sToDay = this.gfn_GetCommDate("toDay");//현재일자
				
				if (sBUDG_MM.substr(0,6) == sToDay.substr(0,6)) {
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "INVH_DD", "");//입력날짜가 마감된경우 오늘날짜로 세팅				
				} else {
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "INVH_DD", sToDay);//입력날짜가 마감된경우 오늘날짜로 세팅
				}
			} else {
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "INVH_DD", sOrgDate);//입력날짜가 마감된경우 변경전 값으로 세팅
			}
			
			return false;
		} else {
			return true;
		}

	}		
};

