this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{		
	if(e.columnid == "INVB_QT") 
	{
		/*var CntbPr    = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "CNTB_PR");*/
		var CntbPr    = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "FORE_PR");
		
		var InvoiceAm = nexacro.floor(CntbPr * e.newvalue,2);
		
		this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "INVB_AM", InvoiceAm );
	}
	else if (e.columnid == "s_chk") {
		if (this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "s_chk") == '1') {		
			
			var sCNTH_NO = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "CNTH_NO");
			var sCNTB_SE = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "CNTB_SE");
			
			//Detail20에 계약번호가 최초 1건만 체크됐을 경우에만 처리함(2건일 경우 return)
			var iChkCnt = this.ds_Detail20.getCaseCount("CNTH_NO == '"+sCNTH_NO+"' && s_chk == '1'");  //체크건수		
			
			if (iChkCnt > 1) return;
			
			//최초 1건만 체크시 로직 처리
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				//Detail10 중복여부 체크	

				iDupCnt = this.ds_Detail10.getCaseCount("CNTH_NO == '"+sCNTH_NO);  
			
				//계약번호가 같고 Detail10에 계약번호가 미존재하고 색상변경이 안된 대상				
				if (this.ds_Detail20.getColumn(iLoop, "CNTH_NO") == sCNTH_NO
				 && iDupCnt == 0
				 && (this.gfn_IsNull(this.ds_Detail20.getColumn(iLoop, "s_conf")) || this.ds_Detail20.getColumn(iLoop, "s_conf") == '0')) {
								
					//체크박스 Check
					this.ds_Detail20.setColumn(iLoop, "s_chk", "1");//Check로 변경					
				}
			}
		}
	}
}; 

