/***********************************************************************************************
 * 02.DG1.05. [Detail그리드2-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Choice = function()
{
	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {
			
			var sCnthNoChk;
			var sCusmCd;
			var sCurrCd;
			var sPricCd;
			
			//거래처, 화폐, 가격조건 추가 체크			
			if (this.ds_Detail10.rowcount > 0) {
				var strMirhNo = this.ds_Detail10.getColumn(0, "INVH_NO");
				var sCusmCd   = this.ds_Detail10.getColumn(0, "CUSM_CD");
				var sCurrCd   = this.ds_Detail10.getColumn(0, "CURR_CD");
				var sPricCd   = this.ds_Detail10.getColumn(0, "PRIC_CD");
			}
						
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					
					if (!this.gfn_IsNull(sCusmCd)){
						if (sCusmCd != this.ds_Detail20.getColumn(iLoop, "CUSM_CD") || sCurrCd != this.ds_Detail20.getColumn(iLoop, "CURR_CD") || sPricCd != this.ds_Detail20.getColumn(iLoop, "PRIC_CD")) {
							var arrMsgArg = ["거래처, 화폐, 가격조건"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 [거래처, 화폐, 가격조건]만 적용 가능합니다.
							return;						
						}
					}
					
					//그리드내 거래처, 화폐, 가격조건 체크
					if(!this.gfn_IsNull(sCnthNoChk) ){							
						if (sCusmCd != this.ds_Detail20.getColumn(iLoop, "CUSM_CD") || sCurrCd != this.ds_Detail20.getColumn(iLoop, "CURR_CD") || sPricCd != this.ds_Detail20.getColumn(iLoop, "PRIC_CD")) {
							var arrMsgArg = ["거래처, 화폐, 가격조건"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 [거래처, 화폐, 가격조건]만 적용 가능합니다.
							return;
						}
					}
					
					sCnthNoChk = this.ds_Detail20.getColumn(iLoop, "CNTH_NO");
					sCusmCd    = this.ds_Detail20.getColumn(iLoop, "CUSM_CD");
					sCurrCd    = this.ds_Detail20.getColumn(iLoop, "CURR_CD");
					sPricCd    = this.ds_Detail20.getColumn(iLoop, "PRIC_CD");
					
					//INVOICE수량 수량 체크 
					if(this.ds_Detail20.getColumn(iLoop, "INVB_QT") == 0){
						var arrMsgArg = ["INVOICE수량"];
						this.gfn_Alert("SM055", arrMsgArg); //[수INVOICE수량량]을(를) 1 이상 입력해 주세요.
						return;
					}
					if(this.ds_Detail20.getColumn(iLoop, "INVB_QT") > this.ds_Detail20.getColumn(iLoop, "MI_INVB_QT") ){
						var arrMsgArg = ["Invoice수량","미Invoice수량"];
						this.gfn_Alert("SM056", arrMsgArg); //[INVB_QT]이(가) [계약수량]을(를) 초과할 수 없습니다.
						return;
					}
				}
			}
			
			var arrCheckRow = [];//체크된 Row
			var sCOMM_CD = "";//회사코드
			var sCNTH_NO = "";//계약번호
			var sCNTB_SE = "";//계약순번
			var sCNTC_SE = "";//계약상세순번
	
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {

					//검색값설정 - 컬럼명:검색값					
					var objSearchList = ["CNTH_NO|" + this.ds_Detail20.getColumn(iLoop, "CNTH_NO")										
										,"WOFH_SQ|" + this.ds_Detail20.getColumn(iLoop, "WOFH_SQ")
										,"CNTB_SE|" + this.ds_Detail20.getColumn(iLoop, "CNTB_SE")
										];					
										
					//[공통] 중복건 체크후 해당건 Skip
					if (!this.gfn_DsSchDupcheck(this.ds_Detail10, objSearchList)) {

						var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)
						//계약상세검색에서 invoice상세로 이동
						
						this.ds_Detail10.copyRow(iAddRow, this.ds_Detail20, iLoop);
						
						this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD")); //회사코드
						this.ds_Detail10.setColumn(iAddRow, "INVH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "INVH_NO")); //INVOICE번호						
						this.ds_Detail10.setColumn(iAddRow, "CNTH_SQ", this.ds_Detail20.getColumn(iLoop, "WOFH_SQ")); //결재차수
						this.ds_Detail10.setColumn(iAddRow, "CNTH_SE", this.ds_Detail20.getColumn(iLoop, "CNTB_SE")); //순번						
	
						this.ds_Detail10.setColumn(iAddRow, "s_chk", "0");
												
						//첫번째일 경우 마스터 업데이트 
						if(this.ds_Detail10.rowcount == 1 ){  
							
							this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CUSM_CD", this.ds_Detail20.getColumn(iLoop,"CUSM_CD"));	//거래처코드
							this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CUSM_NM", this.ds_Detail20.getColumn(iLoop,"CUSM_NM"));	//거래처명							
							this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CURR_CD", this.ds_Detail20.getColumn(iLoop,"CURR_CD"));	//화폐
							this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PRICCD",  this.ds_Detail20.getColumn(iLoop,"PRICCD"));	//가격조건
							this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EXPT_YN",  this.ds_Detail20.getColumn(iLoop,"EXPT_YN"));	//실적제외여부
							this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EXPT_RE",  this.ds_Detail20.getColumn(iLoop,"EXPT_RE"));	//실적제외사유
						}
				
						arrCheckRow.push(iLoop );
						
						//선택항목 비활성화처리
						this.ds_Detail20.setColumn(iLoop, "s_conf", "1");	//계약상세검색 컬러 변경 구분자 업데이트						
						this.ds_Detail20.setColumn(iLoop, "s_chk", "0");	//Uncheck로 변경
					}
				}
			}
			
			this.ds_Detail20.set_rowposition(-1);
			//invoice상세로 Tab이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);
		}
	}
	//공급가액, 부가세
	this.fn_GetSum();
	
};

