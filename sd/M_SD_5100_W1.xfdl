 /*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Detail10] 환율계산 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.fn_Reckoning = function(){
			
			var VatmRt = this.ds_FaVatm.getColumn(this.ds_FaVatm.rowposition,"VATM_RT");//부가세 요율
			var ExchRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");//  환율 
			
			for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
				if(this.ds_Detail10.getColumn(iLoop, "crudType") != "D"){//삭제 아닌것만
					var WoneAm = this.ds_Detail10.getColumn(iLoop, "WONE_AM")	
					var ForeAm = this.ds_Detail10.getColumn(iLoop, "FORE_AM")
					if(this.ds_Master10.getColumn(this.ds_Master10.rowposition,"CURR_CD") =="0090"){//원화
						this.ds_Detail10.setColumn(iLoop, "WONE_PR", WoneAm);	//단가	
						this.ds_Detail10.setColumn(iLoop, "WONE_AM", WoneAm);	//금액
						this.ds_Detail10.setColumn(iLoop, "FORE_PR", 0);		//외화단가		
						this.ds_Detail10.setColumn(iLoop, "FORE_AM", 0);		//외화금액	
						//직수출이 아닌경우만 계산
						if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "TARE_GU") != "0020"){
							this.ds_Detail10.setColumn(iLoop, "WONE_VA", nexacro.floor(VatmRt * ExchRt * WoneAm,2));	//부가세
						}else{
							this.ds_Detail10.setColumn(iLoop, "WONE_VA", 0);	//부가세
						}
						this.ds_Detail10.setColumn(iLoop, "FORE_VA", 0);		//외화부가세
						
					}else{//외화
						this.ds_Detail10.setColumn(iLoop, "WONE_PR",  nexacro.floor(ExchRt * ForeAm,2));	//단가	
						this.ds_Detail10.setColumn(iLoop, "WONE_AM",  nexacro.floor(ExchRt * ForeAm,2));	//금액
						this.ds_Detail10.setColumn(iLoop, "FORE_PR", ForeAm);	//외화단가		
						this.ds_Detail10.setColumn(iLoop, "FORE_AM", ForeAm);	//외화금액	
						
						//직수출이 아닌경우만 계산
						if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "TARE_GU") != "0020"){
							this.ds_Detail10.setColumn(iLoop, "WONE_VA", nexacro.floor(VatmRt * ExchRt * ForeAm,2));	//부가세
							this.ds_Detail10.setColumn(iLoop, "FORE_VA", nexacro.floor(VatmRt * ForeAm,2));				//외화부가세
						}else{
							this.ds_Detail10.setColumn(iLoop, "WONE_VA", 0);	//부가세
							this.ds_Detail10.setColumn(iLoop, "FORE_VA", 0);	//외화부가세
						}
					}
				}
			}
			//마스터 원화, 외화 ,부가세 합산 Master10 에 삽입
			//trace("원화"+this.ds_Detail10.getSum("WONE_AM") + " / "+ this.ds_Master10.getColumn(this.ds_Master10.rowposition,"CURR_CD") );
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM", this.ds_Detail10.getCaseSum("crudType !='D'","WONE_AM"));
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FORE_AM", this.ds_Detail10.getCaseSum("crudType !='D'","FORE_AM"));

			//직수출이 아닌경우만 계산
			if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "TARE_GU") != "0020"){
				if( this.ds_Master10.getColumn(this.ds_Master10.rowposition,"CURR_CD") =="0090"){//원화
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "VTAX_AM", this.ds_Detail10.getCaseSum("crudType !='D'","WONE_VA"));
				}else{
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "VTAX_AM", this.ds_Detail10.getCaseSum("crudType !='D'","FORE_VA"));
				}
			}else{
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "VTAX_AM", 0);
			}
			
			//기존데이터 삭제용 DataSet 설정
			var detail40DelYN = false;
			if(this.ds_Detail40.getRowCount() > 0){
				var detail40DelYN = true;
			}
			
			this.ds_Detail40.clearData();
			this.ds_Detail41.clearData();
			
			//세금계산서 연계 계산
			for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
				//trace("Detail10Count------->"+ this.ds_Detail10.getRowCount());
				if(this.ds_Detail10.getColumn(iLoop, "crudType") != "D"){//삭제 아닌것만
					if(this.ds_Master10.getColumn(this.ds_Master10.rowposition,"CURR_CD") =="0090"){//원화
						var AddnAm = this.ds_Detail10.getColumn(iLoop, "WONE_AM")
					}else{
						var AddnAm = this.ds_Detail10.getColumn(iLoop, "FORE_AM")
					}
						
					//세금계산서연계내역 추가
					if(iLoop <= 2){//3 ROW 까지만..
						//trace("3로우이하------->"+ iLoop);
						var iAddRow40 = this.gfn_GridInsertProcess(this, this.ds_Detail40);//crud_flag 변경-입력(I)
						this.ds_Detail40.setColumn(iAddRow40,"SALH_NO", this.ds_Detail10.getColumn(iLoop,"SALH_NO"));	//매출번호
						this.ds_Detail40.setColumn(iAddRow40,"WOFH_SQ", this.ds_Detail10.getColumn(iLoop,"WOFH_SQ"));	//결재차수
						this.ds_Detail40.setColumn(iAddRow40,"NAME_NM", this.ds_Detail10.getColumn(iLoop,"CNTH_NM"));	//계약명
						//this.ds_Detail40.setColumn(iAddRow40,"SPEC_NM", this.ds_Detail20.getColumn(iLoop,"SPEC_NM"));	//규격
						
						this.ds_Detail40.setColumn(iAddRow40,"SALE_QT", 1);	//수량
						
						if(this.ds_Master10.getColumn(this.ds_Master10.rowposition,"CURR_CD") =="0090"){//원화
							
							this.ds_Detail40.setColumn(iAddRow40,"WONE_PR", AddnAm);	//원화단가(수량이 1이므로 원화금액과 동일)
							this.ds_Detail40.setColumn(iAddRow40,"WONE_AM", AddnAm);	//원화금액(환율 * [미매출검색]의 매출반영금액)
							this.ds_Detail40.setColumn(iAddRow40,"WONE_VA", nexacro.floor(VatmRt * ExchRt * AddnAm,2));	//원화부가세금액 ([매출내역]의 부가세구분의 요율 * 환율 * [미매출검색])
							this.ds_Detail40.setColumn(iAddRow40,"FORE_PR", 0);	//외화단가(0)
							this.ds_Detail40.setColumn(iAddRow40,"FORE_AM", 0);	//외화금액(0)
							this.ds_Detail40.setColumn(iAddRow40,"FORE_VA", 0);	//외화부가세금액(0)
						}else{ 
							
							this.ds_Detail40.setColumn(iAddRow40,"WONE_PR", nexacro.floor(ExchRt * AddnAm,2));	//원화단가원화단가(수량이 1이므로 원화금액과 동일)
							this.ds_Detail40.setColumn(iAddRow40,"WONE_AM", nexacro.floor(ExchRt * AddnAm,2));	//원화금액(환율 * [미매출검색]의 매출반영금액)
							this.ds_Detail40.setColumn(iAddRow40,"WONE_VA", nexacro.floor(VatmRt * ExchRt * AddnAm,2));	//원화부가세금액
							this.ds_Detail40.setColumn(iAddRow40,"FORE_PR", AddnAm);	//외화단가(수량이 1이므로 원화금액과 동일)
							this.ds_Detail40.setColumn(iAddRow40,"FORE_AM", AddnAm);	//외화금액(환율 * [미매출검색]의 매출반영금액)
							this.ds_Detail40.setColumn(iAddRow40,"FORE_VA", nexacro.floor(VatmRt * AddnAm,2));	//외화부가세금액
						}
					}else{//4 ROW 이후 ds_Detail41 에 넣고 추후 합산 하여 ds_Detail40 4 ROW로 카피
						//trace("4로우이상------->"+ iLoop);
						var iAddRow41 = this.gfn_GridInsertProcess(this, this.ds_Detail41);//crud_flag 변경-입력(I)
						this.ds_Detail41.setColumn(iAddRow41,"NAME_NM", this.ds_Detail20.getColumn(iLoop,"CNTH_NM"));	//계약명
						//this.ds_Detail41.setColumn(iAddRow41,"SPEC_NM", this.ds_Detail20.getColumn(iLoop,"SPEC_NM"));	//규격
						this.ds_Detail41.setColumn(iAddRow40,"SALE_QT", 1);	//수량
						if(this.ds_Master10.getColumn(this.ds_Master10.rowposition,"CURR_CD") =="0090"){//원화
							this.ds_Detail41.setColumn(iAddRow41,"WONE_PR", AddnAm);	//원화단가(수량이 1이므로 원화금액과 동일)
							this.ds_Detail41.setColumn(iAddRow41,"WONE_AM", AddnAm);	//원화금액(환율 * [미매출검색]의 매출반영금액)
							this.ds_Detail41.setColumn(iAddRow41,"WONE_VA", nexacro.floor(VatmRt * ExchRt * AddnAm,2));	//원화부가세금액 ([매출내역]의 부가세구분의 요율 * 환율 * [미매출검색])
							this.ds_Detail41.setColumn(iAddRow41,"FORE_PR", 0);	//외화단가(0)
							this.ds_Detail41.setColumn(iAddRow41,"FORE_AM", 0);	//외화금액(0)
							this.ds_Detail41.setColumn(iAddRow41,"FORE_VA", 0);	//외화부가세금액(0)
						}else{ 
							this.ds_Detail41.setColumn(iAddRow41,"WONE_PR", nexacro.floor(ExchRt * AddnAm,2));	//원화단가원화단가(수량이 1이므로 원화금액과 동일)
							this.ds_Detail41.setColumn(iAddRow41,"WONE_AM", nexacro.floor(ExchRt * AddnAm,2));	//원화금액(환율 * [미매출검색]의 매출반영금액)
							this.ds_Detail41.setColumn(iAddRow41,"WONE_VA", nexacro.floor(VatmRt * ExchRt * AddnAm,2));	//원화부가세금액
							this.ds_Detail41.setColumn(iAddRow41,"FORE_PR", AddnAm);	//외화단가(수량이 1이므로 원화금액과 동일)
							this.ds_Detail41.setColumn(iAddRow41,"FORE_AM", AddnAm);	//외화금액(환율 * [미매출검색]의 매출반영금액)
							this.ds_Detail41.setColumn(iAddRow41,"FORE_VA", nexacro.floor(VatmRt * AddnAm,2));	//외화부가세금액
						}
					}
				}
			}
			
			//세금계산서연계내역 합산 추가 (4row 이상일 경우)
			if(iLoop > 3){ 
				var iAddRow40 = this.gfn_GridInsertProcess(this, this.ds_Detail40);//crud_flag 변경-입력(I)
				this.ds_Detail40.setColumn(iAddRow40,"SALH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition,"SALH_NO"));	//매출번호
				this.ds_Detail40.setColumn(iAddRow40,"WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition,"WOFH_SQ"));	//결재차수
				
				if(this.ds_Detail41.getRowCount() == 1){
					this.ds_Detail40.setColumn(iAddRow40,"NAME_NM", this.ds_Detail41.getColumn(0,"NAME_NM"));	//계약명
				}else{
					this.ds_Detail40.setColumn(iAddRow40,"NAME_NM", this.ds_Detail41.getColumn(0,"NAME_NM") +" 외" +  (this.ds_Detail41.getRowCount()-1) +"건");	//계약명
				}
				
