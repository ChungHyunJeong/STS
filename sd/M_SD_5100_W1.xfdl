 /*---------------------------------------------------------------------------------------------
 *    04. [데이터셋-ds_Detail40] 선수금 상세 체크
  ---------------------------------------------------------------------------------------------*/  
  this.fn_DetailInput_tab_Detail_tpg_03_check = function()
{
	//선수금 저장타입변경
	this.ds_Detail30.setColumn(0, "dType", "D"); //삭제
	
	for (var iLoop=0; iLoop<this.ds_Detail30.getRowCount(); iLoop++) {
	
		//선수금 체크
		var AmTotal = 0;
		var AmAddnAm = 0;
		if(this.ds_Master10.getColumn(this.ds_Master10.rowposition,"CURR_CD") =="0090"){//원화
			AmTotal = this.ds_Detail10.getCaseSum("crudType !='D' && CNTH_NO == '" + this.ds_Detail30.getColumn(iLoop, "CNTH_NO")+"'", "WONE_AM");
			AddnAm = this.ds_Detail30.getColumn(iLoop, "ADDN_AM");
		}else{
			AmTotal = this.ds_Detail10.getCaseSum("crudType !='D' && CNTH_NO == '" + this.ds_Detail30.getColumn(iLoop, "CNTH_NO")+"'", "FORE_AM");
			AddnAm = this.ds_Detail30.getColumn(iLoop, "ADDN_AM");
		}
		
		//trace("매출상세 합계"+AmTotal);
		//trace("선수금 금액"+AddnAm);
		if(AddnAm > AmTotal){
			return false;
		}
		
		//선수금 저장타입변경
		if (this.ds_Detail30.getColumn(iLoop, "s_chk") == "1") {
			this.ds_Detail30.setColumn(iLoop, "crudType", "I");
		}else{
			this.ds_Detail30.setColumn(iLoop, "crudType", "");
		}
	}
	return true;
}

