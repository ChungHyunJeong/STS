	this.gfn_InitComService(this);
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_TemhDtFr.value); //기준일자
//	this.ds_Input.setColumn(0, "CLOS_DD", this.div_Search.form.cal_TemhDtTo.value);
	this.ds_Input.setColumn(0, "CNTH_NO", this.div_Search.form.mae_CnthNo.value); // 계약번호
	this.ds_Input.setColumn(0, "CUSM_CD", this.div_Search.form.edt_CusmCd.value); // 거래처코드
	this.ds_Input.setColumn(0, "CUSM_NM", this.div_Search.form.edt_CusmNm.value); // 거래처명
	this.ds_Input.setColumn(0, "DEPM_CD", this.div_Search.form.edt_DepmCd.value); // 부서코드
	this.ds_Input.setColumn(0, "DEPM_NM", this.div_Search.form.edt_DepmNm.value); // 부서명
