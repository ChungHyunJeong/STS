		//trace("iRow : " + iRow);
        var PjthNo = this.ds_Master10.getColumn(iRow, "PJTH_NO");//프로젝트번호
		//trace("PjthNo : " + PjthNo);
		if (this.gfn_IsNull(PjthNo)) {
			this.gfn_Alert("SD111");//선택된 프로젝트번호가 없습니다.\n프로젝트를 선택한 후 작업하세요.
			return false;
		} else {
			this.popCallSd1200(this.ds_Master10.rowposition);
		}
	}	
};

