this.popCallSd1200 = function(paramDs, paramRow) {
	var PjthNo = eval("this."+paramDs).getColumn(paramRow, "PJTH_NO");//프로젝트 번호
	var TemhDtFr = '11111111';//기준일자
	//var TemhDtTo = this.div_Search.form.cal_TemhDtFr.value;//기준일자
	var TemhDtTo = '99991111';//기준일자
	var DepmCd = this.div_Search.form.edt_DepmCd.value;//부서
	var FromDd = this.ds_Input.getColumn(0, "FROM_DD");
	
	var abc_gubun = "";
	var sTabIdx = this.div_MasterView.form.tab_Master.tabindex;
	
	switch(parseInt(sTabIdx)) {
		case 0:
			var sSelectedCol = this.div_MasterView.form.tab_Master.tpg_01.form.grd_Master10.selectendcol;
			var sSelectedColText = this.div_MasterView.form.tab_Master.tpg_01.form.grd_Master10.getCellText(paramRow, sSelectedCol);
			if(sSelectedColText <= 0) { return; }
			if(parseInt(sSelectedCol) < 9 || parseInt(sSelectedCol) > 20) { return; }
			abc_gubun = "A";
		break;
		case 1:
			var sSelectedCol = this.div_MasterView.form.tab_Master.tpg_02.form.grd_Master20.selectendcol;
			var sSelectedColText = this.div_MasterView.form.tab_Master.tpg_02.form.grd_Master20.getCellText(paramRow, sSelectedCol);
			if(sSelectedColText <= 0) { return; }		
			if(parseInt(sSelectedCol) < 9 || parseInt(sSelectedCol) > 20) { return; }
			abc_gubun = "B";
		break;
		case 2:
			var sSelectedCol = this.div_MasterView.form.tab_Master.tpg_03.form.grd_Master30.selectendcol;
			var sSelectedColText = this.div_MasterView.form.tab_Master.tpg_03.form.grd_Master30.getCellText(paramRow, sSelectedCol);
			if(sSelectedColText <= 0) { return; }		
			if(parseInt(sSelectedCol) < 9 || parseInt(sSelectedCol) > 20) { return; }
			abc_gubun = "C";
		break;
		case 3:
			var sSelectedCol = this.div_MasterView.form.tab_Master.tpg_04.form.grd_Master40.selectendcol;
			var sSelectedColText = this.div_MasterView.form.tab_Master.tpg_04.form.grd_Master40.getCellText(paramRow, sSelectedCol);
				if(sSelectedColText <= 0) { return; }
				switch(parseInt(sSelectedCol)) {
					case 11:	case 12:	case 17:	case 18:	case 23:	case 24:	case 29:	case 30:	case 35:	case 36:	case 41:	case 42:	case 47:	case 48:	case 53:	case 54:	case 59:	case 60:	case 65:	case 66:	case 71:	case 72:	case 77:	case 78: // 수주
						abc_gubun = "A";
					break;
					case 13:	case 14:	case 19:	case 20:	case 25:	case 26:	case 31:	case 32:	case 37:	case 38:	case 43:	case 44:	case 49:	case 50:	case 55:	case 56:	case 61:	case 62:	case 67:	case 68:	case 73:	case 74:	case 79:	case 80: // 매출
						abc_gubun = "B";
					break;
					case 15:	case 16:	case 21:	case 22:	case 27:	case 28:	case 33:	case 34:	case 39:	case 40:	case 45:	case 46:	case 51:	case 52:	case 57:	case 58:	case 63:	case 64:	case 69:	case 70:	case 75:	case 76:	case 81:	case 82: // 수금
						abc_gubun = "C";
					break;
					default:
						return;
					break;
				}
		break;		
	}
	
	trace("abc_gubun --> " + abc_gubun);
		
	var sPopupId = "M_SD_1200";
	var sPopupTitle = "수매수계획관리"; //Popup화면 타이틀
	var sPopupFormUrl  = "sd::M_SD_1200.xfdl"; //Popup화면 URL
	var oArg = {paramTitle:sPopupTitle, paramPjthNo:PjthNo, paramTemhDtFr:TemhDtFr, paramTemhDtTo:TemhDtTo, paramDepmCd:DepmCd, paramFromDd:FromDd, paramAbcGubun:abc_gubun}; //Popup화면 전달 파라메터
	var oOption = {title:sPopupTitle, width:"1400", height:"800", popuptype:"TitlePopup"}; //Popup화면 옵션
	var sPopupCallBack = "fn_PopupCallback"; //Popup화면 호출후 Callback 처리 함수

	this.gfn_OpenPopup(sPopupId, sPopupFormUrl, oArg, sPopupCallBack, oOption); //Popup화면 호출 함수
}

