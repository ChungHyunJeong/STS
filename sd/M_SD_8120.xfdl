this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회

			if (this.ds_Master10.getRowCount() > 0) {

				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterView.form.tab_Master.tpg_01.form.grd_Master10);
				
				// pivot 셋팅 ( 탭 페이지가 하나라도 반드시 명명규칙을 따를것 )
				// 명명규칙 : tab_Master : this.div_MasterView.form.tab_Master,     dataset : ds_Master10,     grid : grd_Master10,		grid 첫 컬럼은 No. : currow+1
				//this.gfn_InitPivot();
				
			} else if (this.ds_Master20.getRowCount() > 0) {
				this.gfn_InitGridNoDataText(this.div_MasterView.form.tab_Master.tpg_02.form.grd_Master20);
			} else if (this.ds_Master30.getRowCount() > 0) {
				this.gfn_InitGridNoDataText(this.div_MasterView.form.tab_Master.tpg_03.form.grd_Master30);
			} else if (this.ds_Master40.getRowCount() > 0) {
				this.gfn_InitGridNoDataText(this.div_MasterView.form.tab_Master.tpg_04.form.grd_Master40);
			} else if (this.ds_Master50.getRowCount() > 0) {
				//this.gfn_InitGridNoDataText(this.div_MasterView.form.tab_Master.tpg_05.form.grd_Master50);
				//this.ds_Master50.set_keystring("G:PJTH_NO+PJTB_SE+PJTH_NM+CNTH_NO");  // SUB SUM 
			} else if (this.ds_Master60.getRowCount() > 0) {
				this.gfn_InitGridNoDataText(this.div_MasterView.form.tab_Master.tpg_06.form.grd_Master60);
			} else {
				
				//[공통]그리드NoData Text설정
				//this.gfn_SetGridNoDataText(this.div_MasterView.form.tab_Master.tpg_01.form.grd_Master10);
				
			}		
			break;
		case "COMBO_SEARCH": //콤보조회		
// 			if (this.ds_FaSaum.getRowCount() > 0) {
