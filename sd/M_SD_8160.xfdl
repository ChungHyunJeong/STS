this.grdSetColSize = function(grdId, SearchGubun) {

	
	var grdHeadValue = "";
	var headMergeCnt = 3;
	
	// 일단 초기화하고
	for(var k=0; k<grdId.getCellCount("head");k++) {

		grdHeadValue = grdId.getHeadValue(k);

		switch(grdHeadValue) {

			case "계획금액": case "잔여예상금액": case "예상금액": case "차이금액": case "실적금액": case "계획일자": case "실적일자":
				grdId.setRealColSize("head", k-headMergeCnt, 120);
			break;

		}

	}

	
	switch(SearchGubun) {
		case "SILJUK":
			for(var k=0; k<grdId.getCellCount("head");k++) {
				grdHeadValue = grdId.getHeadValue(k);
				switch(grdHeadValue) {
					case "계획금액": case "잔여예상금액": case "예상금액": case "차이금액": case "계획일자":
						grdId.setRealColSize("head", k-headMergeCnt, 0);
						trace("grdHeadValue ----> " + grdHeadValue);
					break;
				}
			}
		break;
		case "EXPECT":
			for(var k=0; k<grdId.getCellCount("head");k++) {
				grdHeadValue = grdId.getHeadValue(k);
				switch(grdHeadValue) { // 
					//case "계획금액": case "잔여예상금액": case "실적금액": case "차이금액":
					case "잔여예상금액": case "차이금액":
						grdId.setRealColSize("head", k-headMergeCnt, 0);
					break;
				}
			}						
		break;
		case "PLAN":
		break;
	}
}
