 * 01.01. [공통버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.fn_ComBtnSearch = function()
{ 
	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;	

	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	//this.ds_Master10.clearData();
	this.ds_Input.clearData();
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_TemhDtFr.value);
	this.ds_Input.setColumn(0, "DEPM_CD", this.div_Search.form.edt_DepmCd.value);
	this.ds_Input.setColumn(0, "FROM_MM", this.gfn_Left(this.div_Search.form.cal_TemhDtFr.value,6));
	this.ds_Input.setColumn(0, "SYST_ID", this.div_Search.form.edt_SystId.value);
	this.ds_Input.setColumn(0, "PRGM_ID", "M_SD_8120");//프로그램ID
	this.ds_Input.setColumn(0, "THIS_YEAR", this.gfn_Left(this.div_Search.form.cal_TemhDtFr.value,4));
	this.ds_Input.setColumn(0, "PRE_YEAR", nexacro.toNumber(this.gfn_Left(this.div_Search.form.cal_TemhDtFr.value,4))-1);
	this.ds_Input.setColumn(0, "NEXT_YEAR", nexacro.toNumber(this.gfn_Left(this.div_Search.form.cal_TemhDtFr.value,4))+1);
	this.ds_Input.setColumn(0, "RESM_CD", this.div_Search.form.cbo_ProcSt00.value); // 실적사업부
	this.ds_Input.setColumn(0, "INEX_GU", this.div_Search.form.cbo_ProcSt01.value); // 국내외구분
	this.ds_Input.setColumn(0, "INEX_GU", this.div_Search.form.cbo_ProcSt01.value); // 국내외구분
	this.ds_Input.setColumn(0, "ITEM_GU", this.div_Search.form.cbo_ProcSt02.value); // 대표아이템
	
	this.ds_Input.setColumn(0, "SEARCH_FROM", this.div_Search.form.cal_SearchFrom.value); // 조회기간 from
	this.ds_Input.setColumn(0, "SEARCH_TO", this.div_Search.form.cal_SearchTo.value); // 조회기간 to
	this.ds_Input.setColumn(0, "SEARCH_GUBUN", this.div_Search.form.cbo_SearchGubun.value); // 조회구분
	this.ds_Input.setColumn(0, "YM_GUBUN", this.div_Search.form.cbo_YmGubun.value); // 조회구분
	
	this.ds_Input.setColumn(0, "WOFH_ST", this.div_Search.form.chk_WofhYn.value); // 결재미승인포함여부
	this.ds_Input.setColumn(0, "PROC_ST", this.div_Search.form.chk_WofhYn00.value); // 수주실패포함여부
	
	this.ds_Input.setColumn(0, "ORRA_CD", this.div_Search.form.cbo_OrraCd.value); // 수주확율
	this.ds_Input.setColumn(0, "HWAPE", this.div_Search.form.cbo_Hwape.value); // 화폐
	
	
	
	if(!this.gfn_IsNull(this.getOwnerFrame().paramDs)) {
 		if(this.div_Search.form.cbo_YmGubun.value == 'M') {
			if(this.div_Search.form.cbo_SearchGubun.value == 'EXPECT') {
				this.ds_Input.setColumn(0, "SEARCH_TO", this.getOwnerFrame().paramDs.getColumn(0,"FROM_DD")); // 조회기간 to
			} else if(this.div_Search.form.cbo_SearchGubun.value == 'PLAN') {
				//this.ds_Input.setColumn(0, "FROM_DD", this.getOwnerFrame().paramDs.getColumn(0,"FROM_DD")); // 월 계획일때 영업현황의 기준일자를 실적 조회기준으로 잡는다. ( why? 예상은 실적+계획 )
				this.ds_Input.setColumn(0, "FROM_DD", this.getOwnerFrame().paramDs.getColumn(0,"TEMPDT_FR")); // 월 계획일때 영업현황의 기준일자의 전월 마지막일을 기준으로 잡는다.
			}
 		}
		
		this.ds_Input.setColumn(0, "PLANDT_FR", this.getOwnerFrame().paramDs.getColumn(0,"PLANDT_FR"));
		this.ds_Input.setColumn(0, "PLANDT_TO", this.getOwnerFrame().paramDs.getColumn(0,"PLANDT_TO"));	
		this.ds_Input.setColumn(0, "HWAPE", this.getOwnerFrame().paramDs.getColumn(0,"HWAPE"));	
		
		this.ds_Input.setColumn(0, "PGM_CALL_GUBUN", "POPUP");
		
	} else {
// 		var sYmGubun = "";
// 		if(this.gfn_Right(this.div_Search.form.cal_SearchFrom.value,4) = '0101') {
// 			sYmGubun = "Y";
// 		} else {
// 			sYmGubun = "M";
// 		}
//		this.ds_Input.setColumn(0, "YM_GUBUN", sYmGubun); // 조회구분
		this.ds_Input.setColumn(0, "PGM_CALL_GUBUN", "SELF");
		this.ds_Input.setColumn(0, "PLANDT_FR", this.div_Search.form.cal_SearchFrom.value);
		this.ds_Input.setColumn(0, "PLANDT_TO", this.div_Search.form.cal_SearchTo.value);			
	}
	
	
	

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	var selectedTabIdx = this.div_MasterView.form.tab_Master.tabindex;
	var callPageNumber = 10;
	
	switch(selectedTabIdx) {
		case 0: callPageNumber = "10"; break;
		case 1: callPageNumber = "20"; break;
		case 2: callPageNumber = "30"; break;
		case 3: callPageNumber = "40"; break;
		case 4: callPageNumber = "50"; this.div_MasterView.form.tab_Master.tpg_05.form.grd_Master50.set_suppresslevel( "sameskip" ); break;
	} 
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "sd:M_SD_8160_M_SL"+callPageNumber;
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MST_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_SD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Master"+callPageNumber+"=ds_Master"+callPageNumber;//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
}

