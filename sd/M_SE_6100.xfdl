/***********************************************************************************************
 * 02.DG1.05. [Detail그리드2-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Choice = function()
{
	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {
		
			var sCusmCdChk; //거래처
			var sCurrCdChk; //화폐
			var sPricCdChk; //가격조건
			
			//거래처, 화폐, 가격조건 추가 체크(단일 거래처, 화폐,가격조건 만 추가 가능)
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					//선적상세 탭 계약번호 체크
					if(this.ds_Detail10.rowcount > 0 ){
						if(this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.value != this.ds_Detail20.getColumn(iLoop,"CUSM_CD")|| //거래처
						   this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CurrCd.value != this.ds_Detail20.getColumn(iLoop,"CURR_CD")|| //화폐
						   this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_PricCd.value != this.ds_Detail20.getColumn(iLoop,"PRIC_CD")){ //가격조건
 							var arrMsgArg = ["거래처, 화폐, 가격조건"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 [&ITM0]만 적용 가능합니다.
							return;
						}
					}
					
					//그리드내 거래처, 화폐, 가격조건 체크
					if(!this.gfn_IsNull(sCusmCdChk) && !this.gfn_IsNull(sCurrCdChk) && !this.gfn_IsNull(sPricCdChk) ){
						if(sCusmCdChk != this.ds_Detail20.getColumn(iLoop,"CUSM_CD") ||
						   sCurrCdChk != this.ds_Detail20.getColumn(iLoop,"CURR_CD") ||
						   sPricCdChk != this.ds_Detail20.getColumn(iLoop,"PRIC_CD")){
							var arrMsgArg = ["거래처, 화폐, 가격조건"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 [&ITM0]만 적용 가능합니다.	
							return;						
						}
					}
					
					sCusmCdChk = this.ds_Detail20.getColumn(iLoop,"CUSM_CD");
					sCurrCdChk = this.ds_Detail20.getColumn(iLoop,"CURR_CD");
					sPricCdChk = this.ds_Detail20.getColumn(iLoop,"PRIC_CD");
				}
			}
			
			var arrCheckRow = [];//체크된 Row
	
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {

					//검색값설정 - 컬럼명:검색값
					var objSearchList = ["COMM_CD|" + this.ds_Detail20.getColumn(iLoop, "COMM_CD") //회사코드
					                    ,"INVH_NO|" + this.ds_Detail20.getColumn(iLoop, "INVH_NO") //Invoice번호
										,"INVB_SE|" + this.ds_Detail20.getColumn(iLoop, "INVB_SE") //Invoice순번
										];

					//[공통] 중복건 체크후 해당건 Skip
					if (!this.gfn_DsSchDupcheck(this.ds_Detail10, objSearchList)) {

						var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)
						//Invoice검색에서 선적상세로 이동
						this.ds_Detail10.copyRow(iAddRow, this.ds_Detail20, iLoop);
						this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
 						this.ds_Detail10.setColumn(iAddRow, "BOLH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BOLH_NO"));		
						this.ds_Detail10.setColumn(iAddRow, "s_chk", "0");
						
						//첫번째일 경우 마스터 업데이트
						if(this.ds_Detail10.rowcount == 1 ){ 
							
							this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CUSM_CD", this.ds_Detail20.getColumn(iLoop,"CUSM_CD"));	//거래처코드
							this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CUSM_NM", this.ds_Detail20.getColumn(iLoop,"CUSM_NM"));	//거래처명
							this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CURR_CD", this.ds_Detail20.getColumn(iLoop,"CURR_CD"));	//화폐
							this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PRIC_CD", this.ds_Detail20.getColumn(iLoop,"PRIC_CD"));	//가격조건
						}													
				
						arrCheckRow.push(iLoop );
						
						//선택항목 비활성화처리
						this.ds_Detail20.setColumn(iLoop, "s_conf", "1");	//계약대상검색 컬러 변경 구분자 업데이트
						this.ds_Detail20.setColumn(iLoop, "s_chk", "0");	//Uncheck로 변경
					}
				}
			}		
			
			this.ds_Detail20.set_rowposition(-1);			
			
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BOLH_AM",this.ds_Detail10.getSum("BOLB_AM"));	//Master에 금액 표기
			
			//L/C 상세로 Tab이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);
		}
	}
};

