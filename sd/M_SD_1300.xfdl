	svcCallObj.sInputDaset = "ds_Master13=ds_Master13" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Count=ds_Count";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.isSync = true;//동기식으로 설정할지 여부
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
	
	var YyCount = this.ds_Count.getColumn(0, "CNT");
    var sWofhSq = this.ds_Master10.getColumn(iMasterRowPos, "WOFH_SQ");	
	
	//년도 입력시 해당 년도 중목 체크.
	if (sWofhSq == "1") {
	   if (YyCount == 0) {
	       return true;
	   } else {
			var arrMsgArg = [this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ObjhYy.text];
			this.gfn_Alert("SD013", arrMsgArg);//&ITM0년도로 등록된 데이터가 존재합니다.
			
			this.div_MasterInput.set_enable(false);	
			this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);
			this.div_DetailInput.form.tab_Detail.set_enable(false);
			this.div_MasterInput.form.btn_change.set_enable(false);
				
			return false;
	   };
	} else {
	    return true;
	};
};  
 
