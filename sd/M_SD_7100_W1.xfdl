/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	
	
	if (e.newrow > -1) {
		
		
		//수금정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COLH_NO"))) {
			
			//수불구분 비활성화
			//this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_PdrhGu.set_enable(false);
			
			//this.ds_PpRadd.filter("RADM_CD == '"+ this.ds_Master10.getColumn(this.ds_Master10.rowposition,"RADM_CD")  +"'");	//수불사유필터
			//입력영역 버튼 비활성화
			//this.fn_SetGridInputButtonEnable();
			
			//컴포넌트 비활성화
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CotyCd.set_enable(false);	//수금구분
			
			//수금코드 포멧 설정 변경 (기존데이터용)
			var strColhNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COLH_NO")
			//trace(strColhNo.length);
			//trace(strColhNo.substr(0,1)=="S");
			if(strColhNo.length == 10){
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_ColhNo.set_format("######-####");
			}else{
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_ColhNo.set_format("########-###");
			}
			
			
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "COLH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COLH_NO"));
			this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
			this.ds_Input.setColumn(0, "CUSM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD"));
			
			this.ds_InputSch.clearData();
			this.ds_InputSch.addRow();
			this.ds_InputSch.setColumn(0, "COLH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition,"COLH_NO"));
			this.ds_InputSch.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
			//this.ds_Input.setColumn(0, "PRGM_ID", this.fvComPrgmId);
			
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "sd:M_SD_7100_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "sd:M_SD_7100_D_SL30";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			
			
			///////////////////////////////이것 실행하면 무한루프???
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "sd:M_SD_7100_D_SL40";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "sd:M_SD_7100_D_SL50";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_SD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input ds_InputSch=ds_InputSch" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10 ds_Detail30=ds_Detail30 ds_Detail40=ds_Detail40 ds_Detail50=ds_Detail50";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
			//svcCallObj.isSync =true;
			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);		
		}
	}
};

