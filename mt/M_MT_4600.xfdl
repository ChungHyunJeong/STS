this.fn_FormBizInit = function() 
{ 

	//[공통] 날짜정보 설정	
	this.gfn_SetCommFromToDate(this.fvComPrgmId, "MS", this.div_Search.form.cal_PdrhDtFr, this.div_Search.form.cal_PdrhDtTo);//조회조건영역
