/***********************************************************************************************
 * 1. 조회 영역
 ***********************************************************************************************/
/***********************************************************************************************
 * 01.01. [공통버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.fn_ComBtnSearch = function(sWofhNo)
{ 
	//[결재] 결재 PID가 있으면-결재Popup으로 호출시
	if (!this.gfn_IsNull(sWofhNo)) {
		this.fvWfSearchMode = true;//[결재]결재내역 조회모드
		this.fvWfPopupMode = true;//[결재]결재내역 팝업화면으로 호출여부
	}
	
	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;	
	
	//저장후 재조회시-마스터 데이터셋 이벤트 비활성화
	if (this.fvSchKeyYn) {
		this.ds_Master10.set_enableevent(false);
	}
	
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Master10.clearData();
	this.ds_Detail10.clearData();
	this.ds_Detail20.clearData();
	this.ds_Input.clearData();
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
	this.ds_Input.addRow();	
	var sSqlId = "";//쿼리ID
	
	//[결재] 결재내역 조회가 아니면
	if (!this.fvWfSearchMode) {		
		//[업무] 조회조건 입력	
		this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_PdrhDtFr.value);	//요청일자
		this.ds_Input.setColumn(0, "CLOS_DD", this.div_Search.form.cal_PdrhDtTo.value);	//요청일자
		this.ds_Input.setColumn(0, "CNTH_NO", this.div_Search.form.mae_CnthNo.value);	//계약번호	
		this.ds_Input.setColumn(0, "DEPM_CD", this.div_Search.form.edt_DepmCd.value);	//요청부서
		this.ds_Input.setColumn(0, "SESU_ID", this.div_Search.form.edt_SystId.value);	//요청자
		this.ds_Input.setColumn(0, "SAUM_CD", this.div_Search.form.cbo_SaumCd.value);	//사업장		
		this.ds_Input.setColumn(0, "PROC_YN", this.div_Search.form.cbo_SmYn.value);	//처리여부
		this.ds_Input.setColumn(0, "NGBH_NO", this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PdrhNo.value);	//출고번호	
		this.ds_Input.setColumn(0, "CUSM_CD", this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.value);	//계약번호	 CUSM_CD
	                                          
//this.alert( this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PdrhNo.value);

		//결재이력체크
		if (this.div_Search.form.chk_WofhYn.value) {
			this.ds_Input.setColumn(0, "WOFH_YN", "1");
		} else {
			this.ds_Input.setColumn(0, "WOFH_YN", "0");
		};
		sSqlId = "mm:M_MT_4600_M_SL10";//쿼리ID
		//sSqlId = "mm:M_MT_4500_M_SL10";//쿼리ID
	} else {
		this.ds_Input.setColumn(0, "WOFH_NO", sWofhNo);//결재PID
		sSqlId = "mm:M_MT_4400_M_SL11";//쿼리ID
	}
 
	//[공통]서비스정보 초기화
