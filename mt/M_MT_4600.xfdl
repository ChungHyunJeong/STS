 *    [캘린더 날짜-선택] 캘린더 날짜 선택 이벤트
  ---------------------------------------------------------------------------------------------*/
this.div_MasterInput_tab_Master_tpg_01_cal_ProcDd_oncloseup = function(obj:nexacro.Calendar,e:nexacro.CalendarCloseUpEventInfo)
{   
    var sOldDay = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SYCO_DD") ; //변경전 일시
	  	
    var sThisTime = this.gfn_GetCommDate("toDayFull"); //현재시간		
	sThisTime = sThisTime.substr(11, 18);
	var sTime = sThisTime.substr(0, 2) + sThisTime.substr(3, 2) + sThisTime.substr(6, 2)+ '000'; 

////select FORMAT(CONVERT(datetime, '20220729 17:22:23'),'yyyy-MM-dd HH:mm:ss')	
   
	var sDays = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SYCO_DD"); //선택날짜			
	
	if (sOldDay.substr(4, 1)  =='-') {  
	
	    sDays = sDays.toString().substr(0,10);
	    sDays = sDays.substr(0, 4) + sDays.substr(5, 2) + sDays.substr(8, 2);		

	} else {
		sDays = sDays.substr(0,8) ; 
	}

	//sDays = sDays.substr(0,8) ; 
	this.ds_Master10.setColumn(this.ds_Master10.rowposition, "SYCO_DD",  sDays + sTime );	//날짜시간형식 20220730110147000 

};

