					//그리드내 출고번호 체크
					if(!this.gfn_IsNull(sCnthNoChk) ){
						if(sCnthNoChk != this.ds_Detail20.getColumn(iLoop,"MORH_NO") ){
							var arrMsgArg = ["출고요청번호(" + sCnthNoChk + ")"];
