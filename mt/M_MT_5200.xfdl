							this.gfn_Alert("SM057", arrMsgArg); //동일한 [출고요청번호]만 적용 가능합니다.
							return;
						}
					}
					*/
					if (!this.gfn_IsNull(strMirhNo)){
						if (strMirhNo != this.ds_Detail20.getColumn(iLoop, "MORH_NO")) {
							var arrMsgArg = ["출고요청번호(" + strMirhNo + ")"];
