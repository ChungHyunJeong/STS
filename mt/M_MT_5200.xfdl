this.fn_ComBtnSave = function()
{ 
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.fvFileDiv.fn_CheckFileAddYn()) {		
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.		
		return;
	}

	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		//첨부파일정보만 변경시
		if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && this.ds_Master10.getRowCount() > 0) {
			//첨부파일정보 저장
			fvFileInfo = new Object();//[공통] 첨부파일정보
			fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, B-상세(그리드Row), E-기타
			fvFileInfo.CALLBACK_FUNC = "";//호출할 콜백함수
			fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
			fvFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MOPH_NO");//PK번호2-출고요청번호
			//fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
			//첨부파일Div 화면에 Key세팅하게 변경
			this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);
			return;	
		}
	}

	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		if (!this.fn_VaildMasterSave()) return;		
	}
	
	if (!this.fn_Magam()) {return;}
		
	if(this.ds_Detail10.rowcount < 1){
		var arrMsgArg = ["출고관리상세"];
		this.gfn_Alert("SM045", arrMsgArg);//[출고요청상세]는 1건 이상 존재해야 합니다.
		return;
	}
	
	//Detail 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		if (!this.fn_VaildDetailSave()) return;
	}	
	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	//Master 입력 상태이면
	if (sChkRowType == "I") {
	
		//채번-출고번호
		var sTemhNo = this.gfn_GetTbnmNo(this.fvComPrgmId, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MOPH_DD"));//출고번호
		if (this.gfn_IsNull(sTemhNo)) {
			//출고번호를 가져오는데 실패했습니다.
			return;
		}
		
		var sChkTemhNo = "";//출고번호-체크용		
		var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position
		
		//Master 데이터셋에 채번정보 입력-출고번호
		this.ds_Master10.setColumn(iMasterRowPos, "MOPH_NO", sTemhNo);
		
		var sCommCd = this.ds_Master10.getColumn(iMasterRowPos, "COMM_CD");//회사코드
		var sTemhNo = this.ds_Master10.getColumn(iMasterRowPos, "MOPH_NO");//출고요청번호
		var sSaumCd = this.ds_Master10.getColumn(iMasterRowPos, "SAUM_CD");//사업장
		var sMophDd = this.ds_Master10.getColumn(iMasterRowPos, "MOPH_DD");//출고일자
		var sWarmCd = this.ds_Master10.getColumn(iMasterRowPos, "OUWR_CD");//창고
		var sInwrCd = this.ds_Master10.getColumn(iMasterRowPos, "INWR_CD");//창고
		var sRadmGu = this.ds_Master10.getColumn(iMasterRowPos, "RADM_GU");//수불구분
		var sRadmCd = this.ds_Master10.getColumn(iMasterRowPos, "RADM_CD");//수불코드
		var sRaddCd = this.ds_Master10.getColumn(iMasterRowPos, "RADD_CD");//수불사유
		var sWorbNo = this.ds_Master10.getColumn(iMasterRowPos, "WORB_NO");//WORB_NO
				
		//Detail 데이터셋에 기본정보 입력-회사코드/출고번호/결재차수
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			sChkTemhNo = this.ds_Detail10.getColumn(iLoop, "MOPH_NO");//출고번호					
			
			if (this.gfn_IsNull(sChkTemhNo)) {
				this.ds_Detail10.setColumn(iLoop, "COMM_CD", sCommCd);
				this.ds_Detail10.setColumn(iLoop, "MOPH_NO", sTemhNo);
				this.ds_Detail10.setColumn(iLoop, "SAUM_CD", sSaumCd);
				this.ds_Detail10.setColumn(iLoop, "MOPH_DD", sMophDd);
				this.ds_Detail10.setColumn(iLoop, "OUWR_CD", sWarmCd);
				if(sRadmGu == "0070") this.ds_Detail10.setColumn(iLoop, "INWR_CD", sInwrCd);				
				this.ds_Detail10.setColumn(iLoop, "RADM_GU", sRadmGu);
				this.ds_Detail10.setColumn(iLoop, "RADM_CD", sRadmCd);
				this.ds_Detail10.setColumn(iLoop, "RADD_CD", sRaddCd);
				this.ds_Detail10.setColumn(iLoop, "WORB_NO", sWorbNo);
			}
		}
	}	
		
	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		fvFileInfo = new Object();//[공통] 첨부파일정보
		fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
		fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
		fvFileInfo.CALLBACK_FUNC = "fn_ComBtnSearch";//호출할 콜백함수
		fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
		fvFileInfo.PK01_NO = nApp.gvCompany;//PK번호1-회사코드
		fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MOPH_NO");//PK번호2-출고요청번호				
	}		
		
	
	var sCommCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//회사코드
	var sTemhNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MOPH_NO");//출고요청번호
	var sSaumCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SAUM_CD");//사업장
	var sMophDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MOPH_DD");//출고일자
	var sOuWrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OUWR_CD");//창고
	var sInWrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "INWR_CD");//창고
	var sRadmGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RADM_GU");//수불구분
	var sRadmCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RADM_CD");//수불코드
	var sRaddCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RADD_CD");//수불사유
	var sWorbNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WORB_NO");//WORB_NO	
		
		
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {	
		this.ds_Detail10.setColumn(iLoop, "COMM_CD", sCommCd);
		this.ds_Detail10.setColumn(iLoop, "MOPH_NO", sTemhNo);
		this.ds_Detail10.setColumn(iLoop, "SAUM_CD", sSaumCd);
		this.ds_Detail10.setColumn(iLoop, "MOPH_DD", sMophDd);
		this.ds_Detail10.setColumn(iLoop, "OUWR_CD", sOuWrCd);	
		
		if(sRadmGu == "0070") this.ds_Detail10.setColumn(iLoop, "INWR_CD", sInWrCd);
		this.ds_Detail10.setColumn(iLoop, "RADM_GU", sRadmGu);		
		
		this.ds_Detail10.setColumn(iLoop, "RADM_CD", sRadmCd);
		this.ds_Detail10.setColumn(iLoop, "RADD_CD", sRaddCd);
		this.ds_Detail10.setColumn(iLoop, "WORB_NO", sWorbNo);
	};	
		
	//저장시 재조회 Key설정
	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MOPH_NO");//조회Key01	

	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
