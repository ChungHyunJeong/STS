this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{	
	if(e.newvalue == '1') {
		this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.setCellPos(10);
	}
	
	var sMorbQt = "";
	var sMopbQt = "";
	var sStocQt = "";
	
	if (e.columnid == "MOPB_QT") {	
		var sMorbQt = nexacro.toNumber(this.ds_Detail20.getColumn(e.row, "MORB_QT"));
	
 		var sMrpbQt = nexacro.toNumber(this.ds_Detail20.getColumn(e.row, "STOC_QT"));
 		var sJaegQt = nexacro.toNumber(e.newvalue);		
		var sJaegQtOrg = e.oldvalue; 
		if(this.gfn_IsNull(sJaegQt)){sJaegQt = 0;}
		
		if(sMorbQt < sJaegQt)
		{				
			this.ds_Detail20.setColumn(e.row, "MOPB_QT", sJaegQtOrg)			
			
			var arrMsgArg = ["출고수량","요청수량"];			
			
			this.gfn_Alert("SM010", arrMsgArg); //출고수량은(는) 요청수량 이하의 숫자만 입력 가능합니다.
			return;
		 } 

		
		if(sMrpbQt < sJaegQt)
		{				
			this.ds_Detail20.setColumn(e.row, "MOPB_QT", sJaegQtOrg)
			
			//var arrMsgArg = [arrText1,arrText2];
			var arrMsgArg = ["출고수량","재고수량"];			
			
			this.gfn_Alert("SM010", arrMsgArg); //출고수량은(는) 재고수량 이하의 숫자만 입력 가능합니다.
			return;
		 } 
	} else if (e.columnid == "s_chk") {		
		
// 		var value = "0";
// 		var sMorhNo = this.ds_Detail20.getColumn(e.row, "MORH_NO");
// 		
// 		for (var iLoop = 0; iLoop < this.ds_Detail20.getRowCount(); iLoop++) {
// 			if (this.ds_Detail20.getColumn(iLoop, "s_conf") != "1" && this.ds_Detail20.getColumn(iLoop, "STOC_QT") > 0) {
// 				value = (this.ds_Detail20.getColumn(iLoop, "MORH_NO") == sMorhNo) ? e.newvalue : "0";
// 				this.ds_Detail20.setColumn(iLoop, "s_chk", value);
// 			}
// 		}	
		if (this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "s_chk") == '1') {	
			var sMorhNo = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "MORH_NO");
			
			var iChkCnt = this.ds_Detail20.getCaseCount("MORH_NO == '"+sMorhNo+"' && s_chk == '1'"); //체크건수
			if (iChkCnt > 1) return;
			
			//최초 1건만 체크시 로직 처리
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				if (this.ds_Detail20.getColumn(iLoop, "MORH_NO") == sMorhNo ){
					
					sMorbQt = this.ds_Detail20.getColumn(iLoop, "MORB_QT");
					sMopbQt = this.ds_Detail20.getColumn(iLoop, "MOPB_QT");
					sStocQt = this.ds_Detail20.getColumn(iLoop, "STOC_QT");
					
					sMorbQt = nexacro.toNumber(sMorbQt);
					sMopbQt = nexacro.toNumber(sMopbQt);
					sStocQt = nexacro.toNumber(sStocQt);
				
					//체크박스 Check
					if(this.ds_Detail20.getColumn(iLoop, "STOC_QT") > 0){
						this.ds_Detail20.setColumn(iLoop, "s_chk", "1");//Check로 변경
						
						if(sMopbQt > sStocQt){
							this.ds_Detail20.setColumn(iLoop, "MOPB_QT", sStocQt);
						}
						
					}
				}
			}
		}
	}
	
};

