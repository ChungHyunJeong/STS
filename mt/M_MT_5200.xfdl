							return;
						}
					}					
					sCnthNoChk = this.ds_Detail20.getColumn(iLoop,"MORH_NO");
					
					//출고요청 수량 체크 
					if(this.ds_Detail20.getColumn(iLoop, "MOPB_QT") == 0){
						var arrMsgArg = ["출고 수량"];
						this.gfn_Alert("SM055", arrMsgArg); //[요청 수량]을(를) 1 이상 입력해 주세요.
						return;
					}
					if(this.ds_Detail20.getColumn(iLoop, "MOPB_QT") > this.ds_Detail20.getColumn(iLoop, "MI_MOPB_QT") ){
						var arrMsgArg = ["출고 수량", "미출고수량"];
						this.gfn_Alert("SM056", arrMsgArg); //[출고요청 수량]이(가) [계약수량]을(를) 초과할 수 없습니다.
						return;
					}
					
					if(this.ds_Detail20.getColumn(iLoop, "MORB_QT") < this.ds_Detail20.getColumn(iLoop, "MOPB_QT") ){
						var arrMsgArg = ["출고수량", "요쳥수량"];
						this.gfn_Alert("SM056", arrMsgArg); //[출고요청 수량]이(가) [요쳥수량]을(를) 초과할 수 없습니다.
						return;
					}
					
					if(this.ds_Detail20.getColumn(iLoop, "STOC_QT") < this.ds_Detail20.getColumn(iLoop, "MOPB_QT") ){
						var arrMsgArg = ["출고수량", "재고수량"];
						this.gfn_Alert("SM056", arrMsgArg); //[출고요청 수량]이(가) [재고수량]을(를) 초과할 수 없습니다.
