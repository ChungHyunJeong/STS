/***********************************************************************************************
 * 02.MF0.03. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {	
		
		//출고No정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MOPH_NO"))) {
		
			//[월마감제어-버튼/상세그리드]
			var sBUDG_MM = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MOPH_DD"), "");//마감년월
			var sSAUM_CD = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SAUM_CD"), "");//사업장
			
			//마감여부 체크
			var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
			var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
			paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
			paramObj.MODM_CD = "MT";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
			paramObj.ACUM_CD = "";//[선택] 회계단위
			paramObj.BUSM_CD  = "";//[선택] 사업부문
			paramObj.SAUM_CD  = sSAUM_CD;//[선택] 사업장코드
			paramObj.DEPM_CD  = "";//[선택] 부서코드
			paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
			paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
			
			var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		

			//월마감된 경우
			if (bCheck) {
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.set_readonly(true);
				this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.set_readonly(true);
				this.fvFileDiv.fn_SetCopmGridAuth(false);			
				this.fvMmCloseYn = true;//월마감여부-Tab버튼 제어용
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PdrhDd.set_readonly(true);	
				this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(true);					
			} else {			
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.set_readonly(false);
				this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.set_readonly(false);
				this.fvFileDiv.fn_SetCopmGridAuth(true);
				this.fvMmCloseYn = false;//월마감여부-Tab버튼 제어용
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PdrhDd.set_readonly(false);	
				this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(false);					
			}
			
			
			
			
		
			var strWorbNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WORB_NO");
			var iWorbLen  = 0
			if(!this.gfn_IsNull(strWorbNo))
			{
				iWorbLen = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WORB_NO").length;
			}

			//W/O관리 format 변경 (14자리, 13자리, 10, 기타)
			if (iWorbLen == 14) 
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WorbNo.set_format("@@@@-@-@@@@-@@-@@@");		
			else if (iWorbLen == 13) 
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WorbNo.set_format("@@@-@@@@-@@@-@@@");		
			else if (iWorbLen == 12) 
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WorbNo.set_format("@@@-@@@@-@@@-@@");
			else if (iWorbLen == 11) 
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WorbNo.set_format("@@@@-@-@@@@-@@");	
			else if (iWorbLen == 10) 
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WorbNo.set_format("@@@-@@@@-@@@");		
			else 
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WorbNo.set_format("@");	
				
			//전화번호 포멧설정
			//this.gfn_TelFormat(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ATEL_NO"),this.div_MasterInput.form.tab_Master.tpg_01.form.mae_AtelNo);
		
			//수불구분 비활성화
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_PdrhGu.set_enable(false);
						
			this.ds_PpRadm.filter("");
			this.ds_PpRadm.filter("ACCM_GU=='0010' && INOU_GU=='0020' && USED_YN=='1' && RADM_GU=='" + this.ds_Master10.getColumn(this.ds_Master10.rowposition,"RADM_GU") + "'"); //수불코드 필터
			
			this.ds_PpRadd.filter("");
			this.ds_PpRadd.filter("RADM_CD == '"+ this.ds_Master10.getColumn(this.ds_Master10.rowposition,"RADM_CD")  +"'");	//수불사유필터					
			
			//입력영역 버튼 비활성화
			//this.fn_SetGridInputButtonEnable();	
			
			//반입일자, 반입창고 비활성화
