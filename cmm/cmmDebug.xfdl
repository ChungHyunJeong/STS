/************************************************************************************************
 * FORM 변수 선언 영역
 ************************************************************************************************/
this.fvCurrentCombo = "cboDataset"; // 현재선택이 로컬데이터셋인지 / GDS인지의 구분처리 
this.objApp 		= nexacro.getApplication();

/************************************************************************************************
 * FORM EVENT 영역(onload, onbeforeclose)
 ************************************************************************************************/
this.form_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	// Form Load 시 공통 기능 처리
	//this.gfnFormOnLoad(this); //초기화[필수]

    // 로컬 데이터셋을 가져온다. 
	this.fnSaveDatasetList(this.getOwnerFrame().form.opener);
	this.cboDataset.set_index(-1);	
	this.fnMakeDatasetGrid();

	// 글로벌 데이터셋을 가져온다. 
	this.fnSaveGDSList();
	this.dsGDS.set_keystring("S:NAME");

    // 화면의 GRID의 목록을 가져온다. 
	this.fnSaveGridList(this.getOwnerFrame().form.opener);
	this.cboGrid.set_index(0);	

    // 화면의 컴포넌트의 목록을 가져온다. 
	this.fnSaveComponetsList(this.getOwnerFrame().form.opener);
	this.cboComponents.set_index(0);	

	// 화면에서 사용되는 변수목록을 가져온다. 
	this.dsVariable.clearData();
	this.fnSaveVariableList(this.getOwnerFrame().form.opener);
	
	// 글로벌 변수 목록을 가져온다.
	this.fnSaveGlobalVariableList();
	this.dsVariable.set_keystring("S:LVL+NAME");
};

/************************************************************************************************
 * CALLBACK 콜백 처리부분(Transaction, Popup)
 ************************************************************************************************/
 
 
