			this.ds_Detail10.setColumn(iRow, "BGCM_YN", "N");//예산통제여부
		}
	}

	//관리항목 조회
	this.fn_dsAcomMa(this.ds_Detail10.getColumn(iRow, "ACOM_CD"), "SET", bCheckBgcm);		
