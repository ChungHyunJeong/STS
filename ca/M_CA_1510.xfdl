 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "COMBO_SEARCH":					
			if (this.ds_FaSaum.getRowCount() > 0) {
				this.ds_FaSaum.insertRow();
				this.ds_FaSaum.setColumn(0, "SAUM_NM", "ALL");
				//this.div_Search.form.cbo_SaumCd.set_index(0);  
			}
			break;
		case "MST_SEARCH": //조회
			var selectedTabIdx = this.Tab00.tabindex;
			if(selectedTabIdx == 0) {
				if (this.ds_Master10.getRowCount() > 0) {
					
					//[공통]그리드NoData Text 초기화 설정
					this.gfn_InitGridNoDataText(this.Tab00.tpg_01.form.grd_Master10);
					this.ds_Master10.set_rowposition(0);
					
					var selectedTabIdx = this.Tab00.tabindex;
					var sSqlId = "";
					var sOutDatset = "";
			
				} else {
					this.gfn_SetGridNoDataText(this.Tab00.tpg_01.form.grd_Master10);
				}
			} else if(selectedTabIdx == 1) {
				if (this.ds_Master20.getRowCount() > 0) {
					
					//[공통]그리드NoData Text 초기화 설정
					this.gfn_InitGridNoDataText(this.Tab00.Tabpage2.form.grd_Master20);
					this.ds_Master20.set_rowposition(0);
					
					var selectedTabIdx = this.Tab00.tabindex;
					var sSqlId = "";
					var sOutDatset = "";
			
				} else {
					this.gfn_SetGridNoDataText(this.Tab00.Tabpage2.form.grd_Master20);
				}
			} else if(selectedTabIdx == 2) {
				if (this.ds_Master30.getRowCount() > 0) {
					
					//[공통]그리드NoData Text 초기화 설정
					this.gfn_InitGridNoDataText(this.Tab00.Tabpage3.form.grd_Master30);
					this.ds_Master30.set_rowposition(0);
					
					var selectedTabIdx = this.Tab00.tabindex;
					var sSqlId = "";
					var sOutDatset = "";
					
					this.fn_tab3();
			
				} else {
					this.gfn_SetGridNoDataText(this.Tab00.Tabpage3.form.grd_Master30);
				}
			}
	
			break;
		case "MST_SEARCH3":
			if (this.ds_Master31.getRowCount() > 0) {
				this.gfn_InitGridNoDataText(this.Tab00.Tabpage3.form.grd_Master31);
				this.ds_Master31.set_rowposition(0);		
				this.fn_tab3_1();
			} else {
				this.gfn_SetGridNoDataText(this.Tab00.Tabpage3.form.grd_Master31);
			}
		break;
		case "MST_SEARCH3_1":
			if (this.ds_Master32.getRowCount() > 0) {
				this.gfn_InitGridNoDataText(this.Tab00.Tabpage3.form.grd_Master32);
				this.ds_Master32.set_rowposition(0);
			} else {
				this.gfn_SetGridNoDataText(this.Tab00.Tabpage3.form.grd_Master32);
			}
		break;		
		case "SAVE":   //저장
// 			this.fvSchKeyYn = true;//저장후 재조회여부 
// 			this.fn_ComBtnSearch();
			
			break;	
		case "DTL_SEARCH": //조회		
// 			this.ds_Master10.addColumn("s_chk", "string");
// 			this.ds_Master10.addColumn("crudType", "string");

			break;			
			
		case "DTLS_SEARCH": //미출고검색		
			//this.ds_Detail20.addColumn("s_chk", "string");		
			//this.ds_Detail20.addColumn("s_conf", "string");
			break;			
			
		case "DELETE": //삭제		
			this.fn_ComBtnSearch();//조회
			break;							
	}
};

/***********************************************************************************************
 * 13.02. [팝업] CallBack 함수 처리
          strId : 팝업호출시 지정한 팝업ID
	      rtnValue : 팝업창에서 close시 지정한 반환값
          주의 : Open(popuptype=modeless)창은 팝업콜백을 지원하지 않음
 ***********************************************************************************************/ 
this.fn_PopupCallback = function(strId, rtnValue)
{
    //[조회영역]
	//회계단위
	if(strId == "S_ACUM_CD_01"){
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.ACUM_CD)) {
				this.div_Search.form.edt_AcumCd.set_value(arrRtnVal.ACUM_CD);
				this.div_Search.form.edt_AcumNm.set_value(arrRtnVal.ACUM_NM);
			}
		} else {
			this.div_Search.form.edt_AcumCd.set_value("");
			this.div_Search.form.edt_AcumNm.set_value("");
		}
	}
};
 
