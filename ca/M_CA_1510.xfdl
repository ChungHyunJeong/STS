this.ds_Master20_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	
	if (e.newrow > -1) {
	
		var RESULT_TABLE = this.ds_Master20.getColumn(this.ds_Master20.rowposition, "GUBN2");
		this.ds_Input.setColumn(0, "RESULT_TABLE", RESULT_TABLE);
		
		trace("RESULT_TABLE --> " + this.ds_Input.getColumn(0, "RESULT_TABLE"));
		
		//[공통]서비스정보 초기화
		this.gfn_InitComService(this);	
		var selectedTabIdx = this.Tab00.tabindex;
		var sSqlId = "";
		var sOutDatset = "";
		
		if (selectedTabIdx == 1) { // 결산결과
			sSqlId = "ca:M_CA_1510_D_SL20";
			sOutDatset = "ds_Detail20=ds_Detail20";
			this.ds_Detail20.clearData();
		}	

		
		
		//[공통]서비스정보 추가
		var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
		svcAddObj.sSelectSql = sSqlId;
		this.gfn_AddComSearchService(this, svcAddObj);
		
		//[공통]서비스 호출 변수
		var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
		svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
		svcCallObj.sActionName = this.G_SV_CA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
		svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
		//svcCallObj.sOutDatset = "ds_Master"+callPageNumber+"=ds_Master"+callPageNumber;//출력 데이터셋
		svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
		svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
		svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
		svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID	svcCallObj.sOutDatset = "ds_Master10=ds_Master10 ds_Master20=ds_Master20 ds_Master30=ds_Master30 ds_Master40=ds_Master40 ds_Master50=ds_Master50 ds_Master60=ds_Master60 ds_Master70=ds_Master70";//출력 데이터셋
		

		//[공통]서비스 호출
		this.gfn_CallComService(this, svcCallObj);
		
		this.gfn_SetGridSelectRowColor(this.Tab00.Tabpage2.form.grd_Master20, this.ds_Master20.rowposition);//선택한 Row BackgroundColor 설정
		
	}
};

