	var arrRows = this.div_MasterInput.form.tab_Detail.tpg_01.form.grd_Detail10.getSelectedRows();
	var iAddRow = 0;
	
	//이전날짜로 행추가 불가능
	if(e.columnid = "COSM_DD"){
