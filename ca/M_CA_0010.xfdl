{
	
this.gfn_SetGridSelectRowColor(this.div_MasterInput.form.tab_Detail.tpg_01.form.grd_Master10, e.newrow);//선택한 Row BackgroundColor 설정
	//그룹No정보가 있으면 - 상세조회
	if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COSM_DD"))) {
		
		// 적용일자가 오늘날짜보다 이전일 경우 수정 불가능
