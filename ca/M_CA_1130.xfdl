this.fn_Create = function() 
{	 
	this.fvDelChk     =  "Y";	
	
	trace("A");	
	 
	this.ds_Detail10.clearData();		

	var sRephMn     = this.div_MasterInput.form.tab_Master.tpg_01.form.cal_RephMm.value
	                      
	var sPrevYy		= sRephMn.substr(0,4) -1   ;	//전년도 
	var sThisYy		= sRephMn.substr(0,4) ; //당해년도	
	var sFromDd		= (this.div_Search.form.cal_FromDd.value).substr(0,6); //조회-결산년월From 
	var sClosDd		= (this.div_Search.form.cal_ClosDd.value).substr(0,6);   //조회-결산년월To
	var sThisMm     = sRephMn.substr(0,6) ; //당월	

//this.alert(sRephMn);

	this.ds_Input.clearData();
	this.ds_Input.addRow();		
	

//	sSlihYm = sSlihYm.substr(0,6);	
	
	this.ds_Input.setColumn(0,	"PREV_YY", sPrevYy);	//전년도	
	this.ds_Input.setColumn(0,	"THIS_YY", sThisYy);	//당해년도
	this.ds_Input.setColumn(0,	"THIS_MM", sThisMm);	//당월
	this.ds_Input.setColumn(0,  "THIS_M01", sThisYy +'01'); //당해01월
	this.ds_Input.setColumn(0,	"FROM_DD", sFromDd);	//결산년월(From)
	this.ds_Input.setColumn(0,	"CLOS_DD", sClosDd);	//결산년월(To) 
	
	
   //[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object	

	svcAddObj.sSelectSql = "ca:M_CA_1130_D_SL11";		 //하자보수충당금 전표생성조회	
	this.gfn_AddComSearchService(this, svcAddObj);		
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "DTL_SEARCH_JUNPYO";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_CA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID		
	//svcCallObj.isSync = true;//동기식으로 설정할지 여부

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);
	
	
}


