/***********************************************************************************************
 * 15.09. 발의내용 자동 입력
 ***********************************************************************************************/
this.fn_SetAcchNm = function()
{	
	/*
	발의내용요약 / 발의내용 : default 내용
       - '결산년월' + '(' + 결산년월 + ')' + 발의일자 (ex, 결산년월(2021.12) - 2021.12.31)
	(임의로 설정해놓음, 추후에 변경)
	*/
	
	var rowposition  = this.ds_Master10.rowposition;
    var cRephMm = this.div_MasterInput.form.tab_Master.tpg_01.form.cal_RephMm.value;//결산년월
	var sRephMm = "결산년월";//this.div_MasterInput.form.tab_Master.tpg_01.form.sta_RephMm.text;
	var cRephDd = this.div_MasterInput.form.tab_Master.tpg_01.form.cal_RephDd.value;//발의일자
	
	if(this.gfn_IsNull(cRephMm) || this.gfn_IsNull(cRephDd)){
		return;
	}

	var sMenuNm = this.gfn_GetMenuNm(this.fvComPrgmId);//해당화면 메뉴명 조회 미지급정산
	
	var accNm = sMenuNm + " "+  sRephMm + "("  + cRephMm.substr(0, 4) + "." + cRephMm.substr(4, 2) + ") - " + cRephDd.substr(0, 4) + "." + cRephDd.substr(4, 2) + "." + cRephDd.substr(6, 2);
	
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcchNm.set_value(accNm);
	this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_value(accNm);
	
}

