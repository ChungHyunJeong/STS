this.fn_ComBtnSave = function()
{ 

	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10)) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}
 
	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		if (!this.fn_VaildMasterSave()) return;
	}
 	
//잠시막음
// 	if (this.ds_Master20.getRowCount() > 0) {
// 		var sRephNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "REPH_NO");
// 		var sRephNo2 = this.ds_Master20.getColumn(0, "REPH_NO");
// 		if (sRephNo != sRephNo2) {
// 			var arrMsgArg = ["결산년월과 회계단위로 등록된 자료"] //해당 &ITM0(이)가 이미 존재합니다.
// 			this.gfn_Alert("SD008", arrMsgArg);
// 			return;
// 		}
// 	}
	
	//Detail 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		if (!this.fn_VaildDetailSave()) return;
	}
	
	
	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
//this.alert(sChkRowType);	



	//Master 입력 상태이면
	if (sChkRowType == "I") {
	
		//채번-관리번호
		var sTbnmNo = this.gfn_GetTbnmNo(this.fvComPrgmId, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "REPH_DD"));
		if (this.gfn_IsNull(sTbnmNo)) {
			//관리번호를 가져오는데 실패했습니다.
			return;
		}
		
 
		var sChkTemhNo = "";//관리번호-체크용
		var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position
				

		//Master 데이터셋에 채번정보 입력-관리번호
		this.ds_Master10.setColumn(iMasterRowPos, "REPH_NO", sTbnmNo);
		
		var sCommCd = this.ds_Master10.getColumn(iMasterRowPos, "COMM_CD");//회사코드
		var sRephNo = this.ds_Master10.getColumn(iMasterRowPos, "REPH_NO");//관리번호
		var sWofhSq = this.ds_Master10.getColumn(iMasterRowPos, "WOFH_SQ");//결재차수
		
		//Detail10 데이터셋에 기본정보 입력-회사코드/관리번호/결재차수
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {	
			//sChkTemhNo = this.ds_Detail10.getColumn(iLoop, "COMM_CD");//관리번호//org
			sChkTemhNo = this.ds_Detail10.getColumn(iLoop, "REPH_NO");//관리번호 //add
			
			if (this.gfn_IsNull(sChkTemhNo)) {			 
				this.ds_Detail10.setColumn(iLoop, "REPH_NO", sRephNo);
				this.ds_Detail10.setColumn(iLoop, "WOFH_SQ", sWofhSq);  
			}
		} 
	//} //if  
	// 저장시 해당관리번호에 자료있으면 먼저 삭제후 다시 저장
	} else if (this.fvDelChk =="Y") { 
	//this.alert('fvDelChk~~~' + fvDelChk);
	
		this.ds_Input.clearData();
		this.ds_Input.addRow();	
		this.ds_Input.setColumn(0, "REPH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition,"REPH_NO"));
		this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition,"WOFH_SQ"));
		
		//[공통]서비스정보 초기화
		this.gfn_InitComService(this);
		
		//[공통]서비스정보 추가
		var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
		svcAddObj.sSelectSql = "ca:M_CA_1130_D_DL10";  // 해당관리번호 삭제
		//svcAddObj.sSelectSql = "hr:M_HS_8225_D_DL11";
		 
		this.gfn_AddComSearchService(this, svcAddObj);
		
	
		
		//[공통]서비스 호출 변수
		var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
		svcCallObj.sTrid = "DTL_DELETE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
		svcCallObj.sActionName = this.G_SV_CA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
		svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
		svcCallObj.sOutDatset = "";//출력 데이터셋
		svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
		svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
		svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID

		//[공통]서비스 호출
		this.gfn_CallComService(this, svcCallObj);	
	
	}//I  
	
	var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position	
	
	var sCommCd = this.ds_Master10.getColumn(iMasterRowPos, "COMM_CD");//회사코드
	var sRephNo = this.ds_Master10.getColumn(iMasterRowPos, "REPH_NO");//관리번호
	var sWofhSq = this.ds_Master10.getColumn(iMasterRowPos, "WOFH_SQ");//결재차수


	//Detail10 데이터셋에 기본정보 입력-회사코드/관리번호/결재차수
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		sChkTemhNo = this.ds_Detail10.getColumn(iLoop, "REPH_NO");//관리번호
	//	sChkTemhNo = this.ds_Detail10.getColumn(iLoop, "COMM_CD");//관리번호//org

		if (this.gfn_IsNull(sChkTemhNo)) {
			this.ds_Detail10.setColumn(iLoop, "COMM_CD", sCommCd); //add
			this.ds_Detail10.setColumn(iLoop, "REPH_NO", sRephNo);
			this.ds_Detail10.setColumn(iLoop, "WOFH_SQ", sWofhSq);  			
		}
	} 

	
	//저장시 재조회 Key설정
	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "REPH_NO");//조회Key01
	this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//조회Key02

	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
