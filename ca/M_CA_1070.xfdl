 ***********************************************************************************************/
this.fn_ComBtnSearch = function()
{ 

	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;
	
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	//var nTabIdx = this.div_MasterInput.form.tab_Master.tabindex;
	 
// 	if(nTabIdx == 0){
// 		this.ds_Master10.clearData();
// 	}else{
// 		this.ds_Master20.clearData();
// 	}
	this.ds_Input.clearData();
	
    //저장후 재조회시-마스터 데이터셋 이벤트 비활성화
	if (this.fvSchKeyYn) {
		this.ds_Master10.set_enableevent(false);
	}		
	
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	//this.ds_Master10.clearData();
	this.ds_Input.clearData();
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_FromDd.value.substring(0, 6));  //적용년월
	this.ds_Input.setColumn(0, "ACUM_CD",  this.div_Search.form.edt_AcumCd.value); //회계단위
	this.ds_Input.setColumn(0, "PROC_ST", this.div_Search.form.cbo_ProcSt.value);
	this.ds_Input.setColumn(0, "TYMA_NM", this.div_Search.form.cbo_TymaCd.value);


	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	var selectedTabIdx = this.div_MasterInput.form.tab_Master.tabindex;
	var callPageNumber = 10;
	var callPageNumber1;
	
	switch(selectedTabIdx) {
		case 0: 
		/*제조원가*/
			callPageNumber = "10"; 
			this.ds_Master10.createDataset();
			break;		
		case 1: 
		/*재료비_직접*/
			callPageNumber = "20"; 
			callPageNumber1 = "21"; 
			this.ds_Master20.createDataset();
			this.ds_Master21.createDataset();
			break;
		case 2: 
		/*재료비_간접*/
			callPageNumber = "30"; 
			callPageNumber1 = "31"; 
			this.ds_Master30.createDataset();
			this.ds_Master31.createDataset();
			break;
		case 3: 
		/*노무비_직접*/
			callPageNumber = "40"; 
			callPageNumber1 = "41"; 
			this.ds_Master40.createDataset();
			this.ds_Master41.createDataset();
			break;
		case 4: 
		/*노무비_간접*/
			callPageNumber = "50"; 
			callPageNumber1 = "51"; 
			this.ds_Master50.createDataset();
			this.ds_Master51.createDataset();
			break;
		case 5: 
		/*외주비*/
			callPageNumber = "60"; 
			callPageNumber1 = "61"; 
			this.ds_Master60.createDataset();
			this.ds_Master61.createDataset();
			break;
		case 6:
		/*경비_직접*/ 
			callPageNumber = "70"; 
			callPageNumber1 = "71"; 
			this.ds_Master70.createDataset();
			this.ds_Master71.createDataset();
			break;
		case 7: 
		/*경비간접*/
			callPageNumber = "80"; 
			callPageNumber1 = "81"; 
			this.ds_Master80.createDataset();
			this.ds_Master81.createDataset();
			break;
	}
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "ca:M_CA_1070_M_SL"+callPageNumber;
	this.gfn_AddComSearchService(this, svcAddObj);
	
	
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	
	
	if(selectedTabIdx != 0 ) {
		//[공통]서비스정보 추가
		var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
		svcAddObj.sSelectSql = "ca:M_CA_1070_M_SL"+callPageNumber1;
		this.gfn_AddComSearchService(this, svcAddObj);  
		svcCallObj.sOutDatset = "ds_Master"+callPageNumber+"=ds_Master"+callPageNumber
		                      + " " + "ds_Master"+callPageNumber1+"=ds_Master"+callPageNumber1;//출력 데이터셋
		
	} else { 
		svcCallObj.sOutDatset = "ds_Master"+callPageNumber+"=ds_Master"+callPageNumber;//출력 데이터셋
	}
	
	//[공통]서비스 호출 변수
	
	svcCallObj.sTrid = "MST_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_CA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	

	//svcCallObj.sOutDatset += ;//출력 데이터셋1
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId ;//프로그램ID	svcCallObj.sOutDatset = "ds_Master10=ds_Master10 ds_Master20=ds_Master20 ds_Master30=ds_Master30 ds_Master40=ds_Master40 ds_Master50=ds_Master50 ds_Master60=ds_Master60 ds_Master70=ds_Master70";//출력 데이터셋
	

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);
	
}

