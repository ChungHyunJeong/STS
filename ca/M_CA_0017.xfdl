{
	//조회조건 필수입력값 있으면 체크
	//[검색]NULL 체크										 
	var arrArgs = ["this.div_Search.form.cal_FromYy^" + this.div_Search.form.sta_FromYy.text + "|NULL"
			//	  ,"this.div_Search.form.cal_ClosYy^" + this.div_Search.form.sta_FromYy.text + "|NULL"
				  ,"this.div_Search.form.edt_DepmCd^" + this.div_Search.form.sta_DepmCd.text + "|NULL"
				  ];
					  
	var sCheckGroupTitle = "조회조건";//체크그룹 타이틀

		//컴포넌트 Vaild 체크
		if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
	//	this.alert('2222');
			return false;
		}				  
				  
// 	//컴포넌트 Vaild 체크
// 	if (!this.gfn_CheckFormCompValid(this, arrArgs) ) {
// 		return false;
// 	}
	
	return true;
}

