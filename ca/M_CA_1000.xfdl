this.fn_GetDetail = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{

	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Detail10.clearData();
	this.ds_Input.clearData();
	
	//[업무] 조회조건 입력 : 거래처관리 대상(고객정보는 거래처 row 조회시)
	this.ds_Input.addRow();
	
	this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_FromDd.value);
	this.ds_Input.setColumn(0, "CLOS_DD", this.div_Search.form.cal_FromDd.value);
	this.ds_Input.setColumn(0, "MGAS_MM", this.div_Search.form.cal_FromDd.value.substr(0,6));
	this.ds_Input.setColumn(0, "SESU_ID", this.gfn_GetSessionValue("SYST_ID"));
	this.ds_Input.setColumn(0, "PRGM_ID", "M_CA_1000");//프로그램ID
	this.ds_Input.setColumn(0, "ACUM_CD", this.div_Search.form.edt_AcumCd.value);

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object	
	svcAddObj.sSelectSql = "ca:M_CA_1000_D_SL10";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_CA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
	
}


/***********************************************************************************************
 * 15.06. [사용자정의함수] 일괄생성
 ***********************************************************************************************/
this.div_DetailInput_btn_AllExec_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if(this.ds_Detail10.getRowCount()<=0) return;
	//실행완료 데이터가 있는경우..
	if(this.ds_Detail10.findRow("CONF_YN","1")>=0){
		this.gfn_Alert("SM066",["집계실행"]);
		return;
	}	
	
	this.ds_SpRst01.clearData();
	this.ds_SpRst02.clearData();
	this.ds_Detail11.clearData();
	this.ds_Detail11.addRow();
	this.ds_Detail11.copyRow(0, this.ds_Detail10, 0);

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "ca:M_CA_1000_D_IN11";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid 		= "COST_BATCH";			//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName 	= this.G_SV_CA;			//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset 	= "ds_Detail11=ds_Detail11" 	//입력 데이터셋
	svcCallObj.sOutDatset 	= "ds_SpRst01=ds_SpRst01";//출력 데이터셋
	svcCallObj.sOtherArg 	= "";					//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";		//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.isSync = true;//동기식으로 설정할지 여부

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	

	if (this.ds_SpRst01.getRowCount() > 0) {
		if (this.ds_SpRst01.getColumn(0, "O_RET_CODE") == "TRUE") {
			this.gfn_Alert("SM066",["실행"]);
			this.fn_GetDetail();
		} else {
			var arrMsgArg = [this.ds_SpRst01.getColumn(0, "O_RET_CMSG")];
			this.gfn_Alert("SM067", arrMsgArg);	
		}
	} else {
		//this.gfn_Alert("SM067",["실행"]);
		this.gfn_Alert("SM066",["실행"]);
		this.fn_GetDetail();
	}
};

/***********************************************************************************************
 * 15.06. [사용자정의함수] 일괄취소
 ***********************************************************************************************/
this.div_DetailInput_btn_AllCancel_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if(this.ds_Detail10.getRowCount()<=0) return;
	
	//실행완료 데이터가 없는경우..
	if(this.ds_Detail10.findRow("CONF_YN","1") < 0){
		return;
	}
	
	this.ds_Detail11.clearData();
	this.ds_Detail11.addRow();
	this.ds_Detail11.copyRow(0, this.ds_Detail10, this.ds_Detail10.rowposition);
	this.ds_Detail11.setColumn(0,"MODM_CD","ALL");
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
			
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "ca:M_CA_1000_D_DL11";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid 		= "COST_BATCH";			//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName 	= this.G_SV_CA;			//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset 	= "ds_Detail11=ds_Detail11" 	//입력 데이터셋
	svcCallObj.sOutDatset 	= "gds_WfRst=gds_WfRst";//출력 데이터셋
	svcCallObj.sOtherArg 	= "";					//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";		//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.isSync = true;//동기식으로 설정할지 여부

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	

	//결재상태업데이트 결과값 정보가 있으면		
	if (nApp.gds_WfRst.getRowCount() > 0) {
		if (nApp.gds_WfRst.getColumn(0, "O_RET_CODE") == "TRUE") {
			this.gfn_Alert("SM066",["취소"]);
			this.fn_GetDetail();
		} else {
			var arrMsgArg = [nApp.gds_WfRst.getColumn(0, "O_RET_CMSG")];
			this.gfn_Alert("SM067", arrMsgArg);	
		}
	} else {
		this.gfn_Alert("SM067",["취소"]);
	}
};


this.fn_Decide = function(aaa) 
{
	
	var sProcGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PROC_GU");	
		
	if(sProcGu == "0"){
		return 'button';
	}else{
		return 'normal';
	}

	
}


