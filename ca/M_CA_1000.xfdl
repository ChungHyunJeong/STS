  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {
	
		var sProcGu = this.ds_Master10.getColumn(e.newrow, "PROC_GU");					
			
			
		if(sProcGu == "0"){				
			this.div_DetailInput.form.btn_AllExec.set_enable(true);
			this.div_DetailInput.form.btn_AllCancel.set_enable(true);			

			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 5, "displaytype", "buttoncontrol");
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 6, "displaytype", "buttoncontrol");
			
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 5, "edittype", "button");
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 6, "edittype", "button");
			
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 5, "text", "실행");
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 6, "text", "취소");
			
			
		}else{ 
			this.div_DetailInput.form.btn_AllExec.set_enable(false);
			this.div_DetailInput.form.btn_AllCancel.set_enable(false);					
			
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 5, "displaytype", "normal");
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 6, "displaytype", "normal");
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 5, "edittype", "none");
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 6, "edittype", "none");
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 5, "text", "");
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 6, "text", "");

		}
	}
};


