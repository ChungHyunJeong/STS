 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
			//입력영역 활성화
			this.div_MasterInput.set_enable(true);	//조회 후 마스터 화면 활성화 (조회결과가 없을시에도 활성화)
			this.div_DetailInput.set_enable(true);
			//결산집계현황 조회
			this.fn_GetDetail();			
			
			
			if (this.ds_Master10.getRowCount() > 0) {						

				//입력영역 활성화
				//this.div_DetailInput.set_enable(true);

				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10);
				
				var sProcGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PROC_GU");
				
				if(sProcGu == "0"){				
					this.div_DetailInput.form.btn_AllExec.set_enable(true);
					this.div_DetailInput.form.btn_AllCancel.set_enable(true);								

					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 5, "displaytype", "buttoncontrol");
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 6, "displaytype", "buttoncontrol");
					
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 5, "edittype", "button");
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 6, "edittype", "button");
					
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 5, "text", "실행");
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 6, "text", "취소");

					
				}else{ 
					this.div_DetailInput.form.btn_AllExec.set_enable(false);
					this.div_DetailInput.form.btn_AllCancel.set_enable(false);					
					
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 5, "displaytype", "normal");
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 6, "displaytype", "normal");
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 5, "edittype", "none");
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 6, "edittype", "none");
					
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 5, "text", "");
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 6, "text", "");
				}

			} else {
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}

				//입력영역 비활성화
				//this.div_DetailInput.set_enable(false);
				
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10);
				
				
				this.div_DetailInput.form.btn_AllExec.set_enable(false);
				this.div_DetailInput.form.btn_AllCancel.set_enable(false);					
				
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 5, "displaytype", "normal");
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 6, "displaytype", "normal");
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 5, "edittype", "none");
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 6, "edittype", "none");
				
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 5, "text", "");
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 6, "text", "");
				
				
				
			}		

			//저장후 재조회여부-true 
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}	
			
			break;
		case "SAVE":   //저장		
			//결산집계현황 조회
			this.fn_GetDetail();

			break;	

		case "DTL_SEARCH": //조회		
			
			this.ds_Detail10.addColumn("crudType", "string");
			break;			

		case "DELETE": //삭제		
			break;							
	}
};


/***********************************************************************************************
 * 13.02. [팝업] CallBack 함수 처리
          strId : 팝업호출시 지정한 팝업ID
	      rtnValue : 팝업창에서 close시 지정한 반환값
          주의 : Open(popuptype=modeless)창은 팝업콜백을 지원하지 않음
 ***********************************************************************************************/ 
this.fn_PopupCallback = function(strId, rtnValue)
{
	 if(strId == "M_ACUM_CD_01"){
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.ACUM_CD)) {						
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumCd.set_value(arrRtnVal.ACUM_CD);					 
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumNm.set_value(arrRtnVal.ACUM_NM);
			}else{
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumCd.set_value("");					 
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumNm.set_value("");
			}
		}
	}
	
}
