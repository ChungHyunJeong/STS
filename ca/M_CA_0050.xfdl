			trace("bilhDd --> " + bilhDd);
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MTPH_DD", bilhDd);//선급일자-오늘날자로 설정
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MTPH_MM", bilhDd.substring(0,6));//결산년월을 yyyyMM 으로 만든다.
			
			//발의내용요약 자동 입력
			this.fn_SetAcchNm();
			
			break;			
			
		
		case "DELETE": //삭제		
			this.fn_ComBtnSearch();//조회
			break;	
			
		case "MST_SEARCH_EXISTS1":  
			
			break;	
			
		case "MST_DELETE":  
			
			break;	
			
		case "WF_PROCESS": 
			this.ds_Detail10.copyData(this.ds_Detail13, true);
			
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.set_enableredraw(false);
			
			for(var iLoop = 0; iLoop < this.ds_Detail10.rowcount; iLoop++){
				this.ds_Detail10.setColumn(iLoop, "crudType", "I");				
			}			
			
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.set_enableredraw(true);
			
			sCreateChk = 'Y';	//---중요(자재전표생성 버튼을 눌렀을때 기존자료 삭제하고 저장
			
			break;	
			
		case "WF_RECIDE": //[결재]재기안처리		
			if (nApp.gds_WfRst.getRowCount() > 0) {
				if (nApp.gds_WfRst.getColumn(0, "O_RET_CODE") == "TRUE") {
					this.gfn_Alert("WF012");//재기안 처리가 완료되었습니다.
					this.fvSchKeyYn = true;//저장후 재조회여부
					this.fn_ComBtnSearch();//저장후 재조회					
				} else {
					trace(nApp.gds_WfRst.saveXML());
					var arrMsgArg = [nApp.gds_WfRst.getColumn(0, "O_RET_CMSG")];
					this.gfn_Alert("WF013", arrMsgArg);//재기안 처리시 오류가 발생하였습니다.\n&ITM0				
				}
			} else {
				var arrMsgArg = [""];
				this.gfn_Alert("WF013", arrMsgArg);//재기안 처리시 오류가 발생하였습니다.\n&ITM0
			}
			break;					
	}
};

