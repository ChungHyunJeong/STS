this.fn_VaildSearch = function()
{
	//[결재] 결재내역 조회가 아니면 (if문)
	if (!this.fvWfSearchMode) {
		if (!this.gfn_CheckFromToDate(this.fvComPrgmId, this.div_Search.form.cal_TjchDdFr, this.div_Search.form.cal_TjchDdTo)) return false;//조회기간 범위 체크
		//조회조건 필수입력값 있으면 체크
			// Validation 설정
		var arrArgs = ["this.div_Search.form.edt_DepmCd^요청부서|NULL"
					  ]; 
					  
		var sCheckGroupTitle = "검색조건";//체크그룹 타이틀

		//컴포넌트 Vaild 체크
		if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
			return false;
		}
	}
	return true;
}

