	sPks += "ⓐ" + this.ds_Master10.getColumn(iRow, "WOFH_SQ");//결재차수

	var iFindRow = this.ds_FaSaum.findRowExpr("SAUM_CD == '" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SAUM_CD") + "'");
	var sSaumNm  = this.ds_FaSaum.getColumn(iFindRow, "SAUM_NM");
	
	var sMtphGu  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MTPH_GU");
