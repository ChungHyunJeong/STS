this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	//부가세코드 선택시 : 공급가액, 부가세, 합계금액 구하는 Function 호출
	if (e.columnid == "TRJB_AM") {
		if (!this.gfn_IsNull(this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "TRJB_AM"))) {
			this.fn_VtaxAmountSum(); //공급가액, 부가세, 합계 Function 호출
			
			//Detail10 부가세 계산 (단건)
			var iVatmRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "VATM_RT"); //부가세율
			if (!this.gfn_IsNull(iVatmRt)) {	//부가세율 존재시
				var iTrjbAm = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "TRJB_AM"); //운송금액
				
				var iVtaxAm = nexacro.floor(iTrjbAm * iVatmRt, 0); //부가세 = 운송금액 * 부가세율			
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "VTAX_AM", iVtaxAm); //부가세
			}
		}

	// 체크박스 체크해제시 (curdType D 해제)
	} else if (e.columnid == "s_chk") {
		var s_chk = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "s_chk"); 
	
		//s_chk가 변경 대상인 경우
		if (s_chk == '0') {		
			this.fn_VtaxAmountSum(); //공급가액, 부가세, 합계 Function 호출
		}
	}
	
};


