 * 02.MG0.07. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//부가세코드 선택시 : 공급가액, 부가세, 합계금액 구하는 Function 호출
	if (e.columnid == "VATM_CD") {
		var sVatmCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition,"VATM_CD");	//부가세코드
		if (!this.gfn_IsNull(sVatmCd)) {			

			var iFindRow = this.ds_FaVatm.findRowExpr("VATM_CD=='" + sVatmCd + "'");
			var iVatmRt = this.ds_FaVatm.getColumn(iFindRow, "VATM_RT");	//부가세율
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "VATM_RT",iVatmRt);	//부가세율
			
			this.fn_VtaxAmountSum(); //공급가액, 부가세, 합계 Function 호출
			
			//Detail10 부가세 계산 (전체)
			var iVatmRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "VATM_RT"); //부가세율
			var iTrjbAm = 0; //운송금액
			var iVtaxAm = 0; //부가세
			for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
				iTrjbAm = this.ds_Detail10.getColumn(iLoop, "TRJB_AM"); //운송금액
				
				iVtaxAm = nexacro.floor(iTrjbAm * iVatmRt, 0);  	   //부가세 = 운송금액 * 부가세율 (원단위)
 				this.ds_Detail10.setColumn(iLoop, "VTAX_AM", iVtaxAm); //부가세
			}
		}

	}
	
};

