                  <Tabpage id="tpg_02" text="자재전표상세">
                    <Layouts>
                      <Layout>
                        <Grid id="grd_Detail10" taborder="0" left="0" top="2" binddataset="ds_Detail10" autofittype="none" right="0" bottom="0">
                          <Formats>
                            <Format id="default">
                              <Columns>
                                <Column size="35"/>
                                <Column size="80"/>
                                <Column size="55"/>
                                <Column size="120"/>
                                <Column size="80"/>
                                <Column size="90"/>
                                <Column size="250"/>
                                <Column size="170"/>
                                <Column size="55"/>
                                <Column size="80"/>
                                <Column size="100"/>
                                <Column size="123"/>
                                <Column size="332"/>
                              </Columns>
                              <Rows>
                                <Row size="26" band="head"/>
                                <Row size="24"/>
                                <Row size="24" band="summ"/>
                              </Rows>
                              <Band id="head">
                                <Cell text="No." autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="1" text="계약번호" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="2" text="계약순번" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="3" text="W/O" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="4" text="수불일자" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="5" text="자재코드" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="6" text="자재명" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="7" text="규격" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="8" text="단위" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="9" text="수량" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="10" text="금액" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="11" text="수불코드" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="12" text="비고"/>
                              </Band>
                              <Band id="body">
                                <Cell expr="currow + 1" textAlign="center" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="1" text="bind:CNTH_NO" displaytype="mask" textAlign="center" maskedittype="string" maskeditformat="expr:(CNTH_NO.length &gt; 8)?'@@@@-@-@@@@':'@@@-@@@@'" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="2" text="bind:CNTB_SE" displaytype="normal" textAlign="center" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="3" text="bind:WORB_NO" displaytype="mask" textAlign="center" maskedittype="string" maskeditformat="expr:comp.parent.gfn_WorbNoFormat(WORB_NO)" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="4" text="bind:MTPB_DD" displaytype="date" textAlign="center" calendardateformat="yyyy-MM-dd" calendardisplaynulltype="none" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="5" text="bind:ITEM_CD" displaytype="normal" textAlign="left" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="6" text="bind:ITEM_NM" displaytype="normal" textAlign="left" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="7" text="bind:SPEC_NM" displaytype="normal" textAlign="left" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="8" text="bind:UNIT_NM" displaytype="normal" textAlign="center" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="9" text="bind:MTPB_QT" displaytype="mask" textAlign="right" maskeditformat="#,###,###,##0" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="10" text="bind:MTPB_AM" displaytype="mask" textAlign="right" maskeditformat="#,###,###,##0" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="11" text="bind:RADM_NM" displaytype="normal" textAlign="left" autosizecol="none" autosizerow="none" controlautosizingtype="none"/>
                                <Cell col="12" text="bind:REMK_NM" displaytype="normal" textAlign="left"/>
                              </Band>
                              <Band id="summary">
                                <Cell/>
                                <Cell col="1" text="합계" textAlign="center"/>
                                <Cell col="2"/>
                                <Cell col="3"/>
                                <Cell col="4"/>
                                <Cell col="5"/>
                                <Cell col="6"/>
                                <Cell col="7"/>
                                <Cell col="8"/>
                                <Cell col="9" expr="dataset.getSum(&quot;MTPB_QT&quot;)" displaytype="mask" maskeditformat="#,###,###,###,##0"/>
                                <Cell col="10" expr="dataset.getSum(&quot;MTPB_AM&quot;)" displaytype="mask" maskeditformat="#,###,###,###,##0"/>
                                <Cell col="11"/>
                                <Cell col="12"/>
                              </Band>
                            </Format>
                          </Formats>
                        </Grid>
                      </Layout>
