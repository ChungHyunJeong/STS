this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	var iFvFindRow = -1;
	
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	//부서
	if (e.columnid == "DEPM_CD") {
		this.gfn_GetCommDepmGrid(obj, "DEPM_CD", "DEPM_NM");//부서명 검색
	} else if (e.columnid == "ACOM_CD") {
		this.gfn_GetCommAcomGrid(obj, "ACOM_CD", "ACOM_NM"); // 계정명검색
	} else if(e.columnid == "s_chk"){
		return;
	} else if(e.columnid == "crudType"){
		return;
	} 
	
	//이전자료비교
	if(!this.gfn_IsNull(this.ds_Master10.getColumn(e.row, "BGCM_YM"))
	&& !this.gfn_IsNull(this.ds_Master10.getColumn(e.row, "DEPM_CD"))
	&& !this.gfn_IsNull(this.ds_Master10.getColumn(e.row, "ACOM_CD"))
	){
	
		this.fn_PreSearch(this.ds_Master10.getColumn(e.row, "BGCM_YM")
						 , this.ds_Master10.getColumn(e.row, "BGCM_YM")
						 , this.ds_Master10.getColumn(e.row, "DEPM_CD")
						 , this.ds_Master10.getColumn(e.row, "ACOM_CD"));
						 
						 
		iFvFindRow = this.ds_Master10_Pre.findRowExpr("BGCM_YM=='" + this.ds_Master10.getColumn(e.row, "BGCM_YM") +  "' && DEPM_CD=='" + this.ds_Master10.getColumn(e.row, "DEPM_CD") +  "' && ACOM_CD=='"+this.ds_Master10.getColumn(e.row, "ACOM_CD")+"'");//업무Key Row 찾기
		
		if (iFvFindRow > -1) {
			this.ds_Master10.setColumn(e.row, "BGMM_AM", this.ds_Master10_Pre.getColumn(iFvFindRow, "CHAN_AM")); //예산금액(최초)
		}else{
			this.ds_Master10.setColumn(e.row, "BGMM_AM", 0); //예산금액(최초)
		}
						 
	}
};

/***********************************************************************************************
 * 02.MG0.08. [Master 데이터셋] 데이터변경전 이벤트
 ***********************************************************************************************/
this.ds_Master10_cancolumnchange = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
