/*---------------------------------------------------------------------------------------------
 *    [Master 입력영역-Button] 엑셀업로드 Call Back 함수
  ---------------------------------------------------------------------------------------------*/
this.fn_ImportCallback = function(sImportId)
{	
	/*
		  1. 엑셀업로드 
		  2. 이전에 입력한 값이 있는지 확인
		  3. 이전값이 있으면 변경전에 넣어준다.
	*/
	//최소월과 최대월을가져온다
	
    var FromDd = this.ds_Import.getMin("Column0");
	var ClosDd = this.ds_Import.getMax("Column0");
	var iFvFindRow = -1;
    var sBgcmYm = "";
    var sDepmCd = "";
    var sAcomCd = "";
	
	
	
	this.fn_PreSearch(FromDd, ClosDd)
	
	



	this.ds_Master10.set_enableevent(false);
	
	for (var iLoop=0; iLoop<this.ds_Import.getRowCount(); iLoop++) {
		var iCol = this.ds_Import.getColumn(iLoop, "Column0"); 
		
		if (this.gfn_IsNull(iCol)) {
		
			this.ds_Import.deleteRow(iLoop);			
			iLoop = iLoop - 1;
		} else {
			this.ds_Master10.addRow();
			//년월
			sBgcmYm = this.ds_Import.getColumn(iLoop, "Column0");
			//부서코드
			sDepmCd = this.ds_Import.getColumn(iLoop, "Column1");
			//계정코드
			sAcomCd = this.ds_Import.getColumn(iLoop, "Column3");
			
			
			this.ds_Master10.setColumn(iLoop, "BGCM_YM", sBgcmYm); //년월
			this.ds_Master10.setColumn(iLoop, "DEPM_CD", sDepmCd); //부서코드
			this.ds_Master10.setColumn(iLoop, "DEPM_NM", this.ds_Import.getColumn(iLoop, "Column2")); //부서명
			this.ds_Master10.setColumn(iLoop, "ACOM_CD", sAcomCd); //계정코드
			this.ds_Master10.setColumn(iLoop, "ACOM_NM", this.ds_Import.getColumn(iLoop, "Column4")); //계정명
			
			//이전자료 찾기
			iFvFindRow = this.ds_Master10_Pre.findRowExpr("BGCM_YM=='" + sBgcmYm +  "' && DEPM_CD=='" + sDepmCd +  "' && ACOM_CD=='"+sAcomCd+"'");//업무Key Row 찾기
			trace("iFvFindRow = " + iFvFindRow);
			if (iFvFindRow > -1) {
				this.ds_Master10.setColumn(iLoop, "BGMM_AM", this.ds_Master10_Pre.getColumn(iFvFindRow, "CHAN_AM")); //예산금액(최초)
			}else{
				this.ds_Master10.setColumn(iLoop, "BGMM_AM", 0); //예산금액(최초)
			}
			
			//최초입력시 변경금액을 필요없음
			this.ds_Master10.setColumn(iLoop, "CHAN_AM", this.ds_Import.getColumn(iLoop, "Column5")); //예산금액(변경)
			this.ds_Master10.setColumn(iLoop, "REMK_NM", this.ds_Import.getColumn(iLoop, "Column6")); //비고
			
			this.ds_Master10.setColumn(iLoop, "crudType", "I"); //저장시 ds_detail10 의 내용을 저장키 위함.
		}
	} 
	
	this.ds_Master10.set_enableevent(true);  
}

