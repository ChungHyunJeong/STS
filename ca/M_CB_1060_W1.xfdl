// 조회를 하든 추가, 수정을 하든 이 함수에서 예산액을 셋팅한다.
this.fn_setAm = function(v_row, v_reg_am) {

	this.fn_getHanDoAmBeforeAdd(); // 예산 한도액, 사용액 셋팅
	
	// 조정전금액
	var bgad_am = nexacro.toNumber(this.ds_Detail10.getColumn(v_row, "BGAD_AM")); // 조정요청액
	var HanDoAmBeforeAdd = nexacro.toNumber(this.ds_Detail12.getColumn(v_row, "BGMM_AM")); // 한도액
	var UsedAmBeforeAdd = nexacro.toNumber(this.ds_Detail12.getColumn(v_row, "USED_AM")); // 사용액
	
// 	
// 	var bgad_am = nexacro.toNumber(this.ds_Detail10.getColumn(v_row, "BGAD_AM")); // 추가요청액
// 	var HanDoAmBeforeAdd = nexacro.toNumber(this.ds_Detail12.getColumn(v_row, "BGMM_AM")); // 한도액
// 	var UsedAmBeforeAdd = nexacro.toNumber(this.ds_Detail12.getColumn(v_row, "USED_AM")); // 사용액
	
	if(this.gfn_IsNull(bgad_am)) { bgad_am = 0; }
	if(this.gfn_IsNull(HanDoAmBeforeAdd)) { HanDoAmBeforeAdd = 0; }
	if(this.gfn_IsNull(UsedAmBeforeAdd)) { UsedAmBeforeAdd = 0; }
	
	var remainAmBeforAdd = HanDoAmBeforeAdd-UsedAmBeforeAdd; // 잔액
	
	this.ds_Detail10.set_enableevent(false);
	
	this.ds_Detail10.setColumn(v_row, "hando_am_before", HanDoAmBeforeAdd); // 한도액
	this.ds_Detail10.setColumn(v_row, "used_am_before", UsedAmBeforeAdd); // 사용액
	this.ds_Detail10.setColumn(v_row, "remain_am_before", remainAmBeforAdd); // 잔액
	
	// 추가후금액
	this.ds_Detail10.setColumn(v_row, "hando_am_after", HanDoAmBeforeAdd+bgad_am); // 한도액+추가요청액
	this.ds_Detail10.setColumn(v_row, "usage_am_after", bgad_am+remainAmBeforAdd); // 사용가능금액(추가요청액+추가전 잔액)
	
	this.ds_Detail10.set_enableevent(true);
	
}

