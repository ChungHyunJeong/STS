 * 05.03. [Vaildation] Detail 저장 필수값 체크 - 그리드항목
 ***********************************************************************************************/ 
this.fn_VaildDetailSave = function()
{
	//[저장] Validation 설정
	var arrArgs = ["BGRB_YM^NULL"
	              ,"DEPM_CD^NULL"
			      ,"ACOM_CD^NULL"
			      ,"BGAD_AM^NULL"
			      ]; 
				  
	var sCheckGroupTitle = this.div_DetailInput.form.tab_Detail.tpg_01.text;//체크그룹 타이틀
				  
	//Grid Vaild 체크
	if (!this.gfn_CheckGridValid(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10, arrArgs, sCheckGroupTitle) ) {
		return false;
	}

	if(this.ds_Detail10.rowcount == 0) {
		this.gfn_Alert("CA030"); // 예산신청상세 내용이 없습니다.
		return false;
	}

	// 신청구분 체크
	var sGubun = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BGRH_CD"); /* 0010:일반, 0020:법인카드, 0030:계정간이동 */
	
	if(sGubun == "0030") { // 계정간 이동
		if(this.ds_Detail10.getSum("BGAD_AM") != 0) {
			this.gfn_Alert("CA029"); // 계정간이동의 합이 0이 아닙니다.
			return false;
		}
	}
	
	//[예산통제] 예산편성확정 여부 및 예산마감여부 체크
	var arrCheckMonth = new Array();//체크할 편성년월 정보
	var sBGRB_YM = "";//편성년월
	var sCrudType = "";//CRUD 타입
	
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		sCrudType = this.ds_Detail10.getColumn(iLoop, "crudType");//CRUD 타입
		
		if (!this.gfn_IsNull(sCrudType)) {
			sBGRB_YM = this.ds_Detail10.getColumn(iLoop, "BGRB_YM").substr(0,6);//편성년월
			if (arrCheckMonth.indexOf(sBGRB_YM) < 0) {
				arrCheckMonth.push(sBGRB_YM);
			}
		}
	}
	
	if (arrCheckMonth.length > 0) {
		for (var iLoop=0; iLoop<arrCheckMonth.length; iLoop++) {
		
			sBGRB_YM = arrCheckMonth[iLoop];//편성년월
			
			//[예산통제] 예산마감여부 체크
			var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
			var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
			paramObj.MGAS_MM = sBGRB_YM;//마감년월
			paramObj.MODM_CD  = "CB";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
			paramObj.ACUM_CD  = "";//[선택] 회계단위
			paramObj.BUSM_CD  = "";//[선택] 사업부문
			paramObj.SAUM_CD  = "";//[선택] 사업장코드
			paramObj.DEPM_CD  = "";//[선택] 부서코드
			paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
			paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
			
			var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);
			trace('예산이 마감 되었을경우');
			//예산이 마감 되었을경우
			if (bCheck) {
				this.gfn_Alert("CA016");//해당월의 예산이 마감되어서 등록하실수 없습니다.
				trace('예산이 마감 되었을경우1');
				return false;
			}				
			trace('예산통제');
			
			//[예산통제] 예산편성확정여부 체크
			var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
			var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
			paramObj.MGAS_MM  = sBGRB_YM;//마감년월
			paramObj.MODM_CD  = "CC";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
			paramObj.ACUM_CD  = "";//[선택] 회계단위
			paramObj.BUSM_CD  = "";//[선택] 사업부문
			paramObj.SAUM_CD  = "";//[선택] 사업장코드
			paramObj.DEPM_CD  = "";//[선택] 부서코드
			paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
			paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
			
			var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);

			//예산편성확정이 되지 않았을경우
			trace('예산통제1');
			/*//2022.06.07 주석처리
			if (!bCheck) {
				trace('예산통제2');
				
				this.gfn_Alert("CA015");//해당월의 예산편성이 확정되지 않았습니다.\n예산편성 확정후에 사용이 가능합니다.
				//trace("===================");
				return false;
			}
			*/
						
		}
	}
	
	return true;
