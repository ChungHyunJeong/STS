};



// 추가, 수정시 이 함수에서 예산액을 셋팅한다.
this.fn_setAm = function(v_row, v_reg_am) {
//trace("fn_setAm --> " + "v_row -->"+v_row + "v_reg_am-->"+v_reg_am);
	this.fn_getHanDoAmBeforeAdd(v_row); // 예산 한도액, 사용액 셋팅
	
	// 추가전금액
// 	trace(this.ds_Detail12.saveXML());
// 	trace(this.ds_Detail12.getColumn(v_row, "BGMM_AM"));
	var bgad_am = this.ds_Detail10.getColumn(v_row, "BGAD_AM"); // 추가요청액
	var HanDoAmBeforeAdd = this.ds_Detail12.getColumn(0, "BGMM_AM"); // 한도액
	var UsedAmBeforeAdd = this.ds_Detail12.getColumn(0, "USED_AM"); // 사용액
	
	if(this.gfn_IsNull(bgad_am)) { bgad_am = 0; }
	if(this.gfn_IsNull(HanDoAmBeforeAdd)) { HanDoAmBeforeAdd = 0; }
	if(this.gfn_IsNull(UsedAmBeforeAdd)) { UsedAmBeforeAdd = 0; }
	
	var remainAmBeforAdd = nexacro.toNumber(HanDoAmBeforeAdd) - nexacro.toNumber(UsedAmBeforeAdd); // 잔액
	
// 	//trace("bgad_am --> " + bgad_am);
// 	//trace("HanDoAmBeforeAdd --> " + HanDoAmBeforeAdd);
// 	//trace("UsedAmBeforeAdd --> " + UsedAmBeforeAdd);
	
	this.ds_Detail10.set_enableevent(false);
	
	this.ds_Detail10.setColumn(v_row, "BGSU_AM", HanDoAmBeforeAdd); // 한도액
	this.ds_Detail10.setColumn(v_row, "USED_AM", UsedAmBeforeAdd); // 사용액
	this.ds_Detail10.setColumn(v_row, "REMAIN_AM", remainAmBeforAdd); // 잔액
	
	// 추가후금액
	this.ds_Detail10.setColumn(v_row, "hando_am_after", nexacro.toNumber(HanDoAmBeforeAdd) + nexacro.toNumber(bgad_am)); // 한도액+추가요청액
	this.ds_Detail10.setColumn(v_row, "usage_am_after", nexacro.toNumber(bgad_am) + nexacro.toNumber(remainAmBeforAdd)); // 사용가능금액(추가요청액+추가전 잔액)
	
	this.ds_Detail10.set_enableevent(true);
	
}

