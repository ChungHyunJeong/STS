			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
			oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BGRH_NO");//PK번호2-신청번호
			oFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수						
			
			this.fvFileDiv.fn_AtchFileSearch(oFileInfo);			
			
			if (this.ds_Detail10.getRowCount() > 0) {
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);
			} else {
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);			
			}
			//for(var i=0; i<this.ds_Detail10.getRowCount(); i++) {
// 				// 추가전금액
				
 				//this.ds_Detail10.setColumn(i, "hando_am_before", this.ds_Detail10.getColumn(i, "BGSU_AM")); // 한도액
 				//this.ds_Detail10.setColumn(i, "used_am_before", this.ds_Detail10.getColumn(i, "USED_AM")); // 사용액
 				//this.ds_Detail10.setColumn(i, "remain_am_before", this.ds_Detail10.getColumn(i, "REMAIN_AM")); // 잔액
// 				
// 				// 추가후금액
// 				this.ds_Detail10.setColumn(i, "hando_am_after", this.fn_getHanDoAmAfterAdd(this.ds_Detail10.getColumn(i, "BGAD_AM"))); // 한도액
// 				this.ds_Detail10.setColumn(i, "usage_am_after", this.fn_getHanDoAmAfterAdd(this.ds_Detail10.getColumn(i, "BGAD_AM"))-this.fn_getUsedAmBeforeAdd()); // 사용가능금액
//				this.fn_setAmDTLS(i, this.ds_Detail10.getColumn(i, "BGAD_AM"));
				
		//	}
			
			break;			
			
		case "DTLS_SEARCH": //미출고검색		
			this.ds_Detail20.addColumn("s_chk", "string");		
			this.ds_Detail20.addColumn("s_conf", "string");
			
			if (this.ds_Detail20.getRowCount() > 0) {
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20);
			} else {
