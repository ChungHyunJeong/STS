/*---------------------------------------------------------------------------------------------
 *    03. 법인카드예산생성 버튼 클릭
  ---------------------------------------------------------------------------------------------*/
this.div_DetailInput_div_CardInput_btn_CardExec_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if(this.gfn_IsNull(this.div_DetailInput.form.div_CardInput.form.cal_BgdpMm.value)){		
		this.gfn_Alert("SM008", ["예산년월"]);
		return false;
	}
	if(this.ds_Detail10.getRowCount() > 0){	
		this.gfn_Alert("SM059", ["예산신청상세", "예산신청상세 내역"]);
		return false;
	}
	this.ds_InputCard.clearData();
	this.ds_InputCard.addRow();
	this.ds_InputCard.setColumn(0, "BGDP_MM", this.div_DetailInput.form.div_CardInput.form.cal_BgdpMm.value.substr(0,6));
	this.ds_InputCard.setColumn(0, "SYST_ID", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SYST_ID"));
	this.ds_InputCard.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "ca:M_CB_1050_D_SL13";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "CARD_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_CA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_InputCard=ds_InputCard" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
};

/***********************************************************************************************
 * 02.DG0.07. [Detail 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	//trace("e.columnid --> " + e.columnid);
	//부서명
	if (e.columnid == "DEPM_CD") {
		if(this.gfn_IsNull(this.ds_Detail10.getColumn(e.row, "BGRB_YM"))) {
			alert("편성년월을 입력하세요");
			this.ds_Detail10.setColumn(e.row, e.columnid, '');
			return;
		}	
		this.gfn_GetCommDepmGrid(obj, "DEPM_CD", "DEPM_NM");//부서명 검색
	//계정정보
	} else if (e.columnid == "ACOM_CD") {
		if(this.gfn_IsNull(this.ds_Detail10.getColumn(e.row, "BGRB_YM"))) {
			alert("편성년월을 입력하세요");
			this.ds_Detail10.setColumn(e.row, e.columnid, '');
			return;
		}		
		this.gfn_GetCommAcomGrid(obj, "ACOM_CD", "ACOM_NM");//계정정보 검색
	}
	
	var bFlag = true;
	var cBGRB_YM = this.ds_Detail10.getColumn(e.row, "BGRB_YM");
	var oBGRB_YM = this.ds_Detail10.getOrgColumn(e.row, "BGRB_YM");
	var cDEPM_CD = this.ds_Detail10.getColumn(e.row, "DEPM_CD");
	var oDEPM_CD = this.ds_Detail10.getOrgColumn(e.row, "DEPM_CD");
	var cACOM_NM = this.ds_Detail10.getColumn(e.row, "ACOM_NM");
	var oACOM_NM = this.ds_Detail10.getOrgColumn(e.row, "ACOM_NM");
// 	//trace("cBGRB_YM --> " + cBGRB_YM);
// 	//trace("oBGRB_YM --> " + oBGRB_YM);
// 	//trace("cDEPM_CD --> " + cDEPM_CD);
// 	//trace("oDEPM_CD --> " + oDEPM_CD);
// 	//trace("cACOM_NM --> " + cACOM_NM);
// 	//trace("oACOM_NM --> " + oACOM_NM);
	// 어느것을 변경(선택)하든 아래 세 값이 변경되었을시 계산 다시한다.
	if(cBGRB_YM != oBGRB_YM && cDEPM_CD != oDEPM_CD && cACOM_NM != oACOM_NM) {
