this.fn_MsgCallback = function (strId, strVal)
{
	//MASTER 변경사항 저장여부
	if (strId == "CONF.MAST10_CHANGE") {
		if(strVal )
		{
			this.fn_ComBtnSave();//저장 서비스 호출
		} else {
			//데이터 변경여부 체크
			this.gfn_UndoColumn(this.div_MasterInput.form.tab_Detail.tpg_01.form.grd_Detail10);
			this.gfn_UndoColumn(this.div_MasterInput.form.tab_Detail.tpg_01.form.grd_Master10);
		}
	//Detail 변경사항 저장여부
	} else if (strId == "CONF.DETL10_CHANGE") {
		if(strVal )
		{
			this.fn_ComBtnSave();//저장 서비스 호출
		} else {
			this.gfn_UndoColumn(this.div_MasterInput.form.tab_Detail.tpg_01.form.grd_Detail10);	
		}
	}
	//삭제
	else if(strId == "CONF.DELETE"){
		if(strVal )
		{
			this.fn_SvcDelete();//삭제 서비스 호출
		}
	}
}; 
 
