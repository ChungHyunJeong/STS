/***********************************************************************************************
 * 05.02. [Vaildation] 저장 필수값 체크 - 입력항목 
 ***********************************************************************************************/ 
this.fn_VaildSave = function()
{

	//[저장] Validation 설정
	var arrArgs = ["PALM_CD^NULL"
	              ,"PALM_NM^NULL"
	              ,"PGRM_DD^NULL"
			      ]; 
				  
	var sCheckGroupTitle = this.div_MasterInput.form.tab_Detail.tpg_01.text;//체크그룹 타이틀
		  
	//Grid Vaild 체크
	if (!this.gfn_CheckGridValid(this.div_MasterInput.form.tab_Detail.tpg_01.form.grd_Detail10, arrArgs, sCheckGroupTitle)) {
		return false;
	}		

	var nRow  = this.ds_Detail10.findRowExpr("dataset.getRowType(rowidx) == 2 || dataset.getRowType(rowidx) == 4");		
			
	var cPgrmDd   = this.ds_Master10.getColumn(this.ds_Master10.rowposition,  "PGRM_DD")  //적용일자
	
	while (nRow > -1)
	{	
		
		var nRowType  = this.ds_Detail10.getRowType(nRow);
		var strPalmCd = this.ds_Detail10.getColumn(nRow,  "PALM_CD")  //회계기준코드				

		//--입력일때만 체크
		if(nRowType == 2) {
			//---회계기준코드
			this.ds_Input.clearData();
			this.ds_Input.addRow();
			
			this.ds_Input.setColumn(0,	"PALM_CD", strPalmCd);
			this.ds_Input.setColumn(0,	"PGRM_DD", cPgrmDd);
			
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "ca:M_CA_0015_M_SL11";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "MST_SEARCH_EXISTS";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_CA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Exists=ds_Exists";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
			svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
			svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID		
			svcCallObj.isSync = true;//동기식으로 설정할지 여부

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);																							  
			
			if(this.ds_Exists.rowcount > 0){
				var arrMsgArg = ["회계기준코드"];
				this.ds_Detail10.set_rowposition(nRow);
				this.div_MasterInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();
				this.div_MasterInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(this.div_MasterInput.form.tab_Detail.tpg_01.form.grd_Detail10.getBindCellIndex("body","PALM_CD")); 
				
				this.div_MasterInput.form.tab_Detail.tpg_01.form.grd_Detail10.showEditor(true);
				this.gfn_Alert("SM041");//중복된 데이터가 존재합니다.		
				return false; 
	        }
		 }		 
		
		nRow  = this.ds_Detail10.findRowExpr("dataset.getRowType(rowidx) == 2 || dataset.getRowType(rowidx) == 4", nRow + 1);
		
	};	
	
	//=====================================================================================================================//			
	
	return true;
}

