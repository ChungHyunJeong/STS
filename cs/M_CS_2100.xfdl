this.fn_SetWfCompEnable = function()
{ 	

	//[결재]결재내역 팝업화면으로 호출여부
	if (!this.fvWfPopupMode) {
		//[결재] 공통버튼 제어
		var bCommSaveButton = true;//저장버튼 활성화 여부-기본값은 false로 해야함. - 결재승인후 수정일 필요한 화면에서만 true로 설정해야함. 
		
		this.fvWofhStatus = this.gfn_SetCommButtonWf(this, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"), bCommSaveButton);//결재진행상태		
		//this.fvSerhStatus =  this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SERH_ST")=="0010" ? true : false;//서비스 진행상태	(접수 true)	
		this.fvSerhStatus =  this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SERH_ST");
	}
	
	//[결재]결재내역 조회모드이면 Visble false || 결재상태가 "등록"이 아닐경우
	if (this.fvWfSearchMode || this.fvWofhStatus ) {
		//[결재]결재내역 팝업화면으로 호출여부
		if (this.fvWfPopupMode) {
			this.div_Search.set_enable(false);//조회영역 Enable처리
			//this.div_DetailInput.form.tab_Detail.removeTabpage(1);//검색페이지 삭제
		}
		
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);// 그리드 버튼
		//[결재] Div하위의 컴포넌트 Enable 처리
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 입력			
		
		//trace("결재 "+ this.fvWofhStatus);
		//trace("서비스 "+ this.fvSerhStatus);
		this.div_MasterInput.form.btn_CorhM1.set_enable(false);// 접수완료 비활성
		this.div_MasterInput.form.btn_CorhM2.set_enable(false);// 접수취소 비활성
		
		//this.div_MasterInput.form.tab_Master.tpg_01.form.btn_EquiPop.set_enable(true);//장비명 버튼
		//this.div_MasterInput.form.tab_Master.tpg_01.form.btn_SympPop.set_enable(true);//증상 버튼
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_RqrePop.set_enable(true);//요청사항 버튼
		
		this.div_MasterInput.form.tab_Master.tpg_02.form.btn_CorePop.set_enable(true);//확인내용 버튼
			
		this.div_MasterInput.form.tab_Master.tpg_03.form.btn_PrdtPop.set_enable(true);//상세원인 버튼
		this.div_MasterInput.form.tab_Master.tpg_03.form.btn_PrrePop.set_enable(true);//조치내역 버튼
		this.div_MasterInput.form.tab_Master.tpg_03.form.btn_PrasPop.set_enable(true);//사후대책 버튼

	} else {
		/*
		//[결재]결재내역 팝업화면으로 호출여부
		if (this.fvWfPopupMode) {	
			this.div_Search.set_enable(true);//조회영역 Enable처리
		}
		*/
		trace(this.fvSerhStatus);
		if(this.fvSerhStatus=="0010" || this.gfn_IsNull(this.fvSerhStatus) ){//접수 처리전 상태
			this.div_DetailInput.form.div_ComGrdBtn.set_visible(true);
			//[결재] Div하위의 컴포넌트 Enable 처리
			//this.gfn_SetCompGroupEnable(this.div_MasterInput.form, true);//Master 입력 프리폼
			this.gfn_SetCompGroupEnable(this.div_MasterInput.form.tab_Master.tpg_01.form, true);//Master 입력 프리폼
			this.gfn_SetCompGroupEnable(this.div_DetailInput.form, true);//Detail 입력	
			
			//[결재] Div하위의 컴포넌트 Enable 처리 제외할 컴포넌트설정
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SerhNo.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmNm.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmNm.set_readonly(true);
			
			this.div_MasterInput.form.btn_CorhM1.set_enable(true);// 접수완료 활성
			this.div_MasterInput.form.btn_CorhM2.set_enable(false);// 접수취소 비활성
		
		}else{//접수완료 상태
			this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);		// 그리드 버튼
		
			//[결재] Div하위의 컴포넌트 Enable 처리
			this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
			this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 입력			
			
			//trace("결재 "+ this.fvWofhStatus);
			//trace("서비스 "+ this.fvSerhStatus);
			this.div_MasterInput.form.btn_CorhM1.set_enable(false);// 접수완료 비활성
			
			if(this.fvSerhStatus=="0020"){//접수완료 상태일때만 취소 가능
				this.div_MasterInput.form.btn_CorhM2.set_enable(true);// 접수취소 활성
			}
			//this.div_MasterInput.form.tab_Master.tpg_01.form.btn_EquiPop.set_enable(true);//장비명 버튼
			//this.div_MasterInput.form.tab_Master.tpg_01.form.btn_SympPop.set_enable(true);//증상 버튼
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_RqrePop.set_enable(true);//요청사항 버튼
			
			this.div_MasterInput.form.tab_Master.tpg_02.form.btn_CorePop.set_enable(true);//확인내용 버튼
			
			this.div_MasterInput.form.tab_Master.tpg_03.form.btn_PrdtPop.set_enable(true);//상세원인 버튼
			this.div_MasterInput.form.tab_Master.tpg_03.form.btn_PrrePop.set_enable(true);//조치내역 버튼
			this.div_MasterInput.form.tab_Master.tpg_03.form.btn_PrasPop.set_enable(true);//사후대책 버튼
		}

	}
};
 

