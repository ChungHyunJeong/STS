/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	//품목코드
	if (e.columnid == "ITEM_CD") {
		this.gfn_GetCommItemGrid(obj, "ITEM_CD", "ITEM_NM");//거래처명 검색			
	}
	
	//소요시간 계산
	if (e.columnid == "FROM_HR" || e.columnid == "FROM_MI" || e.columnid == "CLOS_HR" || e.columnid == "CLOS_MI" ) {
		
		if(!this.gfn_IsNull( obj.getColumn(e.row,"FROM_HR")) && !this.gfn_IsNull(obj.getColumn(e.row,"CLOS_HR"))){
			var objFrom 	= new Object();//시작시간
			var objTo 		= new Object();//시작시간
			objFrom.Hours 	= obj.getColumn(e.row,"FROM_HR");//시작-시간
			objFrom.Minutes = this.gfn_IsNull(obj.getColumn(e.row,"FROM_MI")) ? "00" : obj.getColumn(e.row,"FROM_MI") ;//시작-분   
			objTo.Hours 	= obj.getColumn(e.row,"CLOS_HR");//종료-시간
			objTo.Minutes 	= this.gfn_IsNull(obj.getColumn(e.row,"CLOS_MI"))? "00" : obj.getColumn(e.row,"CLOS_MI");//종료-분   

			var rtnValue = this.gfn_CalTimeDiffMi(objFrom, objTo);
			obj.setColumn(e.row, 13, rtnValue.Hours + "시간 " + rtnValue.Minutes+"분 ");// 소요시간
			trace(rtnValue.Hours + "시간 " + rtnValue.Minutes +"분 ");
		}
   
	}
   

};

