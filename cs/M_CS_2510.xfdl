 * 05.02. [Vaildation] Master 저장 필수값 체크 - 입력항목 
 ***********************************************************************************************/ 
this.fn_VaildMasterSave = function()
{   

	

	//[저장] Validation 설정
	var arrArgs = [ 
	                "CNTH_NO^NULL" //계약번호
				   ,"CNTB_SE^NULL" //계약순번
	               ,"CALH_DD^NULL" //발급일자
				   , "ISSU_NM^NULL" //Issue명 
				   , "CUSM_CD^NULL" //고객사코드 
				   , "FROM_HR^NULL" //발생시간 
				   , "CLOS_HR^NULL" //종료시간
				   , "RE01_CD^NULL" //원인유형(대/중)
				   , "RE02_CD^NULL" //원인유형(대/중)
				   , "PROB_NM^NULL" //문제현상 
				   , "ACTI_NM^NULL" //조치내용 
				   ];  
				  
	var sCheckGroupTitle = this.div_MasterInput.form.tab_Master.tpg_01.text;//체크그룹 타이틀
	
	// 체크시 edittype = none 인경우 체크 안됨으로 인해
	// text로 변경하여 체크함
	this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.setCellProperty("Body", 3, "edittype", "text");
	this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.setCellProperty("Body", 5, "edittype", "text");
	//this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.setCellProperty("Body", 7, "edittype", "text");
	
	
	
	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckGridValid(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10, arrArgs, sCheckGroupTitle)) {
		this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.setCellProperty("Body", 3, "edittype", "none");
		this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.setCellProperty("Body", 5, "edittype", "none");
		//this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.setCellProperty("Body", 7, "edittype", "none");
		return false;
	}
	
	this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.setCellProperty("Body", 3, "edittype", "none");
	this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.setCellProperty("Body", 5, "edittype", "none");
	//this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.setCellProperty("Body", 7, "edittype", "none");
	return true;
}

