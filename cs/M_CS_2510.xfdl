/***********************************************************************************************
 * 02.MG0.07. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
  
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	var sPopupId = "";
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)

	if(e.columnid == "CUSM_CD"){
		//고객사코드
		this.gfn_GetCommCusmGrid(obj, "CUSM_CD", "CUSM_NM", "H");//코드로 검색
	
	}else if(e.columnid == "DEPM_CD"){
		//원인부서
		this.gfn_GetCommDepmGrid(obj, "DEPM_CD", "DEPM_NM");//부서명 검색
		
	}else if(e.columnid == "RE01_CD"){
		//원인유형_대
		if(this.gfn_IsNull(e.newvalue) || e.newvalue ==""){
			this.ds_Qm020.filter("");
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "RE02_CD", "");
		}else{
			this.ds_Qm020.filter("");
			this.ds_Qm020.filter("OP01_NM == '"+ e.newvalue +"'");
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "RE02_CD", "");
		}

	}else if(e.columnid == "RE02_CD"){
		//원인유형_중
		var iFvFindRow = this.ds_Qm020.findRowExpr("CODM_CD=='" + e.newvalue+ "'");//업무Key Row 찾기
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "RE01_CD", this.ds_Qm020.getColumn(iFvFindRow, "OP01_NM"))
	}else if(e.columnid == "FROM_HR"){
		var from_hr = nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_HR"));
		var clos_hr = nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_HR"));
		if(!this.gfn_IsNull(from_hr) && from_hr > 0){
			if(!this.gfn_IsNull(clos_hr) && clos_hr > 0){
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CALH_CT", (clos_hr - from_hr));
			}
		}
	}else if(e.columnid == "CLOS_HR"){
		var from_hr = nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_HR"));
		var clos_hr = nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_HR"));
		if(!this.gfn_IsNull(from_hr) && from_hr > 0){
			if(!this.gfn_IsNull(clos_hr) && clos_hr > 0){
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CALH_CT", (clos_hr - from_hr));
			}
		}
		
	}else if(e.columnid == "CALH_DD"){
		//발행일자 월마감 
		
		if(!this.gfn_IsNull(e.newvalue) && e.newvalue !=""){
		
			trace("==e.newvalue=" + e.newvalue)
			if(this.fn_CloseCheck(e.newvalue.substr(0,6))){
				this.gfn_Alert("CA025");//해당월의 정보가 마감이 되어서 등록/수정 할 수 없습니다.
				this.ds_Master10.setColumn(e.row, e.columnid, e.oldvalue);
			}

				
		} 
	}
};


this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {
		
		if(this.gfn_IsNull()){
			this.ds_Qm020.filter("");
		}else{
			this.ds_Qm020.filter("");
			this.ds_Qm020.filter("OP01_NM == '"+ this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RE01_CD") +"'");
		}

		
		
	}	
};


