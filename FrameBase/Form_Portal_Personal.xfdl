/***********************************************************************************************
 * 00.08. Form OnSize 이벤트
 ***********************************************************************************************/
this.Form_Portal_Personal_onsize = function(obj:nexacro.Form,e:nexacro.SizeEventInfo)
{
	var iWidth = parseInt((obj.getOffsetWidth() - 50) / 3);
	var iHeight = parseInt((obj.getOffsetHeight() - 50) / 3);	
	
	this.div_TopL.setOffsetWidth(iWidth);
	this.div_MiddleL.setOffsetWidth(iWidth);
	this.div_BottomL.setOffsetWidth(iWidth);
	this.div_TopL.setOffsetHeight(iHeight);
	this.div_MiddleL.setOffsetHeight(iHeight);
	this.div_BottomL.setOffsetHeight(iHeight);	
	this.div_MiddleL.setOffsetTop(iHeight + 25);
	this.div_BottomL.setOffsetTop(iHeight * 2 + 30);	
	
	this.div_TopM.setOffsetWidth(iWidth);
	this.div_MiddleM.setOffsetWidth(iWidth);
	this.div_BottomM.setOffsetWidth(iWidth);
	this.div_TopM.setOffsetLeft(iWidth + 25);
	this.div_MiddleM.setOffsetLeft(iWidth + 25);
	this.div_BottomM.setOffsetLeft(iWidth + 25);	
	this.div_TopM.setOffsetHeight(iHeight);
