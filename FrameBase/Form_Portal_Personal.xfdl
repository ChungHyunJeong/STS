/***********************************************************************************************
 * 15.13. 조회된 데이터 화면 출력
 ***********************************************************************************************/ 
this.fn_DataView = function()
{
	//근무정보
	if (this.ds_Master10.getRowCount() > 0) {
		var sWORK_DD = this.gfn_Nvl(this.ds_Master10.getColumn(0, "WORK_DD"), "0");//일자
		var sWORK_YY = this.gfn_Nvl(this.ds_Master10.getColumn(0, "WORK_YY"), "0");//년
		var sWORK_MM = this.gfn_Nvl(this.ds_Master10.getColumn(0, "WORK_MM"), "0");//월
		var sWORK_DY = this.gfn_Nvl(this.ds_Master10.getColumn(0, "WORK_DY"), "0");//일
		var sENTR_DD = this.gfn_Nvl(this.ds_Master10.getColumn(0, "ENTR_DD"), "");//입사일자
		
		this.div_TopL.form.sta_Days.set_text("- <b v='true'><fc v='#25aae1'>" + this.gfn_AppendComma(sWORK_DD) + "일째</fc></b> 근무중");
		this.div_TopL.form.sta_Year.set_text("- 근속기간 : 만 " + sWORK_YY + "년 " + sWORK_MM + "개월 " + sWORK_DY + "일");
		this.div_TopL.form.sta_EntrDd.set_text(sENTR_DD);			
	} else {
		this.div_TopL.form.sta_Days.set_text("- <b v='true'><fc v='#25aae1'>0일째</fc></b> 근무중");
		this.div_TopL.form.sta_Year.set_text("- 근속기간 : ");
		this.div_TopL.form.sta_EntrDd.set_text("");
	}
	
	//근무시간정보
	if (this.ds_Master11.getRowCount() > 0) {
		var sDIST_CD = "";//구분코드
		var sWORK_TM = "";//시간
		var sTERM_DD = "";//기간
		
		for (var iLoop=0; iLoop<this.ds_Master11.getRowCount(); iLoop++) {
			sDIST_CD = this.gfn_Nvl(this.ds_Master11.getColumn(iLoop, "DIST_CD"), "");//구분코드
			sWORK_TM = this.gfn_Nvl(this.ds_Master11.getColumn(iLoop, "WORK_TM"), "0");//시간
			sTERM_DD = this.gfn_Nvl(this.ds_Master11.getColumn(iLoop, "TERM_DD"), "0");//기간
			
			//시간
			if (sDIST_CD == "A") {
				this.div_TopM.form.sta_Time.set_text("<fc v='#25aae1'>" + sWORK_TM + "</fc> 시간");
			//기간
			} else if (sDIST_CD == "B") {
				this.div_TopM.form.sta_Term.set_text("(" + sTERM_DD + ")");
			}
		}
	} else {
		this.div_TopM.form.sta_Time.set_text("<fc v='#25aae1'>0</fc> 시간");
		this.div_TopM.form.sta_Term.set_text("");
	}
	
	//결재정보
	if (this.ds_Master12.getRowCount() > 0) {
		var sDAYT_CT = this.gfn_Nvl(this.ds_Master12.getColumn(0, "DAYT_CT"), "0");//금일 결재대상 건수
		var sDAYS_CT = this.gfn_Nvl(this.ds_Master12.getColumn(0, "DAYS_CT"), "0");//금일 결재 건수
		var sWEKT_CT = this.gfn_Nvl(this.ds_Master12.getColumn(0, "WEKT_CT"), "0");//금주 결재대상 건수
		var sWEKS_CT = this.gfn_Nvl(this.ds_Master12.getColumn(0, "WEKS_CT"), "0");//금주 결재 건수
		var sMONT_CT = this.gfn_Nvl(this.ds_Master12.getColumn(0, "MONT_CT"), "0");//당월 결재대상 건수
		var sMONS_CT = this.gfn_Nvl(this.ds_Master12.getColumn(0, "MONS_CT"), "0");//당월 결재 건수
		this.div_TopR.form.sta_DaysCt.set_text("- 금일 결재건수 : <b v='true'><fc v='#25aae1'>" + sDAYS_CT + " /</fc> " + sDAYT_CT + " 건</b>");
		this.div_TopR.form.sta_WeekCt.set_text("- 금주 결재건수 : <b v='true'><fc v='#25aae1'>" + sWEKS_CT + " /</fc> " + sWEKT_CT + " 건</b>");
		this.div_TopR.form.sta_MontCt.set_text("- 당월 결재건수 : <b v='true'><fc v='#25aae1'>" + sMONS_CT + " /</fc> " + sMONT_CT + " 건</b>");			
	} else {
		this.div_TopR.form.sta_DaysCt.set_text("- 금일 결재건수 : <b v='true'><fc v='#25aae1'>0 /</fc> 0 건</b>");
		this.div_TopR.form.sta_WeekCt.set_text("- 금주 결재건수 : <b v='true'><fc v='#25aae1'>0 /</fc> 0 건</b>");
		this.div_TopR.form.sta_MontCt.set_text("- 당월 결재건수 : <b v='true'><fc v='#25aae1'>0 /</fc> 0 건</b>");
	}

	//업무일지
	if (this.ds_Master13.getRowCount() > 0) {
		var sWEEK_CT = this.gfn_Nvl(this.ds_Master13.getColumn(0, "WEEK_CT"), "0");//금주건수
		var sWEEK_TT = this.gfn_Nvl(this.ds_Master13.getColumn(0, "WEEK_TT"), "0");//금주대상건수
		this.div_MiddleL.form.sta_WeekCnt.set_text("- 금주 업무일지 : <b v='true'><fc v='#25aae1'>" + sWEEK_CT + " /</fc> " + sWEEK_TT + " 건</b>");
	} else {
		this.div_MiddleL.form.sta_WeekCnt.set_text("- 금주 업무일지 : <b v='true'><fc v='#25aae1'>0 건</b>");
	}
	
	if (this.ds_Master16.getRowCount() > 0) {
		var sMONT_CT = this.gfn_Nvl(this.ds_Master16.getColumn(0, "MONT_CT"), "0");//금월건수
		var sMONT_TT = this.gfn_Nvl(this.ds_Master16.getColumn(0, "MONT_TT"), "0");//금월대상건수
		this.div_MiddleL.form.sta_MonthCnt.set_text("- 당월 업무일지 : <b v='true'><fc v='#25aae1'>" + sMONT_CT + " /</fc> " + sMONT_TT + " 건</b>");
	} else {
		this.div_MiddleL.form.sta_MonthCnt.set_text("- 당월 업무일지 : <b v='true'><fc v='#25aae1'>0 건</b>");	
	}	
	
	//출장정보
	if (this.ds_Master14.getRowCount() > 0) {
		var sWEEK_CT = this.gfn_Nvl(this.ds_Master14.getColumn(0, "WEEK_CT"), "0");//;//금주출장
		var sMONT_CT = this.gfn_Nvl(this.ds_Master14.getColumn(0, "MONT_CT"), "0");//당월출장
		var sBZTH_AM = this.gfn_Nvl(this.ds_Master14.getColumn(0, "BZTH_AM"), "0");//당월출장비
		
