 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
			this.fn_DataView();//조회된 데이터 화면출력
			this.fn_InitPgmCall();
			
			break;
			
	}
};

/*
	JCH 사용
	- 초기프로그램 로딩
*/
this.fn_InitPgmCall = function() {
	trace("fn_InitPgmCall============================================================");
	trace(this.ds_Sm931.rowcount);
	if(this.ds_Sm931.rowcount == 1) {
	
		var prgmId_01 = this.ds_Sm931.getColumn(0, "PRGM_ID_01");
		var prgmId_02 = this.ds_Sm931.getColumn(0, "PRGM_ID_02");
		var prgmId_03 = this.ds_Sm931.getColumn(0, "PRGM_ID_03");
		
		if(!this.gfn_IsNull(prgmId_01)) { this.gfn_CallScrn(prgmId_01); }
		if(!this.gfn_IsNull(prgmId_02)) { this.gfn_CallScrn(prgmId_02); }
		if(!this.gfn_IsNull(prgmId_03)) { this.gfn_CallScrn(prgmId_03); }
		
	}
}

