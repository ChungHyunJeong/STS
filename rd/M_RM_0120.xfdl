this.div_Search_cbo_ItlmCd_onitemchanged = function(obj:nexacro.Combo,e:nexacro.ItemChangeEventInfo)	 
{
	this.ds_RdItmmSch.filter("");
	if (!this.gfn_IsNull(this.div_Search.form.cbo_ItlmCd.value)){
		this.ds_RdItmmSch.filter("ITLM_CD=='"+this.div_Search.form.cbo_ItlmCd.value+"' || ITMM_NM=='ALL'" );		
	}
	this.div_Search.form.cbo_ItmmCd.set_index(0);
	
	if(!this.gfn_IsNull(this.div_Search.form.cbo_ItlmCd.value)
		&& !this.gfn_IsNull(this.div_Search.form.cbo_ItmmCd.value)){
		this.div_Search.form.sta_MakeNm.set_visible(true);
		this.div_Search.form.edt_MakeNm.set_visible(true);
	}else{
		this.div_Search.form.sta_MakeNm.set_visible(false);
		this.div_Search.form.edt_MakeNm.set_visible(false);
		this.div_Search.form.edt_MakeNm.set_value("");
	}
	
};

