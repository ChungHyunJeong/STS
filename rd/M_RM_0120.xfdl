/*---------------------------------------------------------------------------------------------
 *    [Master 입력영역-Button] 엑셀업로드 Call Back 함수
  ---------------------------------------------------------------------------------------------*/
this.fn_ImportCallback = function(sImportId)
{	
	this.ds_Master10.set_enableevent(false);
	
	var nRow = 0;	
	
	for (var iLoop=0; iLoop<this.ds_Import.getRowCount(); iLoop++) {
		var iCol00 = this.ds_Import.getColumn(iLoop, "Column0"); 
		var iCol01 = this.ds_Import.getColumn(iLoop, "Column1"); 
		var iCol02 = this.ds_Import.getColumn(iLoop, "Column2"); 
		var iCol03 = this.ds_Import.getColumn(iLoop, "Column3"); 
		var iCol04 = this.ds_Import.getColumn(iLoop, "Column4"); 
		var iCol05 = this.ds_Import.getColumn(iLoop, "Column5"); 
		var iCol06 = this.ds_Import.getColumn(iLoop, "Column6"); 
		var iCol07 = this.ds_Import.getColumn(iLoop, "Column7"); 
		
		if (!this.gfn_IsNull(iCol00) && (nRow = this.ds_Master10.findRow("ITEM_CD", iCol00)) >= 0) {
			if(!this.gfn_IsNull(iCol01)) this.ds_Master10.setColumn(nRow, "USED_YN", iCol01.substring(0,iCol01.indexOf(" : "))); //사용여부
			if(!this.gfn_IsNull(iCol02)) this.ds_Master10.setColumn(nRow, "REAS_NM", iCol02.substring(0,iCol02.indexOf(" : "))); //미사용사유
			if(!this.gfn_IsNull(iCol03)) this.ds_Master10.setColumn(nRow, "NATM_CD", iCol03.substring(0,iCol03.indexOf(" : "))); //원산지코드
			if(!this.gfn_IsNull(iCol04)) this.ds_Master10.setColumn(nRow, "REFU_YN", iCol04.substring(0,iCol04.indexOf(" : "))); //관세환급여부
			if(!this.gfn_IsNull(iCol05)) this.ds_Master10.setColumn(nRow, "DCCM_YN", iCol05.substring(0,iCol05.indexOf(" : "))); //할인수수료여부
			if(!this.gfn_IsNull(iCol06)) this.ds_Master10.setColumn(nRow, "RTDO_CT", iCol06.substring(0,iCol06.indexOf(" : "))); //구매리드타임(국내)일수
			if(!this.gfn_IsNull(iCol07)) this.ds_Master10.setColumn(nRow, "RTIM_CT", iCol07.substring(0,iCol07.indexOf(" : "))); //구매리드타임(해외)일수
			this.ds_Master10.setColumn(nRow, "s_chk", "1");
			this.ds_Master10.setColumn(nRow, "crudType", "U"); //저장시 ds_detail10 의 내용을 저장키 위함.
		}
	}
	this.ds_Master10.set_enableevent(true);
	
	//데이터 변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}		
}
