this.fn_ComBtnPrint = function()
{ 
	var iSelRow = this.ds_Master10.rowposition;//선택한Row
 
		if (this.ds_Master10.getRowCount() > 0 && iSelRow > -1) {					
// 		
		this.ds_ComWfLine.clearData();  //결재	
		this.ds_ComFileList.clearData(); //첨부파일  
		
		//var sOFFH_GU = this.ds_Master10.getColumn(iSelRow, "OFFH_GU");//문서구분(  기안 : 0010, 0020, 0050 )	
		var sOFFH_GU = ""	;
		var oDs = this.ds_ComWfLine;//결재라인정보
		var oDsAt = "";//결재라인정보-감사			
		//var oDsAt = this.ds_ComWfAtLine;//결재라인정보-감사
		
		var sCommCd = this.ds_Master10.getColumn(iSelRow, "COMM_CD");//회사코드
		var sWofhNo = this.ds_Master10.getColumn(iSelRow, "WOFH_NO");//결재PID
		var sCallBackFunc = "fn_ComWfLineCallback";//콜백함수				
		var sId = '';//콜백함수 구분ID - 문서구분	
 

 	//첨부파일 데이타셋에 담기 
		if ( this.fvFileDiv.ds_Smfilb.rowcount > 0 ) {	 
		
			this.ds_ComFileList.clearData();
			//this.ds_ComFileList.addRow()	
			
			var sFileNmTmp 	  = "";
			var sFileNm    	  = "";	
			var sRemkNmTmp    = "";		
			
			for(var iLoop = 0; iLoop < this.fvFileDiv.ds_Smfilb.rowcount; iLoop++){				
			
				var nRow = this.ds_ComFileList.addRow()	
				
				sFileNmTmp = this.fvFileDiv.ds_Smfilb.getColumn(iLoop, "FILE_NM");
				sRemkNmTmp = this.fvFileDiv.ds_Smfilb.getColumn(iLoop, "REMK_NM");			
				
				this.ds_ComFileList.setColumn(iLoop, "FILE_NM", sFileNmTmp) ;//첨부파일명 	
				this.ds_ComFileList.setColumn(iLoop, "REMK_NM", sRemkNmTmp) ;//비고	

			} //loop					
			
// 			this.ds_ComFileList.setColumn(0, "FILE_NM", sFileNm) ;//첨부파일명 	
// 			this.ds_ComFileList.setColumn(0, "REMK_NM", sRemkNmTmp) ;//비고 			
			
		
	 } //첨부파일

		//레포트 결재라인 조회후 레포트 출력			
		this.gfn_GetCommWfLine(this, oDs, sCommCd, sWofhNo, sCallBackFunc, sId, "", oDsAt);  //결재라인조회
			
	} else {
		this.gfn_Alert("SM083");//출력할 정보가 없습니다.
	}
}

