/***********************************************************************************************
 * 9. 결재요청
 ***********************************************************************************************/
/***********************************************************************************************
 * 09.01. [공통버튼] 결재요청 버튼 클릭 이벤트
 ***********************************************************************************************/  
this.fn_ComBtnDecide = function()
{ 
	var sRpphYn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RPPH_YN"); //완료여부			
	if (sRpphYn == '0') {	//미완료시 
		var arrMsgArg = ["연구프로젝트완료"];		
		this.gfn_Alert("WF037", arrMsgArg); //해당대상은 연구프로젝트완료 정보를 생성하지 않았습니다. 저장 후 결재요청하시기 바랍니다.
		return;
	}
	
	//[공통] 결재요청 Vaildation 체크
	if (!this.fn_VaildWfDecide()) return;
	
	var iRow = this.ds_Master10.rowposition;
	var sPks = this.ds_Master10.getColumn(iRow, "COMM_CD");//회사코드
	sPks += "ⓐ" + this.ds_Master10.getColumn(iRow, "RPJH_NO");//관리번호
	sPks += "ⓐ" + this.ds_Master10.getColumn(iRow, "WOFH_SQ");//결재차수

	//자리숫에 따른 연구프로젝트번호 포멧
	var sNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RPJH_NO")
	var sFormat = "";
	
	if(sNo.length == 7){
		sFormat = this.gfn_StringFormat(this.gfn_Nvl(this.ds_Master10.getColumn(iRow, "RPJH_NO"),""), "@@@-@@@@");	
	}else if(sNo.length == 10){
		sFormat = this.gfn_StringFormat(this.gfn_Nvl(this.ds_Master10.getColumn(iRow, "RPJH_NO"),""), "@-@@@@-@@@@@");
	}else{
		sFormat = this.gfn_StringFormat(this.gfn_Nvl(this.ds_Master10.getColumn(iRow, "RPJH_NO"),""), "");
	}

	//결재명
	var sWofhNm = "[연구프로젝트완료:"+sFormat+"]  " + this.ds_Master10.getColumn(iRow, "RPJH_NM"); //결재명

	
	//자동전표관련 파라메타-자동전표를 사용하는 화면에만 추가 
	var sAtobKey = "@COMM_CD=" + this.ds_Master10.getColumn(iRow, "COMM_CD");//회사코드
	sAtobKey += "ⓐ@RPJH_NO=" + this.ds_Master10.getColumn(iRow, "RPJH_NO");//관리번호
