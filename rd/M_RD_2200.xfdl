/***********************************************************************************************
 * 02.MF0.03. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {
		//[결재] 결재상태에 따른 컴포넌트 제어			
		this.fn_SetWfCompEnable();	

		//연구프로젝트번호정보가 있으면 
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RPJH_NO"))) {
			//내용 Web컴포넌트에 값 세팅
			//웹컴포넌트가 로드 완료되었을경우
			if (this.fvWebCompLoadYn) {
				this.setTimer(998,500);
			}

			var sNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RPJH_NO")
			
			if(sNo.length == 7){
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_RpjhNo.set_format("@@@-@@@@");
			}else if(sNo.length == 10){
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_RpjhNo.set_format("@-@@@@-@@-@@@");
			}else{
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_RpjhNo.set_format("");
			}

			var sRpphYn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RPPH_YN"); //완료여부			
			if (sRpphYn == '0') {	//미완료시 
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WOFH_SQ", "1");//결재차수
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WOFH_YN", "1");//최종여부	
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "RPPH_DD", this.gfn_GetCommDate("toDay"));//완료일자-오늘날자로 설정
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//등록부서코드	
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//등록부서명	
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//등록자사번	
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//등록자명		
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//등록자ID	

				//입력 항목에 포커스
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FromDd.setFocus();			
				
			} else {	//완료시 첨부파일 조회
				//첨부파일 정보 조회
				var oFileInfo = new Object();//[공통] 첨부파일정보
				oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
				oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
				oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
				oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RPJH_NO");//PK번호2-관리번호
				oFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수						
				
				this.fvFileDiv.fn_AtchFileSearch(oFileInfo);			
			
			}
		}		
		
		//Detail이 없음 (연구프로젝트완료 상세화면은 Head 테이블의 연구프로젝트 상세 내용을 보여줌)
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master10] Row 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{

	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무

	var sCanRowChangeYn = false;	//Master 데이터셋 변경여부
	var sOrgFromDd = this.ds_Master10.getOrgColumn(e.oldrow, "FROM_DD");
	var sNewFromDd = this.ds_Master10.getColumn(e.oldrow, "FROM_DD");
	var sOrgClosDd = this.ds_Master10.getOrgColumn(e.oldrow, "CLOS_DD");
	var sNewClosDd = this.ds_Master10.getColumn(e.oldrow, "CLOS_DD");
	var sOrgRemkNm = this.ds_Master10.getOrgColumn(e.oldrow, "REMK_NM");
	var sNewRemkNm = this.ds_Master10.getColumn(e.oldrow, "REMK_NM");

	if (sOrgFromDd != sNewFromDd || sOrgClosDd != sNewClosDd || sOrgRemkNm != sNewRemkNm) {
		sCanRowChangeYn = true;	//Master 데이터셋 변경여부
	}
	
	//데이터 변경여부 체크
	if (sCanRowChangeYn || this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}  
	
// 	//데이터 변경여부 체크 
// 	if (sCanRowChangeYn || this.fvFileDiv.fn_CheckFileAddYn()) {
// 		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
// 		return false;
// 	} else {
// 		this.ds_Master10.reset();	//데이터 초기화
// 	}


	return true;
};  

