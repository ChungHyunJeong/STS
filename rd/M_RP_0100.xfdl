this.fn_ComBtnAdd = function()
{
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)
			||this.gfn_DsIsUpdated(this.ds_Detail10)
			||this.gfn_DsIsUpdated(this.ds_Detail20)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	
	var iAddRow = this.ds_Master10.addRow();
	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", "1");//결재차수
	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");//최종여부
	this.ds_Master10.setColumn(iAddRow, "WOFH_ST", "0010");//결재상태-등록(0010)	
	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD")); //부서코드
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM")); //부서명
	this.ds_Master10.setColumn(iAddRow, "APDP_CD", this.gfn_GetSessionValue("APDP_CD")); //출원부서코드
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID")); //사원코드
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM")); //사원명
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID")); //사번
	this.ds_Master10.setColumn(iAddRow, "INDH_DD", sToDay);
	this.ds_Master10.setColumn(iAddRow, "NATM_CD", "KR");
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_IndhSt.set_index(0); //등록구분
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_IndhGu.set_index(0); //재산권구분
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_InexGu.set_index(0); //국내/국외
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_NatmCd.set_index(0); //출원국가
//	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_PaprGu.set_index(0); //발표구분
	
	//입력영역 활성화
 	this.div_MasterInput.set_enable(true);
 	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
 	this.div_DetailInput.form.tab_Detail.set_enable(true);

	//this.fn_SetWfCompEnable();
	//처음 입력 항목에 포커스
	this.div_MasterInput.form.tab_Master.tpg_01.form.cal_IndhDD.setFocus();
	
	this.ds_Detail10.clearData();//상세정보 초기화
	this.ds_Detail20.clearData();//상세정보 초기화
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
	//첨부파일정보 권한설정
