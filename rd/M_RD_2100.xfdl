{
	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 

}

/***********************************************************************************************
 * 00.04. Form [공통] 초기설정 함수 - 필수
 ***********************************************************************************************/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	this.fvComPrgmId = this.gfn_CommLoadFormPrgmId(this);//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
	this.fvMenuNm = this.gfn_GetMenuNm(this.fvComPrgmId);//해당화면 메뉴명 조회  <== 추가부분 : 위치중요 반드시 this.fvComPrgmId 아래 부분에 넣어야 함

	this.gfn_SetGrid(this.div_MasterView.form.grd_Master10);//[공통]그리드 이벤트 설정 - 출력목록
	this.gfn_SetGrid(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);//[공통]그리드 이벤트 설정
	this.gfn_SetGrid(this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20);//[공통]그리드 이벤트 설정
	this.gfn_SetGrid(this.div_DetailInput.form.tab_Detail.tpg_03.form.grd_Detail30);//[공통]그리드 이벤트 설정
	this.gfn_SetGrid(this.div_DetailInput.form.tab_Detail.tpg_04.form.grd_Detail40);//[공통]그리드 이벤트 설정
	//스플리터 초기화 함수 호출
	this.gfn_InitSpliter(this.sta_HSplitter, "horizontal", -1, -1);
	
	//스플리터 초기화 함수 호출
	this.gfn_InitSpliter(this.sta_VSplitter, "vertical", -1, -1);	

	this.fvMenuNm = this.gfn_GetMenuNm(this.fvComPrgmId);//해당화면 메뉴명 조회
	
	//권한체크 할 컴포넌트 : 컴포넌트 Full Path
