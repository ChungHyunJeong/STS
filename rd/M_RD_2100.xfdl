{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	//부서
	if (e.columnid == "DEPM_CD") {
		this.gfn_GetCommDepmGrid(obj, "DEPM_CD", "DEPM_NM");//부서명 검색
	
	} 
	//사용자
	else if (e.columnid == "SYST_CD") {
		this.gfn_GetCommEmpmGrid(obj, "SYST_CD", "SYST_NM", "SYST_ID");//사원명 검색
	}
	//비중. 참여율, 창여율 합
	else if (e.columnid == "RPJB_RT" || e.columnid == "RPJC_RT" || e.columnid == "ETCP_RT") {

		var nValue = nexacro.toNumber(e.newvalue);
		var nTitle = "비중";
		if(e.columnid == "RPJC_RT") nTitle = "참여율";
		else if (e.columnid == "ETCP_RT") nTitle = "참여율합";
		if(nValue > 100 || nValue < 0){
			this.gfn_Alert("SM056",[nTitle, "100"]);
			obj.setColumn(e.row, e.columnid, e.oldvalue);
			return false;	
		}
		return true;
	}
	
};

