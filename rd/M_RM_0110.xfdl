	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Spec.clearData();
	this.ds_Input.clearData();
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "SESU_ID", this.gfn_GetSessionValue("SYST_ID"));//SYSTEM ID
	this.ds_Input.setColumn(0, "ITLM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition,"ITLM_CD"));	
	this.ds_Input.setColumn(0, "ITMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition,"ITMM_CD"));

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "rd:M_RD_SPEM_C_SL10";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "SPEC_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_RD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Spec=ds_SpecRtn";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.isSync = true;

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
		
	var dsObj,stObj,edtObj,cboObj;	
	
	this.ds_Spec.filter("");
	if (this.ds_Spec.getRowCount() > 0) {	
		for(var i=1;i<=10;i++){
			dsObj = this.all["ds_Spec"+i.toString().padLeft(2,"0")];
			stObj = this.div_MasterInput.form.tab_Master.tpg_01.form.all["sta_Spec"+i.toString().padLeft(2,"0")];
			edtObj = this.div_MasterInput.form.tab_Master.tpg_01.form.all["edt_Spec"+i.toString().padLeft(2,"0")];
			cboObj = this.div_MasterInput.form.tab_Master.tpg_01.form.all["cbo_Spec"+i.toString().padLeft(2,"0")];
		
			this.ds_Spec.filter("");
			this.ds_Spec.filter("ITSM_SORT=='"+i+"'");
			dsObj.copyData(this.ds_Spec, true);
			
			if(dsObj.getRowCount() <= 0){
				break;
			}
			stObj.set_text(dsObj.getColumn(0,"ITSM_NM"));
			//key in 입력
			if(dsObj.getColumn(0,"KEYI_YN")=="1"){
				edtObj.set_readonly(false);
				edtObj.set_visible(true);				
				edtObj.set_cssclass((dsObj.getColumn(0,"ESSE_YN")=="1"? "essential":""));
			}
			//콤보입력
			else{
				cboObj.set_readonly(false);
				cboObj.set_visible(true);
				cboObj.set_cssclass((dsObj.getColumn(0,"ESSE_YN")=="1"? "essential":""));
			}
		}
	}
}

