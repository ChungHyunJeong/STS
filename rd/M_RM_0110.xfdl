 ***********************************************************************************************/ 
this.fn_SetItemCd = function()
{
	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	//Master 입력 상태이면
	if (sChkRowType != "I") {
		return;
	}
	
	var sItem  = "";
	if(this.ds_RdItlm.getColumn(this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItlmCd.index,"CDAT_YN") == "1"){
		sItem += this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITLM_CD"),"");
	}
	if(this.ds_RdItmm.getColumn(this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItmmCd.index,"CDAT_YN") == "1"){
		sItem += this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITMM_CD"),"");
	}
	
	var dsObj,edtObj,cboObj;		
	
	for(var i=1;i<=10;i++){
		dsObj = this.all["ds_Spec"+i.toString().padLeft(2,"0")];
		
		if(dsObj.getColumn(0,"CDAT_YN") == "1" && !this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SP"+i.toString().padLeft(2,"0")+"_CD"))){
			sItem += this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SP"+i.toString().padLeft(2,"0")+"_CD"),"");
		}
	}
	
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ItemCd.set_value(sItem);
}

/***********************************************************************************************
 * 15.06. [자동생성] 품목명
 ***********************************************************************************************/ 
this.fn_SetItemNm = function()
{
	var sItemNm  = "";
	//sItemNm = this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItlmCd.text;
	if(this.ds_RdItmm.getColumn(this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItmmCd.index,"NMAT_YN") == "1"){
		sItemNm += (sItemNm!=""? " ": "")+this.gfn_Nvl(this.ds_RdItmm.getColumn(this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItmmCd.index, "NMAT_NM"),"");
	}
	
	var dsObj,edtObj,cboObj;		
	
	for(var i=1;i<=10;i++){
		dsObj = this.all["ds_Spec"+i.toString().padLeft(2,"0")];
		edtObj = this.div_MasterInput.form.tab_Master.tpg_01.form.all["edt_Spec"+i.toString().padLeft(2,"0")];
		cboObj = this.div_MasterInput.form.tab_Master.tpg_01.form.all["cbo_Spec"+i.toString().padLeft(2,"0")];
		
		if(dsObj.getColumn(0,"NMAT_YN") == "1"){
			
			if(sItemNm.length>0) sItemNm += (sItemNm!=""? " ": "");
			
			if(dsObj.getColumn(0,"KEYI_YN")=="1"){
				sItemNm += this.gfn_Nvl(edtObj.value,"");
			}else{
				sItemNm += this.gfn_Nvl(cboObj.text,"");
			}
		}
	}
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ItemNm.set_value(sItemNm);
}

