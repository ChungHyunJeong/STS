/***********************************************************************************************
 * 02.MF0.02. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {			
		this.ds_RdItmm.filter("");
		if(!this.gfn_IsNull(obj.getColumn(e.newrow, "ITLM_CD"))){
			this.ds_RdItmm.filter("ITLM_CD=='"+obj.getColumn(e.newrow, "ITLM_CD")+"'" );			
		}
				
		//Master 입력/수정/삭제 상태 체크
		var sChkRowType = this.gfn_GetRowType(this.ds_Master10, e.newrow);
		
		//Master 입력 상태이면
		if (sChkRowType == "I") {
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ItemCd.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ItemNm.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItlmCd.set_readonly(false);
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItmmCd.set_readonly(false);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcomCd.set_readonly(false);
		}
		else{
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ItemCd.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ItemNm.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItlmCd.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItmmCd.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcomCd.set_readonly(true);
		}
		
		//사양입력 display 변경
		if(this.fn_SpecReSet()){
			this.fn_SpecSet();
		}
		
		
		if(!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITEM_CD"))){
			
			//첨부파일 정보 조회
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
			oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITEM_CD");//PK번호2
			//oFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3

			this.fvFileDiv.fn_AtchFileSearch(oFileInfo);		
			
		}
		
		//사용여부에 따른 미사용여부 입력란 readonly 컨트롤
		if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "USED_YN")=="1"){
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ReasNm.set_readonly(true);
		}else{
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ReasNm.set_readonly(false);
		}
		
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BEFO_CD"))) {
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_BEFOCD.set_readonly(true);
		} else {
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_BEFOCD.set_readonly(false);
		}
		
		//도면보기버튼설정
		
		var sDWGF_YN = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DWGF_YN");//도면파일유무
		
		//도면파일이 있으면
		if (sDWGF_YN == "O") {
			this.div_MasterInput.form.btn_DwgView.set_enable(true);
		} else {
			this.div_MasterInput.form.btn_DwgView.set_enable(false);
		}
		
		//=========================================================================
		// 변경이력 조회
		//=========================================================================	
		//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
		this.ds_Detail10.clearData();
		this.ds_Input.clearData();
		
		this.ds_Input.addRow();
		this.ds_Input.setColumn(0, "ITEM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITEM_CD"));
				
		//[공통]서비스정보 초기화
		this.gfn_InitComService(this);
		
		//[공통]서비스정보 추가
		var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
		svcAddObj.sSelectSql = "rd:M_RM_0110_D_SL10";
		this.gfn_AddComSearchService(this, svcAddObj);
		
		//[공통]서비스 호출 변수
		var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
		svcCallObj.sTrid = "HIST_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
		svcCallObj.sActionName = this.G_SV_RD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
		svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
		svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10";//출력 데이터셋
		
		svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

		//[공통]서비스 호출
		this.gfn_CallComService(this, svcCallObj);	
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master10] Row 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	

	return true;
};  
/*---------------------------------------------------------------------------------------------
 *    03. [데이터셋-ds_Master10] 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{	
	//this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	//대분류
	if(e.columnid == "ITLM_CD"){	
		obj.set_enableevent(false);
		this.ds_RdItmm.filter("");
		obj.setColumn(e.row, "ITMM_CD","");
		if(!this.gfn_IsNull(e.newvalue)){
			this.ds_RdItmm.filter("ITLM_CD=='"+e.newvalue+"'" );			
		}
		obj.setColumn(e.row, "ITEM_CD","");
		obj.setColumn(e.row, "ITEM_NM","");
		obj.set_enableevent(true);
		this.fn_SpecReSet();
		
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItmmCd.set_index(0);
		this.fn_SpecSet();
		this.fn_SetItemCd();	
	}
	//중분류
	if(e.columnid == "ITMM_CD"){
		var nRow = this.ds_RdItmm.findRow("ITMM_CD",e.newvalue);
		
		if(!this.gfn_IsNull(e.newvalue)){
			obj.set_enableevent(false);
			obj.setColumn(e.row, "ITLM_CD", this.ds_RdItmm.getColumn(nRow, "ITLM_CD"));
			//사양입력 display 변경
			if(this.fn_SpecReSet()){
				this.fn_SpecSet();
			}
			obj.set_enableevent(true);
			
			this.fn_SetItemCd();	
			
			this.fn_SetItemNm();
		}
		
	}
};
