 ************************************************************************/
this.div_MasterInput_btn_DwgView_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if (this.ds_Master10.getRowCount() > 0) {
		var sITEM_CD = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITEM_CD");
		var sITEM_NM = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITEM_NM");
		
		if (!this.gfn_IsNull(sITEM_CD)) {
			var sPopupId = "B_ITED_CD_01";//팝업ID - 도면보기
			var oArg = { ItemCd:sITEM_CD, ItemNm:sITEM_NM};  // Argument처리
			this.gfn_ItedPop(sPopupId, oArg);		
		} else {
			this.gfn_Alert("SM102");//품목코드 정보가 없습니다.
		}
	}
};
