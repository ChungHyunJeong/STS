this.fn_SpecReSet = function()
{
	var dsObj,stObj,edtObj,cboObj;		
	
	for(var i=1;i<=10;i++){
		dsObj = this.all["ds_Spec"+i.toString().padLeft(2,"0")];
		stObj = this.div_MasterInput.form.tab_Master.tpg_01.form.all["sta_Spec"+i.toString().padLeft(2,"0")];
		edtObj = this.div_MasterInput.form.tab_Master.tpg_01.form.all["edt_Spec"+i.toString().padLeft(2,"0")];
		cboObj = this.div_MasterInput.form.tab_Master.tpg_01.form.all["cbo_Spec"+i.toString().padLeft(2,"0")];
		
		dsObj.clearData();
		stObj.set_text("");	
		edtObj.set_readonly(true);
