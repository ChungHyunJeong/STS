 ************************************************************************/

this.div_MasterInput_tab_Master_tpg_01_chk_UsedYn_onchanged = function(obj:nexacro.CheckBox,e:nexacro.CheckBoxChangedEventInfo)
{
	if(e.postvalue=="0" ){
		//사용여부 가 부 일경우
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ReasNm.set_readonly(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ReasNm.set_cssclass("essential"); //미사용사유 
		
	}else{
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ReasNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ReasNm.set_cssclass(""); //미사용사유 
	}
};

this.div_MasterInput_tab_Master_onchanged = function(obj:nexacro.Tab,e:nexacro.TabIndexChangeEventInfo)
{
var iTabIndex = e.postindex;
	
	this.div_MasterInput.form.div_ComGrdBtn.set_enable(true); 
	
	
	if (iTabIndex == 0 || iTabIndex == 1) {
		this.div_MasterInput.form.div_ComGrdBtn.set_visible(false);
		this.div_MasterInput.form.btn_DwgView.set_visible(true);
		
	//Tab-첨부파일
	} else if (iTabIndex == 2) {	
		this.div_MasterInput.form.div_ComGrdBtn.set_visible(true);
		this.div_MasterInput.form.btn_DwgView.set_visible(false);
		this.fn_SetGridFileButton();//[공통] 그리드 파일 버튼 설정		
	
	}
	
};

