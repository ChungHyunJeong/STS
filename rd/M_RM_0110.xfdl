this.fn_ComBtnSave = function()
{ 
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}
	
		//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		//첨부파일정보만 변경시
		if (!this.gfn_DsIsUpdated(this.ds_Master10)  && this.ds_Master10.getRowCount() > 0) {
			//첨부파일정보 저장
			fvFileInfo = new Object();//[공통] 첨부파일정보
			fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, B-상세(그리드Row), E-기타
			fvFileInfo.CALLBACK_FUNC = "";//호출할 콜백함수
			fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
			fvFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITEM_CD");//PK번호2-
			//fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
			//첨부파일Div 화면에 Key세팅하게 변경
			this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);
			return;	
		}
	}
	
		//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		fvFileInfo = new Object();//[공통] 첨부파일정보
		fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
		fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
		fvFileInfo.CALLBACK_FUNC = "fn_ComBtnSearch";//호출할 콜백함수
		fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
		fvFileInfo.PK01_NO = nApp.gvCompany;//PK번호1-회사코드
		fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITEM_CD");//PK번호2
		//fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호2-결재차수
	}	

	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		if (!this.fn_VaildMasterSave()) return;
	}	
		
	//저장시 재조회 Key설정
	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITEM_CD");//조회Key01

	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	//[공통]서비스 호출 변수
	var sInputDaset = "" //입력 데이터셋
	var sOutDatset = "";//출력 데이터셋
	var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		
	//Master 변경건이 있으면 Master 저장
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
		svcAddObj.sInsertSql = "rd:M_RM_0110_M_IN10";//저장Sql
		svcAddObj.sUpdateSql = "rd:M_RM_0110_M_UP10";//수정Sql
		svcAddObj.sDeleteSql = "rd:M_RM_0110_M_DL10";//삭제Sql
		svcAddObj.sSaveFlagColumn = "";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		/*
		//변경이력 저장 안함 2022.02.03
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
		svcAddObj.sUpdateSql = "rd:M_RM_0110_M_UP11";//수정Sql
		this.gfn_AddComSaveService(this, svcAddObj);
		*/
		sInputDaset = "ds_Master10=ds_Master10:U" //입력 데이터셋
		
	}
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "SAVE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_RD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = sInputDaset //입력 데이터셋
	svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
	svcCallObj.sOtherArg = sOtherArg;//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명		
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	

}

/***********************************************************************************************
 * 05.02. [Vaildation] Master 저장 필수값 체크 - 입력항목 
 ***********************************************************************************************/ 
this.fn_VaildMasterSave = function()
{
	//[저장] Validation 설정
	var arrArgs = [	 "this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItlmCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_RdItlm.text + "|NULL"	//대분류
					,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItmmCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_RdItmm.text + "|NULL"	//중분류
					,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ItemCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_ItemCd.text + "|NULL"	//품목코드
					,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ItemNm^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_ItemNm.text + "|NULL"	//품목명
					,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SpecNm^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_SpecNm.text + "|NULL"	//규격
					,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_UnitCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_UnitCd.text + "|NULL"	//단위
			      ]; 


	/*
	사용여부 미 체크시 미사용여부 필수 체크한다.
	*/
	var sUsedYn;
	
	sUsedYn = this.gfn_Nvl(this.div_MasterInput.form.tab_Master.tpg_01.form.chk_UsedYn.value,"0");
	
	if( sUsedYn == "0" && this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "REAS_NM"))){
		var arrMsgArg = [this.div_MasterInput.form.tab_Master.tpg_01.form.sta_ReasNm.text]; //미사용여부
		this.gfn_Alert("SM008", arrMsgArg);//[ &ITM0 ] 계정의 관리항목 &ITM1 (은/는) 필수입력 항목입니다. 
		return;
	}
	
	
	
	var dsObj,stObj,edtObj,cboObj;		
	
	for(var i=1;i<=10;i++){
		dsObj = this.all["ds_Spec"+i.toString().padLeft(2,"0")];
		stObj = this.div_MasterInput.form.tab_Master.tpg_01.form.all["sta_Spec"+i.toString().padLeft(2,"0")];
		edtObj = this.div_MasterInput.form.tab_Master.tpg_01.form.all["edt_Spec"+i.toString().padLeft(2,"0")];
		cboObj = this.div_MasterInput.form.tab_Master.tpg_01.form.all["cbo_Spec"+i.toString().padLeft(2,"0")];
		if(dsObj.getRowCount() <= 0){
			break;
		}
		if(dsObj.getColumn(0, "ESSE_YN")=="1"){
			if(dsObj.getColumn(0, "KEYI_YN")=="1"){
				arrArgs.push("this.div_MasterInput.form.tab_Master.tpg_01.form.edt_Spec"+i.toString().padLeft(2,"0")+"^" + this.div_MasterInput.form.tab_Master.tpg_01.form.all["sta_Spec"+i.toString().padLeft(2,"0")].text + "|NULL");
			}
			else{
				arrArgs.push("this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_Spec"+i.toString().padLeft(2,"0")+"^" + this.div_MasterInput.form.tab_Master.tpg_01.form.all["sta_Spec"+i.toString().padLeft(2,"0")].text + "|NULL");
			}
		}
	}
	
	var sCheckGroupTitle = this.div_MasterInput.form.tab_Master.tpg_01.text;//체크그룹 타이틀

	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
		return false;
	}
	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	

	
	
	
	
	
	//Master 입력 상태이면
	//등록인 경우 품목채번 생성
	if (sChkRowType == "I") {
		//솔라셀 자재 웨이퍼의 경우 가공(SP01_CD=1)
		/*
		if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITLM_CD") == "MC"
			&& this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITMM_CD") == "W"
			&& this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SP01_CD") != "1"){
			//채번번호를 붙히지 않는다.
			return true;
		}
		*/
			
		var nRow = this.ds_RdItmm.findRow("ITMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITMM_CD"));
		var nItemDn = nexacro.toNumber(this.ds_RdItmm.getColumn(nRow, "ITEM_DN"))
		if(nItemDn > 0){

			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_ISEQ.clearData();
			this.ds_InputIseq.clearData();
			
			this.ds_InputIseq.addRow();
			this.ds_InputIseq.setColumn(0, "ITLM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITLM_CD"));
			this.ds_InputIseq.setColumn(0, "ITMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITMM_CD"));
			
			for(var i=1;i<=10;i++){
				var dsObj = this.all["ds_Spec"+i.toString().padLeft(2,"0")];
				
				if(dsObj.getColumn(0,"CDAT_YN") == "1" && !this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SP"+i.toString().padLeft(2,"0")+"_CD"))){
					this.ds_InputIseq.setColumn(0, "SP"+i.toString().padLeft(2,"0")+"_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SP"+i.toString().padLeft(2,"0")+"_CD"));
				}else{
				}
			}	
			this.ds_InputIseq.setColumn(0, "ITEM_DN", nItemDn);
				
			//[공통]서비스 정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object

			svcAddObj.sSelectSql = "rd:M_RM_0110_M_SL11";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "ISEQ_SAVE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_RD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_InputIseq=ds_InputIseq" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_ISEQ=ds_ISEQ";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "";//서버에서 처리가 완료된 후 Callback 받을 Function 명
			svcCallObj.isSync = true;

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);	
					
			//채번실패.
			if (this.ds_ISEQ.getRowCount() <= 0) {
				this.gfn_Alert("RD002");//품목코드일련번호채번이 실패하였습니다.
				fvOrgItemCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITEM_CD");
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ItemCd.set_value(fvOrgItemCd);
				return false;
			}else{
				this.ds_Master10.setColumn(this.ds_Master10.rowposition
											, "ITEM_CD"
											, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITEM_CD")+this.ds_ISEQ.getColumn(0,"ISEQ_CT")
											);
			}
		}
				
		//중복키 확인
		if(!this.fn_DupValid()){
			if(!this.gfn_IsNull(fvOrgItemCd)){
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ItemCd.set_value(fvOrgItemCd);
			}
			return false;
		}
	}
	return true;
}

