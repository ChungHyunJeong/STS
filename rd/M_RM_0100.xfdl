{

	var iTabIndex = e.preindex;
	//품목기준관리
	if(iTabIndex == 0){	
		this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.updateToDataset();
		this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Detail10.updateToDataset();
		//데이터 변경여부 체크
		if (this.gfn_DsIsUpdated(this.ds_Master10) || this.gfn_DsIsUpdated(this.ds_Detail10)) {
			this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.
			return false;
		}
	}
	//품목사양관리
	else{
		this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Master20.updateToDataset();
		this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail20.updateToDataset();
		//데이터 변경여부 체크
		if (this.gfn_DsIsUpdated(this.ds_Master20) || this.gfn_DsIsUpdated(this.ds_Detail20)) {
			this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.
			return false;
		}
	}
	return true;
};

this.div_MasterInput_tab_Master_onchanged = function(obj:nexacro.Tab,e:nexacro.TabIndexChangeEventInfo)
{
	var iTabIndex = e.postindex;
	this.fn_SetTabPageInit(iTabIndex);
	if(iTabIndex == 0){
		this.div_Search.form.cbo_ItlmCd.set_innerdataset("ds_RdItlmSch");
		this.div_Search.form.cbo_ItlmCd.set_index(0);
		this.div_MasterInput.form.btn_plm.set_visible(false);
	}else{
		this.div_Search.form.cbo_ItlmCd.set_innerdataset("ds_RdItlm");
		this.div_Search.form.cbo_ItlmCd.set_index(0);
		
		this.ds_RdItmmSch.filter("");
		if (!this.gfn_IsNull(this.div_Search.form.cbo_ItlmCd.value)){
			this.ds_RdItmmSch.filter("ITLM_CD=='"+this.div_Search.form.cbo_ItlmCd.value+"' || ITMM_NM=='ALL'" );		
		}
		this.div_Search.form.cbo_ItmmCd.set_index(0);
		this.div_MasterInput.form.btn_plm.set_visible(true);
	}
};

