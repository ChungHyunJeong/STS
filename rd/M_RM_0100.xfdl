 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 
	var oMaster, oDetail;
	var sInputDasetM, sInputDasetD;
	var sInsertSqlM, sUpdateSqlM, sDeleteSqlM;
	var sInsertSqlD, sUpdateSqlD, sDeleteSqlD;
	
	//품목 기준 관리
	if(this.div_MasterInput.form.tab_Master.tabindex == 0){
		oMaster = this.ds_Master10;
		oDetail = this.ds_Detail10;
		sInputDasetM = "ds_Master10=ds_Master10:U";
		sInputDasetD = "ds_Detail10=ds_Detail10:U";
					 
		sInsertSqlM = "rd:M_RM_0100_M_IN10";//저장Sql
		sUpdateSqlM = "rd:M_RM_0100_M_UP10";//수정Sql
		sDeleteSqlM = "rd:M_RM_0100_M_DL10";//삭제Sql
			
		sInsertSqlD = "rd:M_RM_0100_D_IN10";//저장Sql
		sUpdateSqlD = "rd:M_RM_0100_D_UP10";//수정Sql
		sDeleteSqlD = "rd:M_RM_0100_D_DL10";//삭제Sql
		//this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITLM_CD");//조회Key01
		//this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "USED_YN");//조회Key02
	
	}
	//품목사양관리
	else{
		oMaster = this.ds_Master20;
		oDetail = this.ds_Detail20;
		sInputDasetM = "ds_Master20=ds_Master20:U";
		sInputDasetD = "ds_Detail20=ds_Detail20:U";
		
		sInsertSqlM = "rd:M_RM_0100_M_IN20";//저장Sql
		sUpdateSqlM = "rd:M_RM_0100_M_UP20";//수정Sql
		sDeleteSqlM = "rd:M_RM_0100_M_DL20";//삭제Sql
		
		sInsertSqlD = "rd:M_RM_0100_D_IN20";//저장Sql
		sUpdateSqlD = "rd:M_RM_0100_D_UP20";//수정Sql
		sDeleteSqlD = "rd:M_RM_0100_D_DL20";//삭제Sql
		//this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ITLM_CD");//조회Key01
		//this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "USED_YN");//조회Key02
		//this.fvSchKey03 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "USED_YN");//조회Key02
	}	

	//변경여부 체크
	if (!this.gfn_DsIsUpdated(oMaster) && !this.gfn_DsIsUpdated(oDetail)) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}

	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(oMaster)) {
		if (!this.fn_VaildMasterSave()) return;
	}
	
	//Detail 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(oDetail)) {
		if (!this.fn_VaildDetailSave()) return;
	}	

	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	//[공통]서비스 호출 변수
	var sInputDaset = "" //입력 데이터셋
	var sOutDatset = "";//출력 데이터셋
	var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		
	//Master 변경건이 있으면 Master 저장
	if (this.gfn_DsIsUpdated(oMaster)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
		svcAddObj.sInsertSql = sInsertSqlM;//저장Sql
		svcAddObj.sUpdateSql = sUpdateSqlM;//수정Sql
		svcAddObj.sDeleteSql = sDeleteSqlM;//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

		this.gfn_AddComSaveService(this, svcAddObj);
		
		sInputDaset = sInputDasetM; //입력 데이터셋

	}
	
	//Detail 변경건이 있으면 Detail 저장
	if (this.gfn_DsIsUpdated(oDetail)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object	
		svcAddObj.sInsertSql = sInsertSqlD;//저장Sql
		svcAddObj.sUpdateSql = sUpdateSqlD;//수정Sql
		svcAddObj.sDeleteSql = sDeleteSqlD;//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		
		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = sInputDasetD //입력 데이터셋
		} else {
			sInputDaset = sInputDaset + " " + sInputDasetD //입력 데이터셋
		}

	}		
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "SAVE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_RD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = sInputDaset //입력 데이터셋
	svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
	svcCallObj.sOtherArg = sOtherArg;//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명		
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);
}

