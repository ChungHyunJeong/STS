 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
	
		case "COMBO_SEARCH": //대분류조회	
			this.ds_RdItlm.clearData();
			this.ds_RdItmm.clearData();
			this.ds_RdItlm.copyData(this.ds_RdItlmSch);
			this.ds_RdItmm.copyData(this.ds_RdItmmSch);
				
			this.ds_RdItlmSch.insertRow(0);
			this.ds_RdItlmSch.setColumn(0,"ITLM_NM","ALL");	
			
			this.div_Search.form.cbo_ItlmCd.set_index(0);	
			
			this.ds_RdItmmSch.filter("");
			if (!this.gfn_IsNull(this.div_Search.form.cbo_ItlmCd.value)){
				this.ds_RdItmmSch.filter("ITLM_CD=='"+this.div_Search.form.cbo_ItlmCd.value+"' || ITMM_NM=='ALL'" );		
			}
			this.div_Search.form.cbo_ItmmCd.set_index(0);
			this.div_Search.form.cbo_UsedYn.set_index(0);
			
			break;
		case "MST10_SEARCH": //조회		
			this.ds_Master10.addColumn("crudType", "string");
			if (this.ds_Master10.getRowCount() > 0) {
				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master10.findRowExpr("ITLM_CD=='" + this.fvSchKey01 + "' && USED_YN=='" + this.fvSchKey02 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}				
				
				
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10);
				
			} else {
				
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10);
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			
			}
			
			//저장후 재조회여부-true
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}			
	
			break;

		case "MST20_SEARCH": //조회				
			this.ds_Master20.addColumn("crudType", "string");
			if (this.ds_Master10.getRowCount() > 0) {
				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master20.findRowExpr("ITLM_CD=='" + this.fvSchKey01 + "' && USED_YN=='" + this.fvSchKey02 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master20.enableevent) {
						this.ds_Master20.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master20.set_rowposition(-1);
						} 
						this.ds_Master20.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master20.set_rowposition(0);
					}
				}				
				
				
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Master20);
				
			} else {
				
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Master20);
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master20.enableevent) {
					this.ds_Master20.set_enableevent(true);
				}
			
			}
			
			//저장후 재조회여부-true
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}	
			break;
		case "DTL10_SEARCH": //조회		
			this.ds_Detail10.addColumn("crudType", "string");
			if (this.ds_Detail10.getRowCount() > 0) {
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Detail10);
			} else {
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Detail10);			
			}
			break;			
			
		case "DTL20_SEARCH": //조회	
			this.ds_Detail20.addColumn("crudType", "string");
			if (this.ds_Detail20.getRowCount() > 0) {
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail20);
			} else {
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail20);			
			}
			break;			
			
		
		case "SAVE":   //저장		
			this.fvSchKeyYn = true;//저장후 재조회여부
			this.fn_ComBtnSearch();//저장후 재조회
			break;	
		case "DELETE": //삭제		
			this.fn_ComBtnSearch();//조회
			break;	
		case "PLM_RUN":
			this.gfn_Alert("HR258");//처리가 완료되었습니다.
			break;							
	}
};

/***********************************************************************************************
 * 13.02. [팝업] CallBack 함수 처리
          strId : 팝업호출시 지정한 팝업ID
	      rtnValue : 팝업창에서 close시 지정한 반환값
          주의 : Open(popuptype=modeless)창은 팝업콜백을 지원하지 않음
 ***********************************************************************************************/ 
this.fn_PopupCallback = function(strId, rtnValue)
{
	if(strId == "S_ACOM_CD_01"){
		//계정 popup
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);				
			if (!this.gfn_IsNull(arrRtnVal.ACOM_CD)) {				
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "ACOM_CD", arrRtnVal.ACOM_CD);
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "ACOM_NM", arrRtnVal.ACOM_NM);				 
			}
		} else {
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "ACOM_CD", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "ACOM_NM", "");	
		}
	}	
};
 
