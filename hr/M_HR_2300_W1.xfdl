		this.ds_HrJobm.filter("");
		var sFilter = this.ds_Detail10.getColumn(e.row, "JOGM_CD");
		var sFilter1 = this.ds_Detail10.getColumn(e.row, "SERM_CD");
		
		this.ds_HrJobm.filter("JOGM_CD == '" + sFilter + "' || JOGM_CD == null" + "SERM_CD == '" + sFilter1 + "' || SERM_CD == null");
	}
}
