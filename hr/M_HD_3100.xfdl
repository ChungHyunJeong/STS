/***********************************************************************************************
 * 03.01. [공통버튼] 복사 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnCopy = function()
{ 
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10) || this.gfn_DsIsUpdated(this.ds_Detail10) || this.gfn_DsIsUpdated(this.ds_Detail11) || this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}
	
	//출장구분에 따라 초기화 방법이 다름
	//시내 - 출장상세의 출장일자만 초기화 함
	//국내 - 출장상세 전체를 초기화 함
	var sBzthGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_GU");	//출장구분

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	this.ds_Master10.set_enableevent(false); //row 생성속도 향상을 위한 이벤트 false

	var iPreRow = this.div_MasterView.form.grd_Master10.getSelectedRows();
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	
	var iAddRow = this.ds_Master10.addRow();
	this.ds_Master10.copyRow(iAddRow, this.ds_Master10, iPreRow);
	
	//기본값 등록
	this.ds_Master10.setColumn(iAddRow, "BZTH_NO", "");//출장번호 초기화
	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", "1");//결재차수
	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");//최종여부	
	this.ds_Master10.setColumn(iAddRow, "BZTH_DD", sToDay);//등록일자-오늘날자로 설정
	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//출장부서코드	
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//출장부서명	
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//출장자사번	
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//출장자명		
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//출장자ID	
	this.ds_Master10.setColumn(iAddRow, "BGDP_CD", this.gfn_GetSessionValue("DEPM_CD"));//예산부서코드	
	this.ds_Master10.setColumn(iAddRow, "BGDP_NM", this.gfn_GetSessionValue("DEPM_NM"));//예산부서명	
	this.ds_Master10.setColumn(iAddRow, "PALM_CD", this.gfn_GetSessionValue("PALM_CD"));//손익코드	
	this.ds_Master10.setColumn(iAddRow, "SCTR_CD", this.gfn_GetSessionValue("SCTR_CD"));//전표손익제어(FA012) 	
	//로그인 사용자의 손익명/전표손익제어 가져오기(COMBO_SEARCH는 사용중인 대상만 가져옴)
	var arrArgs = ["PALM_CD^this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PalmCd","PALM_NM^this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PalmNm"
	              ];
	this.gfn_GetCommEmpm("C", this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd, this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm, this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SystId, "M_SYST_ID_01", arrArgs);//코드로 검색

	//필수체크(연구(0050):연구프로젝트번호, 제조(0010):계약번호)
	var sSctrCd = this.gfn_GetSessionValue("SCTR_CD"); //전표손익제어(FA012)
	if (sSctrCd == '0010') { //판매(0040) --> 제조(0010) : 2022-04-25 변경
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 8, "cssclass", "essential");
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 11, "cssclass", "");
	} else if (sSctrCd == '0050') { //연구(0050)
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 8, "cssclass", "");
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 11, "cssclass", "essential");
	} else {
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 8, "cssclass", "");
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 11, "cssclass", "");
	}
	
	//Master10 초기화 	
	this.ds_Master10.setColumn(iAddRow, "FROM_DD", "");  //출장기간 시작일자
	this.ds_Master10.setColumn(iAddRow, "CLOS_DD", "");  //출장기간 종료일자
	this.ds_Master10.setColumn(iAddRow, "NGHT_QT", "");  //박
	this.ds_Master10.setColumn(iAddRow, "DAYS_QT", "");  //일
	this.ds_Master10.setColumn(iAddRow, "BUDG_MM", "");  //예산년월	
	this.ds_Master10.setColumn(iAddRow, "ACCH_NO", "");  //발의번호
	this.ds_Master10.setColumn(iAddRow, "WOFH_NO", "");  //결재번호
	this.ds_Master10.setColumn(iAddRow, "WOFH_ST", "0010");  //결재상태
	this.ds_Master10.setColumn(iAddRow, "REGI_NM", "");  //등록자명
	this.ds_Master10.setColumn(iAddRow, "REGI_DT", "");  //등록일시
	this.ds_Master10.setColumn(iAddRow, "MODI_NM", "");  //수정자명
	this.ds_Master10.setColumn(iAddRow, "MODI_DT", "");  //수정일시

	this.ds_Master10.set_enableevent(true); //row 생성속도 향상을 위한 이벤트 true
		
	if (sBzthGu == '3') { // 출장구분 시내시
		this.ds_Detail10.set_enableevent(false); //row 생성속도 향상을 위한 이벤트 false	
		this.ds_Detail10.reset();//출장일정상세정보 초기화
		this.ds_Detail11.reset();//출장비상세정보 초기화
		this.ds_Detail15.reset();//동행인 초기화
		
		//Detail crudType "I"로 변경
		for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {	//Detail10
			this.ds_Detail10.setColumn(iLoop, "crudType", "I");  //crudType "I"
			this.ds_Detail10.setColumn(iLoop, "BZTH_NO", "");    //출장번호 초기화
			this.ds_Detail10.setColumn(iLoop, "WOFH_SQ", "1");   //결재차수		
			this.ds_Detail10.setColumn(iLoop, "BZTB_DD", "");    //출장일자
		}
		this.ds_Detail10.set_enableevent(true); //row 생성속도 향상을 위한 이벤트 true
		
		for (var iLoop=0;iLoop<this.ds_Detail11.getRowCountNF();iLoop++) {	//Detail11
			this.ds_Detail11.setColumnNF(iLoop, "crudType", "I");  //crudType "I"
			this.ds_Detail11.setColumnNF(iLoop, "BZTH_NO", "");    //출장번호 초기화
			this.ds_Detail11.setColumnNF(iLoop, "WOFH_SQ", "1");   //결재차수		
		}
		
		for (var iLoop=0;iLoop<this.ds_Detail15.getRowCount();iLoop++) {	//Detail12
			this.ds_Detail15.setColumn(iLoop, "crudType", "I");  //crudType "I"
			this.ds_Detail15.setColumn(iLoop, "BZTH_NO", "");    //출장번호 초기화
			this.ds_Detail15.setColumn(iLoop, "WOFH_SQ", "1");   //결재차수		
		}
		
	} else {	//출장구분 국내(sBzthGu == '1')
		this.ds_Detail10.clearData();//출장일정상세정보 Clear
		this.ds_Detail11.clearData();//출장비상세정보 Clear
		this.ds_Detail15.clearData();//동행인 Clear
	
	}

	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	eval(this.fvAtobDiv).fn_ClearAtobData();//자동전표 정보 초기화

	//첨부파일정보 권한설정
	var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
	oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
	this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	

	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.div_DetailInput.form.tab_Detail.set_enable(true);
	
	//처음 입력 항목에 포커스
	this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.setFocus();

	this.fn_SetWfCompEnable();	//[결재] 결재상태에 따른 컴포넌트 제어			

	//출장표준경비 조회 - 출장자에 따라 변경되야 함			
	this.fn_BztmMaxSearch();	
	
	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
}

