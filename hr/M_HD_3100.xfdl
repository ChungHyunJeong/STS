/***********************************************************************************************
 * 05.03. [Vaildation] Detail 저장 필수값 체크 - 그리드항목
 ***********************************************************************************************/ 
this.fn_VaildDetailSave = function()
{	
	//최초 출장일자와 마지막 출장일자가 동일월이여야 함
	var sFirstDd = this.ds_Detail10.getMin("BZTB_DD"); //최초 출장일자
	var sLastDd  = this.ds_Detail10.getMax("BZTB_DD"); //마지막 출장일자
	
	if (sFirstDd.substring(0, 6) != sLastDd.substring(0, 6)) {
		var arrMsgArg = [this.gfn_StringFormat(sFirstDd.substr(0,6), "@@@@-@@")];	//최초 출장월
		this.gfn_Alert("HR022", arrMsgArg); //출장일자는 동일한 월(2022-04)로만 선택이 가능합니다.

		return false;
	}


	//교통비 대상(본인차량, 대중교통)이 현금+카드 > 0 경우, 출장지/도착지 필수값 체크	
	var crudType = "";	//crudType 변경여부 확인
	var sBztbSe  = "";	//출장순번
	var sBztmCd  = "";	//비용항목
	var sStarNm  = "";	//출발지
	var sArriNm  = "";	//도착지
	var iCashAm  = 0;	//현금
	var iPscaAm  = 0;	//카드
	var iChkCnt  = 0;	//출장일자별로 출발지/도착지는 1건은 필수로 들어가야 함
	var alertMsg = "";	//팝업메시지

	for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
		crudType = this.ds_Detail10.getColumn(iLoop,"crudType"); //crudType 변경여부 확인
		sBztbSe  = this.ds_Detail10.getColumn(iLoop,"BZTB_SE");  //출장순번
		sBztmCd  = this.ds_Detail10.getColumn(iLoop,"BZTM_CD");  //비용항목
		sStarNm  = this.ds_Detail10.getColumn(iLoop,"STAR_NM");  //출발지
		sArriNm  = this.ds_Detail10.getColumn(iLoop,"ARRI_NM");  //도착지
		iCashAm  = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop,"CASH_AM"), -1);  //현금
		iPscaAm  = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop,"PSCA_AM"), -1);  //카드
		iChkCnt  = this.ds_Detail10.getCaseCount("BZTB_SE=='"+sBztbSe+"' && STAR_NM != '' && ARRI_NM != ''")	//출장순번의 도착지, 출발지는 최소 1건이 존재해야 함
		alertMsg = "";	//팝업메시지

		if ((crudType == 'I' || crudType == 'U') && sBztmCd == '0010') {	//교통비 대상(0010) - (현금 + 카드) > 0
		
			if (iChkCnt == 0) {	//출발지, 도착지가 존재하지 않은경우
				if (this.gfn_IsNull(sStarNm) || sStarNm == '') {	//출발지 없는경우
					alertMsg = "출발지";
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(4,iLoop);
				}
				
				if (this.gfn_IsNull(sArriNm) || sArriNm == '') {	//도착지 없는 경우
					if (this.gfn_IsNull(alertMsg)) {		
						alertMsg = "도착지";
						this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(6,iLoop);
					} else {
						alertMsg = alertMsg + ", 도착지";
					}					
				}
				
				if (!(iCashAm >= 0 || iPscaAm >=0)) {	//금액이 미존재시
					if (this.gfn_IsNull(alertMsg)) {		
						alertMsg = "출장경로 존재시 0원 이상의 현금/카드금액";
						this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(12,iLoop);
					} else {
						alertMsg = alertMsg + ", 0원 이상의 현금/카드금액";
					}					
				}
				
			} else {
				if ((this.gfn_IsNull(sStarNm) || sStarNm == '') && (iCashAm >= 0 || iPscaAm >=0) && (!this.gfn_IsNull(sArriNm) || sArriNm != '')) {	//출발지 없는경우
					alertMsg = "출발지";
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(4,iLoop);
				}
				
				if ((this.gfn_IsNull(sArriNm) || sArriNm == '') && (iCashAm >= 0 || iPscaAm >=0) && (!this.gfn_IsNull(sStarNm) || sStarNm != '')) {	//도착지 없는 경우
					if (this.gfn_IsNull(alertMsg)) {		
						alertMsg = "도착지";
						this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(6,iLoop);
					} else {
						alertMsg = alertMsg + ", 도착지";
					}					
				}
				
				if (!(iCashAm >= 0 || iPscaAm >=0) && !(this.gfn_IsNull(sStarNm) || sStarNm == '') && !(this.gfn_IsNull(sArriNm) || sArriNm == '')) {
					if (this.gfn_IsNull(alertMsg)) {		
						alertMsg = "출장경로 존재시 0원 이상의 현금/카드금액";
						this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(12,iLoop);
					} else {
						alertMsg = alertMsg + ", 0원 이상의 현금/카드금액";
					}									
				}
				
			}

			//필수값 누락 발생시
			if (!this.gfn_IsNull(alertMsg)) {		
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();					
			
				var iRow = iLoop + 1;
				var arrMsgArg = [iRow+"번째행 "+alertMsg];
				this.gfn_Alert("SM008", arrMsgArg);//출발지, 도착지는 필수 입력 항목입니다.
				return false;
				
				break;
			}

		}

	}

	//계약/연구개발 체크
	var sSctrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SCTR_CD"); //전표손익제어(FA012)
	
	//[저장] Validation 설정
	var arrArgs = ["BZTB_DD^NULL"
			      ,"BZTM_CD^NULL"
			      ,"BZTD_CD^NULL"
			      ]; 

	//필수여부체크(연구(0050):연구프로젝트번호, 제조(0010):계약번호)
	if (sSctrCd == '0010') { //판매(0040) --> 제조(0010) : 2022-04-25 변경
		arrArgs.push("CNTH_NO^NULL");	//계약번호
	} else if (sSctrCd == '0050') { //연구(0050)
		arrArgs.push("RPJH_NO^NULL");	//연구개발번호
	}

	var sCheckGroupTitle = this.div_DetailInput.form.tab_Detail.tpg_01.text;//체크그룹 타이틀
				  
	//Grid Vaild 체크
	if (!this.gfn_CheckGridValid(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10, arrArgs, sCheckGroupTitle) ) {
		return false;
	}

	return true;
}
 
