/***********************************************************************************************
 * 5. 저장
 ***********************************************************************************************/
/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.gfn_DsIsUpdated(this.ds_Detail15) && !this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}

	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		//첨부파일정보만 변경시
		if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.gfn_DsIsUpdated(this.ds_Detail15) && this.ds_Master10.getRowCount() > 0) {
			//첨부파일정보 저장
			fvFileInfo = new Object();//[공통] 첨부파일정보
			fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, B-상세(그리드Row), E-기타
			fvFileInfo.CALLBACK_FUNC = "";//호출할 콜백함수
			fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
			fvFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO");//PK번호2-출장번호
			fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
			//첨부파일Div 화면에 Key세팅하게 변경
			this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);
			return;	
		}
	}

	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		if (!this.fn_VaildMasterSave()) return;
	}

	//Detail10 출장상세가 1Row 이상이어야 함
	if (this.ds_Detail10.getRowCount() < 1) { 
		this.div_DetailInput.form.tab_Detail.set_tabindex(0); //출장상세 Tab으로 이동
		
		var arrMsgArg = ["출장상세"];
		this.gfn_Alert("SM045", arrMsgArg); //출장상세는 1건이 이상 존재해야 합니다.
		return false;
		
	}

	//Detail 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		if (!this.fn_VaildDetailSave()) return;

		//출장기간 중복여부 (2022-03-22 추가개발)
		if (this.fn_SetTermDuplicationCheck()) {	
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FromDd.setFocus();	//출장시작일자 포커스
			this.gfn_Alert("HR007");//해당 출장일자가 이미 존재합니다.
			return;
		}
	}	

	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	//Master 입력 상태이면
	if (sChkRowType == "I") {
	
		//채번-출장번호
		var sNewKeyNo = this.gfn_GetTbnmNo(this.fvComPrgmId, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_DD"));//프로그램ID, 기준일자
		if (this.gfn_IsNull(sNewKeyNo)) {
			//채번번호를 가져오는데 실패했습니다.
			return;
		}
		
		var sChkKeyNo = "";//채번번호-체크용
		var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position
		
		//Master 데이터셋에 채번정보 입력-출장번호
		this.ds_Master10.setColumn(iMasterRowPos, "BZTH_NO", sNewKeyNo);
		
		var sCommCd = this.ds_Master10.getColumn(iMasterRowPos, "COMM_CD");//회사코드
		var sBzthNo = this.ds_Master10.getColumn(iMasterRowPos, "BZTH_NO");//출장번호
		var sWofhSq = this.ds_Master10.getColumn(iMasterRowPos, "WOFH_SQ");//결재차수
		
		//Detail 데이터셋에 기본정보 입력-회사코드/출장번호/결재차수
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			sChkKeyNo = this.ds_Detail10.getColumn(iLoop, "BZTH_NO");//출장번호
			
			if (this.gfn_IsNull(sChkKeyNo) || sBzthNo != sChkKeyNo) {
				this.ds_Detail10.setColumn(iLoop, "COMM_CD", sCommCd);
				this.ds_Detail10.setColumn(iLoop, "BZTH_NO", sBzthNo);
				this.ds_Detail10.setColumn(iLoop, "WOFH_SQ", sWofhSq);
			}
		}
		
		//Detail15 동행인 데이터셋에 기본정보 입력-회사코드/출장번호/결재차수
		for (var iLoop15=0; iLoop15<this.ds_Detail10.getRowCount(); iLoop15++) {
			sChkKeyNo = this.ds_Detail15.getColumn(iLoop15, "BZTH_NO");//출장번호
			
			if (this.gfn_IsNull(sChkKeyNo) || sBzthNo != sChkKeyNo) {
				this.ds_Detail15.setColumn(iLoop15, "COMM_CD", sCommCd);
				this.ds_Detail15.setColumn(iLoop15, "BZTH_NO", sBzthNo);
				this.ds_Detail15.setColumn(iLoop15, "WOFH_SQ", sWofhSq);
			}
		}
		
	}	

	//Detail10 추가/변경시
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		//Detail11 변경건이 있으면 Detail11에 해당 대상 복사 (단, 교통비 - 본인차량 Row만)
		//출장원관리 Body (T_HD_BZTB 테이블) 처리 - Detail11
		this.ds_Detail11.clearData();
		for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
			var crudType = this.ds_Detail10.getColumn(iLoop,"crudType"); //crudType 변경여부 확인
			var sBztmCd  = this.ds_Detail10.getColumn(iLoop,"BZTM_CD");  //비용항목
			var sBztdCd  = this.ds_Detail10.getColumn(iLoop,"BZTD_CD");  //비용상세항목
			var sStarNm  = this.ds_Detail10.getColumn(iLoop,"STAR_NM");  //출장지
			var sCopyYn  = false;	//Detail11 복사여부

			if (sBztdCd == '1000' && (!this.gfn_IsNull(sStarNm) || sStarNm != '')) {	//본인차량(1000)
				var sCopyYn = true;
			} else if (sBztdCd == '1010' && (!this.gfn_IsNull(sStarNm) || sStarNm != '')) {	//대중교통(1010)
				var sStarNm1000 = this.ds_Detail10.getColumn(iLoop-1,"STAR_NM");  //본인차량 출장지
				
				if (this.gfn_IsNull(sStarNm1000) || sStarNm1000 == '') {	//본인차량 데이터 미존재시 대중교통으로 처리
					var sCopyYn = true;
					if (this.gfn_IsNull(crudType)) {
						crudType = "U";
					}
				} 
			}

			//Detail10 --> Detail11 복사
			if (!this.gfn_IsNull(crudType) && sBztmCd == '0010' && sCopyYn) {	// 교통비-(본인차량 or 대중교통) Row의 경우에만 처리
				//현금 OR 카드금액이 모두 없는경우 현금에 0을 강제로 넣음
				var iAddRow = this.ds_Detail11.addRow();
				if (sChkRowType == "I") {	//Master가 신규이면 상세내용은 신규로 처리
					crudType = "I";
				}				
				this.ds_Detail11.copyRow(iAddRow, this.ds_Detail10, iLoop);				
				this.ds_Detail11.setColumn(iAddRow, "crudType",crudType);
			}
		}		
		
		//Detail10 삭제건이 존재시 Master 출장비, 출장기간 재계산
		var iDtlDelCnt = this.ds_Detail10.getCaseCount("crudType=='D'"); //삭제건
		
		if (iDtlDelCnt > 0) { //삭제건이 0보다 크면 재계산
			//출장비 금액 계산
			var iCashAmSum = this.ds_Detail10.getCaseSum("crudType!='D'", "CASH_AM");  //현금
			var iPscaAmSum = this.ds_Detail10.getCaseSum("crudType!='D'", "PSCA_AM");  //개인카드금액
			
			//출장비 = 현금 + 개인카드금액
			var iBzthAm = iCashAmSum + iPscaAmSum
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BZTH_AM",iBzthAm);	//출장비
			
			
			//출장기간 계산
			var sFirstDd = this.ds_Detail10.getCaseMin("crudType!='D'", "BZTB_DD");
			var sLastDd  = this.ds_Detail10.getCaseMax("crudType!='D'", "BZTB_DD");

