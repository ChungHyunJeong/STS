/***********************************************************************************************
 * 02.MG0.07. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
			
	if (!this.fvCanRowPosChange) {	//canrowposchange 이벤트 발생시 처리제외
	
		// 출장구분
		if (e.columnid == "BZTH_GU") {
			var sToDay = this.gfn_GetCommDate("toDay");//현재일자
			var sBzthGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_GU");  //출장구분

			//X박X일 초기화
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "NGHT_QT", "");	//박(초기화)
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DAYS_QT", "");	//일(초기화)		
			this.ds_Detail10.clearData();	//Detail10 초기화 : 국내/시내 기준으로 재입력
			
			//국내
			if (sBzthGu == '1') {
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", sToDay);	//시작일자 = 오늘일자(11/5 요청)
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", "");		//종료일자 = 초기화
			
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FromDd.set_enable(true);  //시작일자 활성화
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.set_enable(true);  //종료일자 활성화

				//출장비용상세 국내사용여부 필터
				this.ds_Hr363.filter("");
				this.ds_Hr363.filter("OP04_NM == '1'");	//OP04_NM : 국내, OP05_NM : 시내
				
				//출장상세 행추가/행삭제/취소
				var objBtnInfo01List = [];						  
				//입력영역 버튼 비활성화
				objBtnInfo01List = ["COPY|N", "ADD|N", "INSERT|N", "DELETE|N", "CANCEL|N"];
				this.div_DetailInput.form.div_ComGrdBtn.form.fn_SetGrdButtonEnable(objBtnInfo01List);     			
				
				//출장기간 종료일자 커서 이동
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.setFocus();

			//시내
			} else if (sBzthGu == '3') {
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", "");	//시작일자 초기화
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", "");	//종료일자 초기화(시작일자 입력시 동일)
				
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FromDd.set_enable(false);  //시작일자 비활성화
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.set_enable(false);  //종료일자 비활성화

				//출장비용상세 시내사용여부 필터
				this.ds_Hr363.filter("");
				this.ds_Hr363.filter("OP05_NM == '1'");	//OP04_NM : 국내, OP05_NM : 시내

				//출장상세 행추가/행삭제/취소
				var objBtnInfo01List = [];						  
				//입력영역 버튼 비활성화
				objBtnInfo01List = ["COPY|N", "ADD|Y", "INSERT|N", "DELETE|Y", "CANCEL|N"];
				this.div_DetailInput.form.div_ComGrdBtn.form.fn_SetGrdButtonEnable(objBtnInfo01List);         			
			}

		// 출장기간 변경시 (박/일 계산)
		} else if (e.columnid == "FROM_DD" || e.columnid == "CLOS_DD") {
			var sBzthNo  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO"); //출장번호 유무
			var sBzthGu  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_GU"); //출장구분
			var fromDate = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD"); //출장기간(시작일자)
			var closDate = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD"); //출장기간(종료일자)
			var sToDay = this.gfn_GetCommDate("toDay");//현재일자
			var iNghtQt = ""; //박
			var iDaysQt = ""; //일

			//국내
			if (sBzthGu == '1') {
				var sBztbDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, e.columnid);	//선택한 출장기간의 일자
				var sHoliYn = this.fn_HolidaySearch(sBztbDd);	//휴가기간 체크(출장일자)

				//휴가기간 체크
				if (sHoliYn) {	//휴가기간인 경우
					this.gfn_Alert("HR023"); //해당 출장일자는 휴가기간 입니다.

					//시작일자, 종료일자 원복
					if (this.gfn_IsNull(sBzthNo)) {
						this.ds_Master10.setColumn(this.ds_Master10.rowposition, e.columnid, "");
					} else {
						this.ds_Master10.setColumn(this.ds_Master10.rowposition, e.columnid, this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, e.columnid)); //출장기간
					}

					return false;
				}//휴가기간 체크 끝

				//출장상세가 존재하는 경우 초기화 여부 메시지
				if (this.ds_Detail10.getRowCount() > 0 ) {	
					var arrMsgArg = ["출장상세정보"];
					
					if (e.columnid == "FROM_DD") {
						this.gfn_Confirm("HR026", arrMsgArg,  "CONF.FROM_RESET", "fn_MsgCallback");//출장상세 정보가 존재합니다. 초기화 후 진행하시겠습니까?	
					} else if (e.columnid == "CLOS_DD") {
						this.gfn_Confirm("HR026", arrMsgArg,  "CONF.CLOS_RESET", "fn_MsgCallback");//출장상세 정보가 존재합니다. 초기화 후 진행하시겠습니까?				
					}
					
					return false;
				}

				//X박X일 구하기
				if (this.gfn_IsNull(fromDate) || this.gfn_IsNull(closDate)) {
					return false;		

				// 출장기간 오늘일자 이후 선택하는 로직 요청으로 제거(2022-04-25)
				
				} else if (fromDate > closDate) {
					//시작일자, 종료일자 원복
					if (this.gfn_IsNull(sBzthNo)) {
						this.ds_Master10.setColumn(this.ds_Master10.rowposition, e.columnid, "");
					} else {
						this.ds_Master10.setColumn(this.ds_Master10.rowposition, e.columnid, this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, e.columnid)); //출장기간
					}
			
					this.gfn_Alert("SM018");//종료일자가 시작일자보다 빠릅니다
					return false;
					
				} else {

					iNghtQt = this.gfn_GetDiffDate(fromDate, closDate); //박
					iDaysQt = iNghtQt + 1;                              //일
					
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "NGHT_QT", iNghtQt);	//박
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DAYS_QT", iDaysQt);	//일		
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BZTH_AM", "");	//출장비초기화

					/* ### Detail10 날짜별로 생성 ### */
					//1. 초기값 
					this.ds_Detail10.set_enableevent(false); //row 생성속도 향상을 위한 이벤트 false
					this.ds_Detail10.clearData();
					
					var sBztbDd = ""; //출장일자
					var iMaxbSe = 0;  //최종출장순번
					var iBztbSe = 0;  //출장순번
					var iAddRow = 0;  //Detail10 Row
					
					//2. 최초대상이 아닌경우 출장순번 Max를 가져온다
					if (!this.gfn_IsNull(sBzthNo)) {	
						iMaxbSe = this.fn_MaxbSeSearch();
					}

					//3. 예산년월 변경시 출장표준경비 재조회
					var sBudgMm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BUDG_MM");  
					var sLastDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD");  

					if (sBudgMm.substr(0,6) != sLastDd.substr(0,6)) {
						this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BUDG_MM", sLastDd.substr(0,6));   //예상년월
						this.fn_BztmMaxSearch();	//출장표준경비 조회 - 예산년월 기준으로 재조회
					}

					//4. 날짜 차이 계산 (for문에 건수만큼 증가)
					var iDiffDd = this.gfn_GetDiffDate(fromDate, closDate) + 1;
					
					//5. 건수별로 Row 생성 (항목 : OP01_NM, 세부항목 - CODM_CD)
					for(var iLoop=0;iLoop<iDiffDd;iLoop++) {
						sBztbDd = this.gfn_AddDate(fromDate, iLoop); //출장일자(시작일자 기준으로 종료일자까지 증가)

						//출장비용상세 개수만큼 증가
						for (var iRow=0;iRow<this.ds_Hr363.getRowCount();iRow++) {
							iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)

							iBztbSe = nexacro.toNumber(iMaxbSe) + iLoop + 1;	//출장순번
							this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
							this.ds_Detail10.setColumn(iAddRow, "BZTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO"));
							this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
							this.ds_Detail10.setColumn(iAddRow, "BZTB_DD", sBztbDd);                                  //출장일자
							this.ds_Detail10.setColumn(iAddRow, "BZTB_SE", iBztbSe);                          		  //순번
							this.ds_Detail10.setColumn(iAddRow, "STAR_NM", ""); //출발지
							this.ds_Detail10.setColumn(iAddRow, "ARRI_NM", ""); //도착지
							this.ds_Detail10.setColumn(iAddRow, "BZTM_CD", this.ds_Hr363.getColumn(iRow, "OP01_NM")); //항목
							this.ds_Detail10.setColumn(iAddRow, "BZTD_CD", this.ds_Hr363.getColumn(iRow, "CODM_CD")); //세부항목
							this.ds_Detail10.setColumn(iAddRow, "OP06_NM", this.ds_Hr363.getColumn(iRow, "OP06_NM")); //수량(Km/건수) 입력여부
							this.ds_Detail10.setColumn(iAddRow, "OP07_NM", this.ds_Hr363.getColumn(iRow, "OP07_NM")); //산출내역입력여부
							this.ds_Detail10.setColumn(iAddRow, "OP08_NM", this.ds_Hr363.getColumn(iRow, "OP08_NM")); //금액입력여부
							
						}
					}
					
					this.ds_Detail10.set_enableevent(true); //row 생성속도 향상을 위한 이벤트 ture
										
				}
				
			//시내(sBzthGu == '3')의 경우 Detail10의 출장일자 입력시 출장기간 처리
			}
			
		}
		
	}	//this.fvCanRowPosChange 이벤트
		
};

/***********************************************************************************************
 * 02.DG0.01. [Detail그리드-버튼] 복사 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd01Copy = function()
{
};

/***********************************************************************************************
 * 02.DG0.02. [Detail그리드-버튼] 행추가 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd01AddRow = function()
{
	var sBzthGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_GU");  //출장구분

	//시내
	if (sBzthGu == '3') {

		/* ### Detail10 날짜별로 생성 ### */
		//1. 초기값 
		this.ds_Detail10.set_enableevent(false); //row 생성속도 향상을 위한 이벤트 false
		
		var sBztbDd  = ""; //출장일자
		var sFirstDd = this.ds_Detail10.getMin("BZTB_DD"); //최초 출장일자
		var sLastDd  = this.ds_Detail10.getMax("BZTB_DD"); //마지막 출장일자
		var sLastSeq = this.ds_Detail10.getMax("MAXB_SE"); //최종 출장순번
		var sBudgMm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BUDG_MM");  //예산년월

		//출장일자
		if (this.gfn_IsNull(sLastDd)) { //null의 경우 오늘일자 등록
			sBztbDd = this.gfn_GetCommDate("toDay"); //현재일자
		} else {
			sBztbDd = this.gfn_AddDate(sLastDd, 1);   //마지막일자 다음날
		}


		//순번
		if (this.gfn_IsNull(sLastSeq)) { //null의 경우
			sLastSeq = "1";			
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", sBztbDd); //출장기간 시작일자
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", sBztbDd); //출장기간 종료일자
		} else {
			sLastSeq = nexacro.toNumber(sLastSeq)+1;  			
			//추가이므로 출장기간 종료일자만 변경함
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", sBztbDd); //출장기간 종료일자
		}
		
		//예상년월 출장기간 종료일자 월로 적용 요청(2022-02-11)
		if (sBudgMm.substr(0,6) != sBztbDd.substr(0,6)) {		
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BUDG_MM", sBztbDd.substr(0,6)); //출장기간 종료일자
			this.fn_BztmMaxSearch();	//출장표준경비 조회 - 예산년월 기준으로 재조회
		}
		
		//2. 건수별로 Row 생성 (항목 : OP01_NM, 세부항목 - CODM_CD)
		//출장비용상세 개수만큼 증가

		for (var iLoop=0;iLoop<this.ds_Hr363.getRowCount();iLoop++) {
			iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)

			this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
			this.ds_Detail10.setColumn(iAddRow, "BZTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO"));
			this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
			this.ds_Detail10.setColumn(iAddRow, "BZTB_DD", sBztbDd);  //출장일자
			this.ds_Detail10.setColumn(iAddRow, "BZTB_SE", sLastSeq); //순번
			this.ds_Detail10.setColumn(iAddRow, "STAR_NM", ""); //출발지
			this.ds_Detail10.setColumn(iAddRow, "ARRI_NM", ""); //도착지
			this.ds_Detail10.setColumn(iAddRow, "BZTM_CD", this.ds_Hr363.getColumn(iLoop, "OP01_NM")); //항목
			this.ds_Detail10.setColumn(iAddRow, "BZTD_CD", this.ds_Hr363.getColumn(iLoop, "CODM_CD")); //세부항목
			this.ds_Detail10.setColumn(iAddRow, "OP06_NM", this.ds_Hr363.getColumn(iLoop, "OP06_NM")); //수량(Km/건수) 입력여부
			this.ds_Detail10.setColumn(iAddRow, "OP07_NM", this.ds_Hr363.getColumn(iLoop, "OP07_NM")); //산출내역입력여부
			this.ds_Detail10.setColumn(iAddRow, "OP08_NM", this.ds_Hr363.getColumn(iLoop, "OP08_NM")); //금액입력여부
			this.ds_Detail10.setColumn(iAddRow, "BZTH_GU", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_GU")); //출장구분
			this.ds_Detail10.setColumn(iAddRow, "MAXB_SE", sLastSeq); //최종 출장순번
			
		}
		
		this.ds_Detail10.set_enableevent(true); //row 생성속도 향상을 위한 이벤트 ture		
	}
	
