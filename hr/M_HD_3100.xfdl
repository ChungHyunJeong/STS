	sPks += "ⓐ" + this.ds_Master10.getColumn(iRow, "BZTH_NO");//출장번호
	sPks += "ⓐ" + this.ds_Master10.getColumn(iRow, "WOFH_SQ");//결재차수


	//국내 or 시내
	var iFvFindRow = this.ds_Hr351.findRowExpr("CODM_CD=='" + this.ds_Master10.getColumn(iRow, "BZTH_GU") + "'");//국내 OR 시내 찾기
	var sCodmNm = this.ds_Hr351.getColumn(iFvFindRow, "CODM_NM");  //국내시내

