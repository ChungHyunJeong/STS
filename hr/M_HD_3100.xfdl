/***********************************************************************************************
 * 02.DG0.07. [Detail 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)

	var iNowRow  = e.row;
	var sBztbSe  = this.ds_Detail10.getColumn(iNowRow, "BZTB_SE"); //순번
	
	//체크시 해당 프로그램 전체선택
	if (e.columnid == "s_chk") {	
		this.ds_Detail10.set_enableevent(false); //이벤트 false
		
		var chkValue = this.ds_Detail10.getColumn(iNowRow, "s_chk");  //Check(1), UnCheck(0)

		for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
			//같은 순번의 경우에만 Check 이벤트 처리
			if (sBztbSe == this.ds_Detail10.getColumn(iLoop, "BZTB_SE")) { //순번비교
				this.ds_Detail10.setColumn(iLoop, "s_chk", chkValue);// Check/UnCheck 처리
				if (chkValue == '0') {
					this.ds_Detail10.setColumn(iLoop, "crudType", "");// crudType null처리					
				}
			}
		}	

		//출장비, 출장기간 변경
		if (chkValue == '0') {
			//출장비 금액 계산
			var iCashAmSum = this.ds_Detail10.getCaseSum("crudType!='D'", "CASH_AM");  //현금
			var iPscaAmSum = this.ds_Detail10.getCaseSum("crudType!='D'", "PSCA_AM");  //개인카드금액
			var sBudgMm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BUDG_MM");  //예산년월

			//출장비 = 현금 + 개인카드금액
			var iBzthAm = iCashAmSum + iPscaAmSum
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BZTH_AM",iBzthAm);	//출장비
			
			//출장기간 계산
			var sFirstDd = this.ds_Detail10.getCaseMin("crudType!='D'", "BZTB_DD");
			var sLastDd  = this.ds_Detail10.getCaseMax("crudType!='D'", "BZTB_DD");

			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", sFirstDd);  //시작일자
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", sLastDd);   //종료일자
			
			if (sBudgMm.substr(0,6) != sLastDd.substr(0,6)) {
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BUDG_MM", sLastDd.substr(0,6));   //예상년월 (종료일자 월 변경 2022-02-11)				
				this.fn_BztmMaxSearch();	//출장표준경비 조회 - 예산년월 기준으로 재조회
			}
		}
		
		this.ds_Detail10.set_enableevent(true); //이벤트 true
	
	//출장일자
	} else if (e.columnid == "BZTB_DD") {
		
		var sToDay = this.gfn_GetCommDate("toDay");//현재일자
		var sBzthGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_GU");  //출장구분(국내/시내)
		var sFrstDd  = this.ds_Detail10.getColumn(0,"BZTB_DD"); //최초 출장일자		
		var sBztbDd  = this.ds_Detail10.getColumn(iNowRow,"BZTB_DD"); //출장일자		
		var crudType = this.ds_Detail10.getColumn(iNowRow,"crudType"); //crudType (I의 경우 iNowRow+1, U의 경우 getOrgColumn)
		var iDupCnt = this.ds_Detail10.getCaseCount("BZTB_DD=='"+sBztbDd+"'");

		//1. 미래일자 입력불가(2022-04-20)
		if (sBztbDd > sToDay) {
			this.ds_Detail10.set_enableevent(false); // 이벤트 false

			var arrMsgArg = ["출장일자"];
			this.gfn_Alert("HR021", arrMsgArg); //출장일자는 오늘일자 이후는 선택할 수 없습니다.

			if (crudType == "I") {
				this.ds_Detail10.setColumn(iNowRow, "BZTB_DD", this.ds_Detail10.getColumn(iNowRow+1, "BZTB_DD")); //하위날짜로 동기화
			} else {
				this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);	//이전값
			}
			
			this.ds_Detail10.set_enableevent(true); // 이벤트 true
			
			return false;			

		//2. 입력한 일자가 중복이 존재하는지 확인 : 중복시 이전값으로 원복 - return false
		} else if (iDupCnt > 1) {
			this.ds_Detail10.set_enableevent(false); // 이벤트 false
			
			this.gfn_Alert("HR007");//해당 출장일자가 존재합니다.
			
			if (crudType == "I") {
				this.ds_Detail10.setColumn(iNowRow, "BZTB_DD", this.ds_Detail10.getColumn(iNowRow+1, "BZTB_DD")); //하위날짜로 동기화
			} else {
				this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);	//이전값
			}

			this.ds_Detail10.set_enableevent(true); // 이벤트 true

			return false;
			
		//3. 시내의 경우 첫번째 출장일자와 동일한 월만 선택 가능(2022-04-25 추가)
		} else if (sBzthGu == '3' && !this.gfn_IsNull(sFrstDd) && sFrstDd.substr(0,6) != sBztbDd.substr(0,6)) { 	
			this.ds_Detail10.set_enableevent(false); // 이벤트 false

			var arrMsgArg = [this.gfn_StringFormat(sFrstDd.substr(0,6), "@@@@-@@")];	//최초 출장월
			this.gfn_Alert("HR022", arrMsgArg); //출장일자는 동일한 월(2022-04)로만 선택이 가능합니다.
			
			if (crudType == "I") {
				this.ds_Detail10.setColumn(iNowRow, "BZTB_DD", this.ds_Detail10.getColumn(iNowRow+1, "BZTB_DD")); //하위날짜로 동기화
			} else {
				this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);	//이전값
			}

			this.ds_Detail10.set_enableevent(true); // 이벤트 true

			return false;
		
		}
		
		//4. 출장일자가 휴가일자인지 확인
		var sHoliYn = this.fn_HolidaySearch(sBztbDd);	//휴가기간 체크(출장일자)
		if (sHoliYn) {
			this.gfn_Alert("HR023"); //해당 출장일자는 휴가기간 입니다.

			if (crudType == "I") {
				this.ds_Detail10.setColumn(iNowRow, "BZTB_DD", this.ds_Detail10.getColumn(iNowRow+1, "BZTB_DD")); //하위날짜로 동기화
			} else {
				this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);	//이전값
			}

			return false;
		}

		//5. Detail 동일한 Seq에 출장일자 저장
		this.ds_Detail10.set_enableevent(false); // 이벤트 false
		
		for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
			if (sBztbSe == this.ds_Detail10.getColumn(iLoop,"BZTB_SE")) {
				this.ds_Detail10.setColumn(iLoop, "BZTB_DD", sBztbDd); //출장일자
			}

		}

		//6. 입력한 출장일자가 Detail 출장일자에서 최대값인지 최소값인지 확인 후
		//Master 출장기간에 일자 변경 (최소값 : from, 최대값 : close, 최초시 : from/close )
		var sFirstDd = this.ds_Detail10.getMin("BZTB_DD");
		var sLastDd  = this.ds_Detail10.getMax("BZTB_DD");
		var sBudgMm  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BUDG_MM");

		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", sFirstDd);  //시작일자
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", sLastDd);   //종료일자

		if (sBudgMm.substr(0,6) != sLastDd.substr(0,6)) {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BUDG_MM", sLastDd.substr(0,6));   //예상년월 (종료일자 월 변경 2022-02-11)				
			this.fn_BztmMaxSearch();	//출장표준경비 조회 - 예산년월 기준으로 재조회
		}

		this.fn_SetCostDetailProc(); //산출내역 계산 함수 호출 (날짜 변경시 재계산 2022-07-01)
		
		this.ds_Detail10.set_enableevent(true); // 이벤트 true

	//출장지, 경유지, 도착지
	} else if (e.columnid == "STAR_NM" || e.columnid == "STOP_NM" || e.columnid == "ARRI_NM") {
		this.ds_Detail10.set_enableevent(false); // 이벤트 false

		if (!this.gfn_IsNull(e.newvalue) || e.newvalue !='') {	//출장지값이 입력되면
			var iDupCnt = 0;
			
			//출장지
			if (e.columnid == "STAR_NM") {
				iDupCnt  = this.ds_Detail10.getCaseCount("BZTB_SE=='"+sBztbSe+"' && STAR_NM!=''");
			//경유지
			} else if (e.columnid == "STOP_NM") {
				iDupCnt  = this.ds_Detail10.getCaseCount("BZTB_SE=='"+sBztbSe+"' && STOP_NM!=''");
			//도착지
			} else if (e.columnid == "ARRI_NM") {
				iDupCnt  = this.ds_Detail10.getCaseCount("BZTB_SE=='"+sBztbSe+"' && ARRI_NM!=''");
			}
		
			//중복 존재시 메시지 출력
			if (iDupCnt > 1) {
				var arrMsgArg = ["출장경로"];	//출장경로
				this.gfn_Alert("HR024", arrMsgArg); //출장경로 복수입력시 본인차량 출장경로가 저장되오니 대중교통 출장경로 저장시 본인차량 출장경로를 삭제해주세요
			}
			
		}
		
		this.ds_Detail10.set_enableevent(true); // 이벤트 true

	//수량(Km/건수)
	} else if (e.columnid == "BZTB_QT") {
		var iBztbQt = nexacro.toNumber(this.ds_Detail10.getColumn(iNowRow,"BZTB_QT")); //수량 (숫자형으로 변환)
		var sBztdCd  = this.ds_Detail10.getColumn(iNowRow,"BZTD_CD"); //비용상세항목(1000 본인차량, 2000 숙박비(숙박시설), 3000 식사비(출장식비))

		//1. 수량 제약조건 (본인차량 (X), 숙박시설(O), 출장식비(O))
		if (sBztdCd == '2000' && iBztbQt > 1) {	    // 숙박비-숙박시설
			this.ds_Detail10.setColumn(iNowRow, "BZTB_QT", "1"); //수량 = 강제로 1입력
			
			var arrMsgArg = ["숙박비", "1"];	//숙박비 조건 : 1일 1회
			this.gfn_Alert("HR009", arrMsgArg); //숙박비 횟수는 1일 1회 이하입니다.
		} else if (sBztdCd == '3000' && iBztbQt > 3) {	// 식사비-출장식비
			this.ds_Detail10.setColumn(iNowRow, "BZTB_QT", "3"); //수량 = 강제로 3입력
			
			var arrMsgArg = ["식사비", "3"];	//식사비 조건 : 1일 3회
			this.gfn_Alert("HR009", arrMsgArg); //식사비 횟수는 1일 3회 이하입니다.
		}

		//2. 산출내역 계산 함수 호출
		this.fn_SetCostDetailProc(); //산출내역 계산 함수 호출
		
	//법인카드
	} else if (e.columnid == "DEPM_CD") {
	
	//계약번호
	} else if (e.columnid == "CNTH_NO" || e.columnid == "CNTH_NM") {
		this.gfn_GetCommCnthGrid(obj, "CNTH_NO", "CNTH_NM");//계약명 검색		

		this.ds_Detail10.set_enableevent(false); // 이벤트 false
		
		var sCnthNo  = this.ds_Detail10.getColumn(iNowRow,"CNTH_NO"); //계약코드
		var sCnthNm  = this.ds_Detail10.getColumn(iNowRow,"CNTH_NM"); //계약명

		//Detail 동일한 Seq에 계약 저장
		for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
			if (sBztbSe == this.ds_Detail10.getColumn(iLoop,"BZTB_SE")) {
				this.ds_Detail10.setColumn(iLoop, "CNTH_NO", sCnthNo); //계약코드
				this.ds_Detail10.setColumn(iLoop, "CNTH_NM", sCnthNm); //계약명
			}
		}

		this.ds_Detail10.set_enableevent(true); // 이벤트 true

	//연구프로젝트번호
	} else if (e.columnid == "RPJH_NO" || e.columnid == "RPJH_NM") {
		this.gfn_GetCommRpjhGrid(obj, "RPJH_NO", "RPJH_NM");//연구프로젝트명 검색		

		this.ds_Detail10.set_enableevent(false); // 이벤트 false
		
		var sRpjhNo  = this.ds_Detail10.getColumn(iNowRow,"RPJH_NO"); //연구프로젝트
		var sRpjhNm  = this.ds_Detail10.getColumn(iNowRow,"RPJH_NM"); //연구프로젝트명

		//Detail 동일한 Seq에 연구프로젝트 저장
		for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
			if (sBztbSe == this.ds_Detail10.getColumn(iLoop,"BZTB_SE")) {
				this.ds_Detail10.setColumn(iLoop, "RPJH_NO", sRpjhNo); //연구프로젝트
				this.ds_Detail10.setColumn(iLoop, "RPJH_NM", sRpjhNm); //연구프로젝트명
			}
		}

		this.ds_Detail10.set_enableevent(true); // 이벤트 true

	//거래처
	} else if (e.columnid == "CUSM_CD" || e.columnid == "CUSM_NM") {
		this.gfn_GetCommCusmGrid(obj, "CUSM_CD", "CUSM_NM", "A");//거래처명 검색

		this.ds_Detail10.set_enableevent(false); // 이벤트 false
		
		var sCusmNo  = this.ds_Detail10.getColumn(iNowRow,"CUSM_CD"); //거래처코드
		var sCusmNm  = this.ds_Detail10.getColumn(iNowRow,"CUSM_NM"); //거래처명

		//Detail 동일한 Seq에 거래처 저장
		for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
			if (sBztbSe == this.ds_Detail10.getColumn(iLoop,"BZTB_SE")) {
				this.ds_Detail10.setColumn(iLoop, "CUSM_CD", sCusmNo); //거래처코드
				this.ds_Detail10.setColumn(iLoop, "CUSM_NM", sCusmNm); //거래처명
			}
		}

		this.ds_Detail10.set_enableevent(true); // 이벤트 true
		
	//현금, 개인카드금액, 법인카드금액 변경시
	} else if (e.columnid == "CASH_AM" || e.columnid == "PSCA_AM") {
		var sBztmCd  = this.ds_Detail10.getColumn(iNowRow,"BZTM_CD"); //비용항목
		var sBztdCd  = this.ds_Detail10.getColumn(iNowRow,"BZTD_CD"); //비용상세항목(1000 본인차량, 2000 숙박비(숙박시설), 3000 식사비(출장식비))
		var crudType = this.ds_Detail10.getColumn(iNowRow,"crudType"); //crudType (I의 경우 iNowRow+1, U의 경우 getOrgColumn)
		
		//1. 숙박비 or 식사비 합산금액 (현금 + 개인카드금액 + 법인카드금액)
		if (sBztdCd == '2000' || sBztdCd == '3000') {	//숙박비(2000), 식사비(3000)		
			var iBztbQt = nexacro.toNumber(this.gfn_Nvl(this.ds_Detail10.getColumn(iNowRow,"BZTB_QT"),0)); //수량 (숫자형으로 변환)
			var iCashAm = nexacro.toNumber(this.gfn_Nvl(this.ds_Detail10.getColumn(iNowRow,"CASH_AM"),0)); //현금
			var iPscaAm = nexacro.toNumber(this.gfn_Nvl(this.ds_Detail10.getColumn(iNowRow,"PSCA_AM"),0)); //개인카드금액

			//1-1. 출장표준경비 (비용항목 / 비용상세항목 find 후)
			var iFvFindRow = this.ds_HrBztm.findRowExpr("BZTM_CD=='" + sBztmCd + "' && BZTD_CD=='" + sBztdCd + "'");//표준금액 대상찾기
			var sBztmAm = nexacro.toNumber(this.gfn_Nvl(this.ds_HrBztm.getColumn(iFvFindRow, "BZTM_AM"),0));  //표준금액

			//식사비에 동행인수 곱(2022-05-09 추가요청), 숙박비 추가(2022-05-16)
			var sBzthNo = this.ds_Detail10.getColumn(e.row, "BZTH_NO");	//출장번호
			var sWofhSq = this.ds_Detail10.getColumn(e.row, "WOFH_SQ");	//결재차수
			var sBztbSe = this.ds_Detail10.getColumn(e.row, "BZTB_SE");	//순번
			if (this.gfn_IsNull(sBzthNo)) sBzthNo = this.fvComPrgmId;	//출장번호가 null일 경우

			var iTrvlCt = this.ds_Detail15.getCaseCount("BZTH_NO == '"+sBzthNo+"' && WOFH_SQ == '"+sWofhSq+"' && BZTB_SE == '"+sBztbSe+"' && crudType != 'D'"); //동행인수
			iTrvlCt = iTrvlCt + 1;	//동행인 + 본인
			
			//1-2. 합계금액 구하기(입금금액, 한계금액)			
			var iMonyAmSum = iCashAm + iPscaAm; //합계금액
			var iLimtAmSum = sBztmAm * iBztbQt * iTrvlCt; //한계금액 = 표준금액 * 수량 * 동행인 

			//1-3. 입력금액 > 한계금액 큰 경우 메시지 후 원복
			if (iMonyAmSum > iLimtAmSum) {	//입력금액 > 한계금액 조건
				if (sBztdCd == '2000') {	    // 숙박비-숙박시설
					var sMsgItm0 = "숙박비";
				} else if (sBztdCd == '3000') {	// 식사비-출장식비
					var sMsgItm0 = "식사비";		
				}

				if (crudType == "I") {
					this.ds_Detail10.setColumn(iNowRow, e.columnid, "0"); //최초 입력시 0으로 변경
				} else {
					this.ds_Detail10.setColumn(iNowRow, e.columnid, this.ds_Detail10.getOrgColumn(iNowRow, e.columnid)); //이전값
				}
				
				var arrMsgArg = [sMsgItm0, this.gfn_AppendComma(iLimtAmSum,0)];	//메시지내용 (항목제목, 한계금액)
				this.gfn_Alert("HR010", arrMsgArg); //"항목제목" 한도액은 "한계금액"원 입니다
				return false;
			}

		}

		//2. Master 출장비 금액 계산		
		var iCashAmSum = this.ds_Detail10.getCaseSum("crudType!='D'", "CASH_AM");  //현금
		var iPscaAmSum = this.ds_Detail10.getCaseSum("crudType!='D'", "PSCA_AM");  //개인카드금액
		
		//출장비 = 현금 + 개인카드금액
		var iBzthAm = iCashAmSum + iPscaAmSum;

		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BZTH_AM",iBzthAm);	//출장비
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Detail10] 데이터변경전 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_cancolumnchange = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//[월마감제어-등록/수정]
	if (e.columnid == "BZTB_DD") {
	
		var sChkOrgDate = this.ds_Detail10.getOrgColumn(this.ds_Detail10.rowposition, "BZTB_DD");//변경전값		
		var sBUDG_MM = "";//마감년월

		//기등록된 날짜가 있을경우
		if (!this.gfn_IsNull(sChkOrgDate) && this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "crudType") != "I") {
			sBUDG_MM = sChkOrgDate;//마감년월
			
			//마감여부 체크
			var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
			var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
			paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
			paramObj.MODM_CD = "HR";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
			paramObj.ACUM_CD = "";//[선택] 회계단위
			paramObj.BUSM_CD  = "";//[선택] 사업부문
			paramObj.SAUM_CD  = "";//[선택] 사업장코드
			paramObj.DEPM_CD  = "";//[선택] 부서코드
			paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
			paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
			
			var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		

			//월마감된 경우
			if (bCheck) {
				this.gfn_Alert("CA025");//해당월의 정보가 마감이 되어서 등록/수정 할 수 없습니다.
				
				var sOrgDate = this.ds_Detail10.getOrgColumn(this.ds_Detail10.rowposition, "BZTB_DD");//변경전값
				
				//변경전값이 없을경우
				if (this.gfn_IsNull(sOrgDate)) {				
					this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "BZTB_DD", "");//입력날짜가 마감된경우 오늘날짜로 세팅
				} else {
					this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "BZTB_DD", sOrgDate);//입력날짜가 마감된경우 변경전 값으로 세팅
				}
				
				return false;
			}		
		}

		sBUDG_MM = this.gfn_Nvl(e.newvalue, "");//마감년월
		
		//마감여부 체크
		var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
		var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
		paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
		paramObj.MODM_CD = "HR";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
		paramObj.ACUM_CD = "";//[선택] 회계단위
		paramObj.BUSM_CD  = "";//[선택] 사업부문
		paramObj.SAUM_CD  = "";//[선택] 사업장코드
		paramObj.DEPM_CD  = "";//[선택] 부서코드
		paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
		paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
		
		var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		

		//월마감된 경우
		if (bCheck) {
			this.gfn_Alert("CA025");//해당월의 정보가 마감이 되어서 등록/수정 할 수 없습니다.
			
			var sOrgDate = this.ds_Detail10.getOrgColumn(this.ds_Detail10.rowposition, "BZTB_DD");//변경전값
			
			//변경전값이 없을경우
			if (this.gfn_IsNull(sOrgDate)) {				
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "BZTB_DD", "");//입력날짜가 마감된경우 오늘날짜로 세팅
			} else {
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "BZTB_DD", sOrgDate);//입력날짜가 마감된경우 변경전 값으로 세팅
			}
			
			return false;
		} else {
			return true;
		}

	}		
};

/***********************************************************************************************
 * 02.DG0.08. [Detail Tab] 변경 이벤트
 ***********************************************************************************************/
this.div_DetailInput_tab_Detail_onchanged = function(obj:nexacro.Tab,e:nexacro.TabIndexChangeEventInfo)
{
	var iTabIndex = e.postindex;	
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);

	var sBzthGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_GU");  //출장구분 (국내/시내)
	
	//Tab-출장원관리(국내)상세
	if (iTabIndex == 0) {
		this.fn_SetGridInputButton();//[공통] 그리드 입력 버튼 설정
		if (sBzthGu == '1') {	//국내
			this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);			
		}		

	//Tab-자동전표상세
	} else if (iTabIndex == 1) {
		this.fn_SetGridAtobButton();//[공통] 자동전표상세 Div Url 및 입력 버튼 설정, 보이게	
	//Tab-첨부파일
	} else if (iTabIndex == 2) {	
