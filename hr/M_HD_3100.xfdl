/***********************************************************************************************
 * 13.03. [메세지] CallBack 함수 처리
 ***********************************************************************************************/  
this.fn_MsgCallback = function (strId, strVal)
{
	//삭제여부
	if (strId == "CONF.DELETE") {
		if(strVal )
		{
			this.fn_SvcDelete();//삭제 서비스 호출
		}
	//MASTER 변경사항 저장여부
	} else if (strId == "CONF.MAST_CHANGE") {
		if(strVal )
		{
			this.fn_ComBtnSave();//저장 서비스 호출
		} else {
		
			//데이터 변경여부 체크
			if (this.gfn_DsIsUpdated(this.ds_Master10)) {
				this.fvCanRowPosChange = true;	//canrowposchange 이벤트(Row 이동시 oncolumnchanged 이벤트 처리중지)
				this.gfn_UndoColumn(this.div_MasterView.form.grd_Master10);
				this.fvCanRowPosChange = false;	//canrowposchange 이벤트
			}		
			
			if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
				this.ds_Detail10.reset();
				for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
				
					this.ds_Detail10.setColumn(iLoop, "crudType", "");
				}
			
				//this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);
			}
			
			if (this.gfn_DsIsUpdated(this.ds_Detail11)) {
				this.ds_Detail11.reset();
				for (var iLoop=0;iLoop<this.ds_Detail11.getRowCountNF();iLoop++) {
					this.ds_Detail11.setColumnNF(iLoop, "crudType", "");
				}
			
				//this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail11);
			}				

			if (this.gfn_DsIsUpdated(this.ds_Detail15)) {
				this.ds_Detail15.reset();
				for (var iLoop=0;iLoop<this.ds_Detail15.getRowCountNF();iLoop++) {
					this.ds_Detail15.setColumnNF(iLoop, "crudType", "");
				}			
			}				

		}

	//출장기간(시작) 초기화
	} else if (strId == "CONF.FROM_RESET") {
		this.ds_Master10.set_enableevent(false); //row 생성속도 향상을 위한 이벤트 false
		if(strVal) {		
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", "");			
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "NGHT_QT", "");	//박
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DAYS_QT", "");	//일		
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BZTH_AM", "");	//출장비초기화

			//예산년월, 출장기간(시작) 가져오기
			var sBudgMm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BUDG_MM");  
			var sLastDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD");  

			if (sBudgMm.substr(0,6) != sLastDd.substr(0,6)) {
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BUDG_MM", sLastDd.substr(0,6));   //예상년월
				this.fn_BztmMaxSearch();	//출장표준경비 조회 - 예산년월 기준으로 재조회
			}

			this.ds_Detail10.clearData()	//출장상세 초기화
		} else {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "FROM_DD")); //출장기간		
		}
		this.ds_Master10.set_enableevent(true); //row 생성속도 향상을 위한 이벤트 true

	//출장기간(종료) 초기화
	} else if (strId == "CONF.CLOS_RESET") {
		this.ds_Master10.set_enableevent(false); //row 생성속도 향상을 위한 이벤트 false
		if(strVal) {		
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", "");			
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "NGHT_QT", "");	//박
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DAYS_QT", "");	//일		
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BZTH_AM", "");	//출장비초기화

			//예산년월, 최종출장기간 가져오기
			var sBudgMm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BUDG_MM");  
			var sLastDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD");  

			if (sBudgMm.substr(0,6) != sLastDd.substr(0,6)) {
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BUDG_MM", sLastDd.substr(0,6));   //예상년월
				this.fn_BztmMaxSearch();	//출장표준경비 조회 - 예산년월 기준으로 재조회
			}

			this.ds_Detail10.clearData();	//출장상세 초기화
		} else {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "CLOS_DD")); //출장기간		
		}
		this.ds_Master10.set_enableevent(true); //row 생성속도 향상을 위한 이벤트 true

	//[결재] 재기안여부
	} else if (strId == "CONF.RECIDE") {
		if(strVal) {
			this.fn_ComBtnRecideProc();//[결재] 재기안 서비스 호출
		}	
	}
}; 
 
