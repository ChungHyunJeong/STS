/***********************************************************************************************
 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "COMBO_CHANGE":
			var iRow = this.ds_Master10.rowposition;
			this.ds_Master10.setColumn(iRow, "EDCH_NO", this.ds_Master20.getColumn(0, "EDCH_NO")); //사내외교육과정
			
			if (this.ds_Master20.getRowCount() > 0) {
				this.ds_Master10.setColumn(iRow, "EDCH_GU", this.ds_Master20.getColumn(0, "EDCH_GU"));
				this.ds_Master10.setColumn(iRow, "PLAC_NM", this.ds_Master20.getColumn(0, "PLAC_NM"));
				this.ds_Master10.setColumn(iRow, "EDCH_NM", this.ds_Master20.getColumn(0, "EDCH_NM"));
				this.ds_Master10.setColumn(iRow, "ORGA_NM", this.ds_Master20.getColumn(0, "ORGA_NM"));
				this.ds_Master10.setColumn(iRow, "PERS_NM", this.ds_Master20.getColumn(0, "PERS_NM"));
				this.ds_Master10.setColumn(iRow, "FROM_DD", this.ds_Master20.getColumn(0, "FROM_DD"));
				this.ds_Master10.setColumn(iRow, "CLOS_DD", this.gfn_GetCommDate("toDay"));
				this.ds_Master10.setColumn(iRow, "FROM_HR", this.ds_Master20.getColumn(0, "FROM_HR"));
				this.ds_Master10.setColumn(iRow, "FROM_MI", this.ds_Master20.getColumn(0, "FROM_MI"));
				this.ds_Master10.setColumn(iRow, "CLOS_HR", this.ds_Master20.getColumn(0, "CLOS_HR"));
				this.ds_Master10.setColumn(iRow, "CLOS_MI", this.ds_Master20.getColumn(0, "CLOS_MI"));
				this.ds_Master10.setColumn(iRow, "EDCH_CT", this.ds_Master20.getColumn(0, "EDCH_CT")); //교육시간
				this.ds_Master10.setColumn(iRow, "EDCH_AM", this.ds_Master20.getColumn(0, "EDCH_AM")); //교육비용
				this.ds_Master10.setColumn(iRow, "EDRE_NM", this.ds_Master20.getColumn(0, "REMK_NM")); //교육내용
			} else {
				this.ds_Master10.setColumn(iRow, "EDCH_NO", null);
				this.ds_Master10.setColumn(iRow, "EDCH_GU", null);
				this.ds_Master10.setColumn(iRow, "PLAC_NM", null);
				this.ds_Master10.setColumn(iRow, "EDCH_NM", null);
				this.ds_Master10.setColumn(iRow, "ORGA_NM", null);
				this.ds_Master10.setColumn(iRow, "PERS_NM", null);
				this.ds_Master10.setColumn(iRow, "FROM_DD", null);
				this.ds_Master10.setColumn(iRow, "CLOS_DD", null);
				this.ds_Master10.setColumn(iRow, "FROM_HR", null);
				this.ds_Master10.setColumn(iRow, "FROM_MI", null);
				this.ds_Master10.setColumn(iRow, "CLOS_HR", null);
				this.ds_Master10.setColumn(iRow, "CLOS_MI", null);
				this.ds_Master10.setColumn(iRow, "EDCH_CT", null);
				this.ds_Master10.setColumn(iRow, "EDCH_AM", null);
				this.ds_Master10.setColumn(iRow, "EDRE_NM", null);
			
			}
			
			break;
		case "MST_SEARCH": //조회		
	
			if (this.ds_Master10.getRowCount() > 0) {
			
				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master10.findRowExpr("EDRH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ=='" + this.fvSchKey02 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}				
				
				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
				this.div_DetailInput.form.tab_Detail.set_enable(true);
					
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
				
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterView.form.grd_Master10);
				
			} else {
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterView.form.grd_Master10);
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			
				//입력영역 비활성화
				this.div_MasterInput.set_enable(false);
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);
				this.div_DetailInput.form.tab_Detail.set_enable(false);
					
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = false;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);					
			}
			
			//저장후 재조회여부-true
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}			
	
			break;
		case "SAVE":   //저장		

			//첨부파일 추가건이 없으면
			if (!this.fvFileDiv.fn_CheckFileAddYn()) {
				this.fvSchKeyYn = true;//저장후 재조회여부
				this.fn_ComBtnSearch();//저장후 재조회
			} else {
				this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);			
			}
			break;
		case "DELETE": //삭제		
			this.fn_ComBtnSearch();//조회
			break;
			
		case "WF_RECIDE": //재기안처리
			if (nApp.gds_WfRst.getRowCount() > 0) {
				if (nApp.gds_WfRst.getColumn(0, "O_RET_CODE") == "TRUE") {
					this.gfn_Alert("WF012");//재기안 처리가 완료되었습니다.
					this.fvSchKeyYn = true;//재조회여부
					this.fn_ComBtnSearch();//재기안 재조회
				} else {
					var arrMsgArg = [nApp.gds_WfRst.getColumn(0, "O_RET_CMSG")];
					this.gfn_Alert("WF013", arrMsgArg);//재기안 처리시 오류가 발생하였습니다.\n&ITM0				
				}
			} else {
				var arrMsgArg = [""];
				this.gfn_Alert("WF013", arrMsgArg);//재기안 처리시 오류가 발생하였습니다.\n&ITM0
			}
			break;
		case "POPUP_DATA":
			this.ds_Master10.setColumn(this.ds_Master10.rowposition,"EDRE_NM",this.ds_Master30.getColumn(0, "REMK_NM"));
			break;
		case "EDSH_SEARCH": //교육실적등록
			
			break;
	}
};

