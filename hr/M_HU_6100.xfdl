this.ds_Master10_cancolumnchange = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//교육시간
	if(e.columnid == "EDCH_CT"){
		var sEdchCt = e.newvalue;
		
		if(!(sEdchCt >= 0 && sEdchCt <= 100)){
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EDCH_CT", e.oldvalue);
			return false;
		}
	}
	
	return true;  
}
