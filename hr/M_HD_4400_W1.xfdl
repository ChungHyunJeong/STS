/***********************************************************************************************
 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
	
			if (this.ds_Master10.getRowCount() > 0) {

				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master10.findRowExpr("RPTH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ=='" + this.fvSchKey02 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}				
				
				//입력영역 활성화
				this.div_MasterInput.set_enable(true);

				
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	


				this.fvInserMode = false; //입력상태여부 : 조회

			} else {
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			
				//입력영역 비활성화
				this.div_MasterInput.set_enable(false);
					
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = false;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);					
				
			}		
		
			break;
		case "SAVE":   //저장		

			//첨부파일 추가건이 없으면
			if (!this.fvFileDiv.fn_CheckFileAddYn()) {
				this.fvSchKeyYn = true;//저장후 재조회여부
				this.fn_ComBtnSearch();//저장후 재조회
			} else {
				this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);			
			}
			break;	
		case "DTL_SEARCH": //조회		
			//첨부파일 정보 조회
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
			oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RPTH_NO");//PK번호2-계약번호
			oFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
			
			this.fvFileDiv.fn_AtchFileSearch(oFileInfo);			

			//저장후 재조회여부-true 
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}	
			
			this.fvInserMode = false; //입력상태여부 : 조회
			
			//웹컴포넌트 로드 완료된 상태이면
			if (this.fvWebCompLoadYn) {
				if (this.ds_Detail10.getRowCount() > 0) {
					//내용 Web컴포넌트에 값 세팅
					var oWeb1 = this.div_MasterInput.form.tab_Master.tpg_02.form.web_RP01_NM.getProperty("window");
					var sValue1 = this.ds_Detail10.getColumn(0, "RP01_NM");
					oWeb1.callMethod("fn_setData", this.gfn_ReplaceWebEditorData(sValue1));
					
					var oWeb2 = this.div_MasterInput.form.tab_Master.tpg_03.form.web_RP02_NM.getProperty("window");
					var sValue2 = this.ds_Detail10.getColumn(0, "RP02_NM");
					oWeb2.callMethod("fn_setData", this.gfn_ReplaceWebEditorData(sValue2));
					
					var oWeb3 = this.div_MasterInput.form.tab_Master.tpg_04.form.web_RP03_NM.getProperty("window");
					var sValue3 = this.ds_Detail10.getColumn(0, "RP03_NM");
					oWeb3.callMethod("fn_setData", this.gfn_ReplaceWebEditorData(sValue3));
					
					var oWeb4 = this.div_MasterInput.form.tab_Master.tpg_05.form.web_RP04_NM.getProperty("window");
					var sValue4 = this.ds_Detail10.getColumn(0, "RP04_NM");
					oWeb4.callMethod("fn_setData", this.gfn_ReplaceWebEditorData(sValue4));						
				}			
			}
			
			break;	
		case "COMBO_SEARCH": 
			//결재함 화면에 결재번호가 있는 경우 조회 호출
			if (!this.gfn_IsNull(this.parent.parent.fvNowWofhNo)) {
				this.fn_ComBtnSearch(this.parent.parent.fvNowWofhNo); 
			}

			break;					
	}
};


