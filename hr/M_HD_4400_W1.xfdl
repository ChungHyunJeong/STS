this.fvFormComBtnAuthYn = false;//그리드 공통버튼 권한 유무
this.fvFileDiv = this.div_MasterInput.form.tab_Master.tpg_06.form.div_CommFile.form;//[첨부파일]Div
this.fvPathNm = "HR";//[첨부파일]업무파일경로
this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
this.fvSchKeyYn = false;//저장후 재조회여부
this.fvSchKey01 = "";//조회Key01
this.fvSchKey02 = "";//조회Key02
this.fvSchKey03 = "";//조회Key03
this.fvInserMode = true; //입력상태여부 (입력상태-true : 최초저장, 입력상태-false : 기저장)

var fvFileInfo = new Object();//[첨부파일]정보
this.fvComWofhNo = "";//[결재]결재PID

this.fvWebCompLoadYn = false;//웹컴포넌트 로드 완료여부

/***********************************************************************************************
 * 00.03. Form Onload 이벤트
 ***********************************************************************************************/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{ 
	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 
}

/***********************************************************************************************
 * 00.04. Form [공통] 초기설정 함수 - 필수
 ***********************************************************************************************/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this, false);//[공통] 화면로드시 처리 함수(다국어등)
	this.fvComPrgmId = this.gfn_CommLoadFormPrgmId(this);//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID

	this.fn_SetTabComAttachFile();//[공통] 첨부파일 Tab 설정
		 
}

/***********************************************************************************************
 * 00.05. Form [업무] 초기설정 함수
 ***********************************************************************************************/
this.fn_FormBizInit = function() 
{ 

	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	var objCondList = ["WF110|ds_Wf110"];
	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, "");

	//[업무] 내용 Web컴포넌트 Url설정
	this.div_MasterInput.form.tab_Master.tpg_02.form.web_RP01_NM.set_url(this.gfn_GetServerUrl("View"));//웹에디터 Url 설정
	this.div_MasterInput.form.tab_Master.tpg_03.form.web_RP02_NM.set_url(this.gfn_GetServerUrl("View"));//웹에디터 Url 설정
	this.div_MasterInput.form.tab_Master.tpg_04.form.web_RP03_NM.set_url(this.gfn_GetServerUrl("View"));//웹에디터 Url 설정
	this.div_MasterInput.form.tab_Master.tpg_05.form.web_RP04_NM.set_url(this.gfn_GetServerUrl("View"));//웹에디터 Url 설정
		
	//[업무] 일반콤보조회
	this.fn_ComboSearch();
	
	//결재함 화면에 결재번호가 있는 경우 조회 호출 - 일반콤보조회 가 없는경우에는 이쪽에 조회 로직을 넣어야함., 일반콤보가 있으면 일반콤보및 기타 조회 마지막 콜백에서 넣어야 함.
	/*
	if (!this.gfn_IsNull(this.parent.parent.fvNowWofhNo)) {
		this.fn_ComBtnSearch(this.parent.parent.fvNowWofhNo); 
	}
	*/
	
