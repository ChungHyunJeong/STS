this.ds_Detail10_cancolumnchange = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//평가점수
	if(e.columnid == "EDSB_CT"){
		var sEdsbCt = e.newvalue;
		
		if(!(sEdsbCt >= 0 && sEdsbCt <= 10)){
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "EDSB_CT", e.oldvalue);
			return false;
		}
	}
	
	return true;  
}
  
  
