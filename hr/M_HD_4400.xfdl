this.fn_VaildMasterSave = function()
{
	//[저장] Validation 설정
	var arrArgs = ["this.div_MasterInput.form.tab_Master.tpg_01.form.cal_RPTH_DD^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_RPTH_DD.text + "|NULL"
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_DEPM_CD.text + "|NULL"
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SystId^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_EmpmCd.text + "|NULL"
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_MethNo^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_METH_NO.text + "|NULL"
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cal_RPTH_MM^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_RPTH_MM.text + "|NULL"
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_RPTH_CT^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_RPTH_CT.text + "|NULL"
			      ]; 

	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	//Master 입력 상태이면
	if (sChkRowType != "I") {
		arrArgs.push("this.div_MasterInput.form.tab_Master.tpg_01.form.mae_RPTH_NO^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_RPTH_NO.text + "|NULL");
	}
	var sCheckGroupTitle = this.div_MasterInput.form.tab_Master.tpg_01.text;//체크그룹 타이틀

	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
		return false;
	}

	//중복키 확인
	if (sChkRowType == "I" && !this.fn_DupValid()){
		this.ds_Master10.setColumn(this.ds_Master10.rowposition,"RPTH_NO","");
		return false;
	}
	
	return true;
