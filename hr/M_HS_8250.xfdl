			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "hr:M_HS_8250_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			svcAddObj.sSelectSql = "hr:M_HS_8250_D_SL20";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			svcAddObj.sSelectSql = "hr:M_HS_8250_D_SL30";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			svcAddObj.sSelectSql = "hr:M_HS_8250_D_SL40";  //수당조회(출력물용)
			this.gfn_AddComSearchService(this, svcAddObj);
			
