 ***********************************************************************************************/
/***********************************************************************************************
 * 08.01. [공통버튼] 인쇄 버튼 클릭 이벤트
 ***********************************************************************************************/  
this.fn_ComBtnPrint = function()
{ 
	var iSelRow = this.ds_Master10.rowposition;//선택한Row

	if (this.ds_Master10.getRowCount() > 0 && iSelRow > -1) {

		var sReportFile = "M_HS_8250_01";//레포트파일ID

		//마스터출력할 Row 복사
		this.ds_Master10Rt.clearData();
		this.ds_Master10Rt.addRow();
		this.ds_Master10Rt.copyRow(0, this.ds_Master10, this.ds_Master10.rowposition);
		this.ds_Master10Rt.setColumn(0, "POSI_NM", this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_PosiCd.text ) //직위명
		this.ds_Master10Rt.setColumn(0, "BANK_NM", this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SabaCd.text ) //은행명
		this.ds_Master10Rt.setColumn(0, "REAL_NM", this.div_MasterInput.form.tab_Master.tpg_01.form.mae_RealAm.text ) //실지급액

		var vPaboGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PABO_GU")	
		var nRow 	= this.ds_Hr807.findRow( "CODM_CD", vPaboGu);
		var sCodmNm = this.ds_Hr807.getColumn(nRow, "CODM_NM"); //급상여구분		
		this.ds_Master10Rt.setColumn(0, "PABO_GU", sCodmNm) ;//급상여구분		
		
		//지급월          
        var vPayMon = this.div_MasterInput.form.tab_Master.tpg_01.form.mae_MpamMm.text 
			  vPayMon = vPayMon.substr(0, 4) +'년  ' + vPayMon.substr(5, 2)+ '월'	
		    //vPayMon = vPayMon.substr(5, 2)
		this.ds_Master10Rt.setColumn(0, "PAY_MON", vPayMon) ;//지급월	

		var dsObj = new Object();//출력 데이터셋 정보
		dsObj.ds_Master10 = this.ds_Master10Rt;
		dsObj.ds_Detail10 = this.ds_Detail10;
		dsObj.ds_Detail20 = this.ds_Detail20;
		dsObj.ds_Detail40 = this.ds_Detail40;
		dsObj.ds_Detail50 = this.ds_Detail50;
		
		var paObj = new Object();//출력 파라메타 정보	
		paObj.PRNT_NM = this.gfn_GetSessionValue("USEM_NM");//출력자
		paObj.LOGO_UL = this.gfn_GetReportImageUrl("LOGO_UL_01");//이미지-로고
		paObj.SEAL_UL = this.gfn_GetReportImageUrl("SIGN_UL_01");//회사직인
		   
		var oArg = {ProjectFolder:"HR"        //레포트저장 폴더
				  , ReportFile:sReportFile //레포트파일명
				  , ReportDs:dsObj            //레포트파라메타 - 데이터셋
				  , ReportParam:paObj         //레포트파라메타 - 변수
				   };                         //Popup화면 전달 파라메터
				   
		this.gfn_ReportPop("REPORT", oArg, "fn_PopupCallback");//레포트 팝업 호출	
			
	} else {
		this.gfn_Alert("SM083");//출력할 정보가 없습니다.
	}
}

