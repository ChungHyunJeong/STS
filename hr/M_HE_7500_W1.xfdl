this.ds_Detail10_cancolumnchange = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	var sPlabGu = obj.getColumn(e.row, "PLAB_GU");	// 구분
	
	if(e.columnid=="EVEX_CT"){	
		if(sPlabGu=="0020"){
			var nWeigCt = obj.getColumn(e.row, "WEIG_CT");  //비중
			var nEvexCt = e.newvalue;
			
			nWeigCt 	= nexacro.toNumber(nWeigCt);
			nEvexCt 	= nexacro.toNumber(nEvexCt);
			
			if(this.gfn_IsNull(nWeigCt)){nWeigCt=0;}
			if(this.gfn_IsNull(nEvexCt)){nEvexCt=0;}
			
			if(nWeigCt < nEvexCt){
				var arrMsgArg = ["해당 항목은 성과목표 비중이상으로 등록할 수"];
				this.gfn_Alert("MM046", arrMsgArg); 
				return false;
			}
		}
	}	
};
