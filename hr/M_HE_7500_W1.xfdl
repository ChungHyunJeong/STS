this.fn_VaildDetailSave = function()
{
	//[저장] Validation 설정
	var arrArgs = [
					"EVEX_CT^NULL"
				];
 	var sFilter = [
 				  ]; 			  
 	var sRowType = "A";			  
 	var sCheckGroupTitle = this.div_MasterInput.form.tab_Master.tpg_02.text;//체크그룹 타이틀
 				  
 	//Grid Vaild 체크
 	if (!this.gfn_CheckGridValid(this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10, arrArgs, sCheckGroupTitle, sFilter, sRowType) ) {
 		return false;
 	}
	
	/*
	if(this.ds_Detail10.getSum("WEIG_CT") > 100){
		var arrMsgArg = ["비중","100"];
		this.gfn_Alert("SM056", arrMsgArg); //[&ITM0]이(가) [&ITM1]을(를) 초과할 수 없습니다.
		return;
	}
	*/
	
	for(var iLoop=0; iLoop < this.ds_Detail10.rowcount; iLoop++){
		sEvfiNm = this.ds_Detail10.getColumn(iLoop, "EVFI_NM");
		nPlabGu = this.ds_Detail10.getColumn(iLoop, "PLAB_GU");
		nEvexRt = this.ds_Detail10.getColumn(iLoop, "EVEX_RT");		
		iLoop_1 = iLoop + 1
		if (nEvexRt > 9999){
			this.ds_Detail10.set_rowposition(iLoop);
			var arrMsgArg = ["업적평가탭 "+iLoop_1+"행 달성율","9999"];	
			this.gfn_Alert("SM056", arrMsgArg); //[&ITM0]이(가) [&ITM1]을(를) 초과할 수 없습니다.	
			return false;
		}
		
		if(this.gfn_IsNull(sEvfiNm) && nPlabGu == "0010" && this.ds_Detail10.getRowType(iLoop) != 16){
			this.ds_Detail10.set_rowposition(iLoop);
			var arrMsgArg = ["업적평가탭 "+iLoop_1+"행 최종평가의견"];				
			this.gfn_Alert("SM008", arrMsgArg); 
			return false;	
		}	
	}
	
	return true;
}
