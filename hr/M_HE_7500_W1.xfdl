/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	if(e.columnid=="EVEX_CT"){
		var sScopCd = obj.getColumn(e.row, "SCOP_CD");
		var nPlabCt = obj.getColumn(e.row, "PLAB_CT");	//목표 
		var nEvexCt = e.newvalue; // obj.getColumn(e.row, "EVEX_CT");
		var nRet;
		var nWeigCt; //비중
		var nEvwiCt; //가중치
		var sEvgdCd; //등급
		var nEvpoCt; //평가점수
		
		nWeigCt  = obj.getColumn(e.row, "WEIG_CT");
		nPlabGu  = obj.getColumn(e.row, "PLAB_GU");
		
		if(sScopCd == "0020" || sScopCd == "0030" || sScopCd == "0050"){
			nRet =  (nPlabCt + (nPlabCt - nEvexCt)) / nPlabCt * 100;
			 nRet = nexacro.toNumber(nRet);
			 
			 if(this.gfn_IsNull(nRet)){nRet = 0;}
			 
			 //--목표가 0이면서 실적체크
			 if(nPlabCt == 0){
				if(nEvexCt<=0){						
					obj.setColumn(e.row, "EVWI_CT",  1);
					nRet = 100;
				}else if(nEvexCt==1){				
					obj.setColumn(e.row, "EVWI_CT",  0.9);
					nRet = 90;
				}else if(nEvexCt>=2){				
					obj.setColumn(e.row, "EVWI_CT",  0.8);					
					nRet = 80;						
				}			
			 }	 
			 
			 obj.setColumn(e.row, "EVEX_RT",  nRet.toFixed(8));	 			 
			 
		}else{
			if(this.gfn_IsNull(sScopCd) || sScopCd == ""){
				nRet = nEvexCt / nWeigCt * 100;				
			}else{		
				 nRet = nEvexCt / nPlabCt * 100;				 
			}
			
       		//if (nRet >= 120){
			//	nRet = 120
			//} else if (nRet < 80){
			//	nRet = 70
       		//}
			if(nPlabGu == "0010"){
				obj.setColumn(e.row, "EVEX_RT",  nRet.toFixed(8));			 
			}
		}
		//--가중치
		if(nRet > 110){
			nEvwiCt = 1.2;
			sEvgdCd = "S";
		}else if(nRet >= 100 &&  nRet < 110){
			nEvwiCt = 1;
			sEvgdCd = "A";
		}else if(nRet >=  90 &&  nRet < 100){
			nEvwiCt = 0.9;
			sEvgdCd = "B";
		}else if(nRet >=  80 &&  nRet <  90){
			nEvwiCt = 0.8;
			sEvgdCd = "C";
		}else if(nRet <  80){
			nEvwiCt = 0.7;
			sEvgdCd = "D";
		}
		
		if(nPlabGu == "0010"){
			obj.setColumn(e.row, "EVWI_CT",  nEvwiCt);
			obj.setColumn(e.row, "EVGD_CD",  sEvgdCd); //등급
		}
		
		//--평가점수 = (비중 * 달성율 / 100) * 가중치		
		//nEvpoCt = nexacro.round((nWeigCt * nRet) / 100. * nEvwiCt, 2);		
		//--평가점수 = 비중 * 가중치		
		if(nPlabGu == "0010"){
			nEvpoCt = nexacro.round(nWeigCt * nEvwiCt, 2);	
		} else {
			nEvpoCt = nEvexCt;
		}
		obj.setColumn(e.row, "EVPO_CT", nEvpoCt);		

	}
};

