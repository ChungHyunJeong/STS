/***********************************************************************************************
 * 00.04. Form [공통] 초기설정 함수 - 필수
 ***********************************************************************************************/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this, false);//[공통] 화면로드시 처리 함수(다국어등)
	this.fvComPrgmId = "M_HE_7500";//[필수]프로그램ID-화면로드시 메뉴정보 기준으로 재설정됨.
