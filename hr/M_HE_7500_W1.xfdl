/***********************************************************************************************
 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "COMBO_SEARCH": //조회		
			this.ds_Grcd.filter("GRCM_CD == '8010'");
			this.ds_PlabGu.copyData(this.ds_Grcd, true);	//성과목표_구분
			
			this.ds_Grcd.filter("GRCM_CD == '8020'");
			this.ds_PlabCd.copyData(this.ds_Grcd, true);	//성과목표_분류
			this.ds_Grcd.filter("GRCM_CD == '8030'");
			this.ds_UnitCd.copyData(this.ds_Grcd, true);	//성과목표_단위
			this.ds_Grcd.filter("GRCM_CD == '8040'");
			this.ds_ScopCd.copyData(this.ds_Grcd, true);	//성과목표_범위
			
			this.ds_Grcd.filter("GRCM_CD == '1000'");
			this.ds_CapmGu.copyData(this.ds_Grcd, true);	//역량구분
			
			//this.ds_Grcd.filter("GRCM_CD == '2000'");
			//this.ds_CapmCd.copyData(this.ds_Grcd, true);	//역량항목
		case "MST10_SEARCH": //조회		
			this.ds_Master10.addColumn("crudType", "string");
			if (this.ds_Master10.getRowCount() > 0) {

				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master10.findRowExpr("PLRH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ=='" + this.fvSchKey02 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
					
					//한건일경우 체인지 이벤트가 발생하지 않아 강제로 포지션변경하여 이벤트 실행
					this.ds_Master10.set_rowposition(-1);
					this.ds_Master10.set_rowposition(0);
				}				
				
				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
				
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	

				this.fvInserMode = false; //입력상태여부 : 조회
				
				
					//--------------------------------------------
			
				this.ds_Input.clearData();
				this.ds_Input.addRow();		
				this.ds_Wf110Chk.clearData();

				this.ds_Input.setColumn(0,	"WOFH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_NO"));
				this.ds_Input.setColumn(0,	"SYST_ID", this.gfn_GetSessionValue("SYST_ID"));
				
				//[공통]서비스정보 초기화
				this.gfn_InitComService(this);
				
				//[공통]서비스정보 추가
				var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
				svcAddObj.sSelectSql = "hr:M_HE_7500_M_SL15";
				this.gfn_AddComSearchService(this, svcAddObj);
				
				//[공통]서비스 호출 변수
				var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
				svcCallObj.sTrid = "MST_SEARCH_EXISTS";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
				svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
				svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
				svcCallObj.sOutDatset = "ds_Wf110Chk=ds_Wf110Chk";//출력 데이터셋
				svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
				svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
				svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
				svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID		
				svcCallObj.isSync = true;//동기식으로 설정할지 여부

				//[공통]서비스 호출
				this.gfn_CallComService(this, svcCallObj);
				
				this.fvWf110Chk = this.ds_Wf110Chk.getColumn(0, "WFGU_CD");				
				
				if(this.ds_Wf110Chk.rowcount < 1){
					this.fvWf110Chk = "N";
				}			
				
				
			} else {
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			
				//입력영역 비활성화
				this.div_MasterInput.set_enable(false);
					
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = false;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);					
	
			}		
		
			break;
		case "DTL10_SEARCH": //조회
			this.ds_Detail10.addColumn("s_chk", "string");
			this.ds_Detail10.addColumn("crudType", "string");
			this.ds_Detail20.addColumn("s_chk", "string");
			this.ds_Detail20.addColumn("crudType", "string");
			
			//첨부파일 정보 조회
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
			oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PLRH_NO");//PK번호2-관리번호
			oFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수						
			
			this.fvFileDiv.fn_AtchFileSearch(oFileInfo);
			
			break;
		case "SAVE":   //저장		

			//첨부파일 추가건이 없으면
			if (!this.fvFileDiv.fn_CheckFileAddYn()) {
				this.fvSchKeyYn = true;//저장후 재조회여부
				this.fn_ComBtnSearch(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_NO"));//저장후 재조회용 결재번호
			} else {
				this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);			
			}
			break;
	}
};


