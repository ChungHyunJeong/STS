/***********************************************************************************************
 * 15.08. [동기화] 최초 저장시 출장비상세 Detail12 -> Detail11
 ***********************************************************************************************/ 
this.fn_SetDetail11Copy = function()
{
	this.ds_Detail11.clearData();
		
	this.ds_Detail11.set_enableevent(false);
	for (var iLoop=0; iLoop<this.ds_Detail12.getRowCount(); iLoop++) {
		var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail11);//crud_flag 변경-입력(I)			
		this.ds_Detail11.setColumn(iAddRow, "COMM_CD", this.ds_Detail12.getColumn(iLoop, "COMM_CD"));
		this.ds_Detail11.setColumn(iAddRow, "BZTH_NO", this.ds_Detail12.getColumn(iLoop, "BZTH_NO"));
		this.ds_Detail11.setColumn(iAddRow, "BZUB_SE", this.ds_Detail12.getColumn(iLoop, "BZTC_SE"));
		this.ds_Detail11.setColumn(iAddRow, "BZTM_CD", this.ds_Detail12.getColumn(iLoop, "BZTM_CD"));
		this.ds_Detail11.setColumn(iAddRow, "BZTD_CD", this.ds_Detail12.getColumn(iLoop, "BZTD_CD"));
		this.ds_Detail11.setColumn(iAddRow, "BZTB_QT", this.ds_Detail12.getColumn(iLoop, "BZTB_QT"));
		this.ds_Detail11.setColumn(iAddRow, "BZTB_NM", this.ds_Detail12.getColumn(iLoop, "BZTB_NM"));
		this.ds_Detail11.setColumn(iAddRow, "CURR_CD", this.ds_Detail12.getColumn(iLoop, "CURR_CD"));
		this.ds_Detail11.setColumn(iAddRow, "EXCH_RT", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT")); //보고최초 조회시 보고시환율로 적용(20220928)
		this.ds_Detail11.setColumn(iAddRow, "BZTC_AM", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT") * this.ds_Detail12.getColumn(iLoop, "FORE_AM") );  //보고최초 조회시 보고시환율로 적용(20220928)
		//is.ds_Detail11.setColumn(iAddRow, "EXCH_RT", this.ds_Detail12.getColumn(iLoop, "EXCH_RT"));   //기안시 환율적용됨
		//this.ds_Detail11.setColumn(iAddRow, "BZTC_AM", this.ds_Detail12.getColumn(iLoop, "BZTC_AM")); //기안시 환율적용됨
		this.ds_Detail11.setColumn(iAddRow, "FORE_AM", this.ds_Detail12.getColumn(iLoop, "FORE_AM"));
		this.ds_Detail11.setColumn(iAddRow, "BZTM_AM", this.ds_Detail12.getColumn(iLoop, "BZTM_AM"));
		this.ds_Detail11.setColumn(iAddRow, "REMK_NM", this.ds_Detail12.getColumn(iLoop, "REMK_NM"));						
	}
	
	//보고최초조회시 보고시환율로 적용한 금액 마스터에 적용 //보고최초 조회시 보고시환율로 적용(20220928)
	this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BZUH_AM", this.ds_Detail11.getSum("BZTC_AM"));
	
	
	this.ds_Detail11.set_enableevent(true);
	
}
