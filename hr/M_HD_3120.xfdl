/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {
		//[결재] 결재상태에 따른 컴포넌트 제어			
		this.fn_SetWfCompEnable();	

		//출장번호정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO"))) {

			//내용 Web컴포넌트에 값 세팅
			var oWeb = this.div_DetailInput.form.tab_Detail.tpg_02.form.web_ContNm.getProperty("window");
			var sContNm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZUH_NM");//본문내용
			oWeb.callMethod("fn_setData", this.gfn_ReplaceWebEditorData(sContNm));			


			var sRgstYn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RGST_YN");	//등록여부
			
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();  //출장일정상세
			this.ds_Detail11.clearData();  //출장비상세
			this.ds_Detail12.clearData();  //출장기안시 산출내역
			this.ds_Detail15.clearData();  //동행인
			this.ds_Detail13.clearData();  //출장비상세 집계 보고(레포트 출력용) //add
			this.ds_Input.clearData();

			var sSqlId = "";
			var sOutDatset = "";

			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "BZTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO"));
			this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ")); //보고시(등록여부 = 1)
			this.ds_Input.setColumn(0, "BZTH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_SQ"));	//기안시(등록여부 = 0)
			this.ds_Input.setColumn(0, "PRGM_ID", this.fvComPrgmId);

			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "hr:M_HD_3120_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);

			if (sRgstYn == '1') { //등록시 (등록여부 = 1)
				svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
				svcAddObj.sSelectSql = "hr:M_HD_3120_D_SL11";	//보고 테이블
				this.gfn_AddComSearchService(this, svcAddObj);
			}

			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "hr:M_HD_3120_D_SL12";
			this.gfn_AddComSearchService(this, svcAddObj);

			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "hr:M_HD_3101_M_SL11";	//동행인관리 (기안대상 동행인)
			this.gfn_AddComSearchService(this, svcAddObj);						

			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "hr:M_HD_3120_D_SL13";
			this.gfn_AddComSearchService(this, svcAddObj);  //레포트출력용 body //add


			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			
			if (sRgstYn == '1') { //등록시 (등록여부 = 1)
				svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10 ds_Detail11=ds_Detail11 ds_Detail12=ds_Detail12 ds_Detail15=ds_Detail15 ds_Detail13=ds_Detail13 ";//출력 데이터셋 //add
				//svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10 ds_Detail11=ds_Detail11 ds_Detail12=ds_Detail12 ds_Detail15=ds_Detail15 ";//출력 데이터셋
			} else {	 //미등록시 (등록여부 = 0)
				svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10 ds_Detail12=ds_Detail12 ds_Detail15=ds_Detail15 ds_Detail13=ds_Detail13";//출력 데이터셋 //add
				//svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10 ds_Detail12=ds_Detail12 ds_Detail15=ds_Detail15";//출력 데이터셋 
			}

			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);		
		}
		
// // 		//첨부파일 버튼활성화 (20220927)   
		var iTabIndex = this.div_DetailInput.form.tab_Detail.tabindex;		 
		var sWofhSt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST");  //결재상태(20220927)
		var sRgstYn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RGST_YN");	//등록여부
		
		if (iTabIndex == 3) {	
			if ((sRgstYn == "0" && !this.fvWfSearchMode && !this.fvWofhStatus) ||( sWofhSt == "0010" )) {  //조회조건변경(20220927) : 결재상태가 등록이면 첨부파일등록 가능
			//if (sRgstYn == "0" && !this.fvWfSearchMode && !this.fvWofhStatus) {
				this.fn_SetGridFileButton();//[공통] 그리드 파일 버튼 설정
				this.div_DetailInput.form.div_ComGrdBtn.set_visible(true);
			} else {	
				this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);				
				
			}
		}
 		
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master10] Row 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무

	//Detail11은 Row변경시 등록인지 미등록인지에 체크 후 처리해야 함
	var sRgstYn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RGST_YN");	//등록여부				
	var sDtlChange = false;	//미등록 대상 Detail11 값 변경여부	
	if (sRgstYn == '0' && (this.ds_Detail11.getCount("PSCA_NO") > 0 || this.ds_Detail11.getCount("PSCA_DD") > 0)) {
		var sDtlChange = true;
	}

	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10) || (this.gfn_DsIsUpdated(this.ds_Detail11) && sRgstYn == '1') ||  sDtlChange || this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?	
		return false;
	}

	return true;
};  


