/***********************************************************************************************
 * 02.DG0.08. [Detail Tab] 변경 이벤트
 ***********************************************************************************************/
this.div_DetailInput_tab_Detail_onchanged = function(obj:nexacro.Tab,e:nexacro.TabIndexChangeEventInfo)
{
	var iTabIndex = e.postindex;
	var sRgstYn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RGST_YN");	//등록여부
	var sWofhSt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST");  //결재상태(20220927)
	
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	
	//Tab-출장원관리(해외-보고)상세, 출장보고상세
	if (iTabIndex == 0 || iTabIndex == 1) {
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);
	//Tab-자동전표상세
	} else if (iTabIndex == 2) {	
		if (sRgstYn == "1" && !this.fvWfSearchMode && !this.fvWofhStatus) {
			this.div_DetailInput.form.div_ComGrdBtn.set_visible(true);
			this.fn_SetGridAtobButton();//[공통] 자동전표상세 Div Url 및 입력 버튼 설정, 보이게
		} else  {
			this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);
		}
	//Tab-첨부파일
	} else if (iTabIndex == 3) {	
		if ((sRgstYn == "0" && !this.fvWfSearchMode && !this.fvWofhStatus) ||( sWofhSt == "0010" )) {  //조회조건변경(20220927) : 결재상태가 등록이면 첨부파일등록 가능
		//if (sRgstYn == "0" && !this.fvWfSearchMode && !this.fvWofhStatus) {
			this.fn_SetGridFileButton();//[공통] 그리드 파일 버튼 설정
			this.div_DetailInput.form.div_ComGrdBtn.set_visible(true);
		} else {	 
			this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);
			
			
		}
	}
};

