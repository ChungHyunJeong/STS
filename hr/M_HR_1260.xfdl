this.fn_div_create = function() {

	depm_div_max = this.ds_Depm.getMax("XVAL_PO") + depm_Width;
	
	for(var i=0; i<this.ds_DepmLv.rowcount; i++) {
	
		this.ds_DepmLv.setColumn(i, "H_GAP", div_heightGap*(i+1)-div_heightGap);
		var div_level = new Div("div_level" + (i+1), 0, div_heightGap*(i+1)-div_heightGap, depm_div_max, 180, null, null);
		this.div_Org.addChild(div_level.name, div_level);		
		div_level.show();
		
	}
}

this.fn_div_getGap = function(depmLv) {
	var k = this.ds_DepmLv.findRow("DEPM_LV", depmLv);
	if(k > -1) {
		return this.ds_DepmLv.getColumn(k, "H_GAP");
	}
}

// 레벨별 정보취합 ( 상단 선 긋기 )
this.fn_DepmLv = function(depmLv, gropLv, depmCd, xDepmPos, yDepmPos, xLinePos, yLinePos) {

	var k = this.ds_DepmLv.findRow("DEPM_LV", depmLv);
	
	if(k > -1) { // 해당 그룹이 이미 있을때는 합치기
		this.ds_DepmLv.setColumn(k, "DEPM_CD", this.ds_DepmLv.getColumn(k, "DEPM_CD") + "," + depmCd);
		this.ds_DepmLv.setColumn(k, "X_DEPM_PO", this.ds_DepmLv.getColumn(k, "X_DEPM_PO") + "," + xDepmPos);
		this.ds_DepmLv.setColumn(k, "Y_DEPM_PO", this.ds_DepmLv.getColumn(k, "Y_DEPM_PO") + "," + yDepmPos);
		this.ds_DepmLv.setColumn(k, "X_LINE_PO", this.ds_DepmLv.getColumn(k, "X_LINE_PO") + "," + xLinePos);
		this.ds_DepmLv.setColumn(k, "Y_LINE_PO", this.ds_DepmLv.getColumn(k, "Y_LINE_PO") + "," + yLinePos);
	}
}

