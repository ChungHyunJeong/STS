this.fn_GroupTdepLine1 = function() {
	for(var i=1; i<this.ds_GropLvTdep.rowcount; i++) {
		var TDEP_CD = this.ds_GropLvTdep.getColumn(i, "TDEP_CD");
		var split_X_Val = this.ds_GropLvTdep.getColumn(i, "X_LINE_PO").split(",");
		var split_Y_Val = this.ds_GropLvTdep.getColumn(i, "Y_LINE_PO").split(",");
		
		var line_xValPo = -1;
		var line_yValPo = -1;
		var line_Width = -1;
		var line_Height = -1;	
		
		if(split_X_Val.length > 1) {
			var startX = split_X_Val[1];
			var starty = split_Y_Val[1];
			var endL = split_X_Val[split_X_Val.length-1] - startX;
		}
		
		if(endL > 0) { // 2개 이상일때 시작점과 끝점을 연결
		
			line_xValPo = startX;
			line_yValPo = starty;
			line_Width = endL;
			line_Height = 2;
			
			var staLabelLineWb = new Static("sta_line_g" + TDEP_CD, line_xValPo, line_yValPo, line_Width, line_Height, null, null);
			staLabelLineWb.set_background(sLineColor);
			staLabelLineWb.set_textAlign("center");
			this.div_Org.addChild(staLabelLineWb.name, staLabelLineWb);		
			staLabelLineWb.show();
			
		}
	
	}
	
	
}

