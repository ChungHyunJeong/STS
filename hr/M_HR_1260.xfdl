/***********************************************************************************************
 * 1. 조회 영역
 ***********************************************************************************************/
/***********************************************************************************************
 * 01.01. [공통버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.fn_ComBtnSearch = function(sWofhNo)
{ 
	this.divOrgReset(); // 동적생성된 오브젝트 초기화
	
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Input.clearData();
	this.ds_Depm.clearData();	
	
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자 Default
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "DEDH_DD", this.gfn_Left(sToDay, 6));
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "hr:M_HR_1260_M_SL10";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid 		= "DEPM_SEARCH";			//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName 	= this.G_SV_HR;			//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset 	= "ds_Input=ds_Input" 	//입력 데이터셋
	svcCallObj.sOutDatset 	= "ds_Depm=ds_Depm";//출력 데이터셋
	svcCallObj.sOtherArg 	= "";					//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";		//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.isSync = true;//동기식으로 설정할지 여부

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
	
	var ds_DempCnt = this.ds_Depm.getRowCount();
	this.ds_DepmLv.clearData();
	this.ds_GropLv.clearData();
	this.ds_GropLvTdep.clearData();
	
	if (ds_DempCnt > 0) { // 레벨 정보 추출
		
		for(var i=0; i<ds_DempCnt; i++) {
		
			if(!this.gfn_IsNull(this.ds_Depm.getColumn(i, "EXCEPT_G"))) { // 예외처리부서 ( 팀에서 그룹으로 팀원들이 표현되어야 할때... )
				var exceptTargetIdx = this.ds_Depm.findRow("DEPM_CD", this.ds_Depm.getColumn(i, "EXCEPT_G"));
				
				this.ds_Depm.set_enableevent(false);
				
				this.ds_Depm.setColumn(i, "DEPM_WN", this.ds_Depm.getColumn(exceptTargetIdx, "DEPM_WN"));
				this.ds_Depm.setColumn(i, "DEWN_CD", this.ds_Depm.getColumn(exceptTargetIdx, "DEWN_CD"));
				this.ds_Depm.setColumn(i, "DEWN_ID", this.ds_Depm.getColumn(exceptTargetIdx, "DEWN_ID"));
				this.ds_Depm.setColumn(i, "DEWN_NM", this.ds_Depm.getColumn(exceptTargetIdx, "DEWN_NM"));
				
				this.ds_Depm.set_enableevent(true);
			}		
		
			var depmLv = this.ds_Depm.getColumn(i, "DEPM_LV");
			var k = this.ds_DepmLv.findRow("DEPM_LV", depmLv);
			
			if(k < 0) { // 해당 그룹이 없을때 만든다...
				var addR = this.ds_DepmLv.addRow();
				this.ds_DepmLv.setColumn(addR, "DEPM_LV", depmLv);
			}
			
			var depmLvTdep = this.ds_Depm.getColumn(i, "TDEP_CD");
			k = this.ds_DepmLvTdep.findRowExpr("DEPM_LV == '"+depmLv+"' && TDEP_CD == '"+depmLvTdep+"'");
			
			if(k < 0) { // 해당 그룹이 없을때 만든다...
				var addR = this.ds_DepmLvTdep.addRow();
				this.ds_DepmLvTdep.setColumn(addR, "DEPM_LV", depmLv);
				this.ds_DepmLvTdep.setColumn(addR, "TDEP_CD", depmLvTdep);
			}
			
			
			
			var gropLv = this.ds_Depm.getColumn(i, "GROP_LV");
			var k = this.ds_GropLv.findRow("GROP_LV", gropLv);
			
			if(k < 0) { // 해당 그룹이 없을때 만든다...
				var addR = this.ds_GropLv.addRow();
				this.ds_GropLv.setColumn(addR, "GROP_LV", gropLv);
			}
			
			var gropLvTdep = this.ds_Depm.getColumn(i, "TDEP_CD");
			k = this.ds_GropLvTdep.findRowExpr("GROP_LV == '"+gropLv+"' && TDEP_CD == '"+gropLvTdep+"'");
			
			if(k < 0) { // 해당 그룹이 없을때 만든다...
				var addR = this.ds_GropLvTdep.addRow();
				this.ds_GropLvTdep.setColumn(addR, "GROP_LV", gropLv);
				this.ds_GropLvTdep.setColumn(addR, "TDEP_CD", gropLvTdep);
			}				
			
		}
	}
	
	this.fn_div_create();
	
	if (ds_DempCnt > 0) {
	
		var depm_xValPo, depm_yValPo = 0;
		var line_xValPo, line_yValPo, line_Width, line_Height = 0;
		
		var sFont = 'bold 12px/normal "Malgun Gothic"';
		var sFontColor = "#FFFFFF";
		var sLv01BgColor = "#2684DC";
		var sLv02BgColor = "#3a81c4";
		var sLv03BgColor = "#4e7da9";
		var sLv04BgColor = "#4d6882";
		var sLv05BgColor = "#435567";
		var sLv06BgColor = "#394651";
		var sLv07BgColor = "#3d4348";
		var sLv03BgColor = "#809BB0";
		var orgh_image = "theme::default/images/ORGH_TL.png";
		var orgh_image2 = "theme::default/images/ORGH_TL2.png";
		
		for (var i=0; i<this.ds_Depm.getRowCount(); i++) {
			depm_xValPo = parseInt(this.ds_Depm.getColumn(i, "XVAL_PO"));
			depm_yValPo = parseInt(this.ds_Depm.getColumn(i, "YVAL_PO"));
			
			if(this.ds_Depm.getColumn(i, 'GROP_LV') == '1') {
				var staLabel = new Static("sta_org" + i, depm_xValPo, depm_yValPo, depm_Width, depm_Height, null, null);
				staLabel.set_background(sLv01BgColor);
			} else if(this.ds_Depm.getColumn(i, 'GROP_LV') == '2') {
				var staLabel = new Static("sta_org" + i, depm_xValPo, depm_yValPo, depm_Width, depm_Height, null, null);
				staLabel.set_background(sLv02BgColor);
			} else if(this.ds_Depm.getColumn(i, 'GROP_LV') == '3') {
				var staLabel = new Static("sta_org" + i, depm_xValPo, depm_yValPo, depm_Width, depm_Height, null, null);
				staLabel.set_background(sLv03BgColor);
			} else if(this.ds_Depm.getColumn(i, 'GROP_LV') == '4') {
				var staLabel = new Static("sta_org" + i, depm_xValPo, depm_yValPo, depm_Width, depm_Height, null, null);
				staLabel.set_background(sLv04BgColor);
			} else if(this.ds_Depm.getColumn(i, 'GROP_LV') == '5') {
				var staLabel = new Static("sta_org" + i, depm_xValPo, depm_yValPo, depm_Width, depm_Height, null, null);
				staLabel.set_background(sLv05BgColor);
			} else if(this.ds_Depm.getColumn(i, 'GROP_LV') == '6') {
				var staLabel = new Static("sta_org" + i, depm_xValPo, depm_yValPo, depm_Width, depm_Height, null, null);
				staLabel.set_background(sLv06BgColor);				
			} else {
				var staLabel = new Static("sta_org" + i, depm_xValPo, depm_yValPo, depm_Width, depm_Height, null, null);
				staLabel.set_background(sLv07BgColor);
			}
			staLabel.set_text(this.ds_Depm.getColumn(i, "DEPM_NM"));//라벨명
			staLabel.set_textAlign("center");
			staLabel.set_font('bold 11px/normal "Malgun Gothic"');
			staLabel.set_color(sFontColor);
