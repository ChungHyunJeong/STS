this.staBorderClear = function() {
	for (var i=0; i<this.ds_Depm.getRowCount(); i++) {
		var objDiv = "this.div_Org.form.div_level"+this.ds_Depm.getColumn(i, 'DEPM_LV');
		eval(objDiv+".form.sta_tmsu_ig" + i).set_border("8px solid #efefef");
	}
	
	for(var k=0; k<this.div_Org.form.components.length; k++) {
		if(this.div_Org.form.components[k] instanceof nexacro.Static) {
			this.div_Org.form.components[k].set_border("");
		}
	}	
}

this.divOrgReset = function() {

	var pDiv = this.div_Org.form;
	var f = "";
	//for(var k=0; k<pDiv.components.length; k++) { // 최상단 div
	for(var k=pDiv.components.length; k>0; k--){
		if(pDiv.components[k] instanceof nexacro.Div) {
			var lDiv = pDiv.components[k].form;
			//for(var kk=0; kk<lDiv.components.length; kk++) {
			for(var kk=lDiv.components.length; kk>0; kk--){
			
				f = lDiv.removeChild(lDiv.components[kk-1].id);
				f.destroy();
				f = null;
			}
		}
		f = pDiv.removeChild(pDiv.components[k-1].id);
		f.destroy();
		f = null;
		
	}	
}

