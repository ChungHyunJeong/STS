		}
	} else {
		strAutmCd	=	"HS_8225_90";	//4대보험
	}	
	*/
	
	strAutmCd	=	"HS_8225_99";	//급상여전표발의	
	
	if (iCurRow > -1) {
		var atobObj = new Object();//[공통] 자동전표 파라메타 정보 Object
		atobObj.COMM_CD = this.ds_Master10.getColumn(iCurRow, "COMM_CD");//회사코드		
		atobObj.AUTM_CD = strAutmCd;//분류코드-T_FA_AUTM(자동전표환경설정) 테이블 내용 참조
