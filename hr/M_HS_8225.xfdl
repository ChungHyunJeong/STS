this.fn_ComBtnAdd = function()
{
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}
	
	this.fvDelChk     =  "N";

	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_enable(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_readonly(false);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cal_SlihYm.set_enable(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PaymDd.set_enable(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_PaboGu.set_enable(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_InsuGu.set_enable(true);	

	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_RetiCd.set_enable(false);
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm01.set_enable(false);
	this.div_MasterInput.form.tab_Master.tpg_01.form.btn_EmpmCd.set_enable(false);
		 

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
		
	var iAddRow = this.ds_Master10.addRow();
	
	
	
	this.fvMmCloseDay = sToDay;//[월마감] 마감일자
	
	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", "1");	//결재순번
	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");	//최종여부
	this.ds_Master10.setColumn(iAddRow, "WOFH_ST", "0010");	//결재상태	
	
 	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//등록부서코드	
 	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//등록부서명	
//	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", "8700100");//경영지원실 고정
//	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", "경영지원실");//경영지원실
	
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//등록자사번	
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//등록자명		
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//등록자ID	
	
	//회계단위 설정
	this.ds_Master10.setColumn(iAddRow, "ACUM_CD", this.gfn_GetSessionValue("ACUM_CD"));//회계단위코드
	this.ds_Master10.setColumn(iAddRow, "ACUM_NM", this.gfn_GetSessionValue("ACUM_NM"));//회계단위명
	
	var sToYYYYMM = sToDay.substring(0,6);
	var sLimiDd	  = this.gfn_AddMonth(sToDay, 1).substr(0,6) + "10";	
	
	this.ds_Master10.setColumn(iAddRow, "SLIH_DD", sToDay);			//등록일자-오늘날자로 설정
	this.ds_Master10.setColumn(iAddRow, "SLIH_YM", sToYYYYMM);		//귀속년월
	this.ds_Master10.setColumn(iAddRow, "LIMI_DD", sLimiDd);		//납부일자
	this.ds_Master10.setColumn(iAddRow, "PAYM_DD", this.gfn_GetCommDate("monthLastday"));	//지급일자	 	
	
	
	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.fn_SetGridInputButtonEnable();
	this.div_DetailInput.form.tab_Detail.set_enable(true);
	this.div_MasterInput.form.btn_CnthM1.set_visible(true);	//추계액계산버튼
	
	//this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.set_enable(true);
	/*
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_EtaxGu.set_enable(true);	//전자/수기 
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_VatmCd.set_enable(true);	//부가세코드
	//this.div_MasterInput.form.tab_Master.tpg_01.form.edt_VatmNm.set_right("30");	//부가세명
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_VatmNm.set_width("206");	//부가세명
	this.div_MasterInput.form.tab_Master.tpg_01.form.btn_VatmCd.set_visible(true);	//부가세버튼
	
	//외주계약 미등록검색 활성
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_PcohDtFr.set_enable(true);//발주일자
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_PcohDtTo.set_enable(true);//발주일자
	
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmCd.set_enable(true);//거래처
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmNm.set_enable(true);
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.btn_CusmCd.set_enable(true);
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.btn_Search.set_enable(true);//검색버튼
	
	//확정버튼 컨트롤
	this.div_MasterInput.form.btn_CnthM1.set_enable(false);	//계약확정
	this.div_MasterInput.form.btn_CnthM2.set_enable(false);	//계약변경
	*/
	
	//처음 입력 항목에 포커스
	this.div_MasterInput.form.tab_Master.tpg_01.form.cal_SlihYm.setFocus();// 귀속년월
	//this.div_DetailInput.form.tab_Detail.set_tabindex(1);
	
	
	this.ds_Detail10.clearData();//상세정보 초기화
	//this.ds_Detail20.clearData();//검색정보 초기화
	eval(this.fvAtobDiv).fn_ClearAtobData();//자동전표 정보 초기화		
	
	/*			
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
	//첨부파일정보 권한설정
