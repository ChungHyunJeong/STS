this.fn_SetAcchNm	=	function()
{
	
	var nRow 		= this.ds_Hr807.findRowExpr("CODM_CD == '" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PABO_GU") + "'");
	var strYm		= this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SLIH_YM");
	strYM			= strYm.substr(0,4) + "-" + strYm.substr(4,2);
	var strCodmNm 	= this.ds_Hr807.getColumn(nRow, "CODM_NM");
	
	var strInsuGu   = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "INSU_GU");
	var strNm		= ""
	if(strInsuGu=="01"){
		strNm	= strCodmNm;	
	}else{
		strNm	= "4대보험";
	}
	
	if(!this.gfn_IsNull(strCodmNm) && !this.gfn_IsNull(strInsuGu)){
		//this.ds_Master10.setColumn(this.ds_Master10.rowposition, "ACCH_NM", "급상여이체 - " + strNm + ", " + strYM);	
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "ACCH_NM",  strYM + "월 " + strNm + "에 의한 자동 전표");	
	}	
}
