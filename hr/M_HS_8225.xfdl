// 		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm.set_enable(false);
// 		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumCd.set_enable(false);
// 		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumNm.set_enable(false);
// 		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_YochAm.set_enable(false);		

		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SlihNo.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_SlihDd.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_readonly(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_DepmCd.set_enable(true);
