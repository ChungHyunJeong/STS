//-----------------------------------------------------------------------------------------------------------------------------------------
this.fn_getSumAmt = function(){			

			if(this.ds_Detail10.rowcount > 0 ){ 			
				var sTotAm = this.ds_Detail10.getCaseSum("crudType != 'D'","CRED_AM");
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "YOCH_AM", sTotAm);
			} else {
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "YOCH_AM", 0);
			}
			
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);
			
			var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);			
			
			var nRow 		= this.ds_Hr807.findRowExpr("CODM_CD == '" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PABO_GU") + "'");
			var strYm		= this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SLIH_YM");
			strYM			= strYm.substr(0,4) + "-" + strYm.substr(4,2);
			var strCodmNm 	= this.ds_Hr807.getColumn(nRow, "CODM_NM");
			
			var strPaboGu   = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PABO_GU");
			var strInsuGu   = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "INSU_GU");
			
			if(strPaboGu =="0050"){
				this.ds_Input.clearData();
				this.ds_EmpmInfo.clearData();
				this.ds_Input.addRow();
				
				this.ds_Input.setColumn(0, "SYST_ID", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RETI_ID"));
			
			
				//[공통]서비스정보 초기화
				this.gfn_InitComService(this);
				
				//[공통]서비스정보 추가
				var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
				svcAddObj.sSelectSql = "hr:M_HS_8225_M_SL19";
				this.gfn_AddComSearchService(this, svcAddObj);			

				
				//[공통]서비스 호출 변수
				var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
				svcCallObj.sTrid = "MST_SEARCH_EXISTS2";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
				svcCallObj.sActionName = this.G_SV_HS;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
				svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
				svcCallObj.sOutDatset = "ds_EmpmInfo=ds_EmpmInfo";//출력 데이터셋
				svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
				svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
				svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
				svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID		
				svcCallObj.isSync = true;//동기식으로 설정할지 여부

				//[공통]서비스 호출
				this.gfn_CallComService(this, svcCallObj);
			
			}
			
			var strNm		= ""
			if(strInsuGu=="01" || strInsuGu=="0010"){
				strNm	= strCodmNm;	
			}else{
				strNm	= "4대보험";
			}
			
			if(!this.gfn_IsNull(strCodmNm) && !this.gfn_IsNull(strInsuGu)){
				var TotAm  	= "";
				var A0010	= ""
				var A0020	= "";
				var A0030	= "";
				var A0040	= "";
				var A0050	= "";
				var sYoil	= "";
				var sLimiDd = "";
				
				if(strPaboGu =="0050"){
					sLimiDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PAYM_DD");	
				}else{
					sLimiDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "LIMI_DD");	
				}				
				
				if(this.gfn_IsNull(sLimiDd)){
					var sSlihYm  =  this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SLIH_YM")		
					sLimiDd	  	 = this.gfn_AddMonth(sSlihYm + "01", 1).substr(0,6) + "10";		
										
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "LIMI_DD", sLimiDd);						
				}
				
				sYoil = this.gfn_GetDay(sLimiDd);
				
				if(sYoil=="0"){
					sYoil = "(일)";
				}else if(sYoil=="1"){
					sYoil = "(월)";
				}else if(sYoil=="2"){
					sYoil = "(화)";
				}else if(sYoil=="3"){
					sYoil = "(수)";
				}else if(sYoil=="4"){
					sYoil = "(목)";
				}else if(sYoil=="5"){
					sYoil = "(금)";
				}else if(sYoil=="6"){
					sYoil = "(토)";
				}
				
				sLimiDd  = sLimiDd.substr(0,4) + "." + sLimiDd.substr(4,2) + "." + sLimiDd.substr(6,2)
				
				totAm = this.ds_Detail10.getCaseSum("crudType != 'D'","CRED_AM");
				A0010 = this.ds_Detail10.getCaseSum("crudType != 'D' && CRED_AM > 0 && String(REMK_NM).indexOf('국민') >= 0", "CRED_AM");
				A0020 = this.ds_Detail10.getCaseSum("crudType != 'D' && CRED_AM > 0 && String(REMK_NM).indexOf('건강') >= 0", "CRED_AM");				
				A0030 = this.ds_Detail10.getCaseSum("crudType != 'D' && CRED_AM > 0 && String(REMK_NM).indexOf('고용') >= 0", "CRED_AM");
								
				//this.Dataset00.filter("Column0.toString().indexOf('"+this.Edit00.value+"')>-1");				
				
				// 대변 합계금액에서 - ( 국민+건강+요양+고용) 차감한 금액을 산재보험으로 처리
				A0040 = nexacro.toNumber(totAm) - (nexacro.toNumber(A0010) + nexacro.toNumber(A0020) + nexacro.toNumber(A0030));
				
				var totAm = this.gfn_NumberToCommaString(totAm) + "원";
				var A0010 = this.gfn_NumberToCommaString(A0010) + "원";
				var A0020 = this.gfn_NumberToCommaString(A0020) + "원";
				var A0030 = this.gfn_NumberToCommaString(A0030) + "원";
				var A0040 = this.gfn_NumberToCommaString(A0040) + "원";
				var A0050 = this.gfn_NumberToCommaString(A0050) + "원";						
				
				if(strInsuGu=="01" || strInsuGu=="0010"){
				
					if(strPaboGu=="0050"){
						var sRetiMm = this.ds_EmpmInfo.getColumn(0, "RETI_DD");
						var sRetiNm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RETI_NM");						
						sRetiMm 	= sRetiMm.substr(5,2);
												
						var sEntrDd = this.ds_EmpmInfo.getColumn(0, "ENTR_DD");
						var sRetiDd = this.ds_EmpmInfo.getColumn(0, "RETI_DD");
						
						var nRow   = "";
						var nRow1   = "";
						var sRetiAm= ""; 
						var sToigAm=""
						
						nRow       = this.ds_Detail10.findRowExpr("ACOM_CD == '201100801000' &&  String(REMK_NM).indexOf('퇴직자 정산') >= 0");
						sRetiAm	   = this.ds_Detail10.getColumn(nRow, "CRED_AM");						
						
						//-- 급여반환일때
						nRow1      = this.ds_Detail10.findRowExpr("ACOM_CD == '101040305000' &&  String(REMK_NM).indexOf('퇴직자 정산') >= 0");
						
						if(nRow1 > -1){
							sRetiAm	   = this.ds_Detail10.getColumn(nRow1, "DEBI_AM");
						}						
						
						//--퇴직금 받는 직원인지
						nRow       = this.ds_Detail10.findRowExpr("ACOM_CD == '201100802000'");
						
						if(nRow > -1){
							sToigAm	   = this.ds_Detail10.getColumn(nRow, "DEBI_AM");
						}
						
						sRetiAm	   = this.gfn_NumberToCommaString(sRetiAm); 
						sToigAm	   = this.gfn_NumberToCommaString(sToigAm);						
						
						
						var fullTxt	= sRetiMm + "월 퇴직자 정산 (" +  sRetiNm + ")\n\n";
						fullTxt     = fullTxt + "1. 재직기간 : " + sEntrDd + " ~ "  + sRetiDd + "\n\n";
						
						//-- 반환일때
						if(nRow1 > -1){
							fullTxt     = fullTxt + "2. 퇴직정산 미수금 : ￦ " + sRetiAm + "원";
						}else{
							fullTxt     = fullTxt + "2. 퇴직정산 입금액 : ￦ " + sRetiAm + "원 (" + this.ds_EmpmInfo.getColumn(0, "BANK_NM")  + "  " + this.ds_EmpmInfo.getColumn(0, "SAAC_NO")  + ")\n\n";

							if(nRow > -1){
								fullTxt     = fullTxt + "3. 퇴직연금 불입액 : ￦ " + sToigAm + "원\n\n";
								fullTxt     = fullTxt + "4. 지급요청일 : "  + sLimiDd + "  " + sYoil;
							}else{
								fullTxt     = fullTxt + "3. 지급요청일 : "  + sLimiDd + "  " + sYoil;
							}
						}
					
						this.ds_Master10.setColumn(this.ds_Master10.rowposition, "REMK_NM",fullTxt);					
					
					} else {
						var fullAm 	= "급상여이체 - " + strNm + ", " + strYM + " : " + totAm + "\n\n";
						fullAm		= fullAm + "               납부기한 :  "   + sLimiDd + "  " + sYoil; 
						
						this.ds_Master10.setColumn(this.ds_Master10.rowposition, "REMK_NM",fullAm);					
					}
					
				}else if(strInsuGu=="02" || strInsuGu=="0020"){					
					var fullAm 	= "급상여이체 - " + strNm + ", " + strYM + " : " + totAm + "\n\n";
					fullAm		= fullAm + "               납부기한 :  "   + sLimiDd + "  " + sYoil + "\n\n";
					
					fullAm		= fullAm + "               - 국민연금 :  " + A0010 +  "\n";
					fullAm		= fullAm + "               - 건강보험 :  " + A0020 +  "\n";					
					fullAm		= fullAm + "               - 고용보험 :  " + A0030 +  "\n";
					fullAm		= fullAm + "               - 산재보험 :  " + A0040 +  "\n";
					fullAm		= fullAm + "               ==================     " +  "\n";
					fullAm		= fullAm + "                 합      계 :  " + totAm  ;
					
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "REMK_NM", fullAm);	
					
				}
			} else {
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "REMK_NM", "");	
			}										

			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "YOCH_AM", totAm );
} 

