/*---------------------------------------------------------------------------------------------
 *    03. [데이터셋-ds_Master10] Cloumn 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{ 
	if(e.columnid == "SLIH_YM"){
		var nProcMm = 0;
		//var sSlihYm  =  this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SLIH_YM")	
		var sPaboGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition,"PABO_GU");
		var sSlihYm = e.newvalue;
		var sLimiDd	  = this.gfn_AddMonth(sSlihYm, 1).substr(0,6) + "10";
		
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "LIMI_DD", sLimiDd);
		
		sSlihYm = sSlihYm.substr(4,2);
		if(sPaboGu=="0050") {
			if(sSlihYm=="01" || sSlihYm=="04" || sSlihYm=="07" || sSlihYm=="10"){
				nProcMm = 1;
			}else if(sSlihYm=="02" || sSlihYm=="05" || sSlihYm=="08" || sSlihYm=="11"){
				nProcMm = 2;
			}else if(sSlihYm=="03" || sSlihYm=="06" || sSlihYm=="09" || sSlihYm=="12"){
				nProcMm = 3;
			}		
			
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "SLIH_CT", nProcMm);
		}
		this.fn_SetAcchNm();
		
	}else if(e.columnid == "LIMI_DD"){
		this.fn_getSumAmt();	 
		
	}else if(e.columnid == "PABO_GU"){
		var nProcMm = 0;
		var sPaboGu = e.newvalue;
		var sSlihYm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SLIH_YM");
		
		sSlihYm = sSlihYm.substr(4,2);
		
		if(sSlihYm=="01" || sSlihYm=="04" || sSlihYm=="07" || sSlihYm=="10"){
			nProcMm = 1;
		}else if(sSlihYm=="02" || sSlihYm=="05" || sSlihYm=="08" || sSlihYm=="11"){
			nProcMm = 2;
		}else if(sSlihYm=="03" || sSlihYm=="06" || sSlihYm=="09" || sSlihYm=="12"){
			nProcMm = 3;
		}		
		
		if(sPaboGu=="0050"){
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "INSU_GU", "01");
			
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "SLIH_CT", nProcMm);
			
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_InsuGu.set_enable(false);
		
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_RetiCd.set_enable(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm01.set_enable(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_EmpmCd.set_enable(true);
			//this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SlihCt.set_enable(true);
		}else{
			if(sPaboGu=="0010"){			
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "INSU_GU", "");
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_InsuGu.set_enable(true);		
			}else{
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "INSU_GU", "01");
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_InsuGu.set_enable(false);		
			}				
		
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "SLIH_CT", null);
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "RETI_CD", "");
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "RETI_NM", "");
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "RETI_ID", "");
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_RetiCd.set_enable(false);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm01.set_enable(false);
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_EmpmCd.set_enable(false);
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SlihCt.set_enable(false);
		}	
	}
};

