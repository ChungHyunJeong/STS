		this.gfn_Alert("SM008", arrMsgArg);// &ITM0은(는) 필수 입력 항목입니다.
		return false;
	}
	
	if (this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PABO_GU"))) {
		var arrMsgArg = ["급상여구분"];
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_PaboGu.setFocus();
		this.gfn_Alert("SM008", arrMsgArg);// &ITM0은(는) 필수 입력 항목입니다.
		return false;
	}

	if (this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "INSU_GU"))) {
		var arrMsgArg = ["4대보험구분"];		
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_InsuGu.setFocus();		
		this.gfn_Alert("SM008", arrMsgArg);// &ITM0은(는) 필수 입력 항목입니다.		
		return false;
	}
	
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	var sSlihYm		= this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SLIH_YM");	
	var sPaboGu		= this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PABO_GU");
	var sInsuGu		= this.ds_Master10.getColumn(this.ds_Master10.rowposition, "INSU_GU");
	var sSlihCt		= this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SLIH_CT");
	var sRetiCd		= ""
	var sRetiId		= ""
	
	if(sPaboGu=="0050"){
		sRetiCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RETI_CD");
		sRetiId = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RETI_ID");
		
		sSlihCt = nexacro.toNumber(sSlihCt);
		
		if (this.gfn_IsNull(sRetiCd) || this.gfn_IsNull(sRetiId)) {
			var arrMsgArg = ["퇴직자"];		
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_RetiCd.setFocus();		
			this.gfn_Alert("SM008", arrMsgArg);// &ITM0은(는) 필수 입력 항목입니다.		
			return false;
		}
		if (this.gfn_IsNull(sSlihCt) || sSlihCt < 1  || sSlihCt > 3) {
			var arrMsgArg = ["퇴직추계액적용개월"];		
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SlihCt.setFocus();		
			this.gfn_Alert("SM008", arrMsgArg);// &ITM0은(는) 필수 입력 항목입니다.		
			return false;
		}
		
	}
	
	sSlihYm	=	sSlihYm.substr(0,6);

	this.fvDelChk     =  "Y";
	
	if (sChkRowType == "I") {
			
		this.ds_Input.clearData();
		this.ds_Input.addRow();		
		
		this.ds_Exists.clearData();
		this.ds_Exists10.clearData();
		this.ds_Exists17.clearData();
		this.ds_Exists18.clearData();

		this.ds_Input.setColumn(0,	"SLIH_YM", sSlihYm);		
		this.ds_Input.setColumn(0,	"PABO_GU", sPaboGu);
		this.ds_Input.setColumn(0,	"INSU_GU", sInsuGu);
		this.ds_Input.setColumn(0,	"RETI_ID", sRetiId);
			
		
		//[공통]서비스정보 초기화
		this.gfn_InitComService(this);
		
		//[공통]서비스정보 추가
		var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
		svcAddObj.sSelectSql = "hr:M_HS_8225_M_SL15";
		this.gfn_AddComSearchService(this, svcAddObj);
		
		svcAddObj.sSelectSql = "hr:M_HS_8225_M_SL16";
		this.gfn_AddComSearchService(this, svcAddObj);
		
		svcAddObj.sSelectSql = "hr:M_HS_8225_M_SL17";
		this.gfn_AddComSearchService(this, svcAddObj);
		
		svcAddObj.sSelectSql = "hr:M_HS_8225_M_SL18";
		this.gfn_AddComSearchService(this, svcAddObj);
		
		//[공통]서비스 호출 변수
		var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
		svcCallObj.sTrid = "MST_SEARCH_EXISTS1";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
		svcCallObj.sActionName = this.G_SV_HS;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
		svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
		svcCallObj.sOutDatset = "ds_Exists=ds_Exists ds_Exists10=ds_Exists10 ds_Exists17=ds_Exists17 ds_Exists18=ds_Exists18";//출력 데이터셋
		svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
		svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
		svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID		
		svcCallObj.isSync = true;//동기식으로 설정할지 여부

		//[공통]서비스 호출
		this.gfn_CallComService(this, svcCallObj);
		
		if(sPaboGu != "0050"){		
			if(this.ds_Exists.rowcount > 0){
				var arrMsgArg = ["급상여전표발의"];
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_SlihYm.setFocus();
				
				this.gfn_Alert("SD008", arrMsgArg); //급상여전표발의 이미 존재합니다.			
				return false; 
			}
			
			if(this.ds_Exists10.rowcount < 1){
				var nRow = this.ds_Hr807.findRowExpr("CODM_CD == '" + sPaboGu + "'");			
				var strPaboNm = this.ds_Hr807.getColumn(nRow, "CODM_NM");		
			
				var arrMsgArg = [strPaboNm];
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_SlihYm.setFocus();
				
				this.gfn_Alert("HS010", arrMsgArg); //급상여자료가 존재하지 않습니다	
				return false; 
			}
		} 
		
		if(sPaboGu == "0050"){
			if(this.ds_Exists17.rowcount > 0){
				var arrMsgArg = ["급상여전표발의(퇴직자)"];				
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_SlihYm.setFocus();
				
