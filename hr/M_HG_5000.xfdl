/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {

		//상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SEAH_NO"))) {
			
			//이미지-첨부파일 정보 조회
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.PICH_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SEAH_NO");//관리번호
			this.fvImgFileDiv.fn_AtchFileSearch(oFileInfo);			
						
			//첨부파일 정보 조회
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
			oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SEAH_NO");//PK번호2

			this.fvFileDiv.fn_AtchFileSearch(oFileInfo);	
			
		}
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master10] Row 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10) || this.fvImgFileDiv.fn_CheckFileAddYn() || this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	

	return true;
};  

/*---------------------------------------------------------------------------------------------
 *    03. [데이터셋-ds_Master10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.columnid == "USED_YN") {	 
		if(this.ds_Master10.getColumn(e.row, "USED_YN") == "0"){
			var sToDay = this.gfn_GetCommDate("toDay");//현재날짜
			var sToDayFull = this.gfn_GetCommDate("toDayFull");//현재일시
			//trace("sToDay = " + sToDay);
			this.ds_Master10.setColumn(e.row, "DELE_ID", this.gfn_GetSessionValue("SYST_ID"));
			this.ds_Master10.setColumn(e.row, "DELE_NM", this.gfn_GetSessionValue("USEM_NM"));
			this.ds_Master10.setColumn(e.row, "DELE_DT", sToDayFull);
			this.ds_Master10.setColumn(e.row, "DISC_DD", sToDay);
		} else {
			this.ds_Master10.setColumn(e.row, "DELE_ID", null);
			this.ds_Master10.setColumn(e.row, "DELE_NM", null);
			this.ds_Master10.setColumn(e.row, "DELE_DT", null);
			this.ds_Master10.setColumn(e.row, "DISC_DD", null);
		}
	}	

}

