/***********************************************************************************************
 * 05.02. [Vaildation] Master 저장 필수값 체크 - 입력항목 
 ***********************************************************************************************/ 
this.fn_VaildMasterSave = function()
{
	//[저장] Validation 설정
	var arrArgs = ["EMPM_CD^NULL"
				  ,"MPAM_MM^NULL"
				  ,"JSPX_AM^NULL"
				  ];
				   
	var sCheckGroupTitle = this.div_MasterInput.form.tab_Master.tpg_01.text;//체크그룹 타이틀
	
	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckGridValid(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10, arrArgs, sCheckGroupTitle)) {
		return false;
	}
	
// 	//중복체크
// 	var sSaumCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SAUM_CD");//사업장코드
// 	var sWkgmCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WKGM_CD");//작업조코드
// 	var sChkData = this.ds_Master10.getCaseCount("SAUM_CD == '"+sSaumCd+"' && WKGM_CD == '"+sWkgmCd+"'");
// 	
// 	if(sChkData > 1 ){
// 	   this.gfn_Alert("SM041");//중복된 데이터가 존재합니다.
// 	   return false;
// 	}
// 
// 	//Dataset 중복 체크.
// 	if(!this.gfn_VaildDupDataset(this.ds_Master10, "SAUM_CD,WKGM_CD")){
// 		return false;
// 	}

	//사번 중복체크
	for(var i=0 ; i<this.ds_Master10.getRowCount(); i++) {
		var strFirst  = this.ds_Master10.getColumn(i, "SYST_ID");
		var strMpamMm = this.ds_Master10.getColumn(i, "MPAM_MM").substr(0,6);
		
		for(var j=i+1 ; j<this.ds_Master10.getRowCount(); j++) {
			var strNext   = this.ds_Master10.getColumn(j, "SYST_ID");
			var strNextMm = this.ds_Master10.getColumn(j, "MPAM_MM").substr(0,6);			
			
			if(strFirst == strNext  && strMpamMm == strNextMm){
			   this.gfn_Alert("SM041");//중복된 데이터가 존재합니다.
			   this.ds_Master10.set_rowposition(j);
			   return false;
			}
		}
	}


	var nRow  = this.ds_Master10.findRowExpr("dataset.getRowType(rowidx) == 2");	
	
	while (nRow > -1)
	{	
		var nRowType  = this.ds_Master10.getRowType(nRow);
		
		//--입력일때만 체크
		if(nRowType == 2) {
			//---카드번호 존재유무 체크		
			this.ds_Input.clearData();
			this.ds_Input.addRow();
			
			var strJsphCd  = "";
			var strMpamMm  = this.ds_Master10.getColumn(nRow, "MPAM_MM").substr(0,6);			
			var strJspxYy  = strMpamMm.substr(0,4);
			var strMm      = strMpamMm.substr(4,2);			

			if(strMm < "07"){
				strJsphCd = "1";
			}else{
				strJsphCd = "2";
			}

			this.ds_Input.setColumn(0,	"JSPX_YY", strJspxYy);
			this.ds_Input.setColumn(0,	"JSPH_CD", strJsphCd);
			this.ds_Input.setColumn(0,	"MPAM_MM", strMpamMm);
			this.ds_Input.setColumn(0,	"SYST_ID", this.ds_Master10.getColumn(nRow, "SYST_ID"));			
			
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "hr:" + this.fvComPrgmId + "_M_SL11";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "MST_SEARCH_EXISTS";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Exists=ds_Exists";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
			svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
			svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID		
			svcCallObj.isSync = true;//동기식으로 설정할지 여부

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);
			
			if(this.ds_Exists.rowcount > 0){
				var arrMsgArg = ["인정상여"];
				this.ds_Master10.set_rowposition(nRow);
				this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.setCellPos(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.getBindCellIndex("body","SYST_ID"));
				this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.showEditor(true);
				this.gfn_Alert("SD008", arrMsgArg); //인정상여가 이미 존재합니다.			
				return false; 
			}
		};
		//---------------------------------------------------------------//	 
	
		nRow  = this.ds_Master10.findRowExpr("dataset.getRowType(rowidx) == 2", nRow + 1);
	}	


//////////////////////////////////
	return true;
}

