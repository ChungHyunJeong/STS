/*---------------------------------------------------------------------------------------------
 *    [Master 입력영역-Button] 엑셀업로드 Call Back 함수
  ---------------------------------------------------------------------------------------------*/
this.fn_ImportCallback = function(sImportId)
{	
	//this.ds_Detail10.set_enableevent(false); //  SYST_ID 사번이 입력되어야 하므로 이벤트를 false로 하면 안됨
	
	var nRow = 0;	
	
	this.ds_Master10.clearData();
	
	this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.set_enableredraw(false);
	
	//this.ds_Master10.set_enableevent(false);
	var sYM;
	
	for (var iLoop=0; iLoop<this.ds_Import.getRowCount(); iLoop++) {
		var iCol = this.ds_Import.getColumn(iLoop, "Column0"); 
		
		if (!this.gfn_IsNull(iCol)) {
		
			nRow = this.ds_Master10.addRow();
			
			sYM = this.ds_Import.getColumn(iLoop, "Column2");
			
			if(!this.gfn_IsNull(sYM)){
				sYM = nexacro.replaceAll(sYM, " ", "");
				sYM = nexacro.replaceAll(sYM, "-", "");
				sYM = nexacro.replaceAll(sYM, "/", "");
				sYM = nexacro.replaceAll(sYM, ".", "");
				sYM = nexacro.replaceAll(sYM, "년", "");
				sYM = nexacro.replaceAll(sYM, "월", "");
			}
			
			sYM = sYM.substr(0,4) + "" +  sYM.substr(4,2).padLeft(2,"0");
			
			this.ds_Master10.setColumn(nRow, "EMPM_CD", this.ds_Import.getColumn(iLoop, "Column0")); //사번
			this.ds_Master10.setColumn(nRow, "MPAM_MM", sYM); //귀속년월
			this.ds_Master10.setColumn(nRow, "JSPX_AM", this.ds_Import.getColumn(iLoop, "Column3")); //인정상여			
			this.ds_Master10.setColumn(nRow, "REMK_NM", this.ds_Import.getColumn(iLoop, "Column4")); //비고
			
			
			this.ds_Master10.setColumn(nRow, "crudType", "I"); //저장시 ds_detail10 의 내용을 저장키 위함.	
			 
		}		
	}
	//this.ds_Master10.set_enableevent(false);
	this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.set_enableredraw(true);
	
	
}

