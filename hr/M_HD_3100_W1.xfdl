			//레포트 결재라인 조회후 레포트 출력
			this.gfn_GetCommWfLine(this, oDs, sCommCd, sWofhNo, sCallBackFunc, sId, sLineDst);
				
		} else {
			this.gfn_Alert("SM083");//출력할 정보가 없습니다.
		}
}

