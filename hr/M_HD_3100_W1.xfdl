  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {

		//출장번호정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO"))) {

			//[전표보기] 버튼 활성화 - 결재상태 승인시
			if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST") == "0040") {
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(true);
			} else {
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(false);
			}

			//필수체크(연구(0050):연구프로젝트번호, 판매(0040):계약번호)
			var sSctrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SCTR_CD"); //전표손익제어(FA012)
			if (sSctrCd == '0040') { //판매
				this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setCellProperty("head", 9, "cssclass", "essential");
				this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setCellProperty("head", 12, "cssclass", "");
			} else if (sSctrCd == '0050') { //연구
				this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setCellProperty("head", 9, "cssclass", "");
				this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setCellProperty("head", 12, "cssclass", "essential");
			} else {
				this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setCellProperty("head", 9, "cssclass", "");
				this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setCellProperty("head", 12, "cssclass", "");
			}
			
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Detail11.clearData();
			this.ds_Detail15.clearData(); //동행인
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "BZTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO"));
			this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
			this.ds_Input.setColumn(0, "BZTH_GU", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_GU"));
			this.ds_Input.setColumn(0, "WOFH_ST", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"));
			this.ds_Input.setColumn(0, "PRGM_ID", this.fvComPrgmId);

			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "hr:M_HD_3100_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);

			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "hr:M_HD_3101_M_SL10";	//동행인관리
			this.gfn_AddComSearchService(this, svcAddObj);						

			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10 ds_Detail15=ds_Detail15";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);		
		}
	}
};

