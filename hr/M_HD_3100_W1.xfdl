/***********************************************************************************************
 * 15.05. [처리] 산출내역 처리
 ***********************************************************************************************/ 
this.fn_SetCostDetailProc = function()
{
	var iNowRow = this.ds_Detail10.rowposition;
	var iBztbQt = this.ds_Detail10.getColumn(iNowRow,"BZTB_QT"); //수량
	var sBztmCd = this.ds_Detail10.getColumn(iNowRow,"BZTM_CD"); //비용항목
	var sBztdCd = this.ds_Detail10.getColumn(iNowRow,"BZTD_CD"); //비용상세항목(1000 본인차량, 2000 숙박비(숙박시설), 3000 식사비(출장식비))
	var sBztbNm = ""; //산출내역

	//0. 출장표준경비 (비용항목 / 비용상세항목 find 후)
	var iFvFindRow = this.ds_HrBztm.findRowExpr("BZTM_CD=='" + sBztmCd + "' && BZTD_CD=='" + sBztdCd + "'");//표준금액 대상찾기
	var sBztmAm = this.ds_HrBztm.getColumn(iFvFindRow, "BZTM_AM");  //표준금액

	//1. 수량(Km/건수)가 null이나 0일 경우
	if (this.gfn_IsNull(iBztbQt) || iBztbQt == '0') {
		this.ds_Detail10.setColumn(iNowRow,"CASH_AM","0"); //본인차량 현금 0으로 변경
	
	//2. 본인차량
	} else if (sBztdCd == '1000') {	// 교통비-본인차량 Row의 경우에만 처리		
		//2-1. 산출내역 : 123(금액) * 456(거리)
		sBztbNm = this.gfn_AppendComma(sBztmAm,0) + "(금액) * " + this.gfn_AppendComma(iBztbQt,0) + "(거리)"; //산출내역 내용
		
		//2-2. 금액(현금) 계산
		var iCashAm = nexacro.toNumber(sBztmAm) * nexacro.toNumber(iBztbQt); //현금 = 표준금액 * 거리(수량)
		this.ds_Detail10.setColumn(iNowRow,"CASH_AM",iCashAm); //본인차량 현금 저장

		//2-3. Master 출장비 금액 계산
		var iCashAmSum = this.ds_Detail10.getSum("CASH_AM");  //현금
		var iPscaAmSum = this.ds_Detail10.getSum("PSCA_AM");  //개인카드금액
		
		//출장비 = 현금 + 개인카드금액
		var iBzthAm = iCashAmSum + iPscaAmSum;

		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BZTH_AM",iBzthAm);	//출장비

	//3. 숙박시설
	} else if (sBztdCd == '2000') {	// 숙박비-숙박시설
		//3-1. 숙박은 1 이상 불가능 : 1 이상의 경우 메시지 후 1로 강제셋팅
		
		//3-2 산출내역 : 실비정산(1일 한도액 = 60,000)
		sBztbNm = "실비정산(1일 한도액 = " + this.gfn_AppendComma(sBztmAm,0) + " )"; //산출내역 내용
	
	//4. 출장식비
	} else if (sBztdCd == '3000') {	// 식사비-출장식비
		//4-1. 출장식비는 3 이상 불가능 : 3 이상의 경우 메시지 후 3로 강제셋팅
		
		//4-2. 산출내역 : 실비정산(1일 한도액 = 8,000)
		sBztbNm = "실비정산(1식 한도액 = " + this.gfn_AppendComma(sBztmAm,0) + " )"; //산출내역 내용
	}
		
	//9. 산출내역에 생성된 내용 저장
	this.ds_Detail10.setColumn(iNowRow,"BZTB_NM",sBztbNm); //산출내역 저장

};		


