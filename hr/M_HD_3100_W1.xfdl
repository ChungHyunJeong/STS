/***********************************************************************************************
 * 02.MG0.07. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
			
	// 출장구분
	if (e.columnid == "BZTH_GU") {
		var sToDay = this.gfn_GetCommDate("toDay");//현재일자
		var sBzthGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_GU");  //출장구분

		//X박X일 초기화
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "NGHT_QT", "");	//박(초기화)
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DAYS_QT", "");	//일(초기화)		
		this.ds_Detail10.clearData();	//Detail10 초기화 : 국내/시내 기준으로 재입력
		
		//국내
		if (sBzthGu == '1') {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", "");		//시작일자 = 초기화
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", sToDay);	//종료일자 = 오늘일자
		
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FromDd.set_enable(true);  //시작일자 활성화
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.set_enable(true);  //종료일자 활성화

			//출장비용상세 국내사용여부 필터
			this.ds_Hr363.filter("");
			this.ds_Hr363.filter("OP04_NM == '1'");	//OP04_NM : 국내, OP05_NM : 시내
			
			//출장상세 행추가/행삭제/취소
			var objBtnInfo01List = [];						  
			//입력영역 버튼 비활성화
			objBtnInfo01List = ["COPY|N", "ADD|N", "INSERT|N", "DELETE|N", "CANCEL|N"];
			this.div_MasterInput.form.div_ComGrdBtn.form.fn_SetGrdButtonEnable(objBtnInfo01List);     			

		//시내
		} else if (sBzthGu == '3') {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", "");	//시작일자 초기화
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", "");	//종료일자 초기화(시작일자 입력시 동일)
			
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FromDd.set_enable(false);  //시작일자 비활성화
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.set_enable(false);  //종료일자 비활성화

			//출장비용상세 시내사용여부 필터
			this.ds_Hr363.filter("");
			this.ds_Hr363.filter("OP05_NM == '1'");	//OP04_NM : 국내, OP05_NM : 시내

			//출장상세 행추가/행삭제/취소
			var objBtnInfo01List = [];						  
			//입력영역 버튼 비활성화
			objBtnInfo01List = ["COPY|N", "ADD|Y", "INSERT|N", "DELETE|Y", "CANCEL|Y"];
			this.div_MasterInput.form.div_ComGrdBtn.form.fn_SetGrdButtonEnable(objBtnInfo01List);         			
		}

		//출장기간 시작일자 커서 이동
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FromDd.setFocus();

	// 출장기간 변경시 (1박 2일 계산)
	} else if (e.columnid == "FROM_DD" || e.columnid == "CLOS_DD") {
		var sBzthGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_GU");  //출장구분
		var fromDate = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD"); //출장기간(시작일자)
		var closDate = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD"); //출장기간(종료일자)
		var iNghtQt = ""; //박
		var iDaysQt = ""; //일

		//국내
		if (sBzthGu == '1') {
			//X박X일 구하기
			if (!this.gfn_IsNull(fromDate) || !this.gfn_IsNull(closDate)) {
				if (fromDate > closDate) {
					this.gfn_Alert("SM018");//종료일자가 시작일자보다 빠릅니다
					return false;
				}
				
				iNghtQt = this.gfn_GetDiffDate(fromDate, closDate); //박
				iDaysQt = iNghtQt + 1;                              //일
			} 
			
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "NGHT_QT", iNghtQt);	//박
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DAYS_QT", iDaysQt);	//일		

			/* ### Detail10 날짜별로 생성 ### */
			//1. 초기값 
			this.ds_Detail10.set_enableevent(false); //row 생성속도 향상을 위한 이벤트 false
			this.ds_Detail10.clearData();
			
			var sBztbDd = ""; //출장일자
			var iAddRow = 0;  //Detail10 Row
			
			//2. 날짜 차이 계산 (for문에 건수만큼 증가)
			var iDiffDd = this.gfn_GetDiffDate(fromDate, closDate) + 1;
			
			//3. 건수별로 Row 생성 (항목 : OP01_NM, 세부항목 - CODM_CD)
			for(var iLoop=0;iLoop<iDiffDd;iLoop++) {
				sBztbDd = this.gfn_AddDate(fromDate, iLoop); //출장일자(시작일자 기준으로 종료일자까지 증가)

				//출장비용상세 개수만큼 증가
				for (var iRow=0;iRow<this.ds_Hr363.getRowCount();iRow++) {
					iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)

					this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
					this.ds_Detail10.setColumn(iAddRow, "BZTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO"));
					this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
					this.ds_Detail10.setColumn(iAddRow, "BZTB_DD", sBztbDd);                                  //출장일자
					this.ds_Detail10.setColumn(iAddRow, "BZTB_SE", iLoop+1);                                  //순번
					this.ds_Detail10.setColumn(iAddRow, "BZTM_CD", this.ds_Hr363.getColumn(iRow, "OP01_NM")); //항목
					this.ds_Detail10.setColumn(iAddRow, "BZTD_CD", this.ds_Hr363.getColumn(iRow, "CODM_CD")); //세부항목
					this.ds_Detail10.setColumn(iAddRow, "OP06_NM", this.ds_Hr363.getColumn(iRow, "OP06_NM")); //수량(Km/건수) 입력여부
					this.ds_Detail10.setColumn(iAddRow, "OP07_NM", this.ds_Hr363.getColumn(iRow, "OP07_NM")); //산출내역입력여부
					this.ds_Detail10.setColumn(iAddRow, "OP08_NM", this.ds_Hr363.getColumn(iRow, "OP08_NM")); //금액입력여부
					
				}
			}
			
			this.ds_Detail10.set_enableevent(true); //row 생성속도 향상을 위한 이벤트 ture
		
		//시내(sBzthGu == '3')의 경우 Detail10의 출장일자 입력시 출장기간 처리
		}
		
	}
	
};


