 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SubmitDept.set_value(this.gfn_GetSessionValue("DEPM_NM")); 	//부서명
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SubmitName.set_value(this.gfn_GetSessionValue("USEM_NM"));	//담당자명
			break;
		case "SAVE":   //저장
			
			break;
			
		case "DELETE": //삭제		
			
			break;
		case "FILE_DOWN":   //파일다운로드

			if (this.ds_Result.getRowCount() > 0) {
				var sRSLT_CD = this.ds_Result.getColumn(0, "RSLT_CD");//결과코드 : S-성공, E-에러
				var sRSLT_MG = this.ds_Result.getColumn(0, "RSLT_MG");//에러메시지 / 파일명
				
				//성공이면서 파일명이 있으면
				if (sRSLT_CD == "S" && !this.gfn_IsNull(sRSLT_MG)) {
					this.fn_FileDownload(sRSLT_MG);//엑셀파일 다운로드
				}
				
				if (sRSLT_CD == "E") {
					var arrMsgArg = [sRSLT_MG];
					this.gfn_Alert("SM081", arrMsgArg);//파일생성중 오류가 발생했습니다.\n&ITM0
				}
				
			} else {
				var arrMsgArg = [""];
				this.gfn_Alert("SM081", arrMsgArg);//파일생성중 오류가 발생했습니다.\n&ITM0
			}
			break;				
	}
};

