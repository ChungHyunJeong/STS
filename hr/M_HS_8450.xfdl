this.fn_FileDownload = function(sFileId)
{
	try {

		var sPathNm = "temp";
		var sUrl = nEnv.services["svcUrl"].url+"FileUploader?mode=download&subDir=" + sPathNm;
			sUrl += "&fileName="+encodeURI(sFileId.replace(/\+/,"%2B"));	

		this.fdt_File.set_downloadfilename(sFileId);
		this.fdt_File.set_url(sUrl);

		this.fdt_File.download();
	} catch (e) {
		trace(e);
	}
	
}

this.gf_Right = function(strString, nSize)
{
    var nStart 	= String(strString).length;
    var nEnd 	= Number(nStart) - Number(nSize);
    var rtnVal 	= strString.substring(nStart, nEnd);

    return rtnVal;
}
