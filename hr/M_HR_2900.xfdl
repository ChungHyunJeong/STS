/***********************************************************************************************
 * 13.03. [메세지] CallBack 함수 처리
 ***********************************************************************************************/  
this.fn_MsgCallback = function (strId, strVal)
{
	//삭제여부
	if (strId == "CONF.DELETE") {
		if(strVal )
		{
			this.fn_SvcDelete();//삭제 서비스 호출
		}
	//MASTER 변경사항 저장여부
	} else if (strId == "CONF.MAST_CHANGE") {
		if(strVal )
		{
			this.fn_ComBtnSave();//저장 서비스 호출
		} else {
			//데이터 변경여부 체크
			if (this.gfn_DsIsUpdated(this.ds_Master10)) {
				this.gfn_UndoColumn(this.div_MasterView.form.grd_Master10);
			}		
			
			if (this.gfn_DsIsUpdated(this.ds_Detail20)) {
				this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail20);
			}				

			if (this.gfn_DsIsUpdated(this.ds_Detail30)) {
				this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail30);
			}				

		}
		
	//서약서 확정
	} else if (strId == "CONF.RESIGN_FIX_CHANGE") {	
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "RETH_YN", "1");	//사직서 확인여부
		this.div_DetailInput.form.tab_Detail.tpg_01.form.btn_ResignFix.set_visible(false);
		this.div_DetailInput.form.tab_Detail.tpg_01.form.img_ApprSign0.set_visible(true);

	//비밀번호서약 확정
	} else if (strId == "CONF.SECRET_FIX_CHANGE") {
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "SECU_YN", "1");	//비밀보호서약 확인여부		
		this.div_DetailInput.form.tab_Detail.tpg_04.form.div_Cont.form.btn_SecretFix.set_visible(false);
		this.div_DetailInput.form.tab_Detail.tpg_04.form.div_Cont.form.img_ApprSign1.set_visible(true);

	//[결재] 재기안여부
	} else if (strId == "CONF.RECIDE") {
		if(strVal) {
			this.fn_ComBtnRecideProc();//[결재] 재기안 서비스 호출
		}	

	}
	

}; 
 
