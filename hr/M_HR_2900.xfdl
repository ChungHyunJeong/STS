/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail20) && !this.gfn_DsIsUpdated(this.ds_Detail30) && !this.gfn_DsIsUpdated(this.ds_Detail50) && !this.gfn_DsIsUpdated(this.ds_Detail60) && !this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}

	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		//첨부파일정보만 변경시
		if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail20) && !this.gfn_DsIsUpdated(this.ds_Detail30) && !this.gfn_DsIsUpdated(this.ds_Detail50) && !this.gfn_DsIsUpdated(this.ds_Detail60) && this.ds_Master10.getRowCount() > 0) {
			//첨부파일정보 저장
			fvFileInfo = new Object();//[공통] 첨부파일정보
			fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, B-상세(그리드Row), E-기타
			fvFileInfo.CALLBACK_FUNC = "";//호출할 콜백함수
			fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
			fvFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RETH_NO");//PK번호2-관리번호
			fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
			//첨부파일Div 화면에 Key세팅하게 변경
			this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);
			return;	
		}
	}
	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		if (!this.fn_VaildMasterSave()) return;
	}
	
	//Detail20 데이터 유효성 체크 (설문조사)
	if (this.gfn_DsIsUpdated(this.ds_Detail20)) {
		if (!this.fn_VaildDetail20Save()) return;
	};

	//Detail30 데이터 유효성 체크 (인수인계)
	if (this.gfn_DsIsUpdated(this.ds_Detail30)) {
		if (!this.fn_VaildDetail30Save()) return;
		if (!this.fn_VaildDetail30Save1()) return;
	};
	
	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	//Master 입력 상태이면
	if (sChkRowType == "I") {
	
		//채번
		var sRethNo = this.gfn_GetTbnmNo(this.fvComPrgmId, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RETH_DD"));//채번값, 등록일자
		if (this.gfn_IsNull(sRethNo)) {
			//채번을 가져오는데 실패했습니다.
			return;
		}
		
		var sChkRethNo = "";//채번-체크용
		var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position
		
		//Master 데이터셋에 채번정보 입력
		this.ds_Master10.setColumn(iMasterRowPos, "RETH_NO", sRethNo);	//관리번호

		//PK값 정의[필수]
		var sCommCd = this.ds_Master10.getColumn(iMasterRowPos, "COMM_CD");//회사코드
		var sRethNo = this.ds_Master10.getColumn(iMasterRowPos, "RETH_NO");//관리번호
		var sWofhSq = this.ds_Master10.getColumn(iMasterRowPos, "WOFH_SQ");//결재차수
		
		//Detail20 데이터셋에 PK정보 입력 (설문조사)
		if (this.ds_Detail20.getRowCount() > 0) {	//대상 발생시
			sChkRethNo = this.ds_Detail20.getColumn(0, "RETH_NO");//관리번호
				
			if (this.gfn_IsNull(sChkRethNo)) {
				this.ds_Detail20.setColumn(0, "COMM_CD", sCommCd);
				this.ds_Detail20.setColumn(0, "RETH_NO", sRethNo);
				this.ds_Detail20.setColumn(0, "WOFH_SQ", sWofhSq);
			}
		}
		
		//Detail30 데이터셋에 PK정보 입력 (인수인계)
		if (this.ds_Detail30.getRowCount() > 0) {	//대상 발생시
			sChkRethNo = this.ds_Detail30.getColumn(0, "RETH_NO");//관리번호
				
			if (this.gfn_IsNull(sChkRethNo)) {
				this.ds_Detail30.setColumn(0, "COMM_CD", sCommCd);
				this.ds_Detail30.setColumn(0, "RETH_NO", sRethNo);
				this.ds_Detail30.setColumn(0, "WOFH_SQ", sWofhSq);
			}
		}

	}
	
	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		fvFileInfo = new Object();//[공통] 첨부파일정보
		fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
		fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
		fvFileInfo.CALLBACK_FUNC = "fn_ComBtnSearch";//호출할 콜백함수
		fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
		fvFileInfo.PK01_NO = nApp.gvCompany;//PK번호1-회사코드
		fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RETH_NO");//PK번호2-관리번호
		fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
	}	
	
	//저장시 재조회 Key설정
	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RETH_NO");//조회Key01
	this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//조회Key02
	
	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	//[공통]서비스 호출 변수
	var sInputDaset = "" //입력 데이터셋
	var sOutDatset = "";//출력 데이터셋
	var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		
	//Master 변경건이 있으면 Master 저장
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
		svcAddObj.sInsertSql = "hr:M_HR_2900_M_IN10";//저장Sql
		svcAddObj.sUpdateSql = "hr:M_HR_2900_M_UP10";//수정Sql
		svcAddObj.sDeleteSql = "hr:M_HR_2900_M_DL10";//삭제Sql
		svcAddObj.sSaveFlagColumn = "";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

		this.gfn_AddComSaveService(this, svcAddObj);
		
		sInputDaset = "ds_Master10=ds_Master10:U" //입력 데이터셋
	}
	

	//Detail20 변경건이 있으면 Detail20 저장 (설문조사)
	if (this.gfn_DsIsUpdated(this.ds_Detail20)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object	
		svcAddObj.sInsertSql = "hr:M_HR_2900_D_IN20";//저장Sql
		svcAddObj.sUpdateSql = "hr:M_HR_2900_D_UP20";//수정Sql
		svcAddObj.sDeleteSql = "hr:M_HR_2900_D_DL20";//삭제Sql
		svcAddObj.sSaveFlagColumn = "";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		
		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = "ds_Detail20=ds_Detail20:U" //입력 데이터셋
		} else {
			sInputDaset = sInputDaset + " ds_Detail20=ds_Detail20:U" //입력 데이터셋
		}
	}

	//Detail30 변경건이 있으면 Detail30 저장 (인수인계)
	if (this.gfn_DsIsUpdated(this.ds_Detail30)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object	
		svcAddObj.sInsertSql = "hr:M_HR_2900_D_IN30";//저장Sql
		svcAddObj.sUpdateSql = "hr:M_HR_2900_D_UP30";//수정Sql
		svcAddObj.sDeleteSql = "hr:M_HR_2900_D_DL30";//삭제Sql
		svcAddObj.sSaveFlagColumn = "";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		
		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = "ds_Detail30=ds_Detail30:U" //입력 데이터셋
		} else {
			sInputDaset = sInputDaset + " ds_Detail30=ds_Detail30:U" //입력 데이터셋
		}
	}
	

	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "SAVE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = sInputDaset //입력 데이터셋
	svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
	svcCallObj.sOtherArg = sOtherArg;//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명		
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	

}

