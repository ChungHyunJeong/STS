	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//등록자ID	
	this.ds_Master10.setColumn(iAddRow, "RETH_DD", sToDay);//등록일자-오늘날자로 설정
	
    this.ds_Master10.setColumn(this.ds_Master10.rowposition, "POSI_CD",this.gfn_GetSessionValue("POSI_CD")); //직위

	//입력영역 활성화
