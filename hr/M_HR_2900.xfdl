	this.ds_Detail20.clearData();//설문조사 초기화
	this.ds_Detail30.clearData();//인수인계 초기화
	this.ds_Detail50.clearData();//정보보안 초기화
	this.ds_Detail60.clearData();//퇴직면담 초기화
	this.ds_Detail70.clearData();//퇴직면담 초기화
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화

	//Detail 설문조사, 인수인계 작성환경
	this.ds_Detail20.addRow();	//설문조사
	this.ds_Detail30.addRow();	//인수인계

	//첨부파일정보 권한설정
