	this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));	//결재차수
	
	this.ds_Detail10.setColumn(iAddRow, "YPLB_DD", sToDay);
	
	this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();
	this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getBindCellIndex("body","YPLB_DD"), iAddRow);	
	
	
};

