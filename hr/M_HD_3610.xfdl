/***********************************************************************************************
 * 05.03. [Vaildation] Detail 저장 필수값 체크 - 그리드항목
 ***********************************************************************************************/ 
this.fn_VaildDetailSave = function()
{
	//[저장] Validation 설정
	var arrArgs = [ "YPLB_DD^NULL"				   
			   	   ,"YPLB_GU^NULL"				  
				   ,"YPLB_CT^NULL"				  
			      ]; 
				  
	var sCheckGroupTitle = this.div_DetailInput.form.tab_Detail.tpg_01.text;//체크그룹 타이틀
				  
	//Grid Vaild 체크
	if (!this.gfn_CheckGridValid(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10, arrArgs, sCheckGroupTitle) ) {
		return false;
	}		
	
	//날짜체크
	
	var strFromDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD");
	var strClosDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD");
	
	for(var i=0;i<this.ds_Detail10.getRowCount();i++)	
	{
	
		if(this.ds_Detail10.getColumn(i, "crudType") == "I") {		
			var sYplbDd = this.ds_Detail10.getColumn(i, "YPLB_DD");
					
			if(sYplbDd < strFromDd || sYplbDd >  strClosDd)
			{
				var arrMsgArg = ["연차발생기간"];
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getBindCellIndex("body","YPLB_DD"), j);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.showEditor(true);
				this.gfn_Alert("HR013", arrMsgArg); //[연차발생기간]을(를) 범위를 벗어났습니다..			
				return false;
			}
		}
	};
	
	//중복체크	
	
	for(var i=0 ; i<this.ds_Detail10.getRowCount(); i++) {
		var strFirst = this.ds_Detail10.getColumn(i, "YPLB_DD");
		
		for(var j=i+1 ; j<this.ds_Detail10.getRowCount(); j++) {
			var strNext = this.ds_Detail10.getColumn(j, "YPLB_DD");
			
			if(strFirst == strNext){
				//this.ds_Detail10.set_rowposition(j);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getBindCellIndex("body","YPLB_DD"), j);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.showEditor(true);
			   this.gfn_Alert("SM041");//중복된 데이터가 존재합니다.			   
			   return false;
			}
		}
	}
	
	return true;
}
 
