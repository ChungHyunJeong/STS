 /***********************************************************************************************
 * 15.06 [사용자정의 함수] 생성일자, 변경일자 폐기일자 넣기
 ***********************************************************************************************/ 
 this.fn_ChangeDd = function(DpahDd)
 {
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {	
		sAppoCd = this.ds_Detail10.getColumn(iLoop, "APPO_CD")
		
		var nRow = this.ds_Hr115.findRow( "CODM_CD", sAppoCd);
 		var sOp01Nm = this.ds_Hr115.getColumn(nRow, "OP01_NM") 
		
		if (sOp01Nm == '1') {
			this.ds_Detail10.setColumn(iLoop, "CREA_DD", DpahDd); //생성일자-개편일자로
		} else if (sOp01Nm == '2' || sOp01Nm == '3') {	
			this.ds_Detail10.setColumn(iLoop, "CHAN_DD", DpahDd); //변경일자-개편일자로
		} else if (sOp01Nm == '4') {
			this.ds_Detail10.setColumn(iLoop, "DISC_DD", DpahDd); //폐기일자-개편일자로
		}
	}	
 }
 
