/*---------------------------------------------------------------------------------------------
 *    01. [그리드-grd_Detail10] 셀클릭 이벤트
  ---------------------------------------------------------------------------------------------*/
this.div_DetailInput_tab_Detail_tpg_01_grd_Detail10_oncellclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	//그리드 콤보 및 캘린더 클릭시 펼치기
	obj.dropdownCombo();
	obj.dropdownCalendar();
	this.gfn_SetGridSelectRowColor(obj, e.row, true);//선택한 Row BackgroundColor 설정	- 기본그리드 설정된 CSS정보 유지
	sAppoCd = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "APPO_CD")
	sOp01Nm = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "OP01_NM")

	//조직장
	if (e.col == 13) {
	   if (sOp01Nm == '1' || sOp01Nm == '3') {
			//결재진행상태가 아닌경우
			if (!this.fvWofhStatus) { 
				var sPopupId = "B_SYST_ID_01";//팝업ID - 사용자정보
				var oArg = {  EmpmNm:""};//Argument처리
				this.gfn_UserPop(sPopupId, oArg);	
			}
		}
	//상위부서
	} else if (e.col == 16) {
		if (sOp01Nm == '1' || sOp01Nm == '3') {
			//결재진행상태가 아닌경우
			if (!this.fvWofhStatus) { 
				//상위부서는 저장을 먼저하고 그 개편번호로 등록된 BODY 내용의 부서 POPUP을 열어준다.
				//개편번호가 있으면 열어준다.
				if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DPAH_NO"))) {					
					this.fn_Call_Popup();
				}
// 				var sPopupId = "B_DEPM_CD_01";//팝업ID - 부서코드
// 				var oArg = { DepmNm:""};  // Argument처리
// 				this.gfn_DeptPop(sPopupId, oArg);
			}
		}
	//담당임원
	} else if (e.col == 10) {
	   if (sOp01Nm == '1' || sOp01Nm == '3') {
			//결재진행상태가 아닌경우
			if (!this.fvWofhStatus) { 
				var sPopupId = "B_SYST_ID_02";//팝업ID - 사용자정보
				var oArg = {  EmpmNm:""};//Argument처리
				this.gfn_UserPop(sPopupId, oArg);	
			}
		}
	}
};

/***********************************************************************************************
 * 02.DG0.07. [Detail 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	//손익
	if (e.columnid == "PALM_CD") {
		this.gfn_GetCommPalmGrid(obj, "PALM_CD", "PALM_NM");//손익명 검색
	//상위부서
	//영업담당사번
	} else if (e.columnid == "EMPM_CD1") {
		this.gfn_GetCommEmpmGrid(obj, "EMPM_CD1", "EMPM_NM1", "TMSU_ID");//사원명 검색
	//변경구분
 	} else if (e.columnid == "APPO_CD") {
		iAddRow = this.ds_Detail10.rowposition 		
	
 		sAppoCd = this.ds_Detail10.getColumn(iAddRow, "APPO_CD")
		
 		var nRow = this.ds_Hr115.findRow( "CODM_CD", sAppoCd);
 		var sOp01Nm = this.ds_Hr115.getColumn(nRow, "OP01_NM")
		
		this.ds_Detail10.setColumn(iAddRow, "OP01_NM", sOp01Nm)
		
		if (!this.gfn_IsNull(sAppoCd)) {
			if (sOp01Nm == '1') {
				this.ds_Detail10.setColumn(iAddRow, "CREA_DD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DPAH_DD")); //생성일자-개편일자로
				this.ds_Detail10.setColumn(iAddRow, "ACUM_CD", this.gfn_GetSessionValue("ACUM_CD")); //회계단위
				this.ds_Detail10.setColumn(iAddRow, "BUSM_CD", '1000000'); //사업부문
				this.ds_Detail10.setColumn(iAddRow, "PALM_CD", 'ZZZZ'); //손익코드
				this.ds_Detail10.setColumn(iAddRow, "USED_YN", '1'); //사용여부
				this.ds_Detail10.setColumn(iAddRow, "DISP_YN", '1'); //표기여부
				
				this.ds_Detail10.setColumn(iAddRow, "CHAN_DD", ""); //변경일자-null로
				this.ds_Detail10.setColumn(iAddRow, "DISC_DD", ""); //폐기일자-null로
			} else if (sOp01Nm == '2' || sOp01Nm == '3') {		
				//this.ds_Detail10.setColumn(iAddRow, "CREA_DD", ""); //생성일자-null로
				this.ds_Detail10.setColumn(iAddRow, "DISC_DD", ""); //폐기일자-null로
				this.ds_Detail10.setColumn(iAddRow, "CHAN_DD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DPAH_DD")); //변경일자-개편일자로
				this.ds_Detail10.setColumn(iAddRow, "USED_YN", '1'); //사용여부
				this.ds_Detail10.setColumn(iAddRow, "DISP_YN", '1'); //표기여부
			} else if (sOp01Nm == '4') {
				//this.ds_Detail10.setColumn(iAddRow, "CREA_DD", ""); //생성일자-null로
				this.ds_Detail10.setColumn(iAddRow, "CHAN_DD", ""); //변경일자-null로
				this.ds_Detail10.setColumn(iAddRow, "DISC_DD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DPAH_DD")); //폐기일자-개편일자로
				
				this.ds_Detail10.setColumn(iAddRow, "USED_YN", '0'); //사용여부
				this.ds_Detail10.setColumn(iAddRow, "DISP_YN", '0'); //표기여부
			}
		} else if (this.gfn_IsNull(sAppoCd)) {
			this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);
		}
	}
};

