/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 

	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}

	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		if (!this.fn_VaildMasterSave()) return;
	}
  	
  	//Master 중복데이터 확인
	this.fn_VaildMasterNewSave();
	
	for (var iLoop=0;iLoop<this.ds_Master10.getRowCount();iLoop++) {
		var sChkRowType = this.gfn_GetRowType(this.ds_Master10, iLoop);
		if (sChkRowType == "I") {
			var sMethNo = this.ds_Master10.getColumn(iLoop, "METH_NO");//회의체코드
			var sWeemMm = this.ds_Master10.getColumn(iLoop, "WEEM_MM");//회의년월
			var sWeemCt = this.ds_Master10.getColumn(iLoop, "WEEM_CT");//주차

			var iFvFindRow = this.ds_Master20.findRowExpr("METH_NO=='" + sMethNo + "' && WEEM_MM=='" + sWeemMm + "' && WEEM_CT=='" + sWeemCt + "'");//업무Key Row 찾기
			if (iFvFindRow > -1) {
				this.gfn_Alert("SM041");//중복된 데이터가 존재합니다.
				return;
			}
		}
	}
	
	//저장시 재조회 Key설정
	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "METH_NO");//조회Key01
	this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WEEM_MM");//조회Key02
	this.fvSchKey03 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WEEM_CT");//조회Key02

	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	//[공통]서비스 호출 변수
	var sInputDaset = "" //입력 데이터셋
	var sOutDatset = "";//출력 데이터셋
	var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		
		
	//[공통]서비스 정보 추가
	svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
	svcAddObj.sInsertSql = "hr:M_HD_4300_M_IN10";//저장Sql
	svcAddObj.sUpdateSql = "hr:M_HD_4300_M_UP10";//수정Sql
	svcAddObj.sDeleteSql = "hr:M_HD_4300_M_DL10";//삭제Sql
	svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
							 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

	this.gfn_AddComSaveService(this, svcAddObj);


	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "SAVE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Master10=ds_Master10:U" //입력 데이터셋
	svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
	svcCallObj.sOtherArg = sOtherArg;//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명		
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
}

/***********************************************************************************************
 * 05.02. [Vaildation] Master 저장 필수값 체크 - 입력항목 
 ***********************************************************************************************/ 
this.fn_VaildMasterSave = function()
{
	//[저장] Validation 설정
	var arrArgs = [   "METH_NO^NULL"
					 ,"WEEM_MM^NULL"
					 ,"WEEM_CT^NULL"
					 ,"FROM_DD^NULL"
					 ,"CLOS_DD^NULL"
					 ,"WEEM_DD^NULL"
				   ];
				   
	var sCheckGroupTitle = this.div_MasterInput.form.tab_Master.tpg_01.text;//체크그룹 타이틀
	
	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckGridValid(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10, arrArgs, sCheckGroupTitle)) {
		return false;
	}

	//날짜체크
	for(var iLoop=0 ; iLoop<this.ds_Master10.getRowCount(); iLoop++) {
		var sWeemMm = this.ds_Master10.getColumn(iLoop, "WEEM_MM") //회의년월

		if(sWeemMm.trim().length < 6 || sWeemMm.substr(0,4) > "2999" || sWeemMm.substr(0,4) < "1990" || sWeemMm.substr(4,2) < "01" || sWeemMm.substr(4,2) > "12"){
			this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.setCellPos(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.getBindCellIndex("body","WEEM_MM"),  iLoop);
			this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.showEditor(true);
			
			var arrMsgArg = [" 회의년월("+ sWeemMm.substr(0,4) + "-" + sWeemMm.substr(4,2) +")"];
			this.gfn_Alert("SM017", arrMsgArg); //회의년월()은 유효하지 않은 날짜 형식입니다.
			return false;
		}	
	}

	//Dataset 중복 체크
	if(!this.gfn_VaildDupDataset(this.ds_Master10, "METH_NO,WEEM_MM,WEEM_CT")){
		return false;
	}

	return true;
}

