this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{	
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	var sChkRowType = this.ds_Master10.getColumn(e.row, "crudType");
	var sWeemMm = this.ds_Master10.getColumn(e.row, "WEEM_MM");//회의년월
	var sWeemCt = this.ds_Master10.getColumn(e.row, "WEEM_CT");//주차
	var sFromDd = this.ds_Master10.getColumn(e.row, "FROM_DD");//시작일자
	var sClosDd = this.ds_Master10.getColumn(e.row, "CLOS_DD");//종료일자

	//회의년월과 주차가 전부 입력되면
	if (e.columnid == "WEEM_MM" || e.columnid == "WEEM_CT") {
		if (sChkRowType == "I" && !this.gfn_IsNull(sWeemCt) && !this.gfn_IsNull(sWeemMm)) {
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_InputSch.clearData();
			this.ds_HrCalm.clearData();

			//[업무] 조회조건 입력
			this.ds_InputSch.addRow();
			this.ds_InputSch.setColumn(0, "WEEM_MM", sWeemMm);
			this.ds_InputSch.setColumn(0, "WEEM_CT", sWeemCt);
			//this.ds_InputSch.setColumn(0, "CALM_DD", e.newvalue);
			this.fviRow = e.row;
			
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "hr:M_HD_4300_M_SL30";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DATE_CHANGE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_InputSch" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_HrCalm=ds_HrCalm";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj)
		} else {
// 			if (!(e.newvalue >= sFromDd && e.newvalue <= sClosDd)) { // 회의일자 변경시 저장된 회의주차에 맞는 일자가 아닐시 변경못하게 
// 				this.ds_Master10.setColumn(e.row, "WEEM_DD", e.oldvalue);
// 				this.ds_Master10.setColumn(e.row, "crudType", "");
// 			}
		}
	}
	
};

