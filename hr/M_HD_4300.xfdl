 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "COMBO_SEARCH":
			this.div_MasterInput.form.tab_Master.tpg_01.form.div_Search.form.cbo_MethNo.set_index(0);
			
			//[조회영역]
			//회의체
			this.ds_HrMethSch.addColumn("SORT_NO", "string");
			
 			var iAddRow = this.ds_HrMethSch.addRow();
 			this.ds_HrMethSch.setColumn(iAddRow, "METH_NM", "ALL");
 			this.ds_HrMethSch.setColumn(iAddRow, "SORT_NO", "1");
 			this.ds_HrMethSch.set_keystring("S:+SORT_NO+METH_NO");
			
			break;
		case "DATE_CHANGE":
			if (this.ds_HrCalm.getRowCount() > 0) {
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WEEM_MM", this.ds_HrCalm.getColumn(0, "CALM_YM")); //회의년월
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WEEM_CT", this.ds_HrCalm.getColumn(0, "WEEM_CT")); //주차
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", this.ds_HrCalm.getColumn(0, "FROM_DD")); //주차시작일자
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", this.ds_HrCalm.getColumn(0, "CLOS_DD")); //주차종료일자
			} else {
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WEEM_MM", null); //회의년월
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WEEM_CT", "1"); //주차
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", null); //주차시작일자
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", null); //주차종료일자
			
			}

			break;
		case "MST_SEARCH": //조회
			this.ds_Master10.addColumn("s_chk", "string");
			this.ds_Master10.addColumn("crudType", "string");
	
			if (this.ds_Master10.getRowCount() > 0) {

				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master10.findRowExpr("METH_NO=='" + this.fvSchKey01 + "' && WEEM_MM=='" + this.fvSchKey02 + "' && WEEM_CT=='" + this.fvSchKey03 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}				
				
				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
				
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10);
				
			} else {
				
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10);
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			}
			
			//저장후 재조회여부-true
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}			
	
			break;
		case "SAVE":   //저장
			this.fvSchKeyYn = true;//저장후 재조회여부
			this.fn_ComBtnSearch();//저장후 재조회

			break;	
		case "MSTS_SEARCH": //일괄생성
			this.ds_Master10.addColumn("s_chk", "string");
			this.ds_Master10.addColumn("crudType", "string");
		
 			for (var iLoop=0; iLoop<this.ds_Master10.getRowCount(); iLoop++) {
 				if(this.gfn_IsNull(this.ds_Master10.getColumn(iLoop, "COMM_CD"))){
 					this.ds_Master10.setColumn(iLoop, "crudType", "I");
 				}
 			}

			break;	
		case "MSTNEW_SEARCH": //저장시 중복체크	
			break;					
	}
};

