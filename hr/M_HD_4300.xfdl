 		this.ds_Master10.set_enableevent(false);
 	}
 
 	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
  	this.ds_Input.clearData();
  	this.ds_HrCalm.clearData();
  	this.ds_Master10.clearData();
 	
 	//[업무] 조회조건 입력
 	this.ds_Input.addRow();
 	this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_WeemDdFr.value);
 	this.ds_Input.setColumn(0, "CLOS_DD", this.div_Search.form.cal_WeemDdTo.value);
 	this.ds_Input.setColumn(0, "METH_NO", this.div_Search.form.cbo_MethNo.value);
 	
