	//[결재]결재내역 조회모드이면 팝업 불가
	if (!this.fvWfSearchMode && !this.fvWofhStatus) {

		//국가 구분
		if (e.col == 4) {
			// 2Row 이후 최초(0 Row)의 국가가 존재하는지 확인
			var iNowRow = this.ds_Detail10.rowposition;

			if (iNowRow > 0) {
