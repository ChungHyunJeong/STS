	var sWofhNm = "[출장원:해외기안]"+this.gfn_GetSessionValue("USEM_NM")+this.gfn_GetSessionValue("POSI_NM")+" ("+this.gfn_StringFormat(this.ds_Master10.getColumn(iRow, "FROM_DD"),"@@@@.@@.@@")+"~"+this.gfn_StringFormat(this.ds_Master10.getColumn(iRow, "CLOS_DD"),"@@@@.@@.@@")+")";
	
