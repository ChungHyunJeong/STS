/***********************************************************************************************
 * 5. 저장
 ***********************************************************************************************/
/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.gfn_DsIsUpdated(this.ds_Detail11) && !this.gfn_DsIsUpdated(this.ds_Detail15) && !this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}
	
	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		//첨부파일정보만 변경시
		if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.gfn_DsIsUpdated(this.ds_Detail11) && !this.gfn_DsIsUpdated(this.ds_Detail15) && this.ds_Master10.getRowCount() > 0) {
			//첨부파일정보 저장
			fvFileInfo = new Object();//[공통] 첨부파일정보
			fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, B-상세(그리드Row), E-기타
			fvFileInfo.CALLBACK_FUNC = "";//호출할 콜백함수
			fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
			fvFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO");//PK번호2-출장번호
			fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
			//첨부파일Div 화면에 Key세팅하게 변경
			this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);
			return;	
		}
	}

	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		if (!this.fn_VaildMasterSave()) return;
		
		//출장기간 중복여부 (2022-03-22 추가개발)
		if (this.fn_SetTermDuplicationCheck()) {	
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FromDd.setFocus();	//출장시작일자 포커스
			this.gfn_Alert("HR007");//해당 출장일자가 이미 존재합니다.
			return;
		}
	}

	//Detail10 출장일정상세가 1Row 이상이어야 함
	if (this.ds_Detail10.getRowCount() < 1 || this.ds_Detail10.getCaseCount("crudType!='D'") < 1) { 
		this.div_DetailInput.form.tab_Detail.set_tabindex(0); //출장일정상세 Tab으로 이동
		
		var arrMsgArg = ["출장일정상세"];
		this.gfn_Alert("SM045", arrMsgArg); //출장일정상세는 1건이 이상 존재해야 합니다.
		return false;
	}

	//Detail10 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		if (!this.fn_VaildDetail10Save()) return;
	}	

	//Detail11 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail11)) {
		if (!this.fn_VaildDetail11Save()) return;
	}	

	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	//Master 입력 상태이면
	if (sChkRowType == "I") {
	
		//채번-출장번호
		var KeyFormId = "M_HD_3100"; //출장원관리는 국내/시내, 해외의 테이블이 동일해서 국내/시내 Form ID를 사용해야 함.
		var sNewKeyNo = this.gfn_GetTbnmNo(KeyFormId, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_DD"));//프로그램ID, 기준일자
		if (this.gfn_IsNull(sNewKeyNo)) {
			//채번번호를 가져오는데 실패했습니다.
			return;
		}
		
		var sChkKeyNo = "";//채번번호-체크용
		var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position
		
		//Master 데이터셋에 채번정보 입력-출장번호
		this.ds_Master10.setColumn(iMasterRowPos, "BZTH_NO", sNewKeyNo);
		
		var sCommCd = this.ds_Master10.getColumn(iMasterRowPos, "COMM_CD");//회사코드
		var sBzthNo = this.ds_Master10.getColumn(iMasterRowPos, "BZTH_NO");//출장번호
		var sWofhSq = this.ds_Master10.getColumn(iMasterRowPos, "WOFH_SQ");//결재차수
		
		//Detail10 데이터셋에 기본정보 입력-회사코드/출장번호/결재차수
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			sChkKeyNo = this.ds_Detail10.getColumn(iLoop, "BZTH_NO");//출장번호
			
			if (this.gfn_IsNull(sChkKeyNo) || sChkKeyNo != sBzthNo) {
				this.ds_Detail10.setColumn(iLoop, "COMM_CD", sCommCd);
				this.ds_Detail10.setColumn(iLoop, "BZTH_NO", sBzthNo);
				this.ds_Detail10.setColumn(iLoop, "WOFH_SQ", sWofhSq);
			}
		}

		//Detail11 데이터셋에 기본정보 입력-회사코드/출장번호/결재차수 : ### 필터대상 포함 ###
		for (var iLoop=0; iLoop<this.ds_Detail11.getRowCountNF(); iLoop++) {
			sChkKeyNo = this.ds_Detail11.getColumnNF(iLoop, "BZTH_NO");//출장번호
			
			if (this.gfn_IsNull(sChkKeyNo) || sChkKeyNo != sBzthNo) {
				this.ds_Detail11.setColumnNF(iLoop, "COMM_CD", sCommCd);
				this.ds_Detail11.setColumnNF(iLoop, "BZTH_NO", sBzthNo);
				this.ds_Detail11.setColumnNF(iLoop, "WOFH_SQ", sWofhSq);
			}
		}
		
	}	

	//Detail10 추가/변경시
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		//Detail10 삭제건이 존재시 Master 출장비 재계산
		var iDtlDelCnt = this.ds_Detail10.getCaseCount("crudType=='D'"); //삭제건
		
		if (iDtlDelCnt > 0) { //삭제건이 0보다 크면 재계산
			//출장비 금액 계산
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BZTH_AM", this.ds_Detail11.getCaseSum("crudType!='D'", "BZTC_AM"));	//총 출장비 = (출장비 + 식사비)				
		}	
	}

	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		fvFileInfo = new Object();//[공통] 첨부파일정보
		fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
		fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
		fvFileInfo.CALLBACK_FUNC = "fn_ComBtnSearch";//호출할 콜백함수
		fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
		fvFileInfo.PK01_NO = nApp.gvCompany;//PK번호1-회사코드
		fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO");//PK번호2-출장번호
		fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수			
	}	
	
	//저장시 재조회 Key설정
	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO");//조회Key01
	this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//조회Key02

	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	//[공통]서비스 호출 변수
	var sInputDaset = "" //입력 데이터셋
	var sOutDatset = "";//출력 데이터셋
	var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		
	//1. Master 변경건이 있으면 Master 저장
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
		svcAddObj.sInsertSql = "hr:M_HD_3110_M_IN10";//저장Sql
		svcAddObj.sUpdateSql = "hr:M_HD_3110_M_UP10";//수정Sql
		svcAddObj.sDeleteSql = "hr:M_HD_3110_M_DL10";//삭제Sql
		svcAddObj.sSaveFlagColumn = "";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

		this.gfn_AddComSaveService(this, svcAddObj);
		
		sInputDaset = "ds_Master10=ds_Master10:U" //입력 데이터셋

	}

	//2. Detail10 변경건이 있으면 Detail 저장
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {

		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object	
		svcAddObj.sInsertSql = "hr:M_HD_3110_D_IN10";//저장Sql
		svcAddObj.sUpdateSql = "hr:M_HD_3110_D_UP10";//수정Sql
		svcAddObj.sDeleteSql = "hr:M_HD_3110_D_DL10";//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		
		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = "ds_Detail10=ds_Detail10:U" //입력 데이터셋
		} else {
			sInputDaset = sInputDaset + " ds_Detail10=ds_Detail10:U" //입력 데이터셋
		}

	}		

	//3. Detail11 변경건이 있으면 Detail 저장 
	if (this.gfn_DsIsUpdated(this.ds_Detail11)) {

		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object	
		svcAddObj.sInsertSql = "hr:M_HD_3110_D_IN11";//저장Sql
		svcAddObj.sUpdateSql = "hr:M_HD_3110_D_UP11";//수정Sql
		svcAddObj.sDeleteSql = "hr:M_HD_3110_D_DL11";//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		
		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = "ds_Detail11=ds_Detail11:U" //입력 데이터셋
		} else {
			sInputDaset = sInputDaset + " ds_Detail11=ds_Detail11:U" //입력 데이터셋
		}

	}	

	//4. Detail15 변경건이 있으면 Detail15 저장 - 동행인관리
	if (this.gfn_DsIsUpdated(this.ds_Detail15)) {
	
		//복사한것은 변경여부에 대한 체크가 되지 않아 강제로 변경된것으로 인식하게 함
		var sCrudType = "";
		for (var iLoop=0; iLoop<this.ds_Detail15.getRowCount(); iLoop++) {
			sCrudType = this.ds_Detail15.getColumn(iLoop, "crudType");
			
			if (!this.gfn_IsNull(sCrudType)) {
				this.ds_Detail15.setColumn(iLoop, "crudPopup", sCrudType);
			}
		}
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
		svcAddObj.sInsertSql = "hr:M_HD_3101_M_IN10";//저장Sql
		svcAddObj.sUpdateSql = "hr:M_HD_3101_M_UP10";//수정Sql
		svcAddObj.sDeleteSql = "hr:M_HD_3101_M_DL10";//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		
		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = "ds_Detail15=ds_Detail15:U" //입력 데이터셋
		} else {
			sInputDaset = sInputDaset + " ds_Detail15=ds_Detail15:U" //입력 데이터셋
		}

		this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	}
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "SAVE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = sInputDaset //입력 데이터셋
	svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
