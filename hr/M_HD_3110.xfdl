/***********************************************************************************************
 * 15.05. [처리] Detail11 산출내역, 원화금액, 외화금액, 표준금액 처리
 ***********************************************************************************************/ 
this.fn_SetCostDetailProc = function()
{
	//1. [셋팅]Detail11의 산출내역, 화폐, 적용환율, 외화금액, 원화금액, 표준금액을 변경 
	var iBztcAm = 0;    //원화금액
	var iForeAm = 0;    //외화금액
	var iBztmAm = 0;    //표준금액
	var iFvFindRow = 0; //표준경비 컬럼위치
	var sBztmCd = "";   //비용항목
	var iBztbQt = 0;    //수량(일수)
	var sCurrCd = "";	//화폐
	var sBztbNm = "";   //산출내역
	var sBztbNmStay = ""; //산출내역 숙박비
	var sCurrCdMst = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");  //화폐
	var sBzthNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition,"BZTH_NO");	//출장번호 (신규여부 체크)
	var iExchRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");  //환율(등록일자 기준)
	var sSuppCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SUPP_CD");  //지원사항 구분
	var iNghtQt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "NGHT_QT");  //일수(박)
	var iDaysQt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DAYS_QT");  //일수(일)
	var sNatmCd = this.ds_Detail10.getColumn(0, "NATM_CD");  //국가코드
	var sBzamGu = this.ds_Detail10.getColumn(0, "BZAM_GU");  //급지구분
	var iStayYn = this.ds_Detail10.getCaseCount("STAY_YN == '1'");  //기내숙박여부 일수 (체크된 대상)

	this.ds_Detail11.set_enableevent(false); //row 생성속도 향상을 위한 이벤트 false
	
	//입력/복사 상태 or 출장일정상세(Detail10)의 지급기준 "국가코드" 선택여부 확인
	if (!this.gfn_IsNull(sNatmCd)) { //국가코드 선택여부 확인 시작
		
		//2. [생성]Detail11이 없을 시 총 4Row 출장비상세 생성 (HR355 공통코드 기준) : 11/08 추가1Row - 식사비추가
		var iRowCount = this.ds_Detail11.getRowCount();	//Detail11 Row건수 (최초인지, 이미 존재하는지 확인)
		
		if (iRowCount == 0) {	//출장비상세(Detail11) 최초의 경우
			var sBztbDd   = this.ds_Detail10.getColumn(0, "BZTB_DD"); //최초 출발일자			
			
			//HR355 건수만큼 생성(11/8 싯점 총 4Row)
			for (var iLoop=0;iLoop<this.ds_Hr355.getRowCount();iLoop++) {
				sBztmCd = this.ds_Hr355.getColumn(iLoop, "CODM_CD");  //비용항목
				//수량(일수)
				if (sBztmCd == '0020') { //숙박비 일수- 박, 그외 일수 - 일
					iBztbQt = iNghtQt - iStayYn;	//일수(박) - 기내 숙박일수 제외(2022-04-04 추가)
					if (iBztbQt < 0) iBztbQt = 0;	//일수가 0보다 작을 경우 0으로 셋팅
				} else {
					iBztbQt = iDaysQt; //일수(일)
				}

				var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail11);//crud_flag 변경-입력(I)

				this.ds_Detail11.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
				this.ds_Detail11.setColumn(iAddRow, "BZTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO"));
				this.ds_Detail11.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
				this.ds_Detail11.setColumn(iAddRow, "BZTB_DD", sBztbDd);                                  //출발일자(최초 출발일자)
				this.ds_Detail11.setColumn(iAddRow, "BZTB_SE", "1");                                      //출장순번(최초 출장순번 = 1)
				this.ds_Detail11.setColumn(iAddRow, "BZTC_SE", iLoop+1);                                  //비용순번
				this.ds_Detail11.setColumn(iAddRow, "BZTM_CD", sBztmCd);                                  //비용항목
				this.ds_Detail11.setColumn(iAddRow, "BZTD_CD", sBzamGu);                                  //비용상세항목 (급지구분)
				this.ds_Detail11.setColumn(iAddRow, "BZTB_QT", iBztbQt);                                  //수량(일수)
				this.ds_Detail11.setColumn(iAddRow, "crudType", "I");                                     //crudType
			}
		} else {	//출장비상세(Detail11) 이미 존재하는 경우
			this.ds_Detail11.filter("");	//지원사항 구분에 의해 대상을 선정하기 위해 필터 해제
			//일,박 수정
			for (var iLoop=0;iLoop<this.ds_Detail11.getRowCount();iLoop++) {
				sBztmCd = this.ds_Detail11.getColumn(iLoop, "BZTM_CD");  //비용항목
				
				//수량(일수)
				if (sBztmCd == '0020') { //숙박비 일수- 박, 그외 일수 - 일
					iBztbQt = iNghtQt - iStayYn;	//일수(박) - 기내 숙박일수 제외(2022-04-04 추가)
					if (iBztbQt < 0) iBztbQt = 0;	//일수가 0보다 작을 경우 0으로 셋팅
				} else {
					iBztbQt = iDaysQt; //일수(일)
				}

				this.ds_Detail11.setColumn(iLoop, "BZTB_QT", iBztbQt);  //수량(일수)
				
				if (this.gfn_IsNull(sBzthNo)) {	//신규
					this.ds_Detail11.setColumn(iLoop, "crudType","I");  //crudType
				} else {	//기존
					this.ds_Detail11.setColumn(iLoop, "crudType","U");  //crudType
				}

			}
		}
		
		//3. [계산]Detail11 값 계산
		for (var iLoop=0;iLoop<this.ds_Detail11.getRowCount();iLoop++) {
			// 3-1. 출장표준경비(필터조건 : 비용항목 / 구역) 
			sBztmCd = this.ds_Detail11.getColumn(iLoop, "BZTM_CD");  //비용항목
			iBztbQt = this.ds_Detail11.getColumn(iLoop, "BZTB_QT");  //수량(일수)
			
			iFvFindRow = this.ds_HrBztm.findRowExpr("BZTM_CD=='" + sBztmCd + "' && BZTD_CD=='" + sBzamGu + "'");//화폐, 표준금액 찾기
			sCurrCd = this.gfn_Nvl(this.ds_HrBztm.getColumn(iFvFindRow, "CURR_CD"), sCurrCdMst);  //화폐
			iBztmAm = this.gfn_Nvl(this.ds_HrBztm.getColumn(iFvFindRow, "BZTM_AM"), 0);  //표준금액

			sBztbNm = this.gfn_AppendComma(iBztmAm,0) + "(금액) * " + this.gfn_AppendComma(iBztbQt,0) + "(일수)"; //[산출내역] 기본값
			iBztmAm = nexacro.toNumber(iBztmAm) * nexacro.toNumber(iBztbQt); //총 [표준금액] = 표준금액 * 수량(일수)

			// 3-2. 지원구분별 [산출내역] 내용 ### 항목(숙박비) 설명추가 ###
			if (sSuppCd != '9999') {	//[지원구분] "없음"이 아닌 [항목]이 "숙박비"의 경우 [산출내역] 정보추가
				iFvFindRow = this.ds_Hr367.findRowExpr("CODM_CD=='" + sSuppCd + "'");//지원사항 구분코드 (산출내역 추가내용)
				sBztbNmStay = sBztbNm + " / (" + this.ds_Hr367.getColumn(iFvFindRow, "CODM_NM") + ")제공으로 인한 숙박비 금액 : 0";  //[산출내역] 숙박비
			}


			// 3-3. 화폐(KRW / USD) 구분별 원화금액, 외화금액 계산
			if (sCurrCd == '0090') {	//KRW(원화)
				iBztcAm = iBztmAm;								//원화금액 = 총 표준금액
				iForeAm = 0; 									//외화금액 = 0원
			} else {	//USD(외화)
				iBztcAm = nexacro.floor(iBztmAm * nexacro.toNumber(iExchRt), 0);  //원화금액 = 총 표준금액 * 환율
				iForeAm = iBztmAm;								//외화금액 = 총 표준금액
			}

			//3-4. 항목이 출장비의 경우 Master에 해당출장비 저장 / 출장비 화폐 KRW(원화) + 적용환욜 1
			if (sSuppCd == '0060' &&  sBztmCd == '0090') { //지원사항 = 장기출장(2주이상), 항목 = 출장비(0090)
				this.ds_Detail11.setColumn(iLoop, "EXCH_RT", "1"); //적용환율
			} else {
				this.ds_Detail11.setColumn(iLoop, "EXCH_RT", iExchRt); //적용환율
			}
			
			// 3-5. Detail11에 넣기
			if (sSuppCd != '9999' && sBztmCd == '0020') { //지원구분이 "없음"이 아니고, 항목이 "숙박비"의 경우
				if (sSuppCd == '0060') {	//장기출장(2주이상)은 산출내역 정보추가 불필요
					this.ds_Detail11.setColumn(iLoop, "BZTB_NM", sBztbNm); //산출내역
				} else {
					this.ds_Detail11.setColumn(iLoop, "BZTB_NM", sBztbNmStay); //산출내역 (숙박비)
				}
				this.ds_Detail11.setColumn(iLoop, "BZTC_AM", "0"); //원화금액 (0원으로 초기화 11/08)
				this.ds_Detail11.setColumn(iLoop, "FORE_AM", "0"); //외화금액 (0원으로 초기화 11/08)
			} else {
				this.ds_Detail11.setColumn(iLoop, "BZTB_NM", sBztbNm); //산출내역
				this.ds_Detail11.setColumn(iLoop, "BZTC_AM", iBztcAm); //원화금액
				this.ds_Detail11.setColumn(iLoop, "FORE_AM", iForeAm); //외화금액
			}
			this.ds_Detail11.setColumn(iLoop, "CURR_CD", sCurrCd); //화폐
			this.ds_Detail11.setColumn(iLoop, "BZTM_AM", iBztmAm); //표준금액


			// 3-6. [상태]Detail11에 변경값이 있으면 crudType 변경
			var crudType = this.ds_Detail11.getColumn(iLoop, "crudType");  //crudType
			var sOrgBztbNm = this.ds_Detail11.getOrgColumn(iLoop, "BZTB_NM");  //이전 산출내역
			sBztbNm = this.ds_Detail11.getColumn(iLoop, "BZTB_NM");  //현재 산출내역

			if ((sOrgBztbNm != sBztbNm)) {
				if (this.gfn_IsNull(sBzthNo)) {	//신규
					this.ds_Detail11.setColumn(iLoop, "crudType","I");  //crudType
				} else {	//기존
					this.ds_Detail11.setColumn(iLoop, "crudType","U");  //crudType
				}
			} 
		}

		
	} //국가코드 선택여부 확인 끝

	//4. [그리드 보이기]지원사항 조건에 따른 출장비 산출내역 대상 표기 : 필터처리
	//4-1 지원사항 = 장기출장(2주이상)의 경우 : 출장비, 식사비
	if (sSuppCd == '0060') { //지원사항 = 장기출장(2주이상)
		//출장비/식사비 활성화(DELE_YN = 0), 숙박비/잡비 삭제(DELE_YN = 1)
		for (var iLoop=0;iLoop<this.ds_Detail11.getRowCount();iLoop++) {
			sBztmCd = this.ds_Detail11.getColumn(iLoop,"BZTM_CD");
			if (sBztmCd == '0030' || sBztmCd == '0090') {	//활성화 : 출장비, 식사비, 삭제 : 숙박비, 잡비
				this.ds_Detail11.setColumn(iLoop, "DELE_YN","0");  //DELE_YN = 0 (활성화)
			} else {
				this.ds_Detail11.setColumn(iLoop, "DELE_YN","1");  //DELE_YN = 1 (삭제)
				this.ds_Detail11.setColumn(iLoop, "BZTB_NM","");   //산출내역
				this.ds_Detail11.setColumn(iLoop, "BZTC_AM","0");  //원화금액 0원처리
				this.ds_Detail11.setColumn(iLoop, "FORE_AM","0");  //외화금액 0원처리
				this.ds_Detail11.setColumn(iLoop, "BZTM_AM","0");  //표준금액 0원처리
			}			

			if (this.gfn_IsNull(sBzthNo)) {	//신규
				this.ds_Detail11.setColumn(iLoop, "crudType","I");  //crudType
			} else {	//기존
				this.ds_Detail11.setColumn(iLoop, "crudType","U");  //crudType
			}

		}
	
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BZTH_AM", this.ds_Detail11.getCaseSum("crudType!='D'", "BZTC_AM"));	//총 출장비 = (출장비 + 식사비)				
		this.ds_Detail11.filter("BZTM_CD == '0030' || BZTM_CD == '0090'");	//출장비, 식사비			
		
	//4-2 지원사항 = 그외 : 숙박비, 식사비
	} else { 
		//숙박비/잡비활성화(DELE_YN = 0), 출장비/식사비 삭제(DELE_YN = 1)
		for (var iLoop=0;iLoop<this.ds_Detail11.getRowCount();iLoop++) {
			sBztmCd = this.ds_Detail11.getColumn(iLoop,"BZTM_CD");
			if (sBztmCd == '0020' || sBztmCd == '0040') {	//활성화 : 숙박비, 잡비, 삭제 : 출장비, 식사비
				this.ds_Detail11.setColumn(iLoop, "DELE_YN","0");  //DELE_YN = 0 (활성화)
			} else {
				this.ds_Detail11.setColumn(iLoop, "DELE_YN","1");  //DELE_YN = 1 (삭제)
				this.ds_Detail11.setColumn(iLoop, "BZTB_NM","");   //산출내역
				this.ds_Detail11.setColumn(iLoop, "BZTC_AM","0");  //원화금액 0원처리
				this.ds_Detail11.setColumn(iLoop, "FORE_AM","0");  //외화금액 0원처리
				this.ds_Detail11.setColumn(iLoop, "BZTM_AM","0");  //표준금액 0원처리
			}			

			if (this.gfn_IsNull(sBzthNo)) {	//신규
				this.ds_Detail11.setColumn(iLoop, "crudType","I");  //crudType
			} else {	//기존
				this.ds_Detail11.setColumn(iLoop, "crudType","U");  //crudType
			}

		}
	
		//Mater10 출장비 초기화
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BZTH_AM", this.ds_Detail11.getCaseSum("crudType!='D'", "BZTC_AM"));	//총 출장비 = (출장비 + 식사비)				
		this.ds_Detail11.filter("BZTM_CD == '0020' || BZTM_CD == '0040'");	//숙박비, 잡비
		
	}				

	this.ds_Detail11.set_enableevent(true); //row 생성속도 향상을 위한 이벤트 ture

};		


