/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{

	if (e.newrow > -1) {
		//[결재] 결재상태에 따른 컴포넌트 제어			
		this.fn_SetWfCompEnable();	
		

		//출장번호정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO"))) {
			//출장표준경비 조회 - 출장자에 따라 변경
			this.fn_BztmMaxSearch();	
			
			//필수체크(연구(0050):연구프로젝트번호, 제조(0010):계약번호)
			var sSctrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SCTR_CD"); //전표손익제어(FA012)
			if (sSctrCd == '0010') { //판매(0040) --> 제조(0010) : 2022-04-25 변경
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 8, "cssclass", "essential");
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 11, "cssclass", "");
			} else if (sSctrCd == '0050') { //연구(0050)
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 8, "cssclass", "");
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 11, "cssclass", "essential");
			} else {
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 8, "cssclass", "");
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("head", 11, "cssclass", "");
			}

			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Detail11.clearData();
			this.ds_Detail11.filter("");
			this.ds_Detail15.clearData(); //동행인
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "BZTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO"));
			this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
			this.ds_Input.setColumn(0, "PRGM_ID", this.fvComPrgmId);

			//[공통]서비스정보7 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
