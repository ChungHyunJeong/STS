/***********************************************************************************************
 * 02.DG0.07. [Detail 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)

	var iNowRow  = this.ds_Detail10.rowposition;
	//구분(급지) 변경시
	if (e.columnid == "BZAM_GU") {
		
		//선택한 Row 이하에 국가, 구분(급지)를 모두 변경
		for (var iLoop=iNowRow;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
			this.ds_Detail10.setColumn(iLoop, "NATM_CD", this.ds_Detail10.getColumn(iNowRow, "NATM_CD"));  //국가코드
			this.ds_Detail10.setColumn(iLoop, "NATM_NM", this.ds_Detail10.getColumn(iNowRow, "NATM_NM"));  //국가명
			this.ds_Detail10.setColumn(iLoop, "BZAM_GU", this.ds_Detail10.getColumn(iNowRow, "BZAM_GU"));  //구분(급지)		
		}
		
		//Detail11 생성 및 처리
		if (iNowRow == 0) {	//Detail10 1Row 변경시에만 처리

			this.fn_SetCostDetailProc(); //Detail11 산출내역, 금액관련 처리

		}

	//출장일자
	} else if (e.columnid == "BZTB_DD") {		
		var fromDate = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD"); //출장기간(시작일자)
		var closDate = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD"); //출장기간(종료일자)		
		var sBztbDd  = this.ds_Detail10.getColumn(iNowRow,"BZTB_DD"); //출발일자		

		//1. 1Row는 출발일자와 출장기간 시작일자는 동일해야 함
		if (iNowRow == 0 && sBztbDd != fromDate) {
			this.gfn_Alert("HR016");//최초 출발일자는 출장시작일자와 동일해야 합니다.
			
			this.ds_Detail10.setColumn(iNowRow, "BZTB_DD", fromDate); //출발일자
			this.ds_Detail10.setColumn(iNowRow, "ARRI_DD", fromDate); //도착일자
					
			return false;
		//2. 출발일자가 출장기간 시작일자보다 작은지 여부 체크
		} else if (sBztbDd < fromDate) {
			var arrMsgArg = ["출발일자","출장시작일자"];	//메시지내용
			this.gfn_Alert("HR017", arrMsgArg); //출발일자가 출장시작일자보다 작습니다.
		
			this.ds_Detail10.setColumn(iNowRow, "BZTB_DD", ""); //출발일자 초기화
			this.ds_Detail10.setColumn(iNowRow, "ARRI_DD", ""); //도착일자 초기화

			return false;
			
		//3. 출발일자가 출장기간 도착일자보다 큰지 여부 체크
		} else if (sBztbDd > closDate) {
			var arrMsgArg = ["출발일자","출장종료일자"];	//메시지내용
			this.gfn_Alert("HR018", arrMsgArg); //출발일자가 출장종료일자보다 큽니다.
			
			this.ds_Detail10.setColumn(iNowRow, "BZTB_DD", ""); //출발일자 초기화
			this.ds_Detail10.setColumn(iNowRow, "ARRI_DD", ""); //도착일자 초기화

			return false;
					
		//4. 출발일자 변경시 도착일자도 동일한 일자로 변경함
		} else {
			this.ds_Detail10.setColumn(iNowRow, "ARRI_DD", sBztbDd);  //도착일자 = 출발일자
		
		}

	//도착일자
	} else 	if (e.columnid == "ARRI_DD") { 
		var fromDate = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD"); //출장기간(시작일자)
		var closDate = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD"); //출장기간(종료일자)		
		var sAppiDd  = this.ds_Detail10.getColumn(iNowRow,"ARRI_DD"); //도착일자		

		//1. 출발일자가 출장기간 시작일자보다 작은지 여부 체크
		if (sAppiDd < fromDate) {
			var arrMsgArg = ["도착일자","출장시작일자"];	//메시지내용
			this.gfn_Alert("HR017", arrMsgArg); //도착일자가 출장시작일자보다 작습니다.
			
			this.ds_Detail10.setColumn(iNowRow, "ARRI_DD", ""); //도착일자 초기화
			
			return false;
			
		//2. 출발일자가 출장기간 도착일자보다 큰지 여부 체크
		} else if (sAppiDd > closDate) {
			var arrMsgArg = ["도착일자","출장종료일자"];	//메시지내용
			this.gfn_Alert("HR018", arrMsgArg); //도착일자가 출장종료일자보다 큽니다.

			this.ds_Detail10.setColumn(iNowRow, "ARRI_DD", ""); //도착일자 초기화

			return false;
		}
		
	//계약번호
	} else if (e.columnid == "CNTH_NO") {
		this.gfn_GetCommCnthGrid(obj, "CNTH_NO", "CNTH_NM");//계약명 검색		

		this.ds_Detail10.set_enableevent(false); // 이벤트 false
	
		//Detail10의 1Row 이후에 계약코드 저장
		var sCnthNo  = this.ds_Detail10.getColumn(iNowRow,"CNTH_NO"); //계약코드
		var sCnthNm  = this.ds_Detail10.getColumn(iNowRow,"CNTH_NM"); //계약명
		
		for (var iLoop=1;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
			this.ds_Detail10.setColumn(iLoop, "CNTH_NO", sCnthNo); //계약코드
			this.ds_Detail10.setColumn(iLoop, "CNTH_NM", sCnthNm); //계약명
		}

		this.ds_Detail10.set_enableevent(true); // 이벤트 true
		
	//연구프로젝트번호
	} else if (e.columnid == "RPJH_NO") {
		this.gfn_GetCommRpjhGrid(obj, "RPJH_NO", "RPJH_NM");//연구프로젝트명 검색		

		this.ds_Detail10.set_enableevent(false); // 이벤트 false
	
		//Detail10의 1Row 이후에 연구프로젝트코드 저장
		var sRpjhNo  = this.ds_Detail10.getColumn(iNowRow,"RPJH_NO"); //연구프로젝트코드
		var sRpjhNm  = this.ds_Detail10.getColumn(iNowRow,"RPJH_NM"); //연구프로젝트명
		
		for (var iLoop=1;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
			this.ds_Detail10.setColumn(iLoop, "RPJH_NO", sRpjhNo); //연구프로젝트코드
			this.ds_Detail10.setColumn(iLoop, "RPJH_NM", sRpjhNm); //연구프로젝트명
		}

		this.ds_Detail10.set_enableevent(true); // 이벤트 true

	//기내숙박여부
	} else if (e.columnid == "STAY_YN") {
		
		this.fn_SetCostDetailProc(); //Detail11 산출내역, 금액관련 처리

	//체크시 해당 프로그램 전체선택
	} else if (e.columnid == "s_chk") {	
		var chkValue = this.ds_Detail10.getColumn(iNowRow, "s_chk");  //Check(1), UnCheck(0)

		if (chkValue == '0') {
			for(var iLoop=0; iLoop<this.ds_Detail11.getRowCount(); iLoop++) {
				this.ds_Detail11.setColumn(iLoop,"crudType", "");
			}
			this.ds_Master10.setColumn(this.ds_Master10.rowposition,"BZTH_AM", this.ds_Detail11.getCaseSum("crudType!='D'", "BZTC_AM"));	//총 출장비 = 출장비+식사비
		}
	}
	
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Detail11] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail11_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	var iNowRow = this.ds_Detail11.rowposition;
	
	//원화금액과 표준금액 비교 (
	if (e.columnid == "BZTC_AM") {
		var iBztcAm = this.ds_Detail11.getColumn(iNowRow,"BZTC_AM"); //원화금액
		var iBztmAm = this.ds_Detail11.getColumn(iNowRow,"BZTM_AM"); //표준금액
		var sBztmCd = this.ds_Detail11.getColumn(iNowRow,"BZTM_CD"); //항목
		var sSuppCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SUPP_CD");  //지원사항 구분

		if (sSuppCd == '0060' &&  sBztmCd == '0090') { //지원사항 = 장기출장(2주이상), 항목 = 출장비(0090)
			//1. 표준금액 미존재시
			if (this.gfn_IsNull(iBztmAm)) {
				this.ds_Detail11.setColumn(iNowRow, "BZTC_AM","");	//원화금액 초기화
				this.ds_Master10.setColumn(this.ds_Master10.rowposition,"BZTH_AM", this.ds_Detail11.getCaseSum("crudType!='D'", "BZTC_AM"));	//총 출장비 = 출장비+식사비
				
				var arrMsgArg = ["표준금액"];	//메시지내용
				this.gfn_Alert("SM028", arrMsgArg); //해당컬럼 표준금액이 존재하지 않습니다.
				
				return false;

			//2. 원화금액 미존재시 메시지 후 표준금액
			} else if (this.gfn_IsNull(iBztcAm)) {
				this.ds_Detail11.setColumn(iNowRow, "BZTC_AM",iBztmAm);	//원화금액 = 표준금액 변경
				
				this.ds_Master10.setColumn(this.ds_Master10.rowposition,"BZTH_AM", this.ds_Detail11.getCaseSum("crudType!='D'", "BZTC_AM"));	//총 출장비 = 출장비+식사비
				var arrMsgArg = ["원화금액"];	//메시지내용
				this.gfn_Alert("SM008", arrMsgArg); //원화금액은 필수입력 항목입니다.
				
				return false;

			//3. 원화금액 0원 작거나 같은 경우 메시지 후 표준금액
			} else if (iBztcAm <= 0) {
				this.ds_Detail11.setColumn(iNowRow, "BZTC_AM",iBztmAm);	//원화금액 = 표준금액 변경
				this.ds_Master10.setColumn(this.ds_Master10.rowposition,"BZTH_AM", this.ds_Detail11.getCaseSum("crudType!='D'", "BZTC_AM"));	//총 출장비 = 출장비+식사비
				
				var arrMsgArg = ["원화금액"];	//메시지내용
				this.gfn_Alert("SM055", arrMsgArg); //원화금액을 1이상 입력해 주세요
				
				return false; 			

			//4. 원화금액 > 표준금액 큰 경우 메시지 후 표준금액
			} else if (iBztcAm > iBztmAm) {	//원화금액 > 표준금액 조건
				this.ds_Detail11.setColumn(iNowRow, "BZTC_AM",iBztmAm);	//원화금액 = 표준금액 변경
				this.ds_Master10.setColumn(this.ds_Master10.rowposition,"BZTH_AM", this.ds_Detail11.getCaseSum("crudType!='D'", "BZTC_AM"));	//총 출장비 = 출장비+식사비
				
				var arrMsgArg = ["원화금액", "표준금액("+this.gfn_AppendComma(iBztmAm,0)+")"];	//메시지내용 (항목제목, 표준금액)
				this.gfn_Alert("HR018", arrMsgArg); //원화금액은 표준금액(0,000)보다 작아야 합니다.

				return false;
				
			//4. 항목이 출장비의 경우 Master에 해당출장비 저장
			} else {
				this.ds_Master10.setColumn(this.ds_Master10.rowposition,"BZTH_AM", this.ds_Detail11.getCaseSum("crudType!='D'", "BZTC_AM"));	//총 출장비 = 출장비+식사비
		    }
		}
	
	//외화금액과 표준금액 비교
	} else if (e.columnid == "FORE_AM") {
		var iForeAm = this.ds_Detail11.getColumn(iNowRow,"FORE_AM"); //외화금액
		var iBztmAm = this.ds_Detail11.getColumn(iNowRow,"BZTM_AM"); //표준금액
		var iExchRt = this.ds_Detail11.getColumn(iNowRow,"EXCH_RT"); //환율
		var sBztmCd = this.ds_Detail11.getColumn(iNowRow,"BZTM_CD"); //항목

		//1. 표준금액 미존재시
		if (this.gfn_IsNull(iBztmAm)) {
			this.ds_Detail11.setColumn(iNowRow, "FORE_AM","");	//외화금액 초기화

			this.ds_Detail11.setColumn(iNowRow,"BZTC_AM",0); //원화금액
			this.ds_Master10.setColumn(this.ds_Master10.rowposition,"BZTH_AM", this.ds_Detail11.getCaseSum("crudType!='D'", "BZTC_AM"));	//총 출장비 = 출장비+식사비

			var arrMsgArg = ["표준금액"];	//메시지내용
			this.gfn_Alert("SM028", arrMsgArg); //해당컬럼 표준금액이 존재하지 않습니다.
			
			return false;

		//2. 외화금액 미존재시 메시지 후 표준금액
		} else if (this.gfn_IsNull(iForeAm)) {
			this.ds_Detail11.setColumn(iNowRow, "FORE_AM",iBztmAm);	//외화금액 = 표준금액 변경

			var iWoneAm = nexacro.floor(iExchRt * iBztmAm, 0);	//원화금액 = 환율 * 외화금액
			this.ds_Detail11.setColumn(iNowRow,"BZTC_AM",iWoneAm); //원화금액
			this.ds_Master10.setColumn(this.ds_Master10.rowposition,"BZTH_AM", this.ds_Detail11.getCaseSum("crudType!='D'", "BZTC_AM"));	//총 출장비 = 출장비+식사비

			var arrMsgArg = ["외화금액"];	//메시지내용
			this.gfn_Alert("SM008", arrMsgArg); //외화금액은 필수입력 항목입니다.
			
			return false;

		//3. 외화금액 0원 작거나 같은 경우 메시지 후 표준금액 (식사비는 2022-05-24 제외)
		} else if (iForeAm <= 0 && sBztmCd != '0030') {
			this.ds_Detail11.setColumn(iNowRow, "FORE_AM",iBztmAm);	//외화금액 = 표준금액 변경

			var iWoneAm = nexacro.floor(iExchRt * iBztmAm, 0);	//원화금액 = 환율 * 외화금액
			this.ds_Detail11.setColumn(iNowRow,"BZTC_AM",iWoneAm); //원화금액
			this.ds_Master10.setColumn(this.ds_Master10.rowposition,"BZTH_AM", this.ds_Detail11.getCaseSum("crudType!='D'", "BZTC_AM"));	//총 출장비 = 출장비+식사비

			var arrMsgArg = ["외화금액"];	//메시지내용
			this.gfn_Alert("SM055", arrMsgArg); //원화금액을 1이상 입력해 주세요
			
			return false; 			
		
		//4. 외화금액 > 표준금액 큰 경우 메시지 후 표준금액
		} else if (iForeAm > iBztmAm) {	//외화금액 > 표준금액 조건
			this.ds_Detail11.setColumn(iNowRow, "FORE_AM",iBztmAm);	//원화금액 = 표준금액 변경
			
			var iWoneAm = nexacro.floor(iExchRt * iBztmAm, 0);	//원화금액 = 환율 * 외화금액
			this.ds_Detail11.setColumn(iNowRow,"BZTC_AM",iWoneAm); //원화금액
			this.ds_Master10.setColumn(this.ds_Master10.rowposition,"BZTH_AM", this.ds_Detail11.getCaseSum("crudType!='D'", "BZTC_AM"));	//총 출장비 = 출장비+식사비
			
			var arrMsgArg = ["외화금액", "표준금액("+this.gfn_AppendComma(iBztmAm,0)+")"];	//메시지내용 (항목제목, 표준금액)
			this.gfn_Alert("HR018", arrMsgArg); //외화금액은 표준금액(0,000원)보다 작아야 합니다.
			
			return false;
		
		//5. 외화 정상입력시 원화금액 계산(소수점 버림 11/8)
		} else {
			if (!this.gfn_IsNull(iExchRt)) {	//환율이 존재하는 경우
				var iWoneAm = nexacro.floor(iExchRt * iForeAm, 0);	//원화금액 = 환율 * 외화금액
				this.ds_Detail11.setColumn(iNowRow,"BZTC_AM",iWoneAm); //원화금액
				this.ds_Master10.setColumn(this.ds_Master10.rowposition,"BZTH_AM", this.ds_Detail11.getCaseSum("crudType!='D'", "BZTC_AM"));	//총 출장비 = 출장비+식사비
			}
		}
	}
};


