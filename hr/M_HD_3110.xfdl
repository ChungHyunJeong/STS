			var oArg = { 
						  PrgmId:this.fvComPrgmId		                                                //부모프로그램ID
						}; //Popup화면 전달 파라메터 
			var oOption = {title:sPopupTitle, width:"800", height:"500"}; //Popup화면 옵션
			var sPopupCallBack = "fn_PopupCallback"; //Popup화면 호출후 Callback 처리 함수
			this.gfn_OpenPopup(sPopupId, sPopupFormUrl, oArg, sPopupCallBack, oOption); //Popup화면 호출 함수	




		//계약번호
		} else if (e.col == 18) {		
			if (this.ds_Detail10.rowposition == 0) {	//첫줄
				var sPopupId = "B_CNTH_NO_01";//팝업ID - 계약정보
				var oArg = { CnthNo:"", PrgmId:this.fvComPrgmId };//Argument처리
				this.gfn_ContPop(sPopupId, oArg);
			}
			
		//연구프로젝트
		} else if (e.col == 21) {		
			if (this.ds_Detail10.rowposition == 0) {	//첫줄
				var sPopupId = "B_RPJH_NO_01";//팝업ID - 연구프로젝트정보
				var oArg = { RpjhNo:"", PrgmId:this.fvComPrgmId };//Argument처리
				this.gfn_RpjhPop(sPopupId, oArg);	
			}			
		}
	}

	//동행인 관리	
	if (e.col == 22) {		
		if (this.ds_Detail10.rowposition == 0) {	//첫줄
			if (this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition) == 'I') { //신규일 경우 먼저 저장 후 동행인 입력		
				this.gfn_Alert("HR025");//먼저 저장하신 후 동행인을 입력해 주세요.
				return false;	
				
			} else {
				var sPopupId = "M_HD_3100_02";//팝업ID
				var sPopupTitle = "동행인관리"; //Popup화면 타이틀
				var sPopupFormUrl  = "hr::" + sPopupId + ".xfdl"; //Popup화면 URL
				var oArg = { 
							  paramBzthNo:this.ds_Detail10.getColumn(this.ds_Detail10.rowposition,"BZTH_NO")		//출장번호
							, paramWofhSq:this.ds_Detail10.getColumn(this.ds_Detail10.rowposition,"WOFH_SQ")		//결재차수
							, paramBztbDd:this.ds_Detail10.getColumn(this.ds_Detail10.rowposition,"BZTB_DD")		//출장일자
							, paramBztbSe:this.ds_Detail10.getColumn(this.ds_Detail10.rowposition,"BZTB_SE")		//출장순번
							, paramOwnerMaxCount:this.ds_Detail15.getRowCount()										//Detail12 총건수 
							, paramDataset:this.ds_Detail15.saveXML()												//Detail12 데이터셋
							, paramParamPrgmId:this.fvComPrgmId		                                                //부모프로그램명
							, paramWfSearchMode:this.fvWfSearchMode                                                 //[결재]결재내역 조회모드
							, paramWofhStatus:this.fvWofhStatus                                                     //[결재]결재상태
							}; //Popup화면 전달 파라메터 
				var oOption = {title:sPopupTitle, width:"800", height:"500"}; //Popup화면 옵션
				var sPopupCallBack = "fn_PopupCallback"; //Popup화면 호출후 Callback 처리 함수
