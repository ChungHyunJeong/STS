/***********************************************************************************************
 * 02.DG0.02. [Detail그리드-버튼] 행추가 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd01AddRow = function()
{

	//1. 초기값 
	var fromDate = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD"); //출장기간(시작일자)
	var closDate = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD"); //출장기간(종료일자)
	var iNghtQt  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "NGHT_QT"); //박
	var iDaysQt  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DAYS_QT"); //일
	var sLastSeq = this.ds_Detail10.getMax("MAXB_SE"); //최종 출장순번

	//출장기간 입력여부 체크
	if (this.gfn_IsNull(fromDate) || this.gfn_IsNull(closDate)) {
		var arrMsgArg = ["출장기간"];	//메시지내용
		this.gfn_Alert("SM040", arrMsgArg); //출장기간을 선택해주세요.
		return false;

	}	
	
	//순번
	if (this.gfn_IsNull(sLastSeq)) { //null의 경우
		sLastSeq = "1";			
	} else {
		sLastSeq = nexacro.toNumber(sLastSeq)+1;  			
	}
	
	var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)
	
	//2. 추가된 출장일자로 생성
	this.ds_Detail10.set_enableevent(false); // 이벤트 false
	
	this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
	this.ds_Detail10.setColumn(iAddRow, "BZTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO"));
	this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
	this.ds_Detail10.setColumn(iAddRow, "PJTH_NO", this.ds_Detail10.getColumn(0, "PJTH_NO"));
	this.ds_Detail10.setColumn(iAddRow, "PJTB_SE", this.ds_Detail10.getColumn(0, "PJTB_SE"));
	this.ds_Detail10.setColumn(iAddRow, "CNTH_NO", this.ds_Detail10.getColumn(0, "CNTH_NO"));
	this.ds_Detail10.setColumn(iAddRow, "CNTH_NM", this.ds_Detail10.getColumn(0, "CNTH_NM"));
	this.ds_Detail10.setColumn(iAddRow, "RPJH_NO", this.ds_Detail10.getColumn(0, "RPJH_NO"));
	this.ds_Detail10.setColumn(iAddRow, "RPJH_NM", this.ds_Detail10.getColumn(0, "RPJH_NM"));
	this.ds_Detail10.setColumn(iAddRow, "BZTB_SE", sLastSeq); //출장순번
	this.ds_Detail10.setColumn(iAddRow, "FROM_MI", "00"); //출발(분) - Default 0분
	this.ds_Detail10.setColumn(iAddRow, "ARRI_MI", "00"); //도착(분) - Default 0분
	this.ds_Detail10.setColumn(iAddRow, "MAXB_SE", sLastSeq); //최종 출장순번
	
	if (iAddRow == 0) {	//1. Detail10이 1 Row의 경우 출장기간(시작일자) 셋팅
		this.ds_Detail10.setColumn(iAddRow, "BZTB_DD", fromDate);  //출발일자(출장일자)
		this.ds_Detail10.setColumn(iAddRow, "ARRI_DD", fromDate);  //도착일자		
		
	} else {	        //2. Detail10이 2 Row 이상의 경우 출장기간(종료일자) 셋팅
		this.ds_Detail10.setColumn(iAddRow, "BZTB_DD", "");  //출발일자(출장일자)
		this.ds_Detail10.setColumn(iAddRow, "ARRI_DD", "");  //도착일자
		
	}
	
	this.ds_Detail10.set_enableevent(true); // 이벤트 true
		
};

