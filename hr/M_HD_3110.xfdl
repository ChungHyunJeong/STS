this.ds_Master10_cancolumnchange = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	var sSuppCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SUPP_CD"); //지원사항 
	var sToDay 	= this.gfn_GetCommDate("toDay");//현재일자					
	
	if (e.columnid == "FROM_DD" || e.columnid == "CLOS_DD") {
		if(e.columnid == "FROM_DD"){
			var fromDate  = e.newvalue;
			var closDate  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD"); //출장기간(종료일자)
			
			if (fromDate < sToDay && sSuppCd != '0060') {
				var arrMsgArg = ["장기출장이 아닌경우 시작일자"];	//메시지내용
				this.gfn_Alert("HR015", arrMsgArg); //출장기간 시작일자는 현재일자보다 동일하거나 커야 합니다.
				
				return false;
			}
		}else if(e.columnid == "CLOS_DD"){
			var fromDate  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD"); //출장기간(시작일자)
			var closDate  = e.newvalue;
			
			if (closDate < sToDay && sSuppCd != '0060') {
				var arrMsgArg = ["장기출장이 아닌경우 종료일자"];	//메시지내용
				this.gfn_Alert("HR015", arrMsgArg); //출장기간 종료일자는 현재일자보다 동일하거나 커야 합니다.

				return false;
			}
		}			
			
		
		if (!this.gfn_IsNull(fromDate) && !this.gfn_IsNull(closDate)) {					
			if (fromDate > closDate) {				
				this.gfn_Alert("SM018");//종료일자가 시작일자보다 빠릅니다
				return false;
			}
		}	
	}else if(e.columnid == "SUPP_CD"){
		var sSuppCd   = e.newvalue;
		var fromDate  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD"); //출장기간(시작일자)
		var closDate  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD"); //출장기간(종료일자)
		
		if (fromDate < sToDay && sSuppCd != '0060') {
			var arrMsgArg = ["장기출장이 아닌경우 시작일자"];	//메시지내용
			this.gfn_Alert("HR015", arrMsgArg); //출장기간 시작일자는 현재일자보다 동일하거나 커야 합니다.
			
			return false;		
		} else if (closDate < sToDay && sSuppCd != '0060') {
		
			//출장기간 종료일자는 현재일자보다 동일하거나 커야 합니다.	
			
			var arrMsgArg = ["장기출장이 아닌경우 종료일자"];	//메시지내용
			this.gfn_Alert("HR015", arrMsgArg); //출장기간 종료일자는 현재일자보다 동일하거나 커야 합니다.

			return false;
		}
	}
};
