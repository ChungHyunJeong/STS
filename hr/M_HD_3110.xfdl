this.fn_SetTermDuplicationCheck = function()
{ 
	//2022-03-22 요청내용
	//출장기간은 1번만 등록해야 함(중복되어서는 안됨)
	
	this.ds_Master10Dup.clearData();		//중복여부 체크
	this.ds_Input.clearData();
	
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "FROM_DD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD")); //출장시작일자
	this.ds_Input.setColumn(0, "CLOS_DD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD")); //출장종료일자
	this.ds_Input.setColumn(0, "SESU_ID", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SYST_ID")); //출장자
	this.ds_Input.setColumn(0, "BZTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO")); //출장번호
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "hr:M_HD_3110_M_SL15";	//출장기간 중복여부
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "DUP_CHECK";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Master10Dup=ds_Master15";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
    svcCallObj.isSync = true;//동기식으로 설정할지 여부  (동기 - true, 비동기 - false)

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);		

	//동기화 후 Return 값 처리
	if (this.ds_Master10Dup.getColumn(0, "DUP_CNT") > 0) {
		return true;
	} else {
		return false;
	}
	
};


