this.fn_SetWfCompEnable = function()
{ 	 
	//[결재]결재내역 팝업화면으로 호출여부
	if (!this.fvWfPopupMode) {
		//[결재] 공통버튼 제어
		var bCommSaveButton = true;//저장버튼 활성화 여부-기본값은 false로 해야함. - 결재승인후 수정일 필요한 화면에서만 true로 설정해야함. 
		//this.fvWfSearchMode = this.gfn_SetCommButtonWf(this, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"), bCommSaveButton);//결재진행상태		
		this.fvWofhStatus = this.gfn_SetCommButtonWf(this, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"), bCommSaveButton);//결재진행상태		
	}
	trace(this.fvWfSearchMode + "/" +this.fvWofhStatus);
	//[결재]결재내역 조회모드이면 Visble false || 결재상태가 "등록"이 아닐경우
	if (this.fvWfSearchMode || this.fvWofhStatus) {
		//[결재]결재내역 팝업화면으로 호출여부
		if (this.fvWfPopupMode) {
			this.div_Search.set_enable(false);//조회영역 Enable처리
		}
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);
		//[결재] Div하위의 컴포넌트 Enable 처리
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 입력			
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_TjchPop.set_enable(true); //발의내용요약 버튼
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_RemkPop.set_enable(true); //비고(발의내용) 버튼
		this.div_MasterInput.form.btn_CnthM1.set_visible(false);	//추계액계산버튼
		this.div_MasterInput.form.btn_TjchDel.set_visible(false);
		//[전표보기]
		//결재상태-승인인경우 => [전표보기] 버튼 활성화
		if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST") == "0040") {
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(true);
		} else {
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(false);
		}
		
	} else {
		//[결재]결재내역 팝업화면으로 호출여부
		if (this.fvWfPopupMode) {	
			this.div_Search.set_enable(true);//조회영역 Enable처리
		}
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(true);
		//[결재] Div하위의 컴포넌트 Enable 처리
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, true);//Master 입력 프리폼
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form, true);//Detail 입력	
		
		//[결재] Div하위의 컴포넌트 Enable 처리 제외할 컴포넌트설정
		
// 		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_TjchNo.set_enable(false);
// 		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_TjchDd.set_enable(false);
// 		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_enable(false);
// 		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmNm.set_enable(false);
// 		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_enable(false);
// 		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm.set_enable(false);
// 		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumCd.set_enable(false);
// 		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumNm.set_enable(false);
		
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_TjchNo.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_TjchDd.set_readonly(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SlipNo.set_readonly(true);
		
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumCd.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_AcchNo.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd01.set_readonly(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm01.set_readonly(false);		
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_EmpmCd.set_enable(true);
		
		this.div_MasterInput.form.btn_CnthM1.set_visible(true);	//추계액계산버튼
		this.div_MasterInput.form.btn_TjchDel.set_visible(true);
		this.div_MasterInput.form.btn_TjchDel.set_enable(true);		
		
		//[전표보기] 결재진행 상태가 승인이 아닌경우 => [전표보기] 버튼 비활성화
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(false);
		
		
	}
};
