/*---------------------------------------------------------------------------------------------
 *  추계액계산 실행
  ---------------------------------------------------------------------------------------------*/
this.fn_MakeProc= function()
{
	this.ds_HrEmpmIn.clearData();
	this.ds_Detail10.clearData();
	this.ds_Detail11.clearData();
	
	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
			
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "hr:M_HS_8570_M_SL13";	//추계액계산
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MAKE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_HS;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_HrEmpmIn=ds_HrEmpmIn" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Detail11=ds_Detail11";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.isSync = true;// 동기식 설정
	
	this.ds_Detail10.clearData();
	//추계액계산
	for (var iLoop=0; iLoop<this.ds_HrEmpm.getRowCount(); iLoop++) {
		this.ds_HrEmpmIn.clearData();
		this.ds_HrEmpmIn.addRow();
		this.ds_HrEmpmIn.copyRow(0, this.ds_HrEmpm, iLoop);		
		//trace(this.ds_HrEmpmIn.getColumn(this.ds_HrEmpmIn.rowposition, "SYST_ID"));
		
		//[공통]서비스 호출
		this.gfn_CallComService(this, svcCallObj);	
		
		//this.ds_Detail10.addRow();
		var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)
		this.ds_Detail10.copyRow(iAddRow, this.ds_Detail11, 0);		
	}
	
	var strThchMm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "TJCH_MM");
	
	var sTotAm 	= this.gfn_NumberToCommaString(this.ds_Detail10.getSum("TJCB_AM"));
	var sCnt	= this.ds_Detail10.rowcount;		
	
	this.ds_Master10.setColumn(this.ds_Master10.rowposition, "REMK_NM", "퇴직추계액 " +  strThchMm.substr(0,4) + "-" + strThchMm.substr(4,2) + " : " + sTotAm +" (" + sCnt.toString() + "명)");			//발의내용요약		
		
} 

