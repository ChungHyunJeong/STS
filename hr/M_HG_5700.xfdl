  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {
		//[결재] 결재상태에 따른 컴포넌트 제어
		this.fn_SetWfCompEnable();	
		
		//반납번호가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CBAH_NO"))) {
			//신청번호 자리수 확인
			var iCarhLen = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CARH_NO").length;
			
			//신청번호 format 변경 (10자리, 13자리)
			if (iCarhLen > 10) 
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CarhNo.set_format("@@@@@@@@-@@@@");	
			else 
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CarhNo.set_format("@@@@@@-@@@@");
		}		
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master] Row 변경전 이벤트 - ds_Master10
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	

	return true;
};  


