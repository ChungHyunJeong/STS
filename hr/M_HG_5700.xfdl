	} else {
		var arrMsgArg = ["재기안"];
		this.gfn_Alert("SM064", arrMsgArg);//변경된 내용이 있습니다.\n저장 후 &ITM0하시기 바랍니다.
		return false;
	}
	
	//결재진행내역체크
	if (this.ds_Master10.getRowCount() < 1 || this.ds_Master10.rowposition < 0) {
		this.gfn_Alert("WF021", arrMsgArg);//결재내역을 선택한 후 재기안하세요.
		return false;
	}	
	
	return true;
}

