 /*---------------------------------------------------------------------------------------------
 *    [Master 입력영역-Combo] 경조구분 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.div_MasterInput_tab_Master_tpg_01_cbo_FaemCd_onitemchanged = function(obj:nexacro.Combo,e:nexacro.ItemChangeEventInfo)
{
	var sFaemCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FAEM_CD")
	var strPosiCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "POSI_CD");
	
	var nRow 		= this.ds_HdFaem.findRowExpr("FAEM_CD=='" + sFaemCd + "'  &&  POSD_CD <= '" + strPosiCd + "'  &&  POSI_CD >= '" + strPosiCd + "'");	
	var fAem_am		= this.ds_HdFaem.getColumn(nRow, "FAEM_AM");
	var fAem_ct		= this.ds_HdFaem.getColumn(nRow, "FAEM_CT");
	
	this.div_MasterInput.form.tab_Master.tpg_01.form.mae_HoliCt.set_value(fAem_ct);
	//this.div_MasterInput.form.tab_Master.tpg_01.form.mae_FaemAm.set_value(fAem_am);
	
	//입사일자 기준미달시 경조금은 0 이다.	
	if (!this.fn_Chk_Pay()) {
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_FaemAm.set_value(0);
	} else {
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_FaemAm.set_value(fAem_am);
	}
	
	var nRow1 = this.ds_Hr307.findRow( "CODM_CD", obj.value);
	var sOp06Nm = this.ds_Hr307.getColumn(nRow1, "OP06_NM"); //기간(TO)입력제어
	
	if (sOp06Nm == 'N') {
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.set_enable(false);
		
		//FROMM_DD 와 일수를 가지고 휴일을 제외한 날짜를 가지고 온다.
		//출산일경우 기간의 TO로도 계산이 되므로 TO 입력제어가 N 인것은 FROM으로 계산한다.
		this.fn_Set_ClosDd("FROM", "");
	} else if (sOp06Nm == 'Y') {
	
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.set_enable(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.set_value("");
		
		//출산관련 일수 총합이 기준일수 초과시 입력 불가
		if (!this.fn_Chk_HoliCt())  return;
		
		//출산시 일수가 기초값과 같으면 변경이 가능하다.
		//1회 분할했으면 경조금은 0 이다.
		if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "HOLI_CT") != nexacro.toNumber(fAem_ct)) {
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.set_enable(false);
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_FaemAm.set_value(0);
		} else {
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.set_enable(true);
		}
	}
};

