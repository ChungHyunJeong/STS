{
	var arrRows = this.div_MasterView.form.grd_Master10.getSelectedRows();
	this.fvChkMultiWfYn = true;//멀티선택여부
	
	for (var iLoop=0; iLoop < arrRows.length; iLoop++) {
		this.ds_Master10.setColumn(arrRows[iLoop], "s_chk", "0");
	}		
	
	this.fvChkMultiWfYn = false;//멀티선택여부	
};




