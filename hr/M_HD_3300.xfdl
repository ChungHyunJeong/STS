 ***********************************************************************************************/
this.fn_Days_Check = function(sGtdhGu)
{	
	var sHoliCt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "HOLI_CT")
	
	if (this.gfn_IsNull(sHoliCt) || (sHoliCt == 0) ) {
		return false;
	};
	
	//0090:귀국휴가
	if (sGtdhGu == "0090" ) {
		if (sHoliCt > 2) {
			this.gfn_Alert("HD013"); //귀국휴가는 최대 2일만 등록 가능합니다.
			
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", "");
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", "");
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "HOLI_CT", "0");
			return false;
		}
	//0130:장기해외근무
	} else if (sGtdhGu == "0130" ) {
		
		if ( (sHoliCt > 34) && (sHoliCt < 60) ) {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FAEM_AM",  "200000")
		} else if ( (sHoliCt > 59) && (sHoliCt < 80) ) {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FAEM_AM",  "500000")
		} else if ( (sHoliCt > 79) && (sHoliCt < 110) ) {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FAEM_AM",  "1000000")
		} else if ( (sHoliCt > 109) && (sHoliCt < 140) ) {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FAEM_AM",  "1500000")
		} else if ( (sHoliCt > 139) && (sHoliCt < 170) ) {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FAEM_AM",  "2000000")
		} else if ( (sHoliCt > 169) && (sHoliCt < 210) ) {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FAEM_AM",  "2500000")
		} else if ( (sHoliCt > 209)) {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FAEM_AM",  "3000000")
		} else {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FAEM_AM",  "0")
		}
		return true;
	}
	
	return true;
}

