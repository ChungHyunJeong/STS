					var nRow = this.ds_HdFaem.findRowExpr("FAEM_CD=='" + sFaemCd + "'  &&  POSD_CD <= '" + strPosiCd + "'  &&  POSI_CD >= '" + strPosiCd + "'");	
					var fAem_am = this.ds_HdFaem.getColumn(nRow, "FAEM_AM");
					var fAem_ct = this.ds_HdFaem.getColumn(nRow, "FAEM_CT");
					
					this.div_MasterInput.form.tab_Master.tpg_01.form.mae_HoliCt.set_value(fAem_ct);
					this.div_MasterInput.form.tab_Master.tpg_01.form.mae_FaemAm.set_value(fAem_am);
				}
			}
		} else if (sOp06Nm == 'Y') {
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.set_enable(true);
			
			var nRow = this.ds_HdFaem.findRowExpr("FAEM_CD=='" + sFaemCd + "'  &&  POSD_CD <= '" + strPosiCd + "'  &&  POSI_CD >= '" + strPosiCd + "'");	
			var fAem_am = this.ds_HdFaem.getColumn(nRow, "FAEM_AM");
			var fAem_ct = this.ds_HdFaem.getColumn(nRow, "FAEM_CT");
			
			//경조기준관리- 0150:자녀출산(첫째), 0151:자녀출산(둘째), 0152:자녀출산(셋째이상)
			if (sFaemCd == "0150" || sFaemCd == "0151" || sFaemCd == "0151") {
				this.fn_Chk_HoliCt1(sChkRowType)
			}
		}
	}
}

/***********************************************************************************************
 * 16.24. 기간(TO) 입력 제어에 따른 CLOS_DD 넣기
 ***********************************************************************************************/
this.fn_ClosDd_Setitem = function(sGtdhGu)
{
	//기간(TO) 입력 제어 
	//0: CLOS_DD 입력만 막음, 1: CLOS_DD 입력가능, 2: CLOS_DD 입력막고 FROM_DD값으로 넣기
	var nRow = this.ds_Hr305.findRow( "CODM_CD", sGtdhGu);
	var sOp08Nm = this.ds_Hr305.getColumn(nRow, "OP08_NM"); //기간(TO)입력제어
		
	if (this.gfn_IsNull(sOp08Nm)) {
		sOp08Nm = "1"
	};
		
	if (sOp08Nm == "2") {
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD",  this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD"))
	};
}

