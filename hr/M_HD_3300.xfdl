{	
	//신청가능 여부 체크, 장기해외근무는 체크 안한다.
	if (sGtdhGu != "0130" ) {
		if (!this.fn_Check_Date()) return;
	}
	
	//월마감 체크
	if (!this.fn_Chk_CloseDay()) return;
	
	//연차-0010, 병가-0430, 교육-0060, 훈련-0050
	if (sGtdhGu == "0010" || sGtdhGu == "0430"  || sGtdhGu == "0060" || sGtdhGu == "0050") {
		this.div_MasterInput.form.tab_Master.tpg_01.form.sta_HarfCd.set_text("반차구분");		
		this.fn_SetYeam(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SYST_ID")) /* 발생·사용·잔여 연차 계산 */
