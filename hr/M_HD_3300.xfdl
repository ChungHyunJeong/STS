/***********************************************************************************************
 * 16.06. 휴가구분 중 기간(TO) 입력 제어, 경조는 경조구분에 따름
 ***********************************************************************************************/
this.fn_Enable_Check = function(sGtdhGu)
{
	//경조
	if (sGtdhGu != "0020") {
		var nRow = this.ds_Hr305.findRow( "CODM_CD", sGtdhGu);
		var sOp08Nm = this.ds_Hr305.getColumn(nRow, "OP08_NM"); //기간(TO)입력제어
		
		if (this.gfn_IsNull(sOp08Nm)) {
			sOp08Nm = "1"
		};
		
		if (sOp08Nm == "0" || sOp08Nm == "2") {
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.set_enable(false);
		} else {
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.set_enable(true);
		};
	};
	
	//0120: 철야근무(국내), 0100: 철야근무(해외)
	if (sGtdhGu == "0120" || sGtdhGu == "0100") {
