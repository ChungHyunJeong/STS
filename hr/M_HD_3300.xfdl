		this.fn_ClosDd_Setitem(sGtdhGu); /* 기간(TO) 입력 제어에 따른 CLOS_DD 넣기, 반드시 일수계산전에 호출 */
		this.fn_CalcDay(); /* 일수계산 */
		this.fn_Enable_Check(sGtdhGu); /* 기간(TO) 입력 제어 및 기간 셋팅 */
	//반차-0005
	} else if (sGtdhGu == "0005") {
		this.div_MasterInput.form.tab_Master.tpg_01.form.sta_HarfCd.set_text("반차구분");
		this.fn_SetYeam(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SYST_ID")) /* 발생·사용·잔여 연차 계산 */
		this.fn_Clear(sGtdhGu); /* 근무시간 초기화 및 반차구분 활성화, 경조구분 비활성화 */
