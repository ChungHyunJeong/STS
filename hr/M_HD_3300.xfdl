/***********************************************************************************************
 * 05.02.01 [Vaildation] Master 저장 필수값 체크 - 휴가구분별 필수값 체크
 ***********************************************************************************************/ 
 this.fn_VaildMasterSave1 = function()
{
	var sFromDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD") /* 기간(시작일자) */
	var sClosDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD") /* 기간(종료일자) */
	var sGtdhGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "GTDH_GU") /* 휴가구분 */
	var sFaemCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FAEM_CD") /* 경조구분 */
	var sHootCt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "HOOT_CT") /* 특근시간 */
	var sHalfCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "HALF_CD") /* 반차구분 */
	
	var sYear  = this.gfn_GetCommDate("toDay").substr(0,4);//현재일자
	var sFromDd1 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD").substr(0,4);
	var sClosDd1 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD").substr(0,4);
	
	//0010:연차
	if (sGtdhGu == "0010" ) {
		if(sYear != sFromDd1){ //-- 당해년도만 입력가능하게(요청사항임)
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FromDd.setFocus();
			this.gfn_Alert("HD007"); //당해년도 자료만 입력 가능합니다.
			return false;
		}
		
		if(sYear != sClosDd1){ //-- 당해년도만 입력가능하게(요청사항임)
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.setFocus();
			this.gfn_Alert("HD007"); //당해년도 자료만 입력 가능합니다.
			return false;
		}
		
		if (sFromDd1 != sClosDd1) {
			this.gfn_Alert("HD015"); //동일한 년도별로 입력하셔야 합니다.
			return false;
		}
		
		//휴가 기간에 출장원 있는지 확인
		if (!this.fn_Chk_Bzth()) return false;
	// 0005:반차, 0025:반반차
	} else if (sGtdhGu == "0005" || sGtdhGu == "0025") {
		if(sYear != sFromDd1){ //-- 당해년도만 입력가능하게(요청사항임)
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FromDd.setFocus();
			this.gfn_Alert("HD007"); //당해년도 자료만 입력 가능합니다.
			return false;
		}
		
		if(sYear != sClosDd1){ //-- 당해년도만 입력가능하게(요청사항임)
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.setFocus();
			this.gfn_Alert("HD007"); //당해년도 자료만 입력 가능합니다.
			return false;
		}
		
		if (this.gfn_IsNull(sHalfCd)) {
			var arrMsgArg = ["반차구분"];
			this.gfn_Alert("SM008", arrMsgArg);//&ITM0은(는) 필수 입력 항목입니다.
			return false;
		}
		
	//0090:귀국휴가
	} else if (sGtdhGu == "0090"){ 
		if (sFromDd1 != sClosDd1) {
			this.gfn_Alert("HD015"); //동일한 년도별로 입력하셔야 합니다.
			return false;
		}
		
		//휴가 기간에 출장원 있는지 확인
		if (!this.fn_Chk_Bzth()) return false;
	//0020:경조
	} else if (sGtdhGu == "0020") {
		if (this.gfn_IsNull(sFaemCd)) {
			var arrMsgArg = ["경조구분"];
			this.gfn_Alert("SM008", arrMsgArg);//&ITM0은(는) 필수 입력 항목입니다.
			return false;
		}
		
		//휴가 기간에 출장원 있는지 확인
		if (!this.fn_Chk_Bzth()) return false;
		
		//경조기준관리- 0150:자녀출산(첫째), 0151:자녀출산(둘째), 0152:자녀출산(셋째이상)
		//자녀출산은 1회까지만 분할이 가능하다.
		if (sFaemCd == "0150" || sFaemCd == "0151" || sFaemCd == "0151") {
			if (!this.fn_Chk_Birth()) return false;
		}
	//0100:철야근무(해외), 0110:휴일특근(해외), 0120:철야근무(국내), 0070:휴일특근(국내)
	} else if (sGtdhGu == "0100" || sGtdhGu == "0110" || sGtdhGu == "0120" || sGtdhGu == "0070"){
		if (sHootCt == 0 || this.gfn_IsNull(sHootCt)) {
			this.gfn_Alert("HD018"); //특근시간이 없습니다.
			return false;
		}
	//0130:장기해외근무, 조금 이해가 안됨. 구버전 기능과 같이 씀.
	} else if (sGtdhGu == "0130") {
		if (!this.fn_Chk_Gubu()) return false;
	}

	return true;
}

