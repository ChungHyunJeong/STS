/***********************************************************************************************
 * 01.01. [공통버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.fn_ComBtnSearch = function(sWofhNo)
{ 
	//[결재] 결재 PID가 있으면-결재Popup으로 호출시
	if (!this.gfn_IsNull(sWofhNo)) {
		this.fvWfSearchMode = true;//[결재]결재내역 조회모드
		this.fvWfPopupMode = true;//[결재]결재내역 팝업화면으로 호출여부
		
		this.div_Search.form.edt_EmpmCd.set_value(this.getOwnerFrame().paramEmpmCd);
		this.div_Search.form.edt_EmpmNm.set_value(this.getOwnerFrame().paramEmpmNm);
		this.div_Search.form.edt_DepmCd.set_value(this.getOwnerFrame().paramDepmCd);
		this.div_Search.form.edt_DepmNm.set_value(this.getOwnerFrame().paramDepmNm);
		
	}
	
	this.fvMasterSearchMode = false;//마스터내역 조회중여부
	
	//조회전 유효성 체크
	if(!this.fvPopupYn){
		if (!this.fn_VaildSearch()) return;	
	}
	
	//저장후 재조회시-마스터 데이터셋 이벤트 비활성화
	if (this.fvSchKeyYn) {
		this.ds_Master10.set_enableevent(false);
	}

	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Master10.clearData();
	this.ds_Input.clearData();
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();	
	var sSqlId = "";//[결재]쿼리ID
	
	//[결재] 결재내역 조회가 아니면 (if문)
	if (!this.fvWfSearchMode) {
		this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_GtdhDdFr.value);	//신청일자(시작일)
		this.ds_Input.setColumn(0, "CLOS_DD", this.div_Search.form.cal_GtdhDdTo.value);	//신청일자(종료일)
		this.ds_Input.setColumn(0, "DEPM_CD", this.div_Search.form.edt_DepmCd.value);	//등록부서
		this.ds_Input.setColumn(0, "SYST_ID", this.div_Search.form.edt_SystId.value);	//등록자
		this.ds_Input.setColumn(0, "GTDH_GU", this.div_Search.form.cbo_GtdhGu.value);   //휴가구분
		this.ds_Input.setColumn(0, "DATE_GU", this.div_Search.form.cbo_DateGu.value);   //날짜구분
		
		//결재이력체크
		if (this.div_Search.form.chk_WofhYn.value) {
			this.ds_Input.setColumn(0, "WOFH_YN", "1");
		} else {
			this.ds_Input.setColumn(0, "WOFH_YN", "0");
		}
													   
		sSqlId = "hr:M_HD_3300_M_SL10";//쿼리ID
	} else {
		var sGtdhNo = this.getOwnerFrame().paramGtdhNo1;
		var sWofhSq = this.getOwnerFrame().paramWofhSq;

		this.ds_Input.setColumn(0, "WOFH_NO", sWofhNo);//결재PID
		this.ds_Input.setColumn(0, "GTDH_NO", sGtdhNo);//신청번호
		this.ds_Input.setColumn(0, "WOFH_SQ", sWofhSq);//결재차수

		sSqlId = "hr:M_HD_3300_M_SL19";//쿼리ID
	}
	
	//팝업조회시
	if(this.fvPopupYn){		
		this.ds_Input.setColumn(0, "GTDH_NO", this.getOwnerFrame().paramGtdhNo);//관리번호
		this.ds_Input.setColumn(0, "WOFH_SQ", this.getOwnerFrame().paramWofhSq);//결재차수
		
		sSqlId = "hr:M_HD_3300_M_SL18";//쿼리ID
	}
	
