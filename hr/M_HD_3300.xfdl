/***********************************************************************************************
 * 16.21. 경조구분 자녀출산시 일수 CHECK
   - 이미 신청하신 날짜입니다. 메세지 이후 기능 처리용
   - 자녀출산은 1회 분할이 가능하므로 분할 후 CLOS_DD를 막는다.
 ***********************************************************************************************/ 
this.fn_Chk_HoliCt1 = function(sChkRowType) 
{
	var sFaemCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FAEM_CD")
	var strPosiCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "POSI_CD");
	
	var nRow = this.ds_HdFaem.findRowExpr("FAEM_CD=='" + sFaemCd + "'  &&  POSD_CD <= '" + strPosiCd + "'  &&  POSI_CD >= '" + strPosiCd + "'");
	var fAem_ct = this.ds_HdFaem.getColumn(nRow, "FAEM_CT");
	var fAem_am = this.ds_HdFaem.getColumn(nRow, "FAEM_AM");
	
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_FromDd.clearData();
	this.ds_ClosDd.clearData();
	
	this.ds_FromDd.addRow();
	this.ds_FromDd.setColumn(0, "SYST_ID", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SYST_ID"));
	this.ds_FromDd.setColumn(0, "FAEM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FAEM_CD"));
	this.ds_FromDd.setColumn(0, "FAEM_CT", fAem_ct);
	//남은 일수만 가지고 오면 되므로 FROM_DD는 오늘로 넘긴다.
	this.ds_FromDd.setColumn(0, "FROM_DD", this.gfn_GetCommDate("toDay"));
			
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "hr:M_HD_3300_M_SL25";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_FromDd=ds_FromDd" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_ClosDd=ds_ClosDd";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.isSync = true;//동기식으로 설정할지 여부
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);
	
	var sHoliCt = this.ds_ClosDd.getColumn(0, "HOLI_CT")
	var sHoliCount = this.ds_ClosDd.getColumn(0, "HOLI_COUNT")
	
	//신규 입력시만 남은 일수를 넣는다. 안그러면 crudType 이 U 가 되서 변경건으로 인식한다.
	if (sChkRowType == "I") {
 		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "HOLI_CT", sHoliCt)
		
		//첫 분할 후 추가시 CLOS_DD 막는다.
		if (sHoliCount == 1 && sHoliCt != 0) {
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.set_enable(false);
		} else {
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.set_enable(true);
		}
		
	} else {
		//첫 분할 때만 CLOS_DD 를 열어주고 분할 후 남은 일수가 없으면 첫 분할도 막는다.
		if (sHoliCount == 1 && sHoliCt != 0) {
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.set_enable(true);
		} else {
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.set_enable(false);
		}
	}
}

/***********************************************************************************************
 * 16.22. 휴가구분 변경시 월마감 체크
 ***********************************************************************************************/
this.fn_Chk_CloseDay = function()
{
	var sGtdhGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "GTDH_GU") /* 휴가구분 */
	var sFromDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD")
	var sClosDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD")
	
	if (!this.gfn_IsNull(sFromDd)) { 
		var sBUDG_MM = "";//마감년월
		
		//장기해외근무는 FROM_DD 가 아닌 CLOS_DD 로 월마감 체크한다.
		//2022.06.23 장기해외근무는 월마감 체크 안한다.
		if (sGtdhGu != "0130" ) {
			if (!this.gfn_IsNull(this.fvMmCloseDay)) {
				sBUDG_MM = this.gfn_Nvl(this.fvMmCloseDay, "");//마감년월
			} else {
				sBUDG_MM = this.gfn_Nvl(sFromDd, "");//마감년월
			}
			this.fvMmCloseDay = "";		
			var sMGAS_DD = sBUDG_MM	
			
			//마감여부 체크
			var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
			var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
			paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
			paramObj.MODM_CD = "HD";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
			paramObj.ACUM_CD = "";//[선택] 회계단위
			paramObj.BUSM_CD  = "";//[선택] 사업부문
			paramObj.SAUM_CD  = "";//[선택] 사업장코드
			paramObj.DEPM_CD  = "";//[선택] 부서코드
			paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
			paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
			
			paramObj.MGAS_DD  = sMGAS_DD;//[선택] 마감일자		
			
			var bCheck = "";
			
			if(!this.gfn_IsNull(sMGAS_DD) && sMGAS_DD != ""){
				bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크	
			}
			
			//월마감된 경우
			if (bCheck  && !this.gfn_IsNull(sMGAS_DD) && sMGAS_DD != "") {
				this.gfn_Alert("CA025");//해당월의 정보가 마감이 되어서 등록/수정 할 수 없습니다.
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FromDd.set_value("");
				
				return false;
			} else {
				return true;
			}
		}
	}
	
	return true;
}

