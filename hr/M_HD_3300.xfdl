 /*---------------------------------------------------------------------------------------------
 *    [Master 입력영역-Calendar] 기간 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.div_MasterInput_tab_Master_tpg_01_cal_FromDd_onchanged = function(obj:nexacro.Calendar,e:nexacro.ChangeEventInfo)
{
// 	//신청가능 여부 체크
// 	if (!this.fn_Check_Date()) return;
	
	var sGtdhGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "GTDH_GU")
	
	//신청가능 여부 체크, 장기해외근무는 체크 안한다.
	if (sGtdhGu != "0130" ) {
		if (!this.fn_Check_Date()) return;
	}
	
	//경조
	if (sGtdhGu == "0020") {
		//FROMM_DD 와 일수를 가지고 휴일을 제외한 날짜를 가지고 온다.
		///출산일경우 기간의 TO로도 계산이 되므로 FROM으로 구분값 넘긴다.
		this.fn_Set_ClosDd("FROM", "");
		
	} else {
		//일자 체크
		if (!this.fn_Input_Check()) return;	
		// 휴일 체크
		if (!this.fn_CalcTeuk(sGtdhGu)) return;
		//기간(TO) 입력 제어에 따른 CLOS_DD 넣기
		//반드시 이거 먼저 호출
		this.fn_ClosDd_Setitem(sGtdhGu);
		//기간 변경 이벤트
		this.fn_CalcDay();
		//귀국휴가 일수 체크 및 장기해외근무 일수별 수당 계산
		if (!this.fn_Days_Check(sGtdhGu)) return;
		//시차출퇴근제만 클리어시킨다
		if (sGtdhGu == "0200") {
			this.fn_Clear1(sGtdhGu)
		}
	}
};

this.div_MasterInput_tab_Master_tpg_01_cal_ClosDd_onchanged = function(obj:nexacro.Calendar,e:nexacro.ChangeEventInfo)
{
// 	//신청가능 여부 체크
// 	if (!this.fn_Check_Date()) return;
	
	var sGtdhGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "GTDH_GU")
	
	//신청가능 여부 체크, 장기해외근무는 체크 안한다.
	if (sGtdhGu != "0130" ) {
		if (!this.fn_Check_Date()) return;
	}
	
	//경조
	if (sGtdhGu == "0020") {
		var sFaemCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FAEM_CD")
		var strPosiCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "POSI_CD");
		
		var nRow = this.ds_HdFaem.findRowExpr("FAEM_CD=='" + sFaemCd + "'  &&  POSD_CD <= '" + strPosiCd + "'  &&  POSI_CD >= '" + strPosiCd + "'");	
		var fAem_ct = this.ds_HdFaem.getColumn(nRow, "FAEM_CT");
		
		//FROMM_DD 와 일수를 가지고 휴일을 제외한 날짜를 가지고 온다.
		//출산일경우 기간의 TO로도 계산이 되므로 TO으로 구분값 넘긴다.
		this.fn_Set_ClosDd("TO", "");
		
		//TO 일자로 계산후 기본 경조일수 초과시 메세지 호출
		if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "HOLI_CT") > fAem_ct) {
			var arrMsgArg = [fAem_ct+"일"];
			this.gfn_Alert("HD014", arrMsgArg);//신청할수 있는 휴가일수 [ &ITM0 ] 를 초과하였습니다.
			
			//FROM 을 가지고 다시 계산한다.
			//TO로 계산시 일수가 바뀌므로 일수 초과한 것은 정상적이 일수를 넘겨준다.
			this.fn_Set_ClosDd("FROM1", fAem_ct);
		};
	} else {
		//일자 체크
		if (!this.fn_Input_Check()) return;	
		// 휴일 체크
		if (!this.fn_CalcTeuk(sGtdhGu)) return;
		//기간 변경 이벤트
		this.fn_CalcDay();
		//귀국휴가 일수 체크 및 장기해외근무 일수별 수당 계산
		if (!this.fn_Days_Check(sGtdhGu)) return;
		//시차출퇴근제만 클리어시킨다
		if (sGtdhGu == "0200") {
			this.fn_Clear1(sGtdhGu)
		}
	}
};
