/***********************************************************************************************
 * 16.08. 근무시간 계산
 ***********************************************************************************************/
this.fn_Cal_HoonCt = function()
{
	var sGtfrHr = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "GTFR_HR");
	var sGtfrMi = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "GTFR_MI");
	var sGttoHr = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "GTTO_HR");
	var sGttoMi = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "GTTO_MI");
	var sGtdhGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "GTDH_GU");
	
	if ((!this.gfn_IsNull(sGtfrHr)) && (!this.gfn_IsNull(sGtfrMi)) && (!this.gfn_IsNull(sGttoHr)) && (!this.gfn_IsNull(sGttoMi))){
		if((sGtfrHr + sGtfrMi) > (sGttoHr + sGttoMi)) {
			this.gfn_Alert("HD012"); //시작시간이 종료시간보다 크거나 같습니다.
			
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "GTTO_HR", "");
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "GTTO_MI", "");
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "HOOT_CT", "0");
			
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_GttoHr.set_index(-1);
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_GttoMi.set_index(-1);
			
			return false;
		}
	}
	
	var sStart_Hr = nexacro.toNumber(sGtfrHr);
	var sStart_Mi = nexacro.toNumber(sGtfrMi);
	var sEnd_Hr = nexacro.toNumber(sGttoHr);
	var sEnd_Mi = nexacro.toNumber(sGttoMi);
	
	//시차출퇴근제
	if (sGtdhGu == '0200') {
		var sEndTime = sStart_Hr + 9;
		
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_GttoHr.set_value(sEndTime);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_GttoMi.set_value(sGtfrMi);
	} else {
		var sStart_Total = ((sStart_Hr * 3600) + (sStart_Mi * 60));
		var sEnd_Total = (sEnd_Hr * 3600) + (sEnd_Mi * 60);
		
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "HOOT_CT", ((sEnd_Total - sStart_Total)/3600)); 
		
		////0120: 철야근무(국내), 0100: 철야근무(해외)
	//	if (sGtdhGu == "0120" || sGtdhGu == "0100") {
		//	this.ds_Master10.setColumn(this.ds_Master10.rowposition, "HOOT_CT", ((sEnd_Total - sStart_Total)/3600)); 
		//} else {
	//		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "HOOT_CT", (((sEnd_Total - sStart_Total)/3600)-1)); 
	//	}
		
	};
	
	return true;
}

