	//결재내역 체크 - 체크된 내역이 없을경우
	if (this.ds_Master10.getCaseCount("s_chk == '1'") < 1) {
		this.gfn_Alert("WF016");//결재내역을 선택한 후 결재요청하세요.
		return;
	}
	
	var sGtdhGuChk; //휴가구분
	var sSystIdChk; //등록자
	var sDutyOpnm = ""; //직책코드옵션-팀장이상여부 0: 팀원, 1: 팀장이상
	var sRemnCt; //잔여연차
	
	//휴가구분에 따라 결재환경설정이 다르기 때문에 동일한 휴가구분끼리만 일괄결재한다.
	for (var iLoop=0; iLoop<this.ds_Master10.getRowCount(); iLoop++) {
		if (this.ds_Master10.getColumn(iLoop, "s_chk") == "1") {
			//그리드내 휴가 체크
			if(!this.gfn_IsNull(sGtdhGuChk) ){
				if(sGtdhGuChk != this.ds_Master10.getColumn(iLoop,"GTDH_GU")){
					var arrMsgArg = ["휴가구분"];
					this.gfn_Alert("SM057", arrMsgArg); //동일한 [&ITM0]만 적용 가능합니다.	
					return;
				}
			}
			
			//그리드내 등록자 체크
			if(!this.gfn_IsNull(sSystIdChk) ){
				if(sSystIdChk != this.ds_Master10.getColumn(iLoop,"SYST_ID")){
					var arrMsgArg = ["등록자"];
					this.gfn_Alert("SM057", arrMsgArg); //동일한 [&ITM0]만 적용 가능합니다.	
					return;
				}
			}
			
			sGtdhGuChk = this.ds_Master10.getColumn(iLoop,"GTDH_GU");
			sSystIdChk = this.ds_Master10.getColumn(iLoop,"SYST_ID");
			sDutyOpnm = this.ds_Master10.getColumn(iLoop, "DUTY_OP01NM");//팀장이상여부
			sRemnCt = this.ds_Master10.getColumn(iLoop, "REMN_CT"); //잔여연차
		}
	}	
	
	var sPks = "";//PK키값
	var sAtobKey = "";//자동전표관련 키값
	var sCheckYn = "";//체크여부
	var iCheckCnt = 0;//체크건수
	var sManaNo = "";//자동전표관리번호
	var sWofhSq = "";//자동전표결재차수
	var dAmtrSum = 0;//금액합계
	var sWofhNm = "";//결재타이틀명
	
	var sWfUser = "";//결재자정보
	var sWFGU_CD = "";//결재구분
	
	for (var iLoop=0; iLoop<this.ds_Master10.getRowCount(); iLoop++) {
		sCheckYn = this.ds_Master10.getColumn(iLoop, "s_chk");//체크여부

		if (sCheckYn == "1") {
			if (iCheckCnt==0) {
				sPks = this.ds_Master10.getColumn(iLoop, "COMM_CD");//회사코드
				sAtobKey = "@COMM_CD=" + this.ds_Master10.getColumn(iLoop, "COMM_CD");//회사코드
				sManaNo = this.ds_Master10.getColumn(iLoop, "GTDH_NO") //자동전표관리번호-자동전표상세테이블에 사용하는 업무Key
				sWofhSq = this.ds_Master10.getColumn(iLoop, "WOFH_SQ") //자동전표결재차수-자동전표상세테이블에 사용하는 업무Key				
				
				iRow = iLoop;
			} else {
				sPks += "ⓡ" + this.ds_Master10.getColumn(iLoop, "COMM_CD");//회사코드
				sAtobKey += "ⓡ@COMM_CD=" + this.ds_Master10.getColumn(iLoop, "COMM_CD");//회사코드
				sManaNo += ";" + this.ds_Master10.getColumn(iLoop, "GTDH_NO") //자동전표관리번호-자동전표상세테이블에 사용하는 업무Key
				sWofhSq += ";" + this.ds_Master10.getColumn(iLoop, "WOFH_SQ") //자동전표결재차수-자동전표상세테이블에 사용하는 업무Key				
			}
			sPks += "ⓐ" + this.ds_Master10.getColumn(iLoop, "GTDH_NO");//신청번호
			sPks += "ⓐ" + this.ds_Master10.getColumn(iLoop, "WOFH_SQ");//결재차수
				
			//자동전표관련 파라메타-자동전표를 사용하는 화면에만 추가 			
			sAtobKey += "ⓐ@GTDH_NO=" + this.ds_Master10.getColumn(iLoop, "GTDH_NO");//신청번호
			sAtobKey += "ⓐ@WOFH_SQ=" + this.ds_Master10.getColumn(iLoop, "WOFH_SQ");//결재차수	
			
			
			//잔여연차가 마이너스 일대 결재 그외 접수
			//휴가구구분이 연차,반차, 반반차만 사용
			if (sRemnCt < 0) {
				sWFGU_CD = "0010";
			} else {
				sWFGU_CD = "0030";
			};
			
			//팀원
			//휴가구구분이 연차,반차, 반반차만 사용
			if (sDutyOpnm == "0") {
				if (this.gfn_IsNull(sWfUser)) {
					sWfUser = sWFGU_CD + ";" + this.ds_Master10.getColumn(iLoop, "TMSU_ID");
				} else {
					sWfUser = sWfUser + "ⓐ" + sWFGU_CD + ";" + sSystIdChk;
				}
			}
				
			iCheckCnt++;//체크건수
		}
	}
	
	//근태구분명 = 휴가구분
	var iFvFindRow = this.ds_Hr305.findRowExpr("CODM_CD=='" + this.ds_Master10.getColumn(iRow, "GTDH_GU") + "'");//휴가구분
	var sCodmNm = this.ds_Hr305.getColumn(iFvFindRow, "CODM_NM");
	
	//직위명
	var iFrow = this.ds_Hr202.findRowExpr("CODM_CD=='" + this.ds_Master10.getColumn(iRow, "POSI_CD") + "'");//직위	
	var sPosiNm = this.ds_Hr202.getColumn(iFrow, "CODM_NM");
	
	//성명
	var sEmpmNm = this.ds_Master10.getColumn(iRow, "EMPM_NM");//등록자가 바뀔수 있다.
	
	if (iCheckCnt == 1) {
		sWofhNm = "[근태계:"+sCodmNm+"]"+ sEmpmNm + sPosiNm +" ("
	                                     +this.gfn_StringFormat(this.ds_Master10.getColumn(iRow, "FROM_DD"), "@@@@.@@.@@")
										 +"~"+this.gfn_StringFormat(this.ds_Master10.getColumn(iRow, "CLOS_DD"), "@@@@.@@.@@")+")"; //결재명
	} else {
		sWofhNm = "[근태계:"+sCodmNm+"]"+ sEmpmNm + sPosiNm +" ("
	                                     +this.gfn_StringFormat(this.ds_Master10.getColumn(iRow, "FROM_DD"), "@@@@.@@.@@")
										 +"~"+this.gfn_StringFormat(this.ds_Master10.getColumn(iRow, "CLOS_DD"), "@@@@.@@.@@")+") 외 " + (iCheckCnt - 1) + "건";//결재명
	}	
	
	//휴가구분, 휴가구분에 따라서 자동전표 생성여부 때문에 결재업무ID를 쪼갬.
	//연차,반차,반반차는 직송이 있기 때문에 결재업무ID를 쪼깸.
	//잔여연차가 마이너스 이면 조직장이 필수이므로 결재업무ID를 쪼갬.
	var sGtdhGu = this.ds_Master10.getColumn(iRow, "GTDH_GU"); //휴가구분
	var nRemnCt = this.ds_Master10.getColumn(iRow, "REMN_CT"); //잔여연차
	
	//0020: 경조, 0010: 연차, 0005: 반차, 0025:반반차
	if (sGtdhGu == "0020") {
		var sWctmId = "WF_M_HD_3300_W2"
		var sGu = "1"
	} else if (sGtdhGu == "0010" || sGtdhGu == "0005" || sGtdhGu == "0025") {
		if (nRemnCt < 0 ) { 
			var sWctmId = "WF_M_HD_3300_W4"
			var sGu = "2"
		} else {
			var sWctmId = "WF_M_HD_3300_W3"
			var sGu = "2"
		}
	} else {
		var sWctmId = "WF_M_HD_3300_W1"
		var sGu = "1"
	};
	
	if (sGu == "1") {	
		var sPopupId = "C_WF_REQUEST";//팝업ID - 결재요청
		var oArg = { CommCd:this.gfn_GetSessionValue("COMM_CD")//회사코드
				   , ModuCd:"HR"//모듈구분
				   , WctmId:sWctmId//결재업무ID
				   //, SystId:this.gfn_GetSessionValue("SYST_ID")//결재자_시스템사번
				   , SystId:sSystIdChk
				   , WofhNm:sWofhNm//결재명
				   , WofhNo:this.ds_Master10.getColumn(iRow, "WOFH_NO")//결재PID
				   , InnrGb:"1"//내부결재구분(1:신규, 2:내부)
				   , CharSystId:""//담당자_시스템사번
				   , CharWfguCd:""//담당자 결재구분
				   , SworqVr:"3"//결재요청버전(1,2,3)
				   , WfamAmtr:"0"//금액
				   , WfamOpid:""//원천PIID
				   , WfPkvl:sPks//PK값 정보 - PK 값을 순서대로 전달, Row구분자는 ⓡ, 조건 구분자는 ⓐ
				   
				   //자동전표관련 파라메타-자동전표를 사용하는 화면에만 추가
				   , AutmCd:"HD_3300_10" //분류코드-자동전표환경설정 테이블에 등록된 데이터
				   , PrgmId:this.fvComPrgmId //프로그램ID
				   , ManaNo:sManaNo //관리번호-자동전표상세테이블에 사용하는 업무Key
				   , WofhSq:sWofhSq //결재차수-자동전표상세테이블에 사용하는 업무Key
				   , AtobKey:sAtobKey //자동전표쿼리 Key정보
				   };//Argument처리
		this.gfn_WfRequestPop(sPopupId, oArg);	
	} else if (sGu == "2") {	
		var sPopupId = "C_WF_REQUEST";//팝업ID - 결재요청
		var oArg = { CommCd:this.gfn_GetSessionValue("COMM_CD")//회사코드
				   , ModuCd:"HR"//모듈구분
				   , WctmId:sWctmId//결재업무ID
				   //, SystId:this.gfn_GetSessionValue("SYST_ID")//결재자_시스템사번
				   , SystId:sSystIdChk
				   , WofhNm:sWofhNm//결재명
				   , WofhNo:this.ds_Master10.getColumn(iRow, "WOFH_NO")//결재PID
				   , InnrGb:"1"//내부결재구분(1:신규, 2:내부)
				   , CharSystId:""//담당자_시스템사번
				   , CharWfguCd:""//담당자 결재구분
				   , SworqVr:"3"//결재요청버전(1,2,3)
				   , WfamAmtr:"0"//금액
				   , WfamOpid:""//원천PIID
				   , WfPkvl:sPks//PK값 정보 - PK 값을 순서대로 전달, Row구분자는 ⓡ, 조건 구분자는 ⓐ
				   
				   //자동전표관련 파라메타-자동전표를 사용하는 화면에만 추가
				   , AutmCd:"HD_3300_10" //분류코드-자동전표환경설정 테이블에 등록된 데이터
				   , PrgmId:this.fvComPrgmId //프로그램ID
				   , ManaNo:sManaNo //관리번호-자동전표상세테이블에 사용하는 업무Key
				   , WofhSq:sWofhSq //결재차수-자동전표상세테이블에 사용하는 업무Key
				   , AtobKey:sAtobKey //자동전표쿼리 Key정보
				   
				   , WfUser:sWfUser//결재자구분
				   };//Argument처리
		this.gfn_WfRequestPop(sPopupId, oArg);	
	}

}

/***********************************************************************************************
 * 09.02. [공통버튼] 결재요청 유효성체크
 ***********************************************************************************************/  
this.fn_VaildWfDecide = function() {
	
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.fvFileDiv.fn_CheckFileAddYn()) {
		var sWOFH_ST = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST");//결재진행상태
		
		//결재상태에 따른 결재요청 가능여부 체크
		if (!this.gfn_CheckDecideWf(sWOFH_ST)) {
			return false;
		}
		
	} else {
		var arrMsgArg = ["결재요청"];
		this.gfn_Alert("SM064", arrMsgArg);//변경된 내용이 있습니다.\n저장 후 &ITM0하시기 바랍니다.
		return false;
	}
	
	 //결재진행내역체크
   if (this.ds_Master10.getRowCount() < 1 || this.ds_Master10.rowposition < 0) {
      this.gfn_Alert("WF016", arrMsgArg);//결재내역을 선택한 후 결재요청하세요.
      return false;
   }
   
   var sGtdhGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "GTDH_GU")
   
   //[월마감제어-결재요청]	
   //휴가구분이 장기해외근무(0130) 는 기간의 TO로 월마감 체크한다.
	if (sGtdhGu == "0130") {
		var sBUDG_MM = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD"), "");//마감년월
		var sMGAS_DD = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD"), "");//마감일자
	} else {
		var sBUDG_MM = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD"), "");//마감년월
		var sMGAS_DD = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD"), "");//마감일자
	};	
	
	if (!this.gfn_IsNull(sBUDG_MM)) {
		//마감여부 체크
		var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
		var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
		paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
		paramObj.MODM_CD = "HD";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
		paramObj.ACUM_CD = "";//[선택] 회계단위
		paramObj.BUSM_CD  = "";//[선택] 사업부문
		paramObj.SAUM_CD  = "";//[선택] 사업장코드
		paramObj.DEPM_CD  = "";//[선택] 부서코드
		paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)		
		paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
		
		paramObj.MGAS_DD  = sMGAS_DD;//[선택] 마감일자
		
		var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		
		/* 2022.05.24 임시 막음
		//월마감된 경우
		if (bCheck) {
			this.gfn_Alert("CA027");//해당월의 정보가 마감이 되어서 결재요청 할 수 없습니다.
			return false;
		}	
		 */
	}

	return true;
}

