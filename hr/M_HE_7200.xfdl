/***********************************************************************************************
 * 15. [사용자정의 함수]
 ***********************************************************************************************/
this.ds_Master20_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1 && !this.fvMasterSearchMode) {

		//조회된 정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master20.getColumn(this.ds_Master20.rowposition, "DEPM_CD"))) {
		
			this.fvMasterSearchMode = true;//마스터내역 조회중여부
			
			this.ds_Input.clearData();
			this.ds_Detail20.clearData();
			
			var k = this.ds_Input.addRow();
			
			this.ds_Input.setColumn(k, "FROM_DD", this.div_Search.form.spn_BasmDtFr.value);
			this.ds_Input.setColumn(k, "DEPM_CD", obj.getColumn(e.newrow, "DEPM_CD"));
			
			var MAX_DEPV_DD = this.fn_getDepvDD();
			
			this.ds_Input.setColumn(0, "MAX_DEPV_DD", MAX_DEPV_DD);				
			
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "hr:M_HE_7200_D_SL20";
			this.gfn_AddComSearchService(this, svcAddObj);			
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL20_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_PS;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail20=ds_Detail20";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
