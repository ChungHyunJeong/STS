this.fn_ComBtnSearch = function()
{ 
	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;	

	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Input.clearData();
	
	var sSqlId = "";//쿼리ID
	var sOutDatset = "";//출력 데이터셋
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.spn_BasmDtFr.value);
	this.ds_Input.setColumn(0, "CLOS_DD", this.div_Search.form.spn_BasmDtTo.value);
	this.ds_Input.setColumn(0, "DEPM_CD", this.div_Search.form.edt_DepmCd.value);
	this.ds_Input.setColumn(0, "SYST_ID", this.div_Search.form.edt_SystId.value);
	this.ds_Input.setColumn(0, "MBOM_PLDG", this.div_Search.form.cbo_MbomPldg.value);
	this.ds_Input.setColumn(0, "MBOM_PGYN", this.div_Search.form.cbo_MbomPgyn.value);

	var MAX_DEPV_DD = this.fn_getDepvDD();
	
	this.ds_Input.setColumn(0, "MAX_DEPV_DD", MAX_DEPV_DD);		
	
	var iTabIndex = this.div_MasterView.form.tab_Master.tabindex;
	
	//개인별평가결과
	if (iTabIndex == 0) {
		this.ds_Master10.clearData();
		sSqlId = "hr:M_HE_7200_M_SL10";
		sOutDatset = "ds_Master10=ds_Master10";
		
	//평가등급별현황
	} else if (iTabIndex == 1) {
		this.ds_Master20.clearData();
		this.ds_Detail20.clearData();
		sSqlId = "hr:M_HE_7200_M_SL20";
		sOutDatset = "ds_Master20=ds_Master20";
		
	//부서별평균성향조회
	} else if (iTabIndex == 2) {
		this.ds_Master30.clearData();
		this.ds_Detail30.clearData();
		sSqlId = "hr:M_HE_7200_M_SL30";
		sOutDatset = "ds_Master30=ds_Master30";
		
	//다면평가분석결과
	} else if (iTabIndex == 3) {	
		this.ds_Master40.clearData();
		sSqlId = "hr:M_HE_7200_M_SL40";
		sOutDatset = "ds_Master40=ds_Master40";
	}
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = sSqlId;
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MST_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
}

this.fn_getDepvDD = function() {
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "hr:M_HE_7200_D_SL21";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid 		= "DEPM_SEARCH";			//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName 	= this.G_SV_HR;			//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset 	= "ds_Input=ds_Input" 	//입력 데이터셋
	svcCallObj.sOutDatset 	= "ds_Detail21=ds_Detail21";//출력 데이터셋
	svcCallObj.sOtherArg 	= "";					//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";		//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.isSync = true;//동기식으로 설정할지 여부

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
	
	return this.ds_Detail21.getColumn(0, "MAX_DEPV_DD");
}

