/***********************************************************************************************
 * 02.MG0.07. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	
	// 출장기간 변경시 (박/일 계산)
	if (e.columnid == "FROM_DD" || e.columnid == "CLOS_DD") {
		var sBzthNo   = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO"); //출장번호 유무
		var fromDate  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD"); //출장기간(시작일자)
		var closDate  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD"); //출장기간(종료일자)
		var minBztbDd = this.ds_Detail10.getMin("BZTB_DD"); //최초 출발일자
		var maxBztbDd = this.ds_Detail10.getMax("BZTB_DD"); //최종 출발일자
		var minArriDd = this.ds_Detail10.getMin("ARRI_DD"); //최초 도착일자
		var maxArriDd = this.ds_Detail10.getMax("ARRI_DD"); //최종 도착일자
		var sToDay = this.gfn_GetCommDate("toDay");//현재일자		
		
		var iNghtQt = ""; //박
		var iDaysQt = ""; //일

		//X박X일 구하기
		if (this.gfn_IsNull(fromDate) || this.gfn_IsNull(closDate)) {
			return false;
			
		} else if (fromDate > closDate) {
			//시작일자, 종료일자 원복
			if (e.columnid == "FROM_DD") {
				if (this.gfn_IsNull(sBzthNo)) {
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD",	sToDay);
				} else {
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "FROM_DD")); //출장기간(시작일자)
				}
			} else if (e.columnid == "CLOS_DD") {
				if (this.gfn_IsNull(sBzthNo)) {
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD",	"");
				} else {
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "CLOS_DD")); //출장기간(종료일자)
				}
			}
			
			this.gfn_Alert("SM018");//종료일자가 시작일자보다 빠릅니다
			return false;

		} else if (fromDate < sToDay) {
			//출장기간 시작일자는 현재일자보다 동일하거나 커야 합니다.
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD",	sToDay);
			if (closDate < sToDay) {	//출장종료일자가 오늘보다 작은경우 현재일로 변경
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD",	sToDay);
			}
			
			var arrMsgArg = ["시작일자"];	//메시지내용
			this.gfn_Alert("HR015", arrMsgArg); //출장기간 시작일자는 현재일자보다 동일하거나 커야 합니다.
			
			return false;
			
		} else if (closDate < sToDay) {
			//출장기간 종료일자는 현재일자보다 동일하거나 커야 합니다.
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD",	sToDay);
			if (fromDate < sToDay) {	//출장시작일자가 오늘보다 작은경우 현재일로 변경
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD",	sToDay);
			}
			
			var arrMsgArg = ["종료일자"];	//메시지내용
			this.gfn_Alert("HR015", arrMsgArg); //출장기간 종료일자는 현재일자보다 동일하거나 커야 합니다.

			return false;
			
		} else {	//출장기간 변경시
			iNghtQt = this.gfn_GetDiffDate(fromDate, closDate); //박
			iDaysQt = iNghtQt + 1;                              //일
			
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "NGHT_QT", iNghtQt);	//박
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DAYS_QT", iDaysQt);	//일		

			this.ds_Detail10.set_enableevent(false); //row 생성속도 향상을 위한 이벤트 false

			/* ### Detail10(출장상세일정) 존재시 ### */
			if (this.ds_Detail10.getRowCount() > 0) {	//Detail10이 존재시

				//1. Detail10 Row 건수 for문
				for(var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
					//최초 출발일자 = 출장시작일자
					if (minBztbDd == this.ds_Detail10.getColumn(iLoop, "BZTB_DD")) {
						this.ds_Detail10.setColumn(iLoop, "BZTB_DD", fromDate); //출발일자 = 시작일자
						
					//최종 출발일자 = 출장종료일자
					} else if (maxBztbDd == this.ds_Detail10.getColumn(iLoop, "BZTB_DD")) {
					
						this.ds_Detail10.setColumn(iLoop, "BZTB_DD", closDate); //출발일자 = 종료일자
					}
					
					//최초 도착일자 = 출장시작일자
					if (minArriDd == this.ds_Detail10.getColumn(iLoop, "ARRI_DD")) {
						this.ds_Detail10.setColumn(iLoop, "ARRI_DD", fromDate); //도착일자 = 시작일자
						
					//최종 도착일자 = 출장종료일자
					} else if (maxArriDd == this.ds_Detail10.getColumn(iLoop, "ARRI_DD")) {
					
						this.ds_Detail10.setColumn(iLoop, "ARRI_DD", closDate); //도착일자 = 종료일자
					}
					
					this.ds_Detail10.setColumn(iLoop, "crudType", "U");     //crudType = U (set_enableevent 설정으로 강제 적용)
				}
			
				//2. Detail11 존재시 출발일자(출장일자) 변경
				if (this.ds_Detail11.getRowCountNF() > 0) {	//Detail11이 존재시
					for (var iLoop=0;iLoop<this.ds_Detail11.getRowCountNF();iLoop++) {
						this.ds_Detail11.setColumnNF(iLoop, "BZTB_DD", fromDate); //출발일자 = 시작일자
					}

				this.fn_SetCostDetailProc(); //Detail11 산출내역, 금액관련 처리			
					
				}
				
			/* ### Detail10(출장상세일정) 미존재시 총 2Row 생성 ### */
			} else {	//Detail10 최초 입력시
				
				//1. 초기셋팅
				this.ds_Detail10.clearData(); //출장일정상세 (총 2Row 생성)
												
				var sBztbSe = 1;  //출장순번
				var iAddRow = 0;  //행추가 Row
				var iRowCnt = 2;  //Detail10 Row Count
				var sBztbDd = ""; //출발일자
				var sArriDd = ""; //도착일자
				
				//2. 총 2Row의 출장일정상세 생성
				for(var iLoop=0;iLoop<iRowCnt;iLoop++) {
					//1Row 출국하는 일자, 2Row 귀국하는 일자
					if (iLoop == 0) {
						sBztbDd = fromDate; //첫번째 Row 출장기간 출국(출발일자, 도착일자) 
					} else {
						sBztbDd = closDate; //두번째 Row 출장기간 귀국(출발일자, 도착일자)
					}

					iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)

					this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
					this.ds_Detail10.setColumn(iAddRow, "BZTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO"));
					this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
					this.ds_Detail10.setColumn(iAddRow, "BZTB_DD", sBztbDd);                                  //출발일자(출장일자)
					this.ds_Detail10.setColumn(iAddRow, "FROM_MI", "00"); 	                                  //출발(분) - Default 0분
					this.ds_Detail10.setColumn(iAddRow, "BZTB_SE", sBztbSe+iLoop);                            //출장순번
					this.ds_Detail10.setColumn(iAddRow, "ARRI_DD", sBztbDd);                                  //도착일자						
					this.ds_Detail10.setColumn(iAddRow, "ARRI_MI", "00");                                     //도착(분) - Default 0분
					this.ds_Detail10.setColumn(iAddRow, "MAXB_SE", sBztbSe+iLoop);                            //최종순번
				}			
			}
			
			this.ds_Detail10.set_enableevent(true); //row 생성속도 향상을 위한 이벤트 ture
		} 
		
	//지원사항	콤보 변경시
	} else if (e.columnid == "SUPP_CD") {		
		var fromDate = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD"); //출장기간(시작일자)
		var sSuppCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SUPP_CD"); //지원사항
		var sNatmCd = this.ds_Detail10.getColumn(0, "NATM_CD");  //국가(최초입력 라인)
		var sToDay  = this.gfn_GetCommDate("toDay");//현재일자		

		//1. 출발시작일자가 현재일자보다 작은경우 지원사항 구분 변경불가(원복)
		if (fromDate < sToDay) {	
			//이전값으로 원복
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "SUPP_CD",	this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "SUPP_CD"));
			
			this.gfn_Alert("HR020");//출발시작일자가 오늘일자 이전인 경우 지원사항 변경을 할 수 없습니다.
			return false;
		
		//2. [변경]출장비상세(Detail11) 수정
		} else if (!this.gfn_IsNull(sNatmCd) && this.ds_Detail11.getRowCount() > 0) {	//국가(급수구분) 및 Detail11(출장비 산출내역) 존재시

			//출장비상세(Detail11) 데이터 변경여부 체크 후 초기화 (11/8)
			if (this.gfn_DsIsUpdated(this.ds_Detail11)) {
				this.ds_Detail11.reset();
				for (var iLoop=0;iLoop<this.ds_Detail11.getRowCount();iLoop++) {
					this.ds_Detail11.setColumn(iLoop, "crudType",""); //crudType 초기화
				}
			}	
			
			//[필수값]필수값 변경

			this.fn_SetCostDetailProc(); //Detail11 산출내역, 금액관련 처리			
			
		}
	}	
	
};

 
