/***********************************************************************************************
 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
			if (this.ds_Master10.getRowCount() > 0) {
			this.ds_Master10.addColumn("CURR_CD", "string");
			this.ds_Master10.addColumn("EXCH_RT", "string");
			
				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master10.findRowExpr("BZTH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ=='" + this.fvSchKey02 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}				
				
				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
					
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
								
			} else {
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			
				//입력영역 비활성화
				this.div_MasterInput.set_enable(false);
					
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = false;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);					
			}
			
			//저장후 재조회여부-true
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}			
	
			break;
		case "SAVE":   //저장		

			//첨부파일 추가건이 없으면
			if (!this.fvFileDiv.fn_CheckFileAddYn()) {
				this.fvSchKeyYn = true;//저장후 재조회여부
				this.fn_ComBtnSearch();//저장후 재조회
			} else {
				this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);			
			}
			break;	
		case "DTL_SEARCH": //조회		
			this.ds_Detail10.addColumn("s_chk", "string");
			this.ds_Detail10.addColumn("crudType", "string");
			this.ds_Detail11.addColumn("crudType", "string");
			this.ds_Detail15.addColumn("s_chk", "string");
			this.ds_Detail15.addColumn("crudType", "string");
			this.ds_Detail15.addColumn("crudPopup", "string");

			//[공통-자동전표상세] 조회
			if (this.ds_Master10.getRowCount() > 0) {
				var sPRGM_ID = this.fvComPrgmId;//프로그램ID
				var sMANA_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO");//출장번호-업무Key
				var sWOFH_ST = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST");//결재상태
				var sWOFH_SQ = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//결재차수
				var atobObj = new Object();//[공통] 자동전표 파라메타 정보 Object
				atobObj.PRGM_ID = sPRGM_ID;
				atobObj.MANA_NO = sMANA_NO;
				atobObj.WOFH_ST = sWOFH_ST;
				atobObj.WOFH_SQ = sWOFH_SQ;
				
				eval(this.fvAtobDiv).fn_ComBtnSearch(atobObj);//자동전표 상세내역 조회
			}
			
			//첨부파일 정보 조회
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
			oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO");//PK번호2-출장번호
			oFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수						
			
			this.fvFileDiv.fn_AtchFileSearch(oFileInfo);			
			
			if (this.ds_Detail10.getRowCount() > 0) {
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10);
			} else {
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10);			
			}

			if (this.ds_Detail11.getRowCount() > 0) {
				this.ds_Detail11.filter("");
				//지원사항 = 장기출장(2주이상)
				if (this.ds_Master10.getColumn(this.ds_Master10.rowposition,"SUPP_CD") == '0060') { 
					this.ds_Detail11.filter("BZTM_CD == '0030' || BZTM_CD == '0090'");	//출장비, 식사비								
				//지원사항 = 그외 : 숙박비, 식사비
				} else { 
					this.ds_Detail11.filter("BZTM_CD == '0020' || BZTM_CD == '0040'");	//숙박비, 잡비
					
				}			

				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail11);
			} else {
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail11);			
			}

			break;			
			
	}
};

/***********************************************************************************************
 * 13.02. [팝업] CallBack 함수 처리
          strId : 팝업호출시 지정한 팝업ID
	      rtnValue : 팝업창에서 close시 지정한 반환값
          주의 : Open(popuptype=modeless)창은 팝업콜백을 지원하지 않음
 ***********************************************************************************************/ 
this.fn_PopupCallback = function(strId, rtnValue)
{

	//[조회영역]
		
	//[Master영역]
				
	//[Detail영역]
	//계약번호		
	if (strId == "B_CNTH_NO_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);
			
			if (!this.gfn_IsNull(arrRtnVal.CNTH_NO)) {
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "CNTH_NO", arrRtnVal.CNTH_NO);
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "CNTH_NM", arrRtnVal.CNTH_NM);
			}	
		} else {
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "CNTH_NO", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "CNTH_NM", "");
		}		
	//연구프로젝트		
	} else if (strId == "B_RPJH_NO_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);
			
			if (!this.gfn_IsNull(arrRtnVal.RPJH_NO)) {
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "RPJH_NO", arrRtnVal.RPJH_NO);
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "RPJH_NM", arrRtnVal.RPJH_NM);
			}
		} else {
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "RPJH_NO", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "RPJH_NM", "");
		}		
	//[Detail영역]
	//출장국가검색
	} else if (strId == "M_HD_3110_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.BZAM_CD)) {
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "NATM_CD", arrRtnVal.BZAM_CD);
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "NATM_NM", arrRtnVal.BZAM_NM);
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "BZAM_GU", arrRtnVal.BZAM_GU);
			}
		} else {
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "NATM_CD", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "NATM_NM", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "BZAM_GU", "");
		}		
	
	}
};
 
