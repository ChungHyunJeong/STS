/*---------------------------------------------------------------------------------------------
 *    [Master 입력영역-Button] 엑셀업로드 Call Back 함수
  ---------------------------------------------------------------------------------------------*/
this.fn_ImportCallback = function(sImportId)
{	
	//this.ds_Detail10.set_enableevent(false); //  SYST_ID 사번이 입력되어야 하므로 이벤트를 false로 하면 안됨
	
	var nRow    = 0;
	var sEmpmCd = "";
	
	this.fvExcelChk = "Y";	
	
	this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.set_enableredraw(false);
	//this.ds_Master10.set_enableevent(false);  
	
	this.ds_Master10.clearData();	
	
	for (var iLoop=0; iLoop<this.ds_Import.getRowCount(); iLoop++) {
		var iCol = this.ds_Import.getColumn(iLoop, "Column0"); 	
		
		
		if (!this.gfn_IsNull(iCol)) {
		
			nRow = this.ds_Master10.addRow();			
			
			this.ds_Master10.setColumn(nRow, "HRWK_YN", "0");			
			 
			sEmpmCd = this.ds_Import.getColumn(iLoop, "Column0");
			
			this.ds_Master10.setColumn(nRow, "EMPM_CD", sEmpmCd);
			this.ds_Master10.setColumn(nRow, "DUTY_AM", this.ds_Import.getColumn(iLoop, "Column2")); //직책수당			
			this.ds_Master10.setColumn(nRow, "CERT_AM", this.ds_Import.getColumn(iLoop, "Column3")); //자격수당
			this.ds_Master10.setColumn(nRow, "JOBM_AM", this.ds_Import.getColumn(iLoop, "Column4")); //직무수당
			
			this.ds_Master10.setColumn(nRow, "crudType", "U"); //저장시 ds_detail10 의 내용을 저장키 위함.
			
			if(this.gfn_IsNull(this.ds_Master10.getColumn(nRow, "SYST_ID"))) {
				// 퇴사했다는 의미
				this.ds_Master10.deleteRow(nRow);
			}
			
		}		
	}
	this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.set_enableredraw(true);
	
	this.ds_Master10.set_enableevent(true);  
}
 


