/***********************************************************************************************
 * 02.MG0.07. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
 /*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{	
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position
	
	// 사번
	if (e.columnid == "EMPM_CD") {
		var arrArgs = ["DEPM_CD","DEPM_NM","DUTY_NM","POSI_NM"];
		this.gfn_GetCommEmpmGrid(obj, "EMPM_CD", "EMPM_NM", "SYST_ID", arrArgs);//사원명 검색		
	} else {
	
		var BaseAm = this.ds_Master10.getColumn(e.row ,"BASE_AM");
		var OtwkAm = this.ds_Master10.getColumn(e.row ,"OTWK_AM");
		var OthoAm = this.ds_Master10.getColumn(e.row ,"OTHO_AM");
		
		var	DutyAm = this.ds_Master10.getColumn(e.row ,"DUTY_AM");
		var JobmAm = this.ds_Master10.getColumn(e.row ,"JOBM_AM");
		var YearAm = this.ds_Master10.getColumn(e.row ,"YEAR_AM");
		var CertAm = this.ds_Master10.getColumn(e.row ,"CERT_AM");
		var DangAm = this.ds_Master10.getColumn(e.row ,"DANG_AM");
		var VehiAm = this.ds_Master10.getColumn(e.row ,"VEHI_AM");
		
		if(this.gfn_IsNull(BaseAm)) {BaseAm = 0;}
		if(this.gfn_IsNull(OtwkAm)) {OtwkAm = 0;}
		if(this.gfn_IsNull(OthoAm)) {OthoAm = 0;}
		
		if(this.gfn_IsNull(DutyAm)) {DutyAm = 0;}
		if(this.gfn_IsNull(JobmAm)) {JobmAm = 0;}
		if(this.gfn_IsNull(YearAm)) {YearAm = 0;}
		if(this.gfn_IsNull(CertAm)) {CertAm = 0;}
		if(this.gfn_IsNull(DangAm)) {DangAm = 0;}
		if(this.gfn_IsNull(VehiAm)) {VehiAm = 0;}

		this.ds_Master10.setColumn(e.row, "YMSA_AM", BaseAm + OtwkAm + OthoAm + DutyAm + JobmAm + YearAm  + CertAm + DangAm + VehiAm);
		this.ds_Master10.setColumn(e.row, "YMBS_AM", BaseAm + OtwkAm + OthoAm);
		
		this.ds_Master10.setColumn(e.row, "YMDF_AM", (BaseAm + OtwkAm + OthoAm + DutyAm + JobmAm + YearAm  + CertAm + DangAm + VehiAm) - (BaseAm + OtwkAm + OthoAm));	
	}
};

