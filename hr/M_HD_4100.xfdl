			if(this.ds_Detail10.getColumn(i, "CNTH_OP")=="0010"){
				if(this.gfn_IsNull(this.ds_Detail10.getColumn(i, "CNTH_NO"))){
					bChk = false;
					sCols = "영업계약";
				}
			}
		}
		
		if(this.ds_Detail10.getColumn(i, "PJTH_OP")=="0010"){
			if(this.gfn_IsNull(this.ds_Detail10.getColumn(i, "PJTH_NO"))){
