		if (!this.gfn_CheckRecideWf(sWOFH_ST)) {			
			return false;		
		}
		
		//타프로그램에서 생성된 자료 체크
		var sInpuCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "INPU_CD");//근태생성구분
		if (!this.gfn_IsNull(sInpuCd)) {
			this.gfn_Alert("HR245");//자동 생성된 데이터라 재기안 할 수 없습니다.
			return;
		}
		
	} else {
		var arrMsgArg = ["재기안"];
		this.gfn_Alert("SM064", arrMsgArg);//변경된 내용이 있습니다.\n저장 후 &ITM0하시기 바랍니다.
		return false;
	}
	
	return true;
}
