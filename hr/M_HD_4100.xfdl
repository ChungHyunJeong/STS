this.fn_ComBtnAdd = function()
{
	this.ds_Master10.addColumn("s_chk", "string");
	this.ds_Master10.addColumn("crudType", "string");
		
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.
		return;
	}
	
	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자

	//var iAddRow = this.ds_Master10.addRow();
	var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Master10);//crud_flag 변경-입력(I)
	this.fvMmCloseDay = sToDay;//[월마감] 마감일자
	
	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", "1");
	this.ds_Master10.setColumn(iAddRow, "WOFH_ST", "0010");
	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");
	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//부서코드	
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//부서명	
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//등록자사번	
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//등록자명		
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//등록자ID			
	
	this.ds_Master10.setColumn(iAddRow, "WKDH_DD", sToDay);//업무일자-오늘날자로 설정
	
	if(this.ds_Hr440.getRowCount() > 0){
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WhfrHr.set_value(this.ds_Hr440.getColumn(0,"OP01_NM"));
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WhfrMi.set_value(this.ds_Hr440.getColumn(0,"OP02_NM"));
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WhtoHr.set_value(this.ds_Hr440.getColumn(0,"OP03_NM"));
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WhtoMi.set_value(this.ds_Hr440.getColumn(0,"OP04_NM"));
	}
	
	//전표손익제어 찾기
	this.fn_Set_SctrCd(this.gfn_GetSessionValue("DEPM_CD"), iAddRow);
	
	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.div_DetailInput.form.tab_Detail.set_enable(true);
	

	this.div_MasterInput.form.tab_Master.tpg_01.form.btn_EmpmCd.set_enable(true); //add
	

	//처음 입력 항목에 포커스
	//this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.setFocus();
	
	this.ds_Detail10.clearData();//상세정보 초기화
	this.ds_Detail20.clearData();//검색정보 초기화
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
	//첨부파일정보 권한설정
	var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
	oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
	this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
	
	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
}

