				bChk = false;
				sCols += (sCols!=""? ",": "") + "업무상세내역";
			}
		}
		if(this.ds_Detail10.getColumn(i, "WKDB_RT") < 0) {
			
			bChk1 = false;
		}
		
		if(!bChk){
			sRows = this.ds_Detail10.getColumn(i, "WKDM_NM");
			break;
		}
		
		if(!bChk1){
			sRows1 = (i+1) + " 행";
			break;
		}
	}
	
	if(!bChk){		
		var arrMsgArg = [sRows, sCols];
		this.gfn_Alert("HR002",arrMsgArg);//업무[&ITM0]은(는) [&ITM1]이(가) 필수입니다.
		return false;
