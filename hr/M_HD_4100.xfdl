this.div_Search_mae_CnthNo_onkeyup = function(obj:nexacro.MaskEdit,e:nexacro.KeyEventInfo)
{
	if (e.keycode == 8) {
		if (obj.value.length == 7) {
			obj.set_format("@@@-@@@@");
		} else if (obj.value.length == 9) {
			obj.set_format("@@@@-@-@@@@");
		} else {
			obj.set_format("@@@@@@@@@");
		}		
	}
};


