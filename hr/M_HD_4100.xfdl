	//그리드 콤보 및 캘린더 클릭시 펼치기
	obj.dropdownCombo();
	obj.dropdownCalendar();
	this.gfn_SetGridSelectRowColor(obj, e.row, true);//선택한 Row BackgroundColor 설정	- 기본그리드 설정된 CSS정보 유지
	//this.gfn_SetGridSelectRowColor(obj, e.row);//선택한 Row BackgroundColor 설정

	var bCommSaveButton = false;//저장버튼 활성화 여부-기본값은 false로 해야함. - 결재승인후 수정일 필요한 화면에서만 true로 설정해야함. 
	this.fvWofhStatus = this.gfn_SetCommButtonWf(this, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"), bCommSaveButton);//결재진행상태		
	
	if(e.col > 0){
	
      //결재진행상태가 아닌경우
      if (!this.fvWofhStatus) {   
			if(this.gfn_IsNull(this.ds_Detail10.getColumn(e.row, "WKDM_CD"))
				&& e.col != obj.getBindCellIndex("body", "WKDM_NM" )+1){
				var arrMsgArg = ["업무"];
				this.gfn_Alert("PP002", arrMsgArg);//등록된 [&ITM0]이(가) 없습니다.\n\n먼저 [&ITM0]을(를) 등록해 주십시요.
				return false;
			}
			
			//업무조회
			if(e.col == obj.getBindCellIndex("body", "WKDM_NM" )+1){
				var sPopupId = "B_WKDM_NO_01";//팝업ID - 업무일지코드
				var oArg = { WkdmNm:"", PrgmId:this.fvComPrgmId};//Argument처리
				this.fn_WkdmPop(sPopupId, oArg);	
			}
			//계약번호
			else if (e.col == obj.getBindCellIndex("body", "CNTH_NM" )+1) {	
				if(this.ds_Detail10.getColumn(e.row, "CNTH_OP")=="0030") return false;
				var sPopupId = "B_CNTH_NO_01";//팝업ID - 계약정보
				//var oArg = { CnthNo:"", PrgmId:"M_SD_3100" };//Argument처리
				var oArg = { CnthNo:"", PrgmId:this.fvComPrgmId };//Argument처리
				this.gfn_ContPop(sPopupId, oArg);	
			}		
			//영업프로젝트
			else if (e.col == obj.getBindCellIndex("body", "PJTH_NM" )+1) {
				if(this.ds_Detail10.getColumn(e.row, "PJTH_OP")=="0030") return false;
				var sPopupId = "B_PJTH_NO_01";//팝업ID - 프로젝트정보
				//var oArg = { PjthNm:"", PrgmId:"M_SD_3100"};//Argument처리
				var oArg = { PjthNm:"", PrgmId:this.fvComPrgmId};//Argument처리
				this.gfn_PjthPop(sPopupId, oArg);				
			}//생산 W/O 
			else if (e.col == obj.getBindCellIndex("body", "HOGI_NM" )+1) {	
				var sPopupId = "B_WORB_NO_01";//팝업ID - W/O 정보
				var oArg = { HogiNm:"", PrgmId:"M_PP_1200"};//Argument처리
				this.gfn_WorhPop(sPopupId, oArg);			
			}
			//연구프로젝트 ●팝업확정시 수정
			else if (e.col == obj.getBindCellIndex("body", "RPJH_NM" )+1) {	
				if(this.ds_Detail10.getColumn(e.row, "RPJH_OP")=="0030") return false;
				var sPopupId = "B_RPJH_NO_01";//팝업ID - 프로젝트정보
				var oArg = { PjthNm:"", PrgmId:"M_SD_3100"};//Argument처리
				this.gfn_RpjhPop(sPopupId, oArg);	
			}
		}
		//비고상세	
		if (e.col == obj.getBindCellIndex("body", "REMK_NM" )+1) {		
