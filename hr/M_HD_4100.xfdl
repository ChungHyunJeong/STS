this.fn_GtdhChk = function(){
	this.ds_Input.clearData();
		this.ds_Input.addRow();		

	this.ds_Input.setColumn(0,	"SYST_ID", this.ds_Master10.getColumn(this.ds_Master10.rowposition,"SYST_ID"));
	this.ds_Input.setColumn(0,	"WKDH_DD", this.ds_Master10.getColumn(this.ds_Master10.rowposition,"WKDH_DD"));
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "hr:M_HD_4100_M_SL15";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MST_SEARCH_EXISTS1111";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Gtdh=ds_Gtdh";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID		
	svcCallObj.isSync = true;//동기식으로 설정할지 여부

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);
	
	if(this.ds_Gtdh.rowcount > 0){
		var sGtdhGu = this.ds_Gtdh.getColumn(0, "GTDH_GU");
		var sHalfCd = this.ds_Gtdh.getColumn(0, "HALF_CD");
		
		//-- 오후 반반차일(Code : 25) 경우 퇴근시간은 15:30분 고정
		if(sGtdhGu == "0025"){ //반반차
			if(sHalfCd=="25"){
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WHTO_HR", "15");
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WHTO_MI", "30");
				
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WhtoHr.set_enable(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WhtoMi.set_enable(false);
			}		
		}else if(sGtdhGu == "0005"){ //반차
			if(sHalfCd=="20"){
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WHTO_HR", "12");
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WHTO_MI", "30");
				
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WhtoHr.set_enable(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WhtoMi.set_enable(false);
			}
		}		
		
	}else{
		//this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WHTO_HR", "17");
		//this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WHTO_MI", "30");

		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WhtoHr.set_enable(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WhtoMi.set_enable(true);
	}
}

