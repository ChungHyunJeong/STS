this.fn_GridDateSet = function() {
	
	//데이터셋 재설정
	this.ds_Master11.clear();
	
	this.ds_Master11.addColumn("SAUM_CD", "string");//사업자코드
	this.ds_Master11.addColumn("SAUM_NM", "string");//사업장명
	this.ds_Master11.addColumn("CARS_NM", "string");//차량명칭
	this.ds_Master11.addColumn("INOT_YN", "string");//출차여부
	this.ds_Master11.addColumn("CARM_CD", "string");//차량코드
	this.ds_Master11.addColumn("CHECK_GU", "BIGDECIMAL");//선택후 넘기기전여러줄중복체크용
	this.ds_Master11.addColumn("COUNT", "BIGDECIMAL");//선택후 넘기기전여러줄중복체크용
	
	var sDate = "";//날짜
	var sTime = "";//시간
	
	//From~To 날짜의 시간컬럼 동적 생성
	//24시는 없다. 00시부터 있다.
	if (this.ds_FrToDate.getRowCount() > 0) {
		for (var iLoop=0; iLoop < this.ds_FrToDate.getRowCount(); iLoop++) {
			
			sDate = this.ds_FrToDate.getColumn(iLoop, "FRTS_DT");
			
			for (var jLoop=0; jLoop <= 23; jLoop++) {
				sTime = jLoop.toString().padLeft(2, "0");
				this.ds_Master11.addColumn(sDate + "_" + sTime, "string");//시간+날짜
			}
		}
	}
	
	for (var iLoop=0; iLoop<this.ds_Master13.rowcount; iLoop++) {
		var sSaumCd = this.ds_Master13.getColumn(iLoop, "SAUM_CD");
		var sSaumNm = this.ds_Master13.getColumn(iLoop, "SAUM_NM");
		var sCarmCd = this.ds_Master13.getColumn(iLoop, "CARM_CD");	
		var sCarsNm = this.ds_Master13.getColumn(iLoop, "CARS_NM");	
		var sCaroDahr = this.ds_Master13.getColumn(iLoop, "CARO_DAHR");
		var sCaroStat = this.ds_Master13.getColumn(iLoop, "CARO_STAT");
		var sDisptat = this.ds_Master13.getColumn(iLoop, "DISP_STAT");
		
 		var iFvFindRow = this.ds_Master11.findRowExpr("SAUM_CD=='" + sSaumCd + "' && CARM_CD=='" + sCarmCd + "'", 0, this.ds_Master11.rowcount)
		
		if (iFvFindRow == -1) {
			var iRow = this.ds_Master11.addRow();
			
			this.ds_Master11.setColumn(iRow, "SAUM_CD", sSaumCd);
			this.ds_Master11.setColumn(iRow, "SAUM_NM", sSaumNm);
			this.ds_Master11.setColumn(iRow, "CARM_CD", sCarmCd);
			this.ds_Master11.setColumn(iRow, "CARS_NM", sCarsNm);
			this.ds_Master11.setColumn(iRow, sCaroDahr, sDisptat);
			
			this.ds_Master11.setColumn(iRow, "CHECK_GU", 0);
		} else if (iFvFindRow == 0) {
			this.ds_Master11.setColumn(iRow, "SAUM_CD", sSaumCd);
			this.ds_Master11.setColumn(iRow, "SAUM_NM", sSaumNm);
			this.ds_Master11.setColumn(iRow, "CARM_CD", sCarmCd);
			this.ds_Master11.setColumn(iRow, "CARS_NM", sCarsNm);
			
			this.ds_Master11.setColumn(iRow, sCaroDahr, sDisptat);
			
			this.ds_Master11.setColumn(iRow, "CHECK_GU", 0);
		} else {
			this.ds_Master11.setColumn(iFvFindRow, sCaroDahr, sDisptat);
			
			this.ds_Master11.setColumn(iFvFindRow, "CHECK_GU", 0);
		}
	}	
		
	//그리드 설정
	//그리드 고정컬럼 설정
	var sTotFormats       = '';//전체 Foramts
	var sFormatHead       =  '<Formats>';//Formats - Head
	    sFormatHead      += '<Format id="default">';
	var sFormatColumns    = '<Columns>';//Formats - Colums - 변동
	    sFormatColumns   += '<Column size="35" band="left"/>';
		sFormatColumns   += '<Column size="100" band="left"/>';
		sFormatColumns   += '<Column size="155" band="left"/>';
		//sFormatColumns   += '<Column size="100" band="left"/>';
		
	var sFormatColumnsEnd = '</Columns>';//Formats - Colums End
	
	var sFormatRows       = '<Rows>';//Rows
        sFormatRows      += '<Row size="26" band="head"/>';
        sFormatRows      += '<Row size="24" band="head"/>';
        sFormatRows      += '<Row size="24"/>';
        sFormatRows      += '</Rows>';	
		
	var sBandHead         = '<Band id="head">';//Band Head
	var sBandHeadCol      = '<Cell rowspan="2" text="No."/>';//Band Head Col - 변동
	    sBandHeadCol     += '<Cell col="1" rowspan="2" text="사업장"/>';	
		sBandHeadCol     += '<Cell col="2" rowspan="2" text="차량명칭"/>';
		//sBandHeadCol     += '<Cell col="3" rowspan="2" text="출자여부(현재)"/>';
		
	var sBandHeadRow      = '';//Band Head Row - 변동

	var sBandHeadEnd      = '</Band>';//Band Head End
	
	var sBandBody         = '<Band id="body">';//Band Body - 변동
	    sBandBody        += '<Cell expr="currow + 1" textAlign="center"/>';
		sBandBody        += '<Cell col="1" displaytype="normal" textAlign="left" text="bind:SAUM_NM"/>';
		sBandBody        += '<Cell col="2" displaytype="normal" textAlign="left" text="bind:CARS_NM"/>';
		//sBandBody        += '<Cell col="3" textAlign="center" text="bind:INOT_YN"/>';

	var sBandBodyEnd      = '</Band>';//Band Body End
	
	var sFormatEnd        = '</Format>';//Formats - End
	    sFormatEnd       += '</Formats>';
				
	var iColIndex = 2;//동적헤더컬럼 인덱스
	var sFrtoDt = "";//포맷날짜
	
	//From~To 날짜의 시간컬럼 동적 그리드 생성
	if (this.ds_FrToDate.getRowCount() > 0) {
		for (var iLoop=0; iLoop < this.ds_FrToDate.getRowCount(); iLoop++) {
			
			iColIndex++;
			
			sFrtoDt = this.ds_FrToDate.getColumn(iLoop, "FRTO_DT");//포맷날짜
			sDate = this.ds_FrToDate.getColumn(iLoop, "FRTS_DT");
			
			sBandHeadCol     += '<Cell col="' + iColIndex + '" colspan="24" text="' + sFrtoDt + '"/>';			
			
			for (var jLoop=0; jLoop <= 23; jLoop++) {
				sTime = jLoop.toString().padLeft(2, "0");

				if (jLoop > 0) {
					iColIndex++;
				}
				
				sBandHeadRow     += '<Cell row="1" col="' + iColIndex + '" text="' + jLoop + '"/>';//Band Head Row - 변동
				sFormatColumns   += '<Column size="30"/>';
				sBandBody        += '<Cell col="' + iColIndex + '" textAlign="center" text="bind:' + sDate + "_" + sTime + '"/>';
			}
		}
	}	
		
		sTotFormats = sFormatHead + sFormatColumns + sFormatColumnsEnd + sFormatRows + sBandHead + sBandHeadCol + sBandHeadRow + sBandHeadEnd + sBandBody + sBandBodyEnd + sFormatEnd;//전체 Foramts		
//trace(sTotFormats);
	this.tab_Master.tpg_01.form.grd_Master10.set_formats("")	
	this.tab_Master.tpg_01.form.grd_Master10.set_formats(sTotFormats);

}

this.tab_Master_tpg_01_grd_Master10_oncellclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	row = e.row;
	rowspan = this.tab_Master.tpg_01.form.grd_Master10.getCellProperty("body",e.cell,"rowposition");
	text 	= this.tab_Master.tpg_01.form.grd_Master10.getCellProperty("body",e.cell,"text");
 	cellidx = this.tab_Master.tpg_01.form.grd_Master10.getCellProperty("body",e.cell,"col");

	var sSaumCd = this.ds_Master11.getColumn(e.row, "SAUM_CD")
	var sCarmCd = this.ds_Master11.getColumn(e.row, "CARM_CD")
	
	var sDate = nexacro.replaceAll(text, "bind:", "")
	
	if (sDate == "SAUM_NM" || sDate == "CARS_NM" || sDate == "INOT_YN") {
		return false;
	};
	
	var sToDay = this.gfn_GetCommDate("toDayFull");//현재일시
	var sToDay1 = sToDay.substring(0, 4) + sToDay.substr(5, 2) + sToDay.substr(8, 2) + "_" + sToDay.substr(11, 2)
	
	var state = this.ds_Master11.getColumn(row, sDate);
	var iChk = this.ds_Master11.getColumn(row, "CHECK_GU");
	
	if (this.gfn_IsNull(iChk)) {
		iChk = 0
	}
	
	if (state == "▩") {
		return false;
	} else if (state == "V") { 
		this.ds_Master11.setColumn(row, sDate, "");
		
		iChk = iChk - 1
		
		this.ds_Master11.setColumn(row, "CHECK_GU", iChk)
		
		var iFvFindRow = this.ds_Master13.findRowExpr("SAUM_CD=='" + sSaumCd + "' && CARM_CD=='" + sCarmCd + "' && CARO_DAHR =='" + sDate + "'")
		
		if (iFvFindRow > 0) {
			this.ds_Master13.setColumn(iFvFindRow, "DISP_STAT", "")
		}
	} else if (state == "☆") {	
		return false;
	} else {
		if ( sDate < sToDay1 ) {
			var arrMsgArg = [sToDay.substring(0, 4) + sToDay.substr(5, 2) + sToDay.substr(8, 2) + " " + sToDay.substr(11, 2) + "시"];
			this.gfn_Alert("HG008", arrMsgArg);//현재일자 ( [ &ITM0 ] ) 이후만 설정할 수 있습니다.
			
			return false;
		} else {
			this.ds_Master11.setColumn(row, sDate, "V");
			
			iChk = iChk + 1
			
			this.ds_Master11.setColumn(row, "CHECK_GU", iChk)
			
			var iFvFindRow = this.ds_Master13.findRowExpr("SAUM_CD=='" + sSaumCd + "' && CARM_CD=='" + sCarmCd + "' && CARO_DAHR =='" + sDate + "'")
		
			if (iFvFindRow > 0) {
				this.ds_Master13.setColumn(iFvFindRow, "DISP_STAT", "V")
			}
			
			/* 차량 사용요청 기간중 사용 여부 체크 */
			var nRows = this.ds_Master13.extractRows("SAUM_CD=='" + sSaumCd + "' && CARM_CD=='" + sCarmCd + "' && DISP_STAT == 'V'");
			var bChk = true;
			if(nRows.length >= 2){
				for(var i=1; i<nRows.length; i++){
					if(this.ds_Master13.findRowExpr("SAUM_CD=='" + sSaumCd + "' && CARM_CD=='" + sCarmCd + "' && CARO_STAT == '0'", nRows[i-1], nRows[i])>=0){
						bChk = false;
						break;
					}
				}
			}
			if(!bChk){
				this.ds_Master11.setColumn(row, sDate, "");
				this.ds_Master11.setColumn(row, "CHECK_GU", iChk-1);
				this.ds_Master13.setColumn(iFvFindRow, "DISP_STAT", "");
				
				this.gfn_Alert("SD008", ["해당기간 차량사용"]);//현재일자 ( [ &ITM0 ] ) 이후만 설정할 수 있습니다.
				return false;
			}
		}
	};
};

