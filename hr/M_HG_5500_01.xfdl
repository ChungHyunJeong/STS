/***********************************************************************************************
 * 13. 선택
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [팝업버튼] 선택 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.div_PopupTitle_btn_Choice_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	for (var iLoop=0; iLoop<this.ds_Master11.rowcount; iLoop++) {
		var iChk = this.ds_Master11.getColumn(iLoop, "CHECK_GU");
		
		if (iChk != 0) {
			this.ds_Master11.setColumn(iLoop, "COUNT", 1)
		} else {
			this.ds_Master11.setColumn(iLoop, "COUNT", 0)
		}
	}
	
	var sChk = this.ds_Master11.getCaseSum("COUNT!=0",1)
	
	if (sChk == 0) { 
		this.gfn_Alert("SM046");//선택된 내용이 없습니다.
		return false;
	} else if (sChk != 1) {
		this.gfn_Alert("HG009"); //하나의 차량만 선택하십시요.
		return false;
	}
	
	this.ds_Master14.clearData();
	
	for (var iLoop=0; iLoop<this.ds_Master13.rowcount; iLoop++) {
		var sDispStat = this.ds_Master13.getColumn(iLoop, "DISP_STAT")
		
		if (sDispStat == "V") {
			var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Master14);
			
			this.ds_Master14.copyRow(iAddRow, this.ds_Master13, iLoop);
		}
	}
	
	var sNextWeek = this.gfn_GetCommDate("after7Day");//현재일시
	var varCount = this.ds_Master14.getCaseCount("DISP_STAT = 'V'", "CARM_CD");
	var iRowCount = this.ds_Master14.rowcount-1;
	
	var sSaumCd = this.ds_Master14.getColumn(0, "SAUM_CD")
	var sCarmCd = this.ds_Master14.getColumn(0, "CARM_CD")
	var sFromDd = this.ds_Master14.getColumn(0, "BASE_DATE")
	var sFromHr = this.ds_Master14.getColumn(0, "BASE_HOUR")
	
	if (sFromDd > sNextWeek) {
		this.gfn_Alert("HG010"); //일주일 이내로 1박2일만 신청 가능합니다.
		return false;
	}
	
	if (varCount == 1) {
		var sClosDd = sFromDd
		var sClosHr = sFromHr
	} else {
		var sClosDd = this.ds_Master14.getColumn(iRowCount, "BASE_DATE")
		var sClosHr = this.ds_Master14.getColumn(iRowCount, "BASE_HOUR")
	}
	
	this.fn_Choice(sSaumCd, sCarmCd, sFromDd, sFromHr, sClosDd, sClosHr)
}

