 * 13.02. [사용자함수] 선택한 정보 부모창 리턴 함수 
 ***********************************************************************************************/ 
this.fn_Choice = function(sSaumCd, sCarmCd, sFromDd, sFromHr, sClosDd, sClosHr) {
		rtnValue = "({" +
                        ' SAUM_CD:"' + sSaumCd + '"'+  //0.사업장코드
                        ',RQCR_CD:"' + sCarmCd + '"'+  //1.신청차량
                        ',RQFR_DD:"' + sFromDd + '"'+  //2.신청시작일
                        ',RQFR_HR:"' + sFromHr + '"'+  //3.신청시작시
                        ',RQTO_DD:"' + sClosDd + '"'+  //4.신청종료일
                        ',RQTO_HR:"' + sClosHr + '"'+  //5.신청종료시
                "})";

 		this.gfn_PopClose(rtnValue);
}

