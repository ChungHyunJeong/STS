{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회
			this.fn_GridDateSet();//그리드 날짜 세팅
			break;		
		case "COMBO_SEARCH": //콤보조회

			if (!this.gfn_IsNull(this.fvSaumCd)) {
				this.div_Search.form.cbo_SaumCd.set_value(this.fvSaumCd);		
			}		
			break;				
	}
};


