this.tab_Master_tpg_01_grd_Master10_oncelldblclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	var row = e.row
	var col = e.col;
	var totrow = this.ds_Master13.getRowCount();
	var text 	= this.tab_Master.tpg_01.form.grd_Master10.getCellProperty("body",e.cell,"text");
	var rowpos = 0;

	if (row == 0) {
		rowpos = col - 3;
	} else {
		rowpos = (totrow  / (row + 1)) * row + (col - 3);
	}

	var sDate = nexacro.replaceAll(text, "bind:", "")	
	var state = this.ds_Master11.getColumn(row, sDate);
	var sSaumCd = this.ds_Master11.getColumn(row, "SAUM_CD");
	var sCarmCd = this.ds_Master11.getColumn(row, "CARM_CD");

	if (state == "▩" || state == "☆") {
		var sPopupTitle = "차량신청 내역조회"; //Popup화면 타이틀
		var sPopupFormUrl  = "hr::M_HG_5500_03.xfdl"; //Popup화면 URL
		var oArg = {CarhNo:this.ds_Master13.getColumn(rowpos, "CARH_NO")
		          , CarhSq:this.ds_Master13.getColumn(rowpos, "CARH_SQ")
				  , SearchDt:sDate
				  , SearchSt:state
				  , SaumCd:sSaumCd
				  , CarmCd:sCarmCd
