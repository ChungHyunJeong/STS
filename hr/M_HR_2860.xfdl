		series.type = "spline";
		series.data = arrSumData;
		arrSeries.push(series);
	
		chartObject.series = arrSeries;			
		
	}

	//trace(JSON.stringify(chartObject));
 
	oWb.callMethod("fn_updateChart2", nexacro.replaceAll(JSON.stringify(chartObject), "\"", "'"), true);

