	this.div_TopR.form.web_ContNmL.setOffsetWidth(iTopRightWidth-10);
	this.div_TopR.form.web_ContNmR.setOffsetLeft(iTopRightWidth + 20);
	this.div_TopR.form.web_ContNmR.setOffsetWidth(iTopRightWidth-10);	

