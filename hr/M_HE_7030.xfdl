			this.ds_Master10.setColumn(iLoop, "USED_YN", '1'); //사용여부
			this.ds_Master10.setColumn(iLoop, "BASM_CD", this.div_Search.form.cal_FromYy.value);  //평가년도
			
			//피평가자 인사정보
			var iFvFindSystRow = this.ds_Master12.findRowExpr("EMPM_CD=='" + op01 + "'");
			
 			this.ds_Master10.setColumn(iLoop, "EMPM_CD", op01); //피평가자 사번 
			this.ds_Master10.setColumn(iLoop, "SYST_ID", this.ds_Master12.getColumn(iFvFindSystRow, "SYST_ID"));
			this.ds_Master10.setColumn(iLoop, "EMPM_NM", this.ds_Master12.getColumn(iFvFindSystRow, "EMPM_NM"));
			this.ds_Master10.setColumn(iLoop, "DUTY_CD", this.ds_Master12.getColumn(iFvFindSystRow, "DUTY_CD"));
			this.ds_Master10.setColumn(iLoop, "POSI_CD", this.ds_Master12.getColumn(iFvFindSystRow, "POSI_CD"));
			this.ds_Master10.setColumn(iLoop, "ENTR_DD", this.ds_Master12.getColumn(iFvFindSystRow, "ENTR_DD"));
			this.ds_Master10.setColumn(iLoop, "RETI_DD", this.ds_Master12.getColumn(iFvFindSystRow, "RETI_DD"));
			this.ds_Master10.setColumn(iLoop, "DEPM_NM", this.ds_Master12.getColumn(iFvFindSystRow, "DEPM_NM"));
			
			//평가자 인사정보
			var iFvFindEvsuRow = this.ds_Master12.findRowExpr("EMPM_CD=='" + op03 + "'");
			
 			this.ds_Master10.setColumn(iLoop, "EMPM_CD2", op03);//평가자 사번
			this.ds_Master10.setColumn(iLoop, "EVSU_ID",  this.ds_Master12.getColumn(iFvFindEvsuRow, "SYST_ID"));
			this.ds_Master10.setColumn(iLoop, "EMPM_NM2", this.ds_Master12.getColumn(iFvFindEvsuRow, "EMPM_NM"));
			this.ds_Master10.setColumn(iLoop, "DUTY_CD2", this.ds_Master12.getColumn(iFvFindEvsuRow, "DUTY_CD"));
			this.ds_Master10.setColumn(iLoop, "POSI_CD2", this.ds_Master12.getColumn(iFvFindEvsuRow, "POSI_CD"));
			this.ds_Master10.setColumn(iLoop, "DEPM_NM2", this.ds_Master12.getColumn(iFvFindEvsuRow, "DEPM_NM"));
			
			
// 			this.gfn_GetCommEmpmGrid(this.ds_Master10, "EMPM_CD", "EMPM_NM", "SYST_ID", arrArgs);

		}
 	} 
 	
 	this.ds_Master10.set_enableevent(true);  
}

