}
 
/***********************************************************************************************
 * 14. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 14.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회
			if (this.ds_Master10.getRowCount() > 0) {				
				this.sta_PosiNm.set_text(this.ds_Master10.getColumn(0, "POSI_NM"));
				this.sta_EmpmNm.set_text(this.ds_Master10.getColumn(0, "EMPM_NM"));
				this.sta_CompNm.set_text(this.ds_Master10.getColumn(0, "COMP_NM"));
				this.sta_DepmNm.set_text(this.ds_Master10.getColumn(0, "DEPM_NM"));
				
				var sEMSI_ID = this.ds_Master10.getColumn(0, "EMSI_ID");	
				var sEncode = encodeURI(sEMSI_ID.replace(/\+/,"%2B"));
				this.img_Emsi.set_image(this.ds_Master10.getColumn(0, "FILE_UL") + sEncode);
				
				this.chk_CagmYn.set_enable(true);
			} else {
				this.gfn_Alert("HG011");//등록된 싸인이미지가 없습니다. 개인정보관리에서 싸인 이미지 등록하시기 바랍니다.
				this.gfn_PopClose();
			}
			
			break;
		
		case "SAVE":   //저장	
			this.fn_Choice()
			break;
	}
};

