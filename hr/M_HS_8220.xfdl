 * 13.03. [메세지] CallBack 함수 처리
 ***********************************************************************************************/  
this.fn_MsgCallback = function (strId, strVal)
{
	//삭제여부
	if (strId == "CONF.DELETE") {
		if(strVal )
		{
			this.fn_SvcDelete();//삭제 서비스 호출			
		}
	//일괄생성
	} else if (strId == "CONF.CREATE") {
		if(strVal )
		{
			this.fn_AllCreate();//일괄생성
		}		
	//복사
	} else if (strId == "CONF.HEADCOPY") {
		if(strVal )
		{
			this.fn_Copy();//복사
		}		
		
	//[결재] 재기안여부
	} else if (strId == "CONF.RECIDE") {
		if(strVal) {
			this.fn_ComBtnRecideProc();//[결재] 재기안 서비스 호출
		}	
		
 	// 급여계산 ---------------------------------------------------------------//
	} else if (strId == "CONF.CALCPROC") {
		if(strVal) {
			this.fn_PayCalc('1');
		}		
	// 급여삭제 ---------------------------------------------------------------//
	} else if (strId == "CONF.DELPROC") {
		if(strVal) {
			this.fn_PayCalc('2');
		}
	// 급여확정 ---------------------------------------------------------------//
	} else if (strId == "CONF.DECIDEPROC") {
		if(strVal) {
			this.fn_PayCalc('3');
		}
	// 급여취소 ---------------------------------------------------------------//
	} else if (strId == "CONF.CANCELPROC") {
		if(strVal) {
			this.fn_PayCalc('4');
		}		
	//--------------------------------------------------------------------------//	
	
	//MASTER 변경사항 저장여부
	} else if (strId == "CONF.MAST_CHANGE") {
		if(strVal )
		{
			this.fn_ComBtnSave();//저장 서비스 호출
		} else {
					//데이터 변경여부 체크
			if (this.gfn_DsIsUpdated(this.ds_Master10)) {
				this.gfn_UndoColumn(this.div_MasterView.form.grd_Master10);
			}		
		
			if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
				this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);
			}
			
			if (this.gfn_DsIsUpdated(this.ds_Detail20)) {
				this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20);										 
			}

			
		}
	}
}; 

