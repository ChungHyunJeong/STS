 ***********************************************************************************************/
this.div_MasterInput_tab_Master_tpg_01_cbo_JogmCd_onitemchanged = function(obj:nexacro.Combo,e:nexacro.ItemChangeEventInfo)
{
	// 선택한 모듈의 업무분류만 필터링하여 가져옴 : e.postvalue (변경후 값)
	// 필터를 초기화 하고 선택된 모듈코드에 대해 조회해 옴
	// 직렬콤보 초기화
	this.ds_HrSerm.filter("");
	this.ds_HrSerm.filter("JOGM_CD == '"+ obj.value +"' || JOGM_CD == null");
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SermCd.set_index(0);
	
	this.ds_InputJogm.clearData(); //직렬콤보 조회용
	
	var JogmCd = this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_JogmCd.value;
	
	this.ds_InputJogm.addRow();
	this.ds_InputJogm.setColumn(0, "JOGM_CD", JogmCd);
	this.ds_InputJogm.setColumn(0, "USED_YN", "1");	
	
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
