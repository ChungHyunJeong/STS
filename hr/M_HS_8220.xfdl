this.fn_ComBtnSave = function()
{ 
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.gfn_DsIsUpdated(this.ds_Detail20) && !this.fvImgFileDiv.fn_CheckFileAddYn() && !this.fvFileDiv.fn_CheckFileAddYn() ) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.		
		return;
	}
	
	//이미지-첨부파일정보 변경시
	if (this.fvImgFileDiv.fn_CheckFileAddYn()) {
		//이미지-첨부파일정보만 변경시
		if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && this.ds_Master10.getRowCount() > 0) {
			//첨부파일정보 저장
			fvImgFileInfo = new Object();//[공통] 첨부파일정보
			fvImgFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvImgFileInfo.CALLBACK_FUNC = "";//호출할 콜백함수
			fvImgFileInfo.PATH_NM = this.fvImgPathNm;//업무파일경로
			fvImgFileInfo.PICH_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PRCH_NO");//관리번호
			//첨부파일Div 화면에 Key세팅하게 변경
			this.fvImgFileDiv.fn_AtchFileSave(this, fvImgFileInfo);
			
			//첨부파일 저장건이 없으면
			if (!this.fvFileDiv.fn_CheckFileAddYn()) {
				return;	
			}
		}
	}	

	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		//첨부파일정보만 변경시
		if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && this.ds_Master10.getRowCount() > 0) {
			//첨부파일정보 저장
			fvFileInfo = new Object();//[공통] 첨부파일정보
			fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, B-상세(그리드Row), E-기타
			fvFileInfo.CALLBACK_FUNC = "";//호출할 콜백함수
			fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
			fvFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MPAM_MM");//PK번호2-관리번호
			fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PABO_GU");//PK번호3-결재차수
			//첨부파일Div 화면에 Key세팅하게 변경
			//this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);
			return;	
		}
	}

	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		if (!this.fn_VaildMasterSave()) return;
	}
		
	if(this.ds_Detail10.rowcount < 1){
		var arrMsgArg = ["급상여 계산상세"];
		this.gfn_Alert("SM045", arrMsgArg);//[급상여 계산상세]는 1건 이상 존재해야 합니다.
		return;
	}
	
	//Detail 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail10) || this.gfn_DsIsUpdated(this.ds_Detail20)) {
		if (!this.fn_VaildDetailSave()) return;
	}	
	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	//Master 입력 상태이면
	if (sChkRowType == "I") {
	
		//채번-관리번호
		var sTemhNo = this.gfn_GetTbnmNo(this.fvComPrgmId, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PRCH_DD"));//관리번호
		if (this.gfn_IsNull(sTemhNo)) {
			//관리번호를 가져오는데 실패했습니다.
			return;
		}	
		var sChkTemhNo = "";//계약번호-체크용		
		var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position	
		
		//Master 데이터셋에 채번정보 입력-관리번호
		this.ds_Master10.setColumn(iMasterRowPos, "PRCH_NO", sTemhNo);		

		this.ds_Master10.setColumn(iMasterRowPos, "WOFH_SQ", 1);
		this.ds_Master10.setColumn(iMasterRowPos, "WOFH_YN", "1");	
		this.ds_Master10.setColumn(iMasterRowPos, "WOFH_ST", "0010");
				
		var sTemhNo = this.ds_Master10.getColumn(iMasterRowPos, "PRCH_NO");//관리번호
		var nWofhSq = this.ds_Master10.getColumn(iMasterRowPos, "WOFH_SQ");//차수
		
		//Detail 데이터셋에 기본정보 입력-회사코드/출고번호/결재차수
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			sChkTemhNo = this.ds_Detail10.getColumn(iLoop, "PRCH_NO");//관리번호					
			
			if (this.gfn_IsNull(sChkTemhNo)) {		
				this.ds_Detail10.setColumn(iLoop, "PRCH_NO", sTemhNo);
				this.ds_Detail10.setColumn(iLoop, "WOFH_SQ", nWofhSq);
			}
		}
		
		//이미지-첨부파일정보 변경시
		if (this.fvImgFileDiv.fn_CheckFileAddYn()) {
			fvImgFileInfo = new Object();//[공통] 첨부파일정보
			fvImgFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			
			//첨부파일정보 변경시
// 			if (this.fvFileDiv.fn_CheckFileAddYn()) {
// 				fvImgFileInfo.CALLBACK_FUNC = "fn_ImageCallback";//호출할 콜백함수
// 			} else {
// 				fvImgFileInfo.CALLBACK_FUNC = "fn_ComBtnSearch";//호출할 콜백함수
// 			}
// 			fvImgFileInfo.PATH_NM = this.fvImgPathNm;//업무파일경로
// 			fvImgFileInfo.PICH_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PRCH_NO");//관리번호-계약번호

		}	 
	}	
		
	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		fvFileInfo = new Object();//[공통] 첨부파일정보
		fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
		fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
		fvFileInfo.CALLBACK_FUNC = "fn_ComBtnSearch";//호출할 콜백함수
		fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
		fvFileInfo.PK01_NO = nApp.gvCompany;//PK번호1-회사코드
		fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MPAM_MM");//PK번호2- 
		fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PABO_GU");//PK번호2- 
	}		
			

	//저장시 재조회 Key설정
	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MPAM_MM");//조회Key01	
	this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PABO_GU");//조회Key02	

	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	//[공통]서비스 호출 변수
	var sInputDaset = "" //입력 데이터셋
	var sInputDasetMst  = "" //입력 데이터셋
	var sInputDasetDet1 = "" //입력 데이터셋
	var sInputDasetDet2 = "" //입력 데이터셋
	var sInputDasetDet3 = "" //입력 데이터셋
	var sOutDatset = "";//출력 데이터셋
	var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		
	//Master 변경건이 있으면 Master 저장
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
		svcAddObj.sInsertSql = "hr:"+ this.fvComPrgmId +"_M_IN10";//저장Sql
		svcAddObj.sUpdateSql = "hr:"+ this.fvComPrgmId +"_M_UP10";//수정Sql
		svcAddObj.sDeleteSql = "hr:"+ this.fvComPrgmId +"_M_DL10";//삭제Sql
		svcAddObj.sSaveFlagColumn = "";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

		this.gfn_AddComSaveService(this, svcAddObj);
		
		sInputDasetMst = "ds_Master10=ds_Master10:U" //입력 데이터셋
		sInputDaset    = "ds_Master10=ds_Master10:U" //입력 데이터셋
	}
	
	//Detail10 변경건이 있으면 Detail 저장
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object	
		svcAddObj.sInsertSql = "hr:"+ this.fvComPrgmId +"_D_IN10";//저장Sql
		svcAddObj.sUpdateSql = "hr:"+ this.fvComPrgmId +"_D_UP10";//수정Sql
		svcAddObj.sDeleteSql = "hr:"+ this.fvComPrgmId +"_D_DL10";//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		
		sInputDasetDet1 = " ds_Detail10=ds_Detail10:U" //입력 데이터셋
		
		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = "ds_Detail10=ds_Detail10:U" //입력 데이터셋
		} else {
			sInputDaset = sInputDaset + sInputDasetDet1  //입력 데이터셋
		}
	}	

	//Detail20 변경건이 있으면 Detail 저장
	if (this.gfn_DsIsUpdated(this.ds_Detail20)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object	
		svcAddObj.sInsertSql = "hr:"+ this.fvComPrgmId +"_D_IN20";//저장Sql
		svcAddObj.sUpdateSql = "hr:"+ this.fvComPrgmId +"_D_UP20";//수정Sql
		svcAddObj.sDeleteSql = "hr:"+ this.fvComPrgmId +"_D_DL20";//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		
		sInputDasetDet2 = " ds_Detail20=ds_Detail20:U" //입력 데이터셋
		
		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = "ds_Detail20=ds_Detail20:U" //입력 데이터셋
		} else {
			sInputDaset = "";
			if (!this.gfn_IsNull(sInputDasetMst))   sInputDaset = sInputDasetMst;
			if (!this.gfn_IsNull(sInputDasetDet1))  sInputDaset = sInputDaset + sInputDasetDet1;
			sInputDaset = sInputDaset + sInputDasetDet2;
		}
	}

	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "SAVE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조	
	svcCallObj.sInputDaset = sInputDaset //입력 데이터셋
	svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
	svcCallObj.sOtherArg = sOtherArg;//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명		
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	

}

