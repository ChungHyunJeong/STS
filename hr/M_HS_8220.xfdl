	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.fvImgFileDiv.fn_CheckFileAddYn() && !this.fvFileDiv.fn_CheckFileAddYn()) {
