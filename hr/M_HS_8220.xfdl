this.ds_Master10_cancolumnchange = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//[월마감제어-등록/수정]
	if (e.columnid == "MPAM_MM") {
		var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);		
		
		var sBUDG_MM = "";//마감년월
		
		if (!this.gfn_IsNull(this.fvMmCloseDay)) {
			sBUDG_MM = this.gfn_Nvl(this.fvMmCloseDay, "");//마감년월
		} else {
			sBUDG_MM = this.gfn_Nvl(e.newvalue, "");//마감년월
		}
		
		this.fvMmCloseDay = "";
		
		var sMODD_CD = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PABO_GU"), "");//모듈상세구분	

		//마감여부 체크
		var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
		var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
		paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
		paramObj.MODM_CD = "HS";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
		paramObj.ACUM_CD = "";//[선택] 회계단위
		paramObj.BUSM_CD  = "";//[선택] 사업부문
		paramObj.SAUM_CD  = "";//[선택] 사업장코드
		paramObj.DEPM_CD  = "";//[선택] 부서코드
		paramObj.MODD_CD  = sMODD_CD;//[선택] 모듈상세구분(CA900)
		paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
		
		var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		

		//월마감된 경우
		if (bCheck && sChkRowType != 'I') {
			this.gfn_Alert("CA025");//해당월의 정보가 마감이 되어서 등록/수정 할 수 없습니다.
			
			var sOrgDate = this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "MPAM_MM");//변경전값
			
			//변경전값이 없을경우
			if (this.gfn_IsNull(sOrgDate)) {				
				var sToDay = this.gfn_GetCommDate("toDay");//현재일자
				
				if (sBUDG_MM.substr(0,6) == sToDay.substr(0,6)) {
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MPAM_MM", "");//입력날짜가 마감된경우 오늘날짜로 세팅				
				} else {
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MPAM_MM", sToDay);//입력날짜가 마감된경우 오늘날짜로 세팅
				}
			} else {
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MPAM_MM", sOrgDate);//입력날짜가 마감된경우 변경전 값으로 세팅
			}
			
			return false;
		} else {
			return true;
		}
	}else if (e.columnid == "PABO_GU") {
		var sBUDG_MM = "";//마감년월
		
		this.fvMmCloseDay = "";
		
		var sBUDG_MM = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MPAM_MM"), "");//모듈상세구분	
		var sMODD_CD = this.gfn_Nvl(e.newvalue, "");//모듈상세구분	

		//마감여부 체크
		var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
		var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
		paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
		paramObj.MODM_CD = "HS";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
		paramObj.ACUM_CD = "";//[선택] 회계단위
		paramObj.BUSM_CD  = "";//[선택] 사업부문
		paramObj.SAUM_CD  = "";//[선택] 사업장코드
		paramObj.DEPM_CD  = "";//[선택] 부서코드
		paramObj.MODD_CD  = sMODD_CD;//[선택] 모듈상세구분(CA900)
		paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
		
		var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		

		//월마감된 경우
		if (bCheck) {
			this.gfn_Alert("CA025");//해당월의 정보가 마감이 되어서 등록/수정 할 수 없습니다.			

			var sOrgPabeGu = this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "PABO_GU");//변경전값
			
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PABO_GU", sOrgPabeGu);//입력날짜가 마감된경우 변경전 값으로 세팅

			
			return false;
		}
	}
};


