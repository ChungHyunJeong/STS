this.fn_PayCalc = function(strGu) 
{
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		//if (!this.fn_VaildMasterSave()) return;
	}
	
	//저장시 재조회 Key설정
	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MPAM_MM").substr(0, 6);//조회Key01	
	this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PABO_GU");//조회Key02	
		
	nApp.gds_WfRst.clearData();//글로벌 데이터셋
	this.ds_Master10Wf.clearData();
	this.ds_Master10Wf.addRow();
	
	//--급여계산(1), 삭제(2), 확정(3), 취소(4)
	this.ds_Master10Wf.setColumn(0, "CALC_GU",  strGu);	
	this.ds_Master10Wf.setColumn(0, "INPT_YN", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "INPT_YN"));	
	
	this.ds_Master10Wf.setColumn(0, "MPAM_MM", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MPAM_MM").substr(0,6));
	this.ds_Master10Wf.setColumn(0, "MPAM_YM", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MPAM_YM").substr(0,6));
	this.ds_Master10Wf.setColumn(0, "PABO_GU", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PABO_GU"));
	this.ds_Master10Wf.setColumn(0, "MPAM_DD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MPAM_DD"));
	this.ds_Master10Wf.setColumn(0, "ACUM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ACUM_CD"));
	this.ds_Master10Wf.setColumn(0, "DEPM_CD", this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.value);
	this.ds_Master10Wf.setColumn(0, "EMPM_CD", this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SystId.value);	
	this.ds_Master10Wf.setColumn(0, "JOGM_CD", this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_JogmCd.value);	
	this.ds_Master10Wf.setColumn(0, "SERM_CD", this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SermCd.value);	
	this.ds_Master10Wf.setColumn(0, "JOTY_CD", this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_JotyCd.value);
	
	this.ds_Master10Wf.setColumn(0, "SESU_ID", this.gfn_GetSessionValue("SYST_ID"));

	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "hr:M_HS_8220_M_UP11";
	this.gfn_AddComSearchService(this, svcAddObj);	
	
	//[공통]서비스 호출 변수
	var sInputDaset = "";//입력 데이터셋
	var sOutDatset  = "";//출력 데이터셋
	var sOtherArg   = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2			
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	if(strGu == '1') {
		svcCallObj.sTrid = "PAY_CALC";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	} else if(strGu == '2') {
		svcCallObj.sTrid = "PAY_DEL";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	} else if(strGu == '3') {
		svcCallObj.sTrid = "PAY_DECIDE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	} else if(strGu == '4') {
		svcCallObj.sTrid = "PAY_CANCEL";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	}
	svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset 	= "ds_Master10Wf=ds_Master10Wf" 	//입력 데이터셋
	svcCallObj.sOutDatset 	= "gds_WfRst=gds_WfRst";//출력 데이터셋
	svcCallObj.sOtherArg 	= "";					//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";		//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);		
}
 


