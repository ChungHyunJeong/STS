this.fn_ValidChk = function(strGu)
{
	if(this.gfn_IsNull(this.div_MasterInput.form.tab_Master.tpg_01.form.cal_MpamMm.value)){
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_MpamMm.setFocus();
		var arrMsgArg = ["귀속년월"];
		this.gfn_Alert("SM008", arrMsgArg);//귀속년월은 필수 입력 항목입니다.		
		return false;
	}

	if(this.gfn_IsNull(this.div_MasterInput.form.tab_Master.tpg_01.form.cal_MpamYm.value)){
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_MpamYm.setFocus();
		var arrMsgArg = ["지급년월"];
		this.gfn_Alert("SM008", arrMsgArg);//귀속년월은 필수 입력 항목입니다.		
		return false;
	}

	
	if(this.gfn_IsNull(this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_MpamGu.value)){
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_MpamGu.setFocus();
		var arrMsgArg = ["급상여구분"];
		this.gfn_Alert("SM008", arrMsgArg);//급상여구분은 필수 입력 항목입니다.		
		return false;
	}

	var sPaboGu = this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_MpamGu.value;//급상여구분
	
	if(strGu == 'A') 
	{
		if(this.gfn_IsNull(this.div_MasterInput.form.tab_Master.tpg_01.form.cal_MpamDd.value)){
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_MpamDd.setFocus();
			var arrMsgArg = ["지급일자"];
			this.gfn_Alert("SM008", arrMsgArg);//지급일자는 필수 입력 항목입니다.		
			return false;
		}

		if(this.gfn_IsNull(this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumCd.value)){
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumCd.setFocus();
			var arrMsgArg = ["회계단위"];
			this.gfn_Alert("SM008", arrMsgArg);//회계단위는 필수 입력 항목입니다.		
			return false;
		}
		
		if (this.gfn_IsNull(this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.value) && sPaboGu == "0050") {
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.setFocus();
			var arrMsgArg = ["사번"];
			this.gfn_Alert("SM008", arrMsgArg);//사번은 필수 입력 항목입니다.		
			return false;
		}
		
	} else if (strGu == 'B') {
		if (this.gfn_IsNull(this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.value) && sPaboGu == "0050") {
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.setFocus();
			var arrMsgArg = ["사번"];
			this.gfn_Alert("SM008", arrMsgArg);//사번은 필수 입력 항목입니다.		
			return false;
		}
	}
	
	return true;
}
  
