 ***********************************************************************************************/ 
this.fn_HrMakeFile = function()
{ 
	var iRow = this.ds_Master10.rowposition;
	this.ds_FileInput.clearData();
	this.ds_FileInput.addRow();
		
	this.ds_FileInput.setColumn(0, "G_COMM", this.ds_Master10.getColumn(iRow, "COMM_CD"));//회사코드
	this.ds_FileInput.setColumn(0, "JSPH_YY", this.ds_Master10.getColumn(iRow, "JSPH_YY"));//귀속년도
	this.ds_FileInput.setColumn(0, "JSPH_CD", this.ds_Master10.getColumn(iRow, "JSPH_CD"));//반기(공통)
	this.ds_FileInput.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(iRow, "WOFH_SQ"));//차수
	this.ds_FileInput.setColumn(0, "SQL_ID", "hr:M_HS_8460_D_SL20");//호출SQL-ID
	
	var sSAUM_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SAUM_NO");//사업자등록번호
	var sFileName = "";//파일명
	
	//사업자등록번호가 있으면
	if (!this.gfn_IsNull(sSAUM_NO)) {
		var iLength = sSAUM_NO.length;
		sFileName = sSAUM_NO.substring(0, (iLength-3)) + "." + this.gfn_Right(sSAUM_NO, 3);

	} else {
		return;
	}
	
	this.ds_FileInput.setColumn(0, "FILE_NM", "SC" + sFileName);//파일명
	
	this.ds_Result.clearData();	
	
   	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);	
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "FILE_DOWN";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = "HrMakeFileAction";//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sMethodName = "makeTaxFile";// Action 안에서 호출해야 하는 메소드 명, 기본은 execute를 호출하도록 되어있음	
	svcCallObj.sInputDaset = "ds_FileInput=ds_FileInput" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Result=ds_Result";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명	
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);			
	
	
}

