this.fn_MsgCallback = function (strId, strVal)
{
	//삭제여부
	if (strId == "CONF.DELETE") {
		if(strVal )
		{
			this.fn_SvcDelete();//삭제 서비스 호출
		}
	//MASTER 변경사항 저장여부
	} else if (strId == "CONF.MAST_CHANGE") {
		if(strVal )
		{
			this.fn_ComBtnSave();//저장 서비스 호출
		} else {
			//데이터 변경여부 체크
			if (this.gfn_DsIsUpdated(this.ds_Master10)) {
				this.gfn_UndoColumn(this.div_MasterView.form.grd_Master10);
			}		
			
			if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
				for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
					this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10, iLoop);
				}
			}			
		}		
		
 	// 차수변경 ---------------------------------------------------------------//
	} else if (strId == "CONF.CHANPROC") {
		if(strVal) {
			this.fn_Calc('1');
		}
 	// 근로소득집계 ----------------------------------------------------------//
	} else if (strId == "CONF.CALCPROC") {
		if(strVal) {
			this.fn_Calc('2');
		}
 	// 신고자료마감 ---------------------------------------------------------//
	} else if (strId == "CONF.DECIPROC") {
		if(strVal) {
			this.fn_Calc('3');
		}			
 	// 신고마감취소 ---------------------------------------------------------//
	} else if (strId == "CONF.CANCPROC") {
		if(strVal) {
			this.fn_Calc('4');
		}			
 	// 신고자료생성 ----------------------------------------------------------//
	} else if (strId == "CONF.CREAPROC") {
		if(strVal) {
			this.fn_Calc('5');
		}
	}
}; 
 
