this.fn_SumAmt = function(JspbAm, NtaxAm, BonuAm){
	var JspbAm  = nexacro.toNumber(JspbAm);
	var NtaxAm  = nexacro.toNumber(NtaxAm);
	var BonuAm  = nexacro.toNumber(BonuAm);
	
	if(this.gfn_IsNull(JspbAm) || isNaN(JspbAm)) {JspbAm = 0;}
	if(this.gfn_IsNull(NtaxAm) || isNaN(JspbAm)) {NtaxAm = 0;}
	if(this.gfn_IsNull(BonuAm) || isNaN(BonuAm)) {BonuAm = 0;}
 
	//return JspbAm + NtaxAm + BonuAm;	
	return JspbAm + BonuAm;	 //비과세는 제외
}


