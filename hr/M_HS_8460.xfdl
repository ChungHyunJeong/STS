			this.ds_FaSaum.filter("SAUM_CD == '0010'");		
			
			break;
	
		case "MST_SEARCH": //조회		
	
			if (this.ds_Master10.getRowCount() > 0) {
			
				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master10.findRowExpr("JSPH_YY=='" + this.fvSchKey01 + "' && JSPH_CD=='" + this.fvSchKey02 + "' && WOFH_SQ=='" + this.fvSchKey03 + "'");//업무Key Row 찾기
					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
