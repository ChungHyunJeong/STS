/***********************************************************************************************
 * 06.02. 급여계산(1), 삭제(2), 확정(3), 취소(4)
 ***********************************************************************************************/  
this.fn_Calc = function(strGu) 
{
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		//if (!this.fn_VaildMasterSave()) return;
	}
	
	//저장시 재조회 Key설정
	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JSPH_YY").substr(0, 6);//조회Key01	
	this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JSPH_CD");//조회Key02	
	this.fvSchKey03 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//조회Key03
		
	nApp.gds_WfRst.clearData();//글로벌 데이터셋
	this.ds_Input.clearData();
	this.ds_Input.addRow();
	
	//--차수변경(1), 근로소득집계(2) 
	this.ds_Input.setColumn(0, "CALC_GU",  strGu);	
	this.ds_Input.setColumn(0, "JSPH_YY", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JSPH_YY"));
	this.ds_Input.setColumn(0, "JSPH_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JSPH_CD"));
	this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
	
//	this.ds_Master10Wf.setColumn(0, "SESU_ID", this.gfn_GetSessionValue("SYST_ID"));

	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
		
	//[공통]서비스 호출 변수
	var sInputDaset = "";//입력 데이터셋
	var sOutDatset  = "";//출력 데이터셋
	var sOtherArg   = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2			
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	if(strGu == '1') {
		
		this.fvSchKey03 = nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ")) + 1  ;//조회Key03		
		
		svcAddObj.sSelectSql = "hr:M_HS_8460_M_UP11";
		svcCallObj.sTrid = "PAY_CHAN";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	} else if(strGu == '2') {
		svcAddObj.sSelectSql = "hr:M_HS_8460_M_UP12";
		svcCallObj.sTrid = "PAY_CALC";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	} else if(strGu == '3') {
		svcAddObj.sSelectSql = "hr:M_HS_8460_M_UP13";
		svcCallObj.sTrid = "PAY_DECI";//Transaction ID 등 callback에서 넘겨 받을 String을 지정		
	} else if(strGu == '4') {
		svcAddObj.sSelectSql = "hr:M_HS_8460_M_UP14";
		svcCallObj.sTrid = "PAY_CANC";//Transaction ID 등 callback에서 넘겨 받을 String을 지정		
	} else if(strGu == '5') {
		svcAddObj.sSelectSql = "hr:M_HS_8460_M_UP15";
		svcCallObj.sTrid = "PAY_CREA";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	}
	
	if(strGu == "5") {
		var strsOutDatset	=  "gds_WfRst=gds_WfRst";
	} else {
		var strsOutDatset	=  "ds_Detail10=ds_Detail10";
	}		
	
	this.gfn_AddComSearchService(this, svcAddObj);	
	
	svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset 	= "ds_Input=ds_Input" 	//입력 데이터셋
	svcCallObj.sOutDatset 	= strsOutDatset;//출력 데이터셋	
	svcCallObj.sOtherArg 	= "";					//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";		//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);		
}



/***********************************************************************************************
 * 02.MF0.03. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {
	
	
		//귀속년도가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JSPH_YY"))) {
			var strJsphYn	= this.ds_Master10.getColumn(e.newrow, "JSPH_YN");
			var strWofhMaxYn  =  this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ_MAXYN");																			   
			
			if(strJsphYn == "1" || strWofhMaxYn == "N"){
				//this.div_MasterInput.form.tab_Master.tpg_01.set_enable(false);
				this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
								
				this.div_MasterInput.form.btn_Change.set_enable(false);
				this.div_MasterInput.form.btn_Group.set_enable(false);
				this.div_MasterInput.form.btn_Create.set_enable(false);
				this.div_MasterInput.form.btn_Close.set_enable(false);
				if(strJsphYn == "1"){
					this.div_MasterInput.form.btn_Cancle.set_enable(true);
				}else{
					this.div_MasterInput.form.btn_Cancle.set_enable(false);
				}
			}else{
				this.div_MasterInput.form.tab_Master.tpg_01.set_enable(true);
				this.gfn_SetCompGroupEnable(this.div_MasterInput.form, true);//Master 입력 프리폼
				
				this.div_MasterInput.form.btn_Change.set_enable(true);
				this.div_MasterInput.form.btn_Group.set_enable(true);
				this.div_MasterInput.form.btn_Create.set_enable(true);
				this.div_MasterInput.form.btn_Close.set_enable(true);
				if(strJsphYn == "1"){
					this.div_MasterInput.form.btn_Cancle.set_enable(true);
				}else{
					this.div_MasterInput.form.btn_Cancle.set_enable(false);
				}
			}		
		
			this.div_MasterInput.form.tab_Master.tpg_01.form.spn_JsphYy.set_enable(false);
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_JsphCd.set_enable(false);
			
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "JSPH_YY", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JSPH_YY"));
			this.ds_Input.setColumn(0, "JSPH_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JSPH_CD"));
			this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
			
			this.ds_Input.setColumn(0, "DEPM_CD", this.div_Search.form.edt_DepmCd.value);
			this.ds_Input.setColumn(0, "SESU_ID", this.div_Search.form.edt_EmpmCd.value);
					
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "hr:M_HS_8460_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);		
		}
	}
};

