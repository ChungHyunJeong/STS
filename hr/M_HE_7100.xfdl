/*---------------------------------------------------------------------------------------------
 *  19. 01 계획미승인자  (버튼) 클릭 이벤트
  ---------------------------------------------------------------------------------------------*/

//계획미승인자 체크
this.div_MasterView_chk_PawfLst_onclick = function(obj:nexacro.CheckBox,e:nexacro.ClickEventInfo)
{	
	var vChk = this.div_MasterView.form.chk_PawfLst.value; //계획등록 여부 
	var vChk1 = this.div_MasterView.form.chk_PlwfLst.value; //실적등록 여부 
	
	this.ds_Master10.set_enableevent(false);
	
	//계획미승인자 체크 
	if(vChk == true ) {   		
		
		//실적미등록 & 평가자 해제
		if (vChk1 == true) {		
			
			this.div_MasterView.form.chk_PlwfLst.set_value("0") ;
			 			
			for (var iLoop=0; iLoop < this.ds_Master10.getRowCount(); iLoop++) {			
				this.ds_Master10.setColumn(iLoop, "MAIL_CHK", "0"); //평가자 해제
				this.ds_Master10.setColumn(iLoop, "MAIL_CHK2", "0"); //피평가자 해제
			}
		}
	
		
		//피평가자 체크
		for (var iLoop=0; iLoop < this.ds_Master10.getRowCount(); iLoop++) {
			var sPlan = this.ds_Master10.getColumn(iLoop, "PAWF_ST");		
			
			if (sPlan != "0040") {
			//if (this.gfn_IsNull(sPlan) || sPlan ==""|| sPlan == "0010"|| sPlan == "0020"|| sPlan == "0030") {
				this.ds_Master10.setColumn(iLoop, "MAIL_CHK2", "1"); //피평가자 체크
				this.ds_Master10.setColumn(iLoop, "MAIL_CHK", "1"); //평가자 체크
			}
			
		}

	//계획미승인자 해제
	} else {	
		for (var iLoop=0; iLoop < this.ds_Master10.getRowCount(); iLoop++) {
		
			this.ds_Master10.setColumn(iLoop, "MAIL_CHK2", "0"); //피평가자 해제		
			this.ds_Master10.setColumn(iLoop, "MAIL_CHK", "0"); //평가자 해제		
		}
	}
	
	this.ds_Master10.set_enableevent(true);
};


