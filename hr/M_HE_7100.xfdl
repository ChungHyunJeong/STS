/*---------------------------------------------------------------------------------------------
 *  19. 03 미평가  (버튼) 클릭 이벤트
  ---------------------------------------------------------------------------------------------*/
//다면평가진행현황 미평가 체크
this.div_MasterView_chk_NtRated_onclick = function(obj:nexacro.CheckBox,e:nexacro.ClickEventInfo)
{
	var vChk = this.div_MasterView.form.chk_NtRated.value; //미평가 여부 	
	//this.alert(vChk);
	
	this.ds_Master20.set_enableevent(false);
	
	// 미평가 체크  MPED_GU
	if(vChk == true ) {  
		
		//피평가자 체크
		for (var iLoop=0; iLoop < this.ds_Master20.getRowCount(); iLoop++) {
			var sNtRated = this.ds_Master20.getColumn(iLoop, "MPED_GU");	// 평가여부(0:평가안함,1:평가함,2:미평가)	
			var sSendChk = this.ds_Master20.getColumn(iLoop, "SEND_CHK");   // 잔디알람 전송체크 
			var sSystId  = this.ds_Master20.getColumn(iLoop, "EVSU_ID");  //평가자 시스템사번
			var sEmpmCd  = this.ds_Master20.getColumn(iLoop, "EVSU_CD");  //평가자 사번 
			var sEmpmNm  = this.ds_Master20.getColumn(iLoop, "EVSU_NM"); 
			
		//	this.alert(sSystId +"////" + sEmpmCd +"~~~~"+  sEmpmNm);
			
			//미평가인사람만 
			if (sNtRated == "2") {					
				var iFvFindRow = this.ds_UserInfo.findRowExpr("SYST_ID=='" + sSystId + "'");				
				var sEmpmCd = this.ds_UserInfo.getColumn(iFvFindRow, "EMPM_CD");
				var sSendChk = this.ds_UserInfo.getColumn(iFvFindRow, "SEND_CHK");
		 
				//알람전송체크 여부
				if (sSendChk =="N") {
					this.ds_UserInfo.setColumn(iFvFindRow, "SEND_CHK", "Y"); ////알람발송 체크
					this.ds_Master20.setColumn(iLoop, "MAIL_CHK", "1"); //알람발송 체크
				}						
			}			
		}
	//체크해제	
	} else {	
		for (var iLoop=0; iLoop < this.ds_Master20.getRowCount(); iLoop++) {
		
			this.ds_Master20.setColumn(iLoop, "MAIL_CHK", "0"); //평가자 해제		
			
			for (var iLoop1=0; iLoop1 < this.ds_UserInfo.getRowCount(); iLoop1++) {
				this.ds_UserInfo.setColumn(iLoop1, "SEND_CHK", "N"); //발신정보 해제
			}	
		}
	}
	
	this.ds_Master20.set_enableevent(true);
};


