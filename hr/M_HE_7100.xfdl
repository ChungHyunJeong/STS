/***********************************************************************************************
 * 16.05. [함수] 중복데이터 제거
 ***********************************************************************************************/ 
this.fn_distinct = function(sParam)
{
	var aData = new Array();		
	
	aData = sParam.split(",");	
	
	//this.alert(aData);
	
	var aRtn = new Array();
	var bFlag;
	var vDist;
	
	for (var i = 0; i < aData.length; i++) {
		vDist = aData[i];		
		bFlag = false;
		for (var j = 0; j < aRtn.length; j++) {
			if (aRtn[j] == vDist) {
				bFlag = true;
				break;
			}
		}
		
		if (bFlag == false) {
			aRtn[aRtn.length] = vDist;			
		}		
		
	}
	
	return aRtn;  
}


