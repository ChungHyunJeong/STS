/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SYST_ID"))) {
			var sBonuGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BONU_GU");
			
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_BonuAm.set_enable(false); //상여적용금액
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_BonuCd.set_enable(false); //상여적용율_기준코드
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_BounRt.set_enable(false); //상여적용율
			
			//적용금액
			if (sBonuGu == "0") {
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_BonuAm.set_enable(true);
			
			//적용율
			} else if (sBonuGu == "1") {
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_BonuCd.set_enable(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_BounRt.set_enable(true);
			
			//적용율+적용금액
			} else if (sBonuGu == "2") {
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_BonuAm.set_enable(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_BonuCd.set_enable(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_BounRt.set_enable(true);
			
			}
		
		}
	
	}
}
/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master10] Row 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	

	return true;
}

/*---------------------------------------------------------------------------------------------
 *    03. [데이터셋-ds_Master10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	
	if (e.columnid == "BONU_GU"){
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_BonuAm.set_enable(false); //상여적용금액
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_BonuCd.set_enable(false); //상여적용율_기준코드
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_BounRt.set_enable(false); //상여적용율
		
		//적용금액
		if (e.newvalue == "0") {
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_BonuAm.set_enable(true);
		//적용율
		} else if (e.newvalue == "1") {
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_BonuCd.set_enable(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_BounRt.set_enable(true);
		
		//적용율 + 적용금액
		} else if (e.newvalue == "2") {
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_BonuAm.set_enable(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_BonuCd.set_enable(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_BounRt.set_enable(true); 
		}
	}
	
	//this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
};

