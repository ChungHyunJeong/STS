/***********************************************************************************************
 * 05.02. [Vaildation] Master 저장 필수값 체크 - 입력항목 
 ***********************************************************************************************/ 
this.fn_VaildMasterSave = function()
{
	var strSerhGu  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SERH_GU");
		
	if(strSerhGu == "0010"){
		var iSerhQt  =  this.ds_Master10.getColumn(this.ds_Master10.rowposition,   "SERH_QT");		
		
		if (this.gfn_IsNull(iSerhQt) || iSerhQt < 1 || iSerhQt > 9){
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SerhQt.setFocus();
			this.gfn_Alert("HG003");//발급수량은 1~9까지 입력하셔야 됩니다.
			return false;
		}	
	}else if(strSerhGu == "0020"){
		var strSeahNo  =  this.ds_Master10.getColumn(this.ds_Master10.rowposition,  "SEAH_NO");
		
		if (this.gfn_IsNull(strSeahNo) || strSeahNo == "" || strSeahNo.trim().length < 1){
			var arrMsgArg = ["사용인감"];
			this.gfn_Alert("SM008", arrMsgArg);//[필수 입력 항목입니다
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SeahNo.setFocus();
			
			return false;
		}	
	}else if(strSerhGu == "0030"){
		var strSeahNo  =  this.ds_Master10.getColumn(this.ds_Master10.rowposition,  "SEAH_NO");		
			
		var strFromMm	=	this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD");
		var strClosMm	=	this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD");
		
		if(this.gfn_IsNull(strFromMm)){		
			var arrMsgArg = ["반출기간(시작)"];
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FromDd.setFocus();
			this.gfn_Alert("SM008", arrMsgArg);//[필수 입력 항목입니다			
			return false;
		}
		if(this.gfn_IsNull(strClosMm)){		
			var arrMsgArg = ["반출기간(종료)"];
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.setFocus();
			this.gfn_Alert("SM008", arrMsgArg);//[필수 입력 항목입니다			
			return false;
		}
		
		if(strFromMm > strClosMm){		
			var arrMsgArg = ["반출기간(시작)","반출기간(종료)"];
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosDd.setFocus();
			this.gfn_Alert("SM056", arrMsgArg);//[&ITM0]이(가) [&ITM1]을(를) 초과할 수 없습니다.
			return false;
		}
		
		if (this.gfn_IsNull(strSeahNo) || strSeahNo == "" || strSeahNo.trim().length < 1){
			var arrMsgArg = ["사용인감"];
			this.gfn_Alert("SM008", arrMsgArg);//[필수 입력 항목입니다
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SeahNo.setFocus();
			
			return false;
		}			

		
	}

	//[저장] Validation 설정 
	var arrArgs = ["this.div_MasterInput.form.tab_Master.tpg_01.form.cal_SerhDd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_SerhDd.text + "|NULL" //등록일자
				  ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_DepmCd.text + "|NULL" //등록부서코드
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SystId^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_EmpmCd.text + "|NULL" //등록자코드
                  ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_DocuGu^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_DocuGu.text + "|NULL" //문서구분
				  ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SerhNm^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_SerhNm.text + "|NULL" //사용처
				  
				  ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SerhGu^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_SerhGu.text + "|NULL" //인감구분
				  ,"this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_RemkNm.text + "|NULL" //사유
			      ]; 
				  
	var sCheckGroupTitle = this.div_MasterInput.form.tab_Master.tpg_01.text;//체크그룹 타이틀

	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
		return false;
	}

	return true;
}
