			}
		} else {
			this.ds_Detail100.setColumn(this.ds_Detail100.rowposition, "EMSC_CD", "");
			this.ds_Detail100.setColumn(this.ds_Detail100.rowposition, "SCHM_NM", "");
			this.ds_Detail100.setColumn(this.ds_Detail100.rowposition, "SCHM_GU", "");
		}
		
	//자격면허코드
	} else if (strId == "B_EMLI_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.CERM_CD)) {
