/***********************************************************************************************
 * 17. [그리드-grd_Detail90] 재직기간 계산
 ***********************************************************************************************/ 
 this.fn_GridSetColumn = function(iRow)
 {
 	var iFromDd = this.ds_Detail90.getColumn(iRow, "FROM_DD");//시작일자
 	var iClosDd = this.ds_Detail90.getColumn(iRow, "CLOS_DD");//종료일자
	
	this.ds_InputDate.clearData();	
	this.ds_Detail91.reset();
		
	if ((!this.gfn_IsNull(iFromDd)) && (!this.gfn_IsNull(iClosDd))) {		
	
		this.ds_InputDate.addRow();
		this.ds_InputDate.setColumn(0, "SYST_ID", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SYST_ID"));
		this.ds_InputDate.setColumn(0, "DETAIL_ROW", iRow);
		this.ds_InputDate.setColumn(0, "FROM_DD", iFromDd);
		this.ds_InputDate.setColumn(0, "CLOS_DD", iClosDd);
		
		//[공통]서비스정보 초기화
		this.gfn_InitComService(this);
			
		var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
		svcAddObj.sSelectSql = "hr:M_HR_2100_D_SL91";
		this.gfn_AddComSearchService(this, svcAddObj);
		
		var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
		svcCallObj.sTrid = "";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
		svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
		svcCallObj.sInputDaset = "ds_InputDate=ds_InputDate" //입력 데이터셋
		svcCallObj.sOutDatset = "ds_Detail91=ds_Detail91";//출력 데이터셋
		svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
		svcCallObj.isSync = true;//동기식으로 설정할지 여부		
		
		//[공통]서비스 호출
		this.gfn_CallComService(this, svcCallObj);	
		
		if (this.ds_Detail91.rowcount > 0 ) {
			var iAddRow = this.ds_Detail91.getColumn(0, "DETAIL_ROW");
			
			this.ds_Detail90.setColumn(iAddRow, "WORK_CT", this.ds_Detail91.getColumn(0, "WORK_CT"));
		}
 	};
 };

