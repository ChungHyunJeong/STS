	//this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd
	var sMsg =  " - " + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_JsmtGubn.text + " : " + this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_JsmtGubn.value + "\n" 
