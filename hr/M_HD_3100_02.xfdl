 ***********************************************************************************************/
this.fvComPrgmId = "";//[부모창파라메타] 프로그램ID
this.fvCommCd = "";//[부모창파라메타] 회사코드
this.fvBzthNo = "";//[부모창파라메타] 출장번호
this.fvWofhSq = "";//[부모창파라메타] 결재차수
this.fvBztbDd = "";//[부모창파라메타] 출장일자
this.fvBztbSe = "";//[부모창파라메타] 출장순번
this.fvOwnerMaxCount = "";//[부모창파라메타] 총건수
this.fvDataset = "";//[부모창파라메타] 데이터셋

this.fvFormComBtnAuthYn = false;//그리드 공통버튼 권한 유무
this.fvWfSearchMode = false;//[결재]결재내역 조회모드
this.fvWofhStatus = false;//[결재]결재상태 조회모드 (등록=false, 그외 true)


/***********************************************************************************************
 * 00.03. Form Onload 이벤트
 ***********************************************************************************************/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{
	//부모창 파라메타 처리
	this.fvCommCd = this.getOwnerFrame().paramCommCd;//[부모창파라메타] 회사코드
	this.fvBzthNo = this.getOwnerFrame().paramBzthNo;//[부모창파라메타] 출장번호
	this.fvWofhSq = this.getOwnerFrame().paramWofhSq;//[부모창파라메타] 결재차수
	this.fvBztbDd = this.getOwnerFrame().paramBztbDd;//[부모창파라메타] 출장일자	
	this.fvBztbSe = this.getOwnerFrame().paramBztbSe;//[부모창파라메타] 출장순번	
	this.fvOwnerMaxCount = this.getOwnerFrame().paramOwnerMaxCount; //[부모창파라메타] 총건수
	this.fvDataset = this.getOwnerFrame().paramDataset; //[부모창파라메타] 데이터셋
	this.fvWfSearchMode = this.getOwnerFrame().paramWfSearchMode;//[결재]결재내역 조회모드
	this.fvWofhStatus = this.getOwnerFrame().paramWofhStatus;//[결재]결재상태
	this.fvComPrgmId = this.getOwnerFrame().PrgmId;//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID

	//최초 동행인시 부모화면ID로 임시사용
	if (this.gfn_IsNull(this.fvBzthNo)) {
		this.fvBzthNo = this.fvComPrgmId; //출장번호가 null ==> 최초의 경우 부모화면ID로 대체
	}

	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 

}

/***********************************************************************************************
 * 00.04. Form [공통] 초기설정 함수 - 필수
 ***********************************************************************************************/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	this.gfn_SetGrid(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10);//[공통]그리드 이벤트 설정 - 프로젝트목록

	//[결재]결재내역 조회모드가 아니면-등록 && 결재상태가 "등록"인 경우
	if (!this.fvWfSearchMode && !this.fvWofhStatus) {
		this.div_MasterInput.form.div_ComGrdBtn.set_visible(true);
		this.div_PopupTitle.form.btn_Choice.set_visible(true);
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, true);//Master 입력 프리폼
				
		//권한체크 할 컴포넌트 : 컴포넌트 Full Path
		var objCompCheckList = ["this.div_MasterInput.form.div_ComGrdBtn"];		
		if (this.gfn_CommCompAuthCheck(this, objCompCheckList))
		{
			this.fvFormComBtnAuthYn = true;//그리드 공통버튼 권한 유무
		}
		
		this.fn_SetGridInputButton();//[공통] 그리드 입력 버튼 설정
	} else {
		this.div_MasterInput.form.div_ComGrdBtn.set_visible(false);
		this.div_PopupTitle.form.btn_Choice.set_visible(false);
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
		
	}

}

/***********************************************************************************************
 * 00.05. Form [업무] 초기설정 함수
 ***********************************************************************************************/
this.fn_FormBizInit = function() 
{ 
	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	var objCondList = ["HR202|ds_Hr202"];
	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, "");

	//초기값 설정
	this.div_Search.form.cal_BztbDd.set_value(this.fvBztbDd); //출장일자

	//창 오픈시 조회 부모창에서 전달해온 데이터셋을 화면에 보여줌
	this.fn_ComBtnSearch();
}

