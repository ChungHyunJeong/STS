 * 15.09. [사용자함수] POPUP 선택
 ***********************************************************************************************/ 
this.div_PopupTitle_btn_Choice_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{

	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}

	//선택전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		if (!this.fn_VaildMasterSave()) return;
	}

	if (this.ds_Master10.rowcount > -1) {	
		//팝업 데이터셋에서 부모 데이터셋으로 복사		
		for (var iLoop=0; iLoop<this.ds_Master10.getRowCount(); iLoop++) {
			var crudType = this.ds_Master10.getColumn(iLoop, "crudType"); //crudType
			var crudPopup = this.ds_Master10.getColumn(iLoop, "crudPopup"); //팝업 대상 추가 "ADD"

			if (crudPopup != "A") { // 팝업 ADD가 아님 (신규(I) OR 수정(U) OR 삭제(D))
				for (var iRow=0; iRow<this.fvOwnerMaxCount;iRow++) {
					if (this.getOwnerFrame().form.opener.ds_Detail15.getColumn(iRow, "COMM_CD") == this.ds_Master10.getColumn(iLoop, "COMM_CD")
					 && this.getOwnerFrame().form.opener.ds_Detail15.getColumn(iRow, "BZTH_NO") == this.ds_Master10.getColumn(iLoop, "BZTH_NO")
					 && nexacro.toNumber(this.getOwnerFrame().form.opener.ds_Detail15.getColumn(iLoop, "WOFH_SQ")) == nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"))
					 && this.getOwnerFrame().form.opener.ds_Detail15.getColumn(iRow, "BZTB_SE") == this.ds_Master10.getColumn(iLoop, "BZTB_SE")
					 && this.getOwnerFrame().form.opener.ds_Detail15.getColumn(iRow, "SYST_ID") == this.ds_Master10.getColumn(iLoop, "SYST_ID") ) {
					 
						this.getOwnerFrame().form.opener.ds_Detail15.setColumn(iRow, "EMPM_NM", this.ds_Master10.getColumn(iLoop, "EMPM_NM"));
						this.getOwnerFrame().form.opener.ds_Detail15.setColumn(iRow, "EMPM_CD", this.ds_Master10.getColumn(iLoop, "EMPM_CD"));
						this.getOwnerFrame().form.opener.ds_Detail15.setColumn(iRow, "DEPM_CD", this.ds_Master10.getColumn(iLoop, "DEPM_CD"));
						this.getOwnerFrame().form.opener.ds_Detail15.setColumn(iRow, "DEPM_NM", this.ds_Master10.getColumn(iLoop, "DEPM_NM"));
						this.getOwnerFrame().form.opener.ds_Detail15.setColumn(iRow, "DEPM_NM", this.ds_Master10.getColumn(iLoop, "DEPM_NM"));
						this.getOwnerFrame().form.opener.ds_Detail15.setColumn(iRow, "REMK_NM", this.ds_Master10.getColumn(iLoop, "REMK_NM"));
						this.getOwnerFrame().form.opener.ds_Detail15.setColumn(iRow, "s_chk", this.ds_Master10.getColumn(iLoop, "s_chk"));
						this.getOwnerFrame().form.opener.ds_Detail15.setColumn(iRow, "crudType", this.ds_Master10.getColumn(iLoop, "crudType"));
						
					}
				}

			} else { //신규이면서 팝업ADD이면 (추가)
				// 행추가 Row 복사
				var iAddRow = this.getOwnerFrame().form.opener.ds_Detail15.addRow();
				this.getOwnerFrame().form.opener.ds_Detail15.copyRow(iAddRow, this.ds_Master10, iLoop);
				this.getOwnerFrame().form.opener.ds_Detail15.setColumn(iAddRow, "crudPopup", ""); //팝업 "ADD"상태
				
			}
		}
		
		//창닫기
		this.gfn_PopClose();
		
	} else {
		this.gfn_Alert("SM046");//선택된 내용이 없습니다.
		return;		
	}

};


