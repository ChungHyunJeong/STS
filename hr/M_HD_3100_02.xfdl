	this.ds_Master10.setColumn(iAddRow, "COMM_CD", this.fvCommCd);
	this.ds_Master10.setColumn(iAddRow, "BZTH_NO", this.fvBzthNo);
	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", this.fvWofhSq);
	this.ds_Master10.setColumn(iAddRow, "BZTB_DD", this.fvBztbDd);
