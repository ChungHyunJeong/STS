/***********************************************************************************************
 * 02.MG0.04. [Master그리드-버튼] 행삭제 클릭 이벤트
 ***********************************************************************************************/
this.fn_MasterGrd01DeleteRow = function()
{

	//신규(I)에 대한 삭제시 부모건수(this.fvOwnerMaxCount) 확인 후 삭제
	if (this.ds_Master10.getColumn(this.ds_Master10.rowposition,"crudType") == "I" && this.fvOwnerMaxCount > 0) {
		for (var iLoop=0; iLoop<this.fvOwnerMaxCount;iLoop++) {
			if (this.getOwnerFrame().form.opener.ds_Detail15.getColumn(iLoop, "COMM_CD") == this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD")
			 && this.getOwnerFrame().form.opener.ds_Detail15.getColumn(iLoop, "BZTH_NO") == this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTH_NO")
			 && nexacro.toNumber(this.getOwnerFrame().form.opener.ds_Detail15.getColumn(iLoop, "WOFH_SQ")) == nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"))
			 && this.getOwnerFrame().form.opener.ds_Detail15.getColumn(iLoop, "BZTB_SE") == this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BZTB_SE")
			 && this.getOwnerFrame().form.opener.ds_Detail15.getColumn(iLoop, "SYST_ID") == this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SYST_ID") ) {
			 
			    this.getOwnerFrame().form.opener.ds_Detail15.deleteRow(iLoop);
				this.fvOwnerMaxCount = this.fvOwnerMaxCount -1; //부모 전체건수 - 1
				
				break;
			}
		}
	}

	this.gfn_GridDelProcess(this, this.ds_Master10);//crud_flag 변경-삭제(D)	

};

