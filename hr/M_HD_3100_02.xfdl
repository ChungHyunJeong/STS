this.fn_ComBtnSearch = function()
{ 
	//상세화면에서 조회하지 않고 부모창에서 조회한것을 가져와서 처리함
	//2021-08-26 설계자 변경
	this.ds_Master10.loadXML(this.fvDataset);
	this.ds_Master10.filter("");		
	this.ds_Master10.filter("BZTH_NO == '"+this.fvBzthNo+"' && WOFH_SQ == '"+this.fvWofhSq+"' && BZTB_SE == '"+this.fvBztbSe+"'");
	this.ds_Master10.addColumn("s_chk", "string");
	this.ds_Master10.addColumn("crudType", "string");
	
}

/***********************************************************************************************
 * 01.02. [Vaildation] 조회 필수값 체크
 ***********************************************************************************************/
this.fn_VaildSearch = function()
{

	//[검색]NULL 체크
	var arrArgs = ["this.div_Search.form.cal_BztbDd^"+ this.div_Search.form.sta_BztbDd.text +"|NULL"
	              ]; 
	
	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs) ) {
		return false;
	}
		
	return true;

}

