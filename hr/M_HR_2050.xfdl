			var oOption = {width:"460", height:"260"}; //Popup화면 옵션

			this.gfn_OpenPopup("M_HR_2050_02", sPopupFormUrl, oArg, "fn_PopupCallback", oOption); //Popup화면 호출 함수
		} else {
			var arrMsgArg = ["사용자"];
			this.gfn_Alert("SM040", arrMsgArg);//&ITM0을(를) 선택하세요.
			return;
		}
	} else {
		var arrMsgArg = ["사용자"];
		this.gfn_Alert("SM040", arrMsgArg);//&ITM0을(를) 선택하세요.
		return;	
	}
};




