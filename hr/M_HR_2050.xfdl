this.div_MasterInput_tab_Master_tpg_03_cbo_JogmCd_onitemchanged = function(obj:nexacro.Combo,e:nexacro.ItemChangeEventInfo)
{
	// 선택한 모듈의 업무분류만 필터링하여 가져옴 : e.postvalue (변경후 값)
	// 필터를 초기화 하고 선택된 모듈코드에 대해 조회해 옴
	// 직렬콤보 초기화
	this.ds_HrSerm.filter("");
