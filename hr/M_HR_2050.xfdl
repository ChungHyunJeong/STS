/***********************************************************************************************
 * 05.03.02 [Vaildation] Detail10-가족사항-주민번호 유효성 체크
***********************************************************************************************/
this.fn_VaildJumnNo1 = function()
{
	if (this.ds_Detail10.rowcount > 0 ) {
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			var sNathCd = this.ds_Detail10.getColumn(iLoop, "NATM_CD");
			var sFajuNo = this.ds_Detail10.getColumn(iLoop, "FAJU_NO");
			
			//국적:한국
 			if (sNathCd == "KR") {
				var bSsn = this.gfn_IsSSN(sFajuNo);  //주민번호여부 체크
				if(!bSsn) {
					var arrMsgArg = [ (iLoop+1) + " 번째행 주민등록번호"] ;
					this.gfn_Alert("SM042", arrMsgArg); //주민등록번호를 확인하세요
					return false;
				}
			} else {
				var bSsn1 = this.gfn_IsFrnrIdNo(sFajuNo);  //외국인번호여부 체크
				if(!bSsn1) {
					var arrMsgArg = [(iLoop+1) + " 번째행 외국인번호"];
					this.gfn_Alert("SM042", arrMsgArg); //외국인번호를 확인하세요
					return false;
				}
 			}
 		}
	}
	
	return true;
}

