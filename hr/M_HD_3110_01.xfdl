/***********************************************************************************************
 * 13.02. [사용자함수] 선택한 정보 부모창 리턴 함수 
 ***********************************************************************************************/ 
this.fn_Choice = function(iRow) {
	if (iRow > -1) {	
	
         rtnValue = "({" +
                        " COMM_CD:'" + this.ds_Master10.getColumn(iRow, "COMM_CD") + "'"+  //0.회사코드
                        ",BZAM_GU:'" + this.ds_Master10.getColumn(iRow, "BZAM_GU") + "'"+  //1.지급구분
                        ",BZAM_CD:'" + this.ds_Master10.getColumn(iRow, "BZAM_CD") + "'"+  //2.국가코드
                        ",BZAM_NM:'" + this.ds_Master10.getColumn(iRow, "BZAM_NM") + "'"+  //3.국가명
                        ",SORT_NO:'" + this.ds_Master10.getColumn(iRow, "SORT_NO") + "'"+  //4.정렬순서
                        ",REMK_NM:'" + this.ds_Master10.getColumn(iRow, "REMK_NM") + "'"+  //5.비고
                "})";
					
		this.gfn_PopClose(rtnValue);
	} else {
		this.gfn_Alert("SM046");//선택된 내용이 없습니다.
		return;		
	}
}

