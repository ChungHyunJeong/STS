this.fn_EmahNm = function(EmahDd)
{
	var iRow = this.ds_Master10.rowposition;
	
	var iFvFindRow = this.ds_Hr211.findRowExpr("CODM_CD=='" + this.ds_Master10.getColumn(iRow, "APPM_GU") + "'");//발령구분
	var sCodmNm = this.ds_Hr211.getColumn(iFvFindRow, "CODM_NM");
 	var sOp01Nm = this.ds_Hr211.getColumn(iFvFindRow, "OP01_NM"); //그리드포멧
		
	//발령내용
	if (!this.gfn_IsNull(sCodmNm)) {
		var sEmahNm = "인사발령 - " + sCodmNm + " (" + this.gfn_StringFormat(EmahDd, "@@@@.@@.@@")+")";
	} else {
		var sEmahNm = "인사발령 - " + " (" + this.gfn_StringFormat(EmahDd, "@@@@.@@.@@")+")";
	};	
	
	this.ds_Master10.setColumn(iRow, "EMAH_NM", sEmahNm);
	
	if (this.ds_Detail10.rowposition > -1) {
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			this.ds_Detail10.setColumn(iLoop, "OP01_NM", sOp01Nm); //그리드포멧
		};
	};
}

