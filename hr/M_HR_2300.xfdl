/***********************************************************************************************
 * 05.04. [Vaildation] Detail 이메일, 성명 중복 체크, 수습시작일 수습종료일 비교-채용
 ***********************************************************************************************/ 
this.fn_EmalCheck = function()
{
	var crudType = "";
	
	for(var i = 0; i < this.ds_Detail10.rowcount; i++) {
		var chkRow = "";
		chkRow = i;		
		
		crudType = this.ds_Detail10.getColumn(i, "crudType");
		if(this.gfn_IsNull(crudType)) {crudType = "";}
			
		if(crudType!="D"){		
				
			for(var j = 0; j < this.ds_Detail10.rowcount; j++) {
				if(this.ds_Detail10.getColumn(chkRow, "EMPM_NM") == this.ds_Detail10.getColumn(j, "EMPM_NM"))
				
				if(chkRow != j) {
					var arrMsgArg = [(j+1),"성명"];	//메시지내용
					this.gfn_Alert("HR113", arrMsgArg); //[ &ITM0 ] 행 &ITM1 이 중복 되었습니다.
					return false;
				}
				
				if(this.ds_Detail10.getColumn(chkRow, "EMAL_NM") == this.ds_Detail10.getColumn(j, "EMAL_NM"))
				
				if(chkRow != j) {
					var arrMsgArg = [(j+1),"이메일"];	//메시지내용
					this.gfn_Alert("HR113", arrMsgArg); //[ &ITM0 ] 행 &ITM1 이 중복 되었습니다.
					return false;
				}

			}
		}
		
		var sStfrDd = this.ds_Detail10.getColumn(i, "STFR_DD")
		var sSttoDd = this.ds_Detail10.getColumn(i, "STTO_DD")
		
		if (!this.gfn_IsNull(sSttoDd)) {
			if(this.gfn_IsNull(sStfrDd)) {
				var arrMsgArg = [(i+1)];	//메시지내용
				this.gfn_Alert("HR115", arrMsgArg); //[ &ITM0 ] 행 수습시작일을 입력하십시요.
				return false;
			}
		}
		
		if (!this.gfn_IsNull(sStfrDd) && !this.gfn_IsNull(sSttoDd)) { 
			if (sSttoDd < sStfrDd) {
				var arrMsgArg = [(i+1)];	//메시지내용
				this.gfn_Alert("HR116", arrMsgArg); //[ &ITM0 ] 행 수습시작일이 수습종료일보다 작습니다.
				return false;
			}
		}
	}
	
	var sEmalNm = "";
	var sEmalNms = "";
	var nRowChk  = 0;
	
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		crudType = this.ds_Detail10.getColumn(iLoop, "crudType");
		if(this.gfn_IsNull(crudType)) {crudType = "";}
			
		if(crudType!="D"){ 
			nRowChk++;
			
			var sEmalNm = this.ds_Detail10.getColumn(iLoop, "EMAL_NM");
			
// 			if ( iLoop == 0) {
// 				sEmalNms = "'" + sEmalNm + "'";
// 			} else {
// 				sEmalNms += ",'" + sEmalNm + "'";
// 			}
			
			if ( nRowChk == 1) {
				sEmalNms = "'" + sEmalNm + "'";
			} else {
				sEmalNms += ",'" + sEmalNm + "'";
			}
		}
	}
	
	this.ds_InputEmal.clearData();
	
	this.ds_InputEmal.addRow();
	this.ds_InputEmal.setColumn(0, "EMAL_NM", sEmalNms);
	this.ds_InputEmal.setColumn(0, "EMAH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EMAH_NO"));
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	if (this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EMAH_NO"))) {
		svcAddObj.sSelectSql = "hr:M_HR_2300_D_SL11";
	} else if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EMAH_NO"))) {
		svcAddObj.sSelectSql = "hr:M_HR_2300_D_SL12";
	}
	
	this.gfn_AddComSearchService(this, svcAddObj);	
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "GROUP_CODE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = "";//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_InputEmal=ds_InputEmal" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_EmalCnt=ds_EmalCnt";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "_gfn_Callback_ComFunc";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.isSync = true;//동기식으로 설정할지 여부

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);
	
	if (this.ds_EmalCnt.getRowCount() > 0) {
		//ds_EmalCnt 에 여러줄 나와도 고치고 저장하면 줄이 줄어드므로 첫줄만 get함.
		var sEmalNm1 = this.ds_EmalCnt.getColumn(0, "EMAL_NM");
		
		var iFindRow = this.ds_Detail10.findRow("EMAL_NM", sEmalNm1);
		
		if (iFindRow > -1) {
			var arrMsgArg = [(iFindRow+1),"이메일"];//메시지내용
			this.gfn_Alert("HR114", arrMsgArg); //[ &ITM0 ] 행 &ITM1 (으)로 등록된 데이터가 존재합니다.
			return false;
		}
	}
	
	return true;
}

