this.div_DetailInput_tab_Detail_onchanged = function(obj:nexacro.Tab,e:nexacro.TabIndexChangeEventInfo)
{
	var iTabIndex = e.postindex;
	
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	
	var sAppmGu = this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_AppmGu.value;
	
	var nRow = this.ds_Hr211.findRow( "CODM_CD", sAppmGu);
 	var sOp01Nm = this.ds_Hr211.getColumn(nRow, "OP01_NM"); //그리드포멧
	
	//Tab-인사발령상세
	this.div_MasterInput.form.btn_XlsxUp.set_visible(false); //엑셀업로드 숨기기
	this.div_MasterInput.form.btn_XlsxDown.set_visible(false); //엑셀다운로드 숨기기
	if (iTabIndex == 0) {
		/*this.fn_SetGridInputButton();//[공통] 그리드 입력 버튼 설정*/ 
		if (sOp01Nm == "01") {
			this.fn_SetGridInputButton() //[공통] 그리드 입력 버튼 설정
			this.div_MasterInput.form.btn_XlsxUp.set_visible(true); //엑셀업로드 보이기
			this.div_MasterInput.form.btn_XlsxDown.set_visible(true); //엑셀다운로드 보이기
		/*-그외*/
		} else {
		//this.alert('111');
			if (sAppmGu == "0420" && !this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EMAH_NO"))){
				this.fn_SetGridInputButtonVisible();//[공통] 검색 Tab 버튼 설정(비활성)
				this.div_MasterInput.form.btn_XlsxUp.set_visible(true); //엑셀업로드 보이기
				this.div_MasterInput.form.btn_XlsxDown.set_visible(true); //엑셀다운로드 보이기
			} else {
				this.fn_SetGridInputButtonVisible();//[공통] 검색 Tab 버튼 설정(비활성)
				this.div_MasterInput.form.btn_XlsxUp.set_visible(false); //엑셀업로드 숨기기
				this.div_MasterInput.form.btn_XlsxDown.set_visible(false); //엑셀다운로드 숨기기
			}
		}
	//Tab-대상자검색
	} else if (iTabIndex == 1) {
		this.fn_SetGridChoiceButton();//[공통] 검색 Tab 버튼 설정
	//Tab-첨부파일
	} else if (iTabIndex == 2) {	
		this.fn_SetGridFileButton();//[공통] 그리드 파일 버튼 설정
	}
};

