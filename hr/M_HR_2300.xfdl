/***********************************************************************************************
 * 02.DG0.07. [Detail 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	//발령후부서
	if (e.columnid == "DEPM_CD") {
		var sToDay = this.gfn_GetCommDate("toDay");//현재일자
		var sEmahDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EMAH_DD")
		
		if(sToDay.substr(0,6) != sEmahDd.substr(0,6)){
			//sEmahDd = sToDay;
			//조직변경 후 선발령시 문제 발생으로 주석처리 22.12.28
		}
		
		this.gfn_GetCommDepmGrid(obj, "DEPM_CD", "DEPM_NM", sEmahDd);//부서명 검색
	//손익정보
	} else if (e.columnid == "PALM_CD") {
		this.gfn_GetCommPalmGrid(obj, "PALM_CD", "PALM_NM");//손익 검색
	//겸직조직
	} else if (e.columnid == "ADDP_CD") {		
		this.gfn_GetCommDepmGrid(obj, "ADDP_CD", "ADDP_NM");//부서명 검색
	//이메일		
	} else if (e.columnid == "FIRST_EMAL") {
	    var sEmalNm = this.ds_Detail10.getColumn(e.row, "EMAL_NM");
		var sFirstNm = nexacro.trim(this.ds_Detail10.getColumn(e.row, "FIRST_EMAL"));
		var sLastNm = this.ds_Detail10.getColumn(e.row, "LAST_EMAL");
		
		var iFvFindRow = this.ds_Hr211.findRowExpr("CODM_CD=='" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "APPM_GU") + "'");//발령구분
		var sCodmNm = this.ds_Hr211.getColumn(iFvFindRow, "CODM_NM");
		var sOp01Nm = this.ds_Hr211.getColumn(iFvFindRow, "OP01_NM"); //그리드포멧	
		
		if (sOp01Nm == "01") {
			this.ds_Detail10.setColumn(e.row, "EMAL_NM", sFirstNm + sLastNm);
		}
	//연봉
	} else if (e.columnid == "ANNU_AM") {
		var iAnnuAm = this.ds_Detail10.getColumn(e.row, "ANNU_AM");//연봉
		var iSalaAm = ""; //월급여
		
		//iSalaAm = nexacro.floor(iAnnuAm / 12, 2); 
		iSalaAm = nexacro.ceil(iAnnuAm / 12, -1); 
		
		this.ds_Detail10.setColumn(e.row, "SALA_AM", iSalaAm);
	} 
};

