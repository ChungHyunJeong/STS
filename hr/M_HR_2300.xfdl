{	
	sDepmCd = this.div_DetailInput.form.tab_Detail.tpg_01.form.div_Search.form.edt_DepmCd.value; //부서코드
	sDepmNm = this.div_DetailInput.form.tab_Detail.tpg_01.form.div_Search.form.edt_DepmNm.value; //부서명
    sPosiCd = this.div_DetailInput.form.tab_Detail.tpg_01.form.div_Search.form.cbo_PosiCd.value; //직위
	sRankCd = this.div_DetailInput.form.tab_Detail.tpg_01.form.div_Search.form.cbo_RankCd.value; //직급
	
	if(this.gfn_IsNull(sPosiCd) && this.gfn_IsNull(sRankCd) && this.gfn_IsNull(sDepmCd)) {
		this.gfn_Alert("HR003");//일괄적용할 데이터를 선택하십시요.
		return false;
	};
	
	if(this.ds_Detail10.rowcount > 0 ){
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			
			if( !this.gfn_IsNull(sDepmCd) ){
