/*---------------------------------------------------------------------------------------------
 *    [Master 입력영역-Button] 엑셀업로드 Call Back 함수
  ---------------------------------------------------------------------------------------------*/
this.fn_ImportCallback = function(sImportId)
{	
	this.ds_Detail10.set_enableevent(false);
	
	for (var iLoop=0; iLoop<this.ds_Import.getRowCount(); iLoop++) {
		if (sImportId == "MASTER10_IPT_0420") {
		
			if (this.ds_Detail10.getColumn(iLoop, "EMPM_CD") == this.ds_Import.getColumn(iLoop, "Column3")){
				this.ds_Detail10.setColumn(iLoop, "ANNU_AM", this.ds_Import.getColumn(iLoop, "Column21"));
				this.ds_Detail10.setColumn(iLoop, "SALA_AM", this.ds_Import.getColumn(iLoop, "Column22"));
			}
			
			this.ds_Detail10.setColumn(iLoop, "crudType", "U");
		} else {
			var iCol = this.ds_Import.getColumn(iLoop, "Column0"); 
			
			if (this.gfn_IsNull(iCol)) {
			
				this.ds_Import.deleteRow(iLoop);			
				iLoop = iLoop - 1;
			} else {
				var iRow = this.ds_Detail10.addRow();
				
				this.ds_Detail10.setColumn(iRow, "EMPM_CD", this.ds_Import.getColumn(iLoop, "Column0"));
				this.ds_Detail10.setColumn(iRow, "EMPM_NM", this.ds_Import.getColumn(iLoop, "Column1"));
			}
		}
	}
	
	this.ds_Detail10.set_enableevent(true);
}

