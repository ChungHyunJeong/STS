					this.ds_Detail10.setColumn(iAddRow, "EMPM_NM", this.ds_Detail20.getColumn(iLoop, "EMPM_NM")); //성명
					
					this.ds_Detail10.setColumn(iAddRow, "BFDP_CD", this.ds_Detail20.getColumn(iLoop, "DEPM_CD")); //발령전부서코드
					this.ds_Detail10.setColumn(iAddRow, "BFDP_NM", this.ds_Detail20.getColumn(iLoop, "DEPM_NM")); //발령전부서명
					this.ds_Detail10.setColumn(iAddRow, "BFDT_CD", this.ds_Detail20.getColumn(iLoop, "DUTY_CD")); //발령전직책
					this.ds_Detail10.setColumn(iAddRow, "BFPS_CD", this.ds_Detail20.getColumn(iLoop, "POSI_CD")); //발령전직위
					this.ds_Detail10.setColumn(iAddRow, "BFRN_CD", this.ds_Detail20.getColumn(iLoop, "RANK_CD")); //발령전직급
					this.ds_Detail10.setColumn(iAddRow, "BFLC_CD", this.ds_Detail20.getColumn(iLoop, "LACO_CD")); //발령전직계
					this.ds_Detail10.setColumn(iAddRow, "BFJT_CD", this.ds_Detail20.getColumn(iLoop, "JOTY_CD")); //발령전직종
					this.ds_Detail10.setColumn(iAddRow, "BFJG_CD", this.ds_Detail20.getColumn(iLoop, "JOGM_CD")); //발령전직군코드
					this.ds_Detail10.setColumn(iAddRow, "BFSR_CD", this.ds_Detail20.getColumn(iLoop, "SERM_CD")); //발령전직렬코드
					this.ds_Detail10.setColumn(iAddRow, "BFJB_CD", this.ds_Detail20.getColumn(iLoop, "JOBM_CD")); //발령전직무코드
					this.ds_Detail10.setColumn(iAddRow, "JOGM_NM", this.ds_Detail20.getColumn(iLoop, "JOGM_NM")); //발령전직군명
					this.ds_Detail10.setColumn(iAddRow, "SERM_NM", this.ds_Detail20.getColumn(iLoop, "SERM_NM")); //발령전직렬명
					this.ds_Detail10.setColumn(iAddRow, "JOBM_NM", this.ds_Detail20.getColumn(iLoop, "JOBM_NM")); //발령전직무명
					this.ds_Detail10.setColumn(iAddRow, "BFUI_WO", this.ds_Detail20.getColumn(iLoop, "UNIT_WO")); //발령전담당업무
 					this.ds_Detail10.setColumn(iAddRow, "BFWP_CD", this.ds_Detail20.getColumn(iLoop, "WOPL_CD")); //발령전근무지					
					this.ds_Detail10.setColumn(iAddRow, "GEND_GU", this.ds_Detail20.getColumn(iLoop, "GEND_GU")); //성별
					this.ds_Detail10.setColumn(iAddRow, "BIRT_DD", this.ds_Detail20.getColumn(iLoop, "BIRT_DD")); //생년월일
					this.ds_Detail10.setColumn(iAddRow, "REMK_NM", this.ds_Detail20.getColumn(iLoop, "REMK_NM")); //비고
					this.ds_Detail10.setColumn(iAddRow, "BFJB_CD1", this.ds_Detail20.getColumn(iLoop, "JOBM_CD2")); //발령전직무코드-콤보용					
					
					this.ds_Detail10.setColumn(iAddRow, "EMAL_NM", this.ds_Detail20.getColumn(iLoop, "EMAL_NM")); //이메일
 					this.ds_Detail10.setColumn(iAddRow, "FIRST_EMAL", this.ds_Detail20.getColumn(iLoop, "FIRST_EMAL")); //이메일_앞
 					this.ds_Detail10.setColumn(iAddRow, "LAST_EMAL", this.ds_Detail20.getColumn(iLoop, "LAST_EMAL")); //이매일_뒤
					
