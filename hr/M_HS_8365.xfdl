this.div_MasterInput_btn_CorhM1_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		var arrMsgArg = [""];
		this.gfn_Alert("SM002", arrMsgArg);//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.
		return false;
	}	 
	
	//자료복사 Confirm
	this.gfn_Confirm("HR005", "", "CONF.MAST_COPY", "fn_MsgCallback");//전년도 자료로 복사 시 기존 자료는 삭제가 됩니다.\n전년도 자료를 복사하시겠습니까?
	return false;
	
};
/*---------------------------------------------------------------------------------------------
 *    03. [Master 전년도자료복사-Button] 복사 실행 이벤트
  ---------------------------------------------------------------------------------------------*/
this.fn_MasterCopy = function()
{
	

	//복사시 재조회 Key설정
	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JSSD_YEAR");//조회Key01
	
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();
 	this.ds_Input.setColumn(0, "JSSD_YEAR", this.div_Search.form.spn_JssdYear.value); 
	this.ds_Input.setColumn(0, "COPY_YEAR", this.div_Search.form.spn_JssdYear.value-1); 

	
	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스 정보 추가
	svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "hr:M_HS_8365_M_DL11";//수정Sql
	svcAddObj.sSaveFlagColumn = "";
	this.gfn_AddComSearchService(this, svcAddObj);
	
		
	svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "hr:M_HS_8365_M_IN11";//저장Sql
	svcAddObj.sSaveFlagColumn = "";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MST_COPY";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명		
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
	
};


