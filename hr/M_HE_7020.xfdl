/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.gfn_DsIsUpdated(this.ds_Detail20) && !this.gfn_DsIsUpdated(this.ds_Detail30) && !this.gfn_DsIsUpdated(this.ds_Detail40) && !this.gfn_DsIsUpdated(this.ds_Detail50)) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}

	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		if (!this.fn_VaildMasterSave()) return;
	}
  	
  	//Master 중복데이터 확인
	this.fn_VaildMasterNewSave();
	
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	if (sChkRowType == "I") {
		var sBasmCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BASM_CD");
		var iFvFindRow = this.ds_Master20.findRowExpr("BASM_CD=='" + sBasmCd + "'");//업무Key Row 찾기

		if (iFvFindRow > -1) {
			this.gfn_Alert("SM041");//중복된 데이터가 존재합니다.
			return;
		}
	}
	
	//Detail10 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		if (!this.fn_VaildDetailSave()) return;
	}
	//Detail20 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail20)) {
		if (!this.fn_VaildDetail20Save()) return;
	}
	//Detail30 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail30)) {
		if (!this.fn_VaildDetail30Save()) return;
	}
	//Detail40 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail40)) {
		if (!this.fn_VaildDetail40Save()) return;
	}
	//Detail50 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail50)) {
		if (!this.fn_VaildDetail50Save()) return;
	}
	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	//Master 입력 상태이면
	if (sChkRowType == "I") {
		
		var sChkBasmCd = "";//체크용
		var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position
		
		var sCommCd = this.ds_Master10.getColumn(iMasterRowPos, "COMM_CD");//회사코드
		var sBasmCd = this.ds_Master10.getColumn(iMasterRowPos, "BASM_CD");//평가년도
		
		//Detail10 데이터셋에 기본정보 입력-회사코드/평가년도
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			sChkBasmCd = this.ds_Detail10.getColumn(iLoop, "COMM_CD");
			
			if (this.gfn_IsNull(sChkBasmCd)) {
				this.ds_Detail10.setColumn(iLoop, "BASM_CD", sBasmCd);
			}
		}
		//Detail20 데이터셋에 기본정보 입력-회사코드/평가년도
		for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
			sChkBasmCd = this.ds_Detail20.getColumn(iLoop, "COMM_CD");
			
			if (this.gfn_IsNull(sChkBasmCd)) {
				this.ds_Detail20.setColumn(iLoop, "BASM_CD", sBasmCd);
			}
		}
		//Detail30 데이터셋에 기본정보 입력-회사코드/평가년도
		for (var iLoop=0; iLoop<this.ds_Detail30.getRowCount(); iLoop++) {
			sChkBasmCd = this.ds_Detail30.getColumn(iLoop, "COMM_CD");
			
			if (this.gfn_IsNull(sChkBasmCd)) {
				this.ds_Detail30.setColumn(iLoop, "BASM_CD", sBasmCd);
			}
		}
		//Detail40 데이터셋에 기본정보 입력-회사코드/평가년도
		for (var iLoop=0; iLoop<this.ds_Detail40.getRowCount(); iLoop++) {
			sChkBasmCd = this.ds_Detail40.getColumn(iLoop, "COMM_CD");
			
			if (this.gfn_IsNull(sChkBasmCd)) {
				this.ds_Detail40.setColumn(iLoop, "BASM_CD", sBasmCd);
			}
		}
		//Detail50 데이터셋에 기본정보 입력-회사코드/평가년도
		for (var iLoop=0; iLoop<this.ds_Detail50.getRowCount(); iLoop++) {
			sChkBasmCd = this.ds_Detail50.getColumn(iLoop, "COMM_CD");
			
			if (this.gfn_IsNull(sChkBasmCd)) {
				this.ds_Detail50.setColumn(iLoop, "BASM_CD", sBasmCd);
			}
		}
	}	
	
	//저장시 재조회 Key설정
	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BASM_CD");//조회Key01

	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	//[공통]서비스 호출 변수
	var sInputDaset = "" //입력 데이터셋
	var sOutDatset = "";//출력 데이터셋
	var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		
	//Master 변경건이 있으면 Master 저장
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
		svcAddObj.sInsertSql = "hr:M_HE_7020_M_IN10";//저장Sql
		svcAddObj.sUpdateSql = "hr:M_HE_7020_M_UP10";//수정Sql
		svcAddObj.sDeleteSql = "hr:M_HE_7020_M_DL10";//삭제Sql
		svcAddObj.sSaveFlagColumn = "";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

		this.gfn_AddComSaveService(this, svcAddObj);
		
		sInputDaset = "ds_Master10=ds_Master10:U" //입력 데이터셋
	}
	
	//Detail10 변경건이 있으면 Detail 저장
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object	
		svcAddObj.sInsertSql = "hr:M_HE_7020_D_IN10";//저장Sql
		svcAddObj.sUpdateSql = "hr:M_HE_7020_D_UP10";//수정Sql
		svcAddObj.sDeleteSql = "hr:M_HE_7020_D_DL10";//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		
		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = "ds_Detail10=ds_Detail10:U" //입력 데이터셋
		} else {
			sInputDaset = sInputDaset + " ds_Detail10=ds_Detail10:U" //입력 데이터셋
		}
	}
	
	//Detail20 변경건이 있으면 Detail 저장
	if (this.gfn_DsIsUpdated(this.ds_Detail20)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object	
		svcAddObj.sInsertSql = "hr:M_HE_7020_D_IN20";//저장Sql
		svcAddObj.sUpdateSql = "hr:M_HE_7020_D_UP20";//수정Sql
		svcAddObj.sDeleteSql = "hr:M_HE_7020_D_DL20";//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		
		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = "ds_Detail20=ds_Detail20:U" //입력 데이터셋
		} else {
			sInputDaset = sInputDaset + " ds_Detail20=ds_Detail20:U" //입력 데이터셋
		}
