/***********************************************************************************************
 * 02.DG0.02. [Detail그리드-버튼] 행추가 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd01AddRow = function()
{
	var sDatasetNm = "";
	var sTabIndex = this.div_DetailInput.form.tab_Detail.tabindex;
	
	//평가등급
	if (sTabIndex == 0) {
		sDatasetNm = this.ds_Detail10;
	//목표난이도
	} else if(sTabIndex == 1) {
		sDatasetNm = this.ds_Detail20;
	//평가비율
	} else if(sTabIndex == 2) {
		sDatasetNm = this.ds_Detail30;
	//승진자격
	} else if(sTabIndex == 3) {
		sDatasetNm = this.ds_Detail40;
	//업적평가
	} else if(sTabIndex == 4) {
		sDatasetNm = this.ds_Detail50;
	}
	
	var iAddRow = this.gfn_GridInsertProcess(this, sDatasetNm);//crud_flag 변경-입력(I)
	sDatasetNm.setColumn(iAddRow, "SORT_NO", sDatasetNm.rowcount);
	sDatasetNm.setColumn(iAddRow, "BASM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BASM_CD"));
	
	if (sTabIndex == 3) {
		sDatasetNm.setColumn(iAddRow, "TOTA_YN", '0');
		sDatasetNm.setColumn(iAddRow, "PAPE_YN", '0');
		sDatasetNm.setColumn(iAddRow, "CSRE_CT", '0');
		sDatasetNm.setColumn(iAddRow, "LANG_YN", '0');
		sDatasetNm.setColumn(iAddRow, "EDUC_YN", '0');
		sDatasetNm.setColumn(iAddRow, "REPO_YN", '0');
		sDatasetNm.setColumn(iAddRow, "INTE_YN", '0');
	} else if (sTabIndex == 4) {
		sDatasetNm.setColumn(iAddRow, "BASE_YN", '0');
	}
};

