					this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20, iLoop);
				}
			}
			if (this.gfn_DsIsUpdated(this.ds_Detail30)) {
				for (var iLoop=0;iLoop<this.ds_Detail30.getRowCount();iLoop++) {
					this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_03.form.grd_Detail30, iLoop);
				}
			}
			if (this.gfn_DsIsUpdated(this.ds_Detail40)) {
				for (var iLoop=0;iLoop<this.ds_Detail40.getRowCount();iLoop++) {
					this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_04.form.grd_Detail40, iLoop);
				}
			}
			if (this.gfn_DsIsUpdated(this.ds_Detail50)) {
				for (var iLoop=0;iLoop<this.ds_Detail50.getRowCount();iLoop++) {
					this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_05.form.grd_Detail50, iLoop);
				}
