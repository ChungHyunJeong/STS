/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 
	//변경여부 체크
 	if (!this.gfn_DsIsUpdated(this.ds_Master10)) {
 		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
 		return;
 	}
  
  	//저장전 유효성 체크
  	//Master 데이터 유효성 체크
  	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
  		if (!this.fn_VaildMasterSave()) return;
  	}

	//중복 체크(귀속년도, 기준구분, 하한금액)
	var sJstaYear = this.ds_Master10.getColumn(this.ds_Master10.rowposition,"JSTA_YEAR"); //귀속년도
	var sJstaGubn = this.ds_Master10.getColumn(this.ds_Master10.rowposition,"JSTA_GUBN"); //기준구분
	var sJstaTamt = this.ds_Master10.getColumn(this.ds_Master10.rowposition,"JSTA_TAMT"); //하한금액
	var sChkData = "JSTA_YEAR =='"+ sJstaYear +"'&& JSTA_GUBN =='"+ sJstaGubn + "'&& JSTA_TAMT =='"+ sJstaTamt + "'";
	
	if(this.ds_Master10.getCaseCount(sChkData) > 1 ){
	   this.gfn_Alert("SM041");//중복된 데이터가 존재합니다.
	   return;
	}	
	
  	//Master 입력/수정/삭제 상태 체크
  	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
  	
  	//저장시 재조회 Key설정
  	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//조회Key01
  	this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JSTA_YEAR");//조회Key02
  	this.fvSchKey03 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JSTA_GUBN");//조회Key03

  	//[공통]서비스 정보 초기화	
  	this.gfn_InitComService(this);
  	
  	//[공통]서비스정보 추가
  	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
  	
  	//[공통]서비스 호출 변수
  	var sInputDaset = "" //입력 데이터셋
  	var sOutDatset = "";//출력 데이터셋
  	var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
  	
	
	for (var iLoop=0; iLoop<this.ds_Master10.getRowCount(); iLoop++) {
		if(this.ds_Master10.getColumn(iLoop, "crudType") == "U"){
			this.ds_Master10.setColumn(iLoop, "dType", "D");//d_flag 변경-입력(D)	
			this.ds_Master10.setColumn(iLoop, "crudType", "I");//crud_flag 변경-입력(I)	
		}
	}
		
  	//Master 변경건이 있으면 Master 저장
  	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
  		
		
  		//[공통]서비스 정보 추가
  		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
		svcAddObj.sDeleteSql = "hr:M_HS_8360_M_DL10";//수정Sql
		svcAddObj.sSaveFlagColumn = "dType";
		this.gfn_AddComSaveService(this, svcAddObj);
		
		
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
		svcAddObj.sInsertSql = "hr:M_HS_8360_M_IN10";//저장Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
  								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		this.gfn_AddComSaveService(this, svcAddObj);
  		
  		sInputDaset = "ds_Master10=ds_Master10:U" //입력 데이터셋
  
  	}
	
  	
  	//[공통]서비스 호출 변수
  	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
  	svcCallObj.sTrid = "SAVE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
  	svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
  	svcCallObj.sInputDaset = sInputDaset; //입력 데이터셋
  	svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
  	svcCallObj.sOtherArg = sOtherArg;//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
  	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명		
  	
  	//[공통]서비스 호출
  	this.gfn_CallComService(this, svcCallObj);	
}

