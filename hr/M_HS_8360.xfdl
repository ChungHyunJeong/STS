 * 02.MG0.02. [Master그리드-버튼] 행추가 클릭 이벤트
 ***********************************************************************************************/
this.fn_MasterGrd01AddRow = function()
{
	//var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	var sJstaYear = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JSTA_YEAR");
	var sJstaGubn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JSTA_GUBN");
	var sJstaFamt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JSTA_FAMT");
	
	var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Master10);//crud_flag 변경-입력(I)
	if(this.gfn_IsNull(sJstaYear)){
	 	this.ds_Master10.setColumn(iAddRow,"JSTA_YEAR", this.div_Search.form.spn_JstaYear.value); //년도
		this.ds_Master10.setColumn(iAddRow,"JSTA_GUBN", this.div_Search.form.cbo_JstaGubn.value); //기준구분
		this.ds_Master10.setColumn(iAddRow,"JSTA_TAMT", 0); //하한금액
	}else{
		this.ds_Master10.setColumn(iAddRow,"JSTA_YEAR", sJstaYear); //년도
		this.ds_Master10.setColumn(iAddRow,"JSTA_GUBN", sJstaGubn); //기준구분
		this.ds_Master10.setColumn(iAddRow,"JSTA_TAMT", sJstaFamt); //하한금액
	}
	this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.setCellPos(6); 
	this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.showEditor(true);
};

