 * 1. 조회 영역
 ***********************************************************************************************/
/***********************************************************************************************
 * 01.01. [공통버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.fn_ComBtnSearch = function(sDst)
{ 
	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;	
	
	var sCondi = this.div_Search.form.edt_SechVl.value;
	
	if(this.fvClickChk=="N"){
		if(this.gfn_IsNull(sCondi) || sCondi == ""){
			this.fvClickChk = "Y";
		}else{
			this.fvClickChk = "N";
		}	
	}

	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Input.clearData();
	this.ds_Detail10.clearData();
	this.ds_Detail11.clearData();
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();

	var sSql = "";//호출SqlId
	var sDEPM_CD = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DEPM_CD");//부서코드
	
	this.ds_Input.setColumn(0, "EMPV_DD", this.fvDemhMm.substr(0,6));
		
	//부서정보로 조회
	if (sDst == "DEPM") {
		if (this.ds_Master10.getRowCount() > 0 && !this.gfn_IsNull(sDEPM_CD)) {
					
			this.ds_Input.setColumn(0, "DEPM_CD", sDEPM_CD);
			
			//전체부서로 조회시
			if (sDEPM_CD == "1000000") {
				var sDutyCd = "";//직책
				var sDutyCds = "";//조회대상 직책
				for (var iLoop=0; iLoop < this.ds_Hr201.getRowCount(); iLoop++) {
					sDutyCd = this.ds_Hr201.getColumn(iLoop, "CODM_CD");//직책
					
					//팀장이상이면
					if (this.ds_Hr201.getColumn(iLoop, "OP01_NM") == "1") {
						if (this.gfn_IsNull(sDutyCds)) {
							sDutyCds = "'" + sDutyCd + "'";
						} else {
							sDutyCds += ",'" + sDutyCd + "'";
						}					
					}
				}				
			}
			
			this.ds_Input.setColumn(0, "DUTY_CD", sDutyCds);//조회 직책
			
		} else {
			return;
		}
		sSql = "hr:M_HR_1220_M_SL10";//호출SqlId
	} else {
		sSql = "hr:M_HR_1220_M_SL11";//호출SqlId
		this.ds_Input.setColumn(0, "SECH_CN", this.div_Search.form.cbo_SechCn.value);
		this.ds_Input.setColumn(0, "SECH_VL", this.div_Search.form.edt_SechVl.value);
	}
	
	this.ds_Input.setColumn(0, "RETY_YN", "N");//재직구분-재직	
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = sSql;
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid 		= "DTL_SEARCH";			//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName 	= this.G_SV_HR;			//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset 	= "ds_Input=ds_Input" 	//입력 데이터셋
	svcCallObj.sOutDatset 	= "ds_Detail10=ds_Empm";//출력 데이터셋
	svcCallObj.sOtherArg 	= "";					//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";		//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);		
	
}

