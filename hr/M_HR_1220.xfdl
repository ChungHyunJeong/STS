/***********************************************************************************************
 * 15.01. 조직도 내역 데이터 설정
 ***********************************************************************************************/ 
this.fn_SetViewData = function()
{
	var iMod = 0;//나머지
	var iAddRow = 0;
	var sEMPM_CD = "";//사번
	var sEMPM_NM = "";//성명
	var sMTEL_NO = "";//휴대전화번호
	var sPOSI_NM = "";//직위
	var sDUTY_NM = "";//직책
	var sDEPM_NM = "";//부서명
	var sEMAL_NM = "";//이메일
	var sOTEL_NO = "";//사내전화
	var sJOBM_NM = "";//직무
	var sFILE_EL = "";//이미지파일Path
	var sEMPO_ID = "";//사진파일ID
	var sFILE_UL = "";//이미지파일URL
	var sEncode = "";//파일명 인코딩
	var sEMAD_YN = "";//겸직여부
	var iLoopCnt = 0;
	var iDupCnt = 0;//사번중복건수

	var sChkDepmCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DEPM_CD");//부서코드-체크
	
	this.ds_Detail11.set_enableevent(false);
	
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		iMod = iLoopCnt % 2;
		sEMPM_CD = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "EMPM_CD"), "");//사번
		sEMPM_NM = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "EMPM_NM"), "");//성명
		sMTEL_NO = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "MTEL_NO"), "");//휴대전화번호
		sPOSI_NM = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "POSI_NM"), "");//직위
		sDUTY_NM = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "DUTY_NM"), "");//직책
		sDEPM_NM = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "DEPM_NM"), "");//부서명
		sEMAL_NM = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "EMAL_NM"), "");//이메일
		sOTEL_NO = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "OTEL_NO"), "");//사내전화
		sJOBM_NM = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "JOBM_NM"), "");//직무
		sFILE_EL = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "FILE_EL"), "");//이미지파일Path
		sEMPO_ID = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "EMPO_ID"), "");//사진파일ID		
		sEMAD_YN = this.gfn_Nvl(this.ds_Detail10.getColumn(iLoop, "EMAD_YN"), "");//겸직여부
		
		var sEncode = encodeURI(sEMPO_ID.replace(/\+/,"%2B"));//파일명 인코딩
		sFILE_UL = sFILE_EL + sEncode;//이미지파일URL		
		
		//부서코드-시너스텍이면서 겸직인경우 출력 제외
		if (!(sChkDepmCd == "1000000" && sEMAD_YN == "O")) {
		
			//중복건수 체크
			if (this.ds_Detail11.findRow("EMPM_CD", sEMPM_CD) > -1 || this.ds_Detail11.findRow("EMPM_CD2", sEMPM_CD) > -1) {
				iDupCnt++;//사번중복건수
			}
					
			if (iMod == 0) {
				iAddRow = this.ds_Detail11.addRow();
				this.ds_Detail11.setColumn(iAddRow, "EMPM_CD", sEMPM_CD);//사번
				this.ds_Detail11.setColumn(iAddRow, "EMPM_NM", sEMPM_NM);//성명
				this.ds_Detail11.setColumn(iAddRow, "MTEL_NO", sMTEL_NO);//휴대전화번호
				this.ds_Detail11.setColumn(iAddRow, "POSI_NM", sPOSI_NM);//직위
				this.ds_Detail11.setColumn(iAddRow, "DUTY_NM", sDUTY_NM);//직책
				//겸직
				if (sEMAD_YN == "O") {
					this.ds_Detail11.setColumn(iAddRow, "DEPM_NM", sDEPM_NM + "(겸직)");//부서명
				} else {
					this.ds_Detail11.setColumn(iAddRow, "DEPM_NM", sDEPM_NM);//부서명
				}
				this.ds_Detail11.setColumn(iAddRow, "EMAL_NM", sEMAL_NM);//이메일
				this.ds_Detail11.setColumn(iAddRow, "OTEL_NO", sOTEL_NO);//사내전화
				this.ds_Detail11.setColumn(iAddRow, "JOBM_NM", sJOBM_NM);//직무
				this.ds_Detail11.setColumn(iAddRow, "FILE_UL", sFILE_UL);//이미지파일URL
			} else {
				this.ds_Detail11.setColumn(iAddRow, "EMPM_CD2", sEMPM_CD);//사번
				this.ds_Detail11.setColumn(iAddRow, "EMPM_NM2", sEMPM_NM);//성명
				this.ds_Detail11.setColumn(iAddRow, "MTEL_NO2", sMTEL_NO);//휴대전화번호
				this.ds_Detail11.setColumn(iAddRow, "POSI_NM2", sPOSI_NM);//직위
				this.ds_Detail11.setColumn(iAddRow, "DUTY_NM2", sDUTY_NM);//직책
				//겸직
				if (sEMAD_YN == "O") {
					this.ds_Detail11.setColumn(iAddRow, "DEPM_NM2", sDEPM_NM + "(겸직)");//부서명
				} else {
					this.ds_Detail11.setColumn(iAddRow, "DEPM_NM2", sDEPM_NM);//부서명
				}
				this.ds_Detail11.setColumn(iAddRow, "EMAL_NM2", sEMAL_NM);//이메일
				this.ds_Detail11.setColumn(iAddRow, "OTEL_NO2", sOTEL_NO);//사내전화
				this.ds_Detail11.setColumn(iAddRow, "JOBM_NM2", sJOBM_NM);//직무
				this.ds_Detail11.setColumn(iAddRow, "FILE_UL2", sFILE_UL);//이미지파일URL		
			}
			
			iLoopCnt++;
		}
				
	}
	
	this.fvTotDepmCnt = iLoopCnt - iDupCnt;//부서직원수 = 부서직원수 - 중복사번

	this.ds_Detail11.set_enableevent(true);
	this.ds_Detail11.set_rowposition(0);
	
	
}

