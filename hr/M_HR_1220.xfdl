 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
			if (this.ds_Master10.getRowCount() > 0) {
				this.div_MasterView.form.tab_Master.tpg_01.form.grd_Master10.setTreeStatus(0,true);
			}
			
			//전체직원수
			if (this.ds_EmpmCnt.getRowCount() > 0) {
				var sCnt = this.ds_EmpmCnt.getColumn(0, "CNT");
				this.fvTotEmpmCnt = this.gfn_AppendComma(sCnt);//전체직원수
				
					this.div_MasterView.form.sta_Cnt.set_text("전체인원 : " + this.gfn_AppendComma(sCnt) + "명");
			}
			
			this.fn_ComBtnSearch("DEPM");//해당 부서코드로 검색 
		
			break;		
		case "DTL_SEARCH": //사원조회	
			var sCondi = this.div_Search.form.edt_SechVl.value;
			
			if (this.ds_Detail10.getRowCount() > 0) {				
				this.fn_SetViewData();//조직도 내역 데이터 설정
				var sDEPM_NM = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DEPM_NM");//부서명
				
				if(this.fvClickChk == "Y"){				
					this.div_MasterView.form.sta_Cnt.set_text(sDEPM_NM + " : " + this.fvTotDepmCnt + "명 / 전체인원 : " + this.fvTotEmpmCnt + "명");
				}else{
// 					if(this.gfn_IsNull(sCondi) || sCondi == ""){				
// 						this.div_MasterView.form.sta_Cnt.set_text(sDEPM_NM + " : " + this.fvTotDepmCnt + "명 / 전체인원 : " + this.fvTotEmpmCnt + "명");
// 					}else{					
						this.div_MasterView.form.sta_Cnt.set_text("전체인원 : " + this.fvTotEmpmCnt + "명");
//					}
				}
			} else {
				var sDEPM_NM = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DEPM_NM");//부서명
				//if(this.gfn_IsNull(sCondi) || sCondi == ""){
				if(this.fvClickChk == "Y"){				
					this.div_MasterView.form.sta_Cnt.set_text(sDEPM_NM + " : 0명 / 전체인원 : " + this.fvTotEmpmCnt + "명");			
				}else{
					this.div_MasterView.form.sta_Cnt.set_text("전체인원 : " + this.fvTotEmpmCnt + "명");			
				}
			}
			
			this.fvClickChk="N";
			
			break;				
	}
};

