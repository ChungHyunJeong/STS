	this.ds_Input.setColumn(0, "FROM_YY", this.div_Search.form.spn_FromYy.value);
	this.ds_Input.setColumn(0, "SYST_ID", this.div_Search.form.edt_SystId.value);	

	//[공통]서비스정보 초기화
