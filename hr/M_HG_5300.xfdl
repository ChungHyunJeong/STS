this.fn_juminchk = function(obj)
{	

	var strChk = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "REGI_YN");
	
	if(!this.gfn_IsNull(strChk) && strChk == '1') {
		this.div_DetailInput.form.tab_Detail.tpg_01.form.mae_JumnNo.set_format('@@@@@@-@@@@@@@');
