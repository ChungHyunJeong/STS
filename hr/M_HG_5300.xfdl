/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}

	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		//첨부파일정보만 변경시
		if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && this.ds_Master10.getRowCount() > 0) {
			//첨부파일정보 저장
			fvFileInfo = new Object();//[공통] 첨부파일정보
			fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, B-상세(그리드Row), E-기타
			fvFileInfo.CALLBACK_FUNC = "";//호출할 콜백함수
			fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
			fvFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CERH_NO");//PK번호2-신청번호
			fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
			//첨부파일Div 화면에 Key세팅하게 변경
			this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);
			return;	
		}
	}

	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		if (!this.fn_VaildMasterSave()) return;
	}
	
	//Detail 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		if (!this.fn_VaildDetailSave()) return;
	}	
	
	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	//Master 입력 상태이면
	if (sChkRowType == "I") {
	
		//채번-신청번호
		//var sCerhNo  = this.gfn_GetTbnmNo(this.fvComPrgmId);//신청번호
		var sCerhNo  = this.gfn_GetTbnmNo(this.fvComPrgmId, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CERH_DD"));//프로그램ID, 기준일자-신청일자
		if (this.gfn_IsNull(sCerhNo)) {
			//신청번호를 가져오는데 실패했습니다.
			return;
		}
		
		var sChkCerhNo = "";//신청번호-체크용
		var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position
		
		//Master 데이터셋에 채번정보 입력-신청번호
		this.ds_Master10.setColumn(iMasterRowPos, "CERH_NO", sCerhNo);
		
		var sCommCd = this.ds_Master10.getColumn(iMasterRowPos, "COMM_CD");//회사코드
		var sCerhNo = this.ds_Master10.getColumn(iMasterRowPos, "CERH_NO");//신청번호
		var sWofhSq = this.ds_Master10.getColumn(iMasterRowPos, "WOFH_SQ");//결재차수
		
		
		//Detail 데이터셋에 기본정보 입력-회사코드/신청번호/결재차수
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			sChkCerhNo = this.ds_Detail10.getColumn(iLoop, "CERH_NO");//신청번호
			
			if (this.gfn_IsNull(sChkCerhNo)) {
				this.ds_Detail10.setColumn(iLoop, "COMM_CD", sCommCd);
				this.ds_Detail10.setColumn(iLoop, "CERH_NO", sCerhNo);
				this.ds_Detail10.setColumn(iLoop, "WOFH_SQ", sWofhSq);
			}
		}	
		
	}	
	
	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		fvFileInfo = new Object();//[공통] 첨부파일정보
		fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
		fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
		fvFileInfo.CALLBACK_FUNC = "fn_ComBtnSearch";//호출할 콜백함수
		fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
		fvFileInfo.PK01_NO = nApp.gvCompany;//PK번호1-회사코드
		fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CERH_NO");//PK번호2-신청번호
		fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수			
	}	
	
	//저장시 재조회 Key설정
	this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CERH_NO");//조회Key01
	this.fvSchKey02 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//조회Key02

	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	//[공통]서비스 호출 변수
	var sInputDaset = "" //입력 데이터셋
	var sOutDatset = "";//출력 데이터셋
	var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		
	//Master 변경건이 있으면 Master 저장
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object				 
		svcAddObj.sInsertSql = "hr:M_HG_5300_M_IN10";//저장Sql		
		svcAddObj.sUpdateSql = "hr:M_HG_5300_M_UP10";//수정Sql
		svcAddObj.sSaveFlagColumn = "";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

		this.gfn_AddComSaveService(this, svcAddObj);
		
		sInputDaset = "ds_Master10=ds_Master10:U" //입력 데이터셋

	}
	
	//Detail 변경건이 있으면 Detail 저장
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object	
		svcAddObj.sInsertSql = "hr:M_HG_5300_D_IN10";//저장Sql
		svcAddObj.sUpdateSql = "hr:M_HG_5300_D_UP10";//수정Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		
		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = "ds_Detail10=ds_Detail10:U" //입력 데이터셋
		} else {
			sInputDaset = sInputDaset + " ds_Detail10=ds_Detail10:U" //입력 데이터셋
		}

	}		
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "SAVE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = sInputDaset //입력 데이터셋
	svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
	svcCallObj.sOtherArg = sOtherArg;//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명		
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	

}

/***********************************************************************************************
 * 05.02. [Vaildation] Master 저장 필수값 체크 - 입력항목 
 ***********************************************************************************************/ 
this.fn_VaildMasterSave = function()
{
	var strCerhGu	=	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CerhGu.value;
	var strRetiYn	=	this.ds_Master15.getColumn(0, "RETI_YN");

	if(strCerhGu == "0010" && strRetiYn == "Y"){
	//	this.fn_EmpmNull();
		this.gfn_Alert("HG001");//재직증명서는 재직자만 발급가능합니다.
		return false;
	}
	
	if(strCerhGu == "0030" && strRetiYn == "N"){
	//	this.fn_EmpmNull();
		this.gfn_Alert("HG002");//퇴직증명서는 퇴직자만 발급가능합니다.
		return false;
	}
	
	if(strCerhGu == "0070"){
		var strFromMm	=	this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_MM");
		var strClosMm	=	this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_MM");
		
		if(this.gfn_IsNull(strFromMm)){		
			var arrMsgArg = ["귀속년월(시작)"];
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FromMm.setFocus();
			this.gfn_Alert("SM008", arrMsgArg);//[필수 입력 항목입니다			
			return false;
		}
		if(this.gfn_IsNull(strClosMm)){		
			var arrMsgArg = ["귀속년월(종료)"];
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosMm.setFocus();
			this.gfn_Alert("SM008", arrMsgArg);//[필수 입력 항목입니다			
			return false;
		}
		
		if(strFromMm > strClosMm){		
			var arrMsgArg = ["귀속년월(시작)","귀속년월(종료)"];
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosMm.setFocus();
			this.gfn_Alert("SM056", arrMsgArg);//[&ITM0]이(가) [&ITM1]을(를) 초과할 수 없습니다.
			return false;
		}
	}else if(strCerhGu == "0040" || strCerhGu == "0060"){
		var strCerhYy	=	this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CerhYy.value;
		if(this.gfn_IsNull(strCerhYy)){			
			var arrMsgArg = ["귀속년도"];
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CerhYy.setFocus();
			this.gfn_Alert("SM008", arrMsgArg);//[필수 입력 항목입니다			
			return false;
		}
		
		if(strCerhYy < "1900" || strCerhYy > "9999" || strCerhYy.length != 4){	 	
			var arrMsgArg = ["귀속년도"];
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CerhYy.setFocus();
			this.gfn_Alert("SM008", arrMsgArg);//[필수 입력 항목입니다			
			return false;
		}
	}

	var iCerhQt   = this.ds_Master10.getColumn(this.ds_Master10.rowposition,  "CERH_QT");
	iCerhQt		  = nexacro.toNumber(iCerhQt);	
	
	if (this.gfn_IsNull(iCerhQt) || iCerhQt < 1 || iCerhQt > 9){
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CerhQt.setFocus();		
		this.gfn_Alert("HG005");//신청수량은 1~9까지 입력하셔야 됩니다.
		return false;
	}

	//[저장] Validation 설정
	var arrArgs = ["this.div_MasterInput.form.tab_Master.tpg_01.form.cal_CerhDd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_CerhDd.text + "|NULL" //신청일자
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_DepmCd.text + "|NULL" //신청부서
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SystId^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_EmpmCd.text + "|NULL" //신청자
				  ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CerhGu^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_CerhGu.text + "|NULL" //신청구분
				  ,"this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_RemkNm.text + "|NULL" //용도			
				  ,"this.div_DetailInput.form.tab_Detail.tpg_01.form.edt_AddrNm^" + this.div_DetailInput.form.tab_Detail.tpg_01.form.sta_AddrNm.text + "|NULL" //주소
			      ]; 
				  
	var sCheckGroupTitle = this.div_MasterInput.form.tab_Master.tpg_01.text;//체크그룹 타이틀

	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
		return false;
	}

	return true;
}

