this.fn_VaildSearch = function()
{
	//[결재] 결재내역 조회가 아니면
	if (!this.fvWfSearchMode) {
		if (!this.gfn_CheckFromToDate(this.fvComPrgmId, this.div_Search.form.cal_CerhDdFr, this.div_Search.form.cal_CerhDdTo)) return false;//조회기간 범위 체크
		//조회조건 필수입력값 있으면 체크
	}
	
		var arrArgs = ["this.div_Search.form.edt_DepmCd^등록부서코드|NULL"
		              , "this.div_Search.form.edt_DepmNm^등록부서명|NULL"
					  ]; 
					  
		var sCheckGroupTitle = "조회조건";//체크그룹 타이틀

		//컴포넌트 Vaild 체크
		if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
			return false;
		}
		
		return true;
}

