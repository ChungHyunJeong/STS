			var EdrhNo = this.ds_Master10.getColumn(iRow, "EDRH_NO");
			var WofnSq = this.ds_Master10.getColumn(iRow, "WOFH_SQ");
			var sPopupId = "WF_M_HU_6200";//팝업ID - 계정코드 
			var oArg = {paramTitle:"교육실적등록", EdrhNo:EdrhNo, WofnSq:WofnSq};//Argument처리
			this.gfn_EdrhPop(sPopupId, oArg); 
			}	
	//비고상세
	} else if (e.col == 15) {	
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "REMK_NM"))) {
