/***********************************************************************************************
 * 05.03. [Vaildation] Detail 저장 필수값 체크 - 그리드항목
 ***********************************************************************************************/ 
this.fn_VaildDetailSave = function()
{

	//trace("1");

	if (this.ds_Detail10.getRowCount() == 0) {
		var arrMsgArg = [this.div_DetailInput.form.tab_Detail.tpg_01.text];
		this.gfn_Alert("HR001", arrMsgArg);// 업적평가 데이터가 없습니다.
		return false;
	};
	
// 	if (this.ds_Detail20.getRowCount() == 0) {
// 		var arrMsgArg1 =[this.div_DetailInput.form.tab_Detail.tpg_02.text];
// 		this.gfn_Alert("HR001", arrMsgArg1);//역량평가 데이터가 없습니다.
// 		return false;
// 	};


	//[저장] Validation 설정
	var arrArgs = ["EVEX_CT^NULL"  /* 성과실적-평가자 실적 */
	              , "EVFI_NM^NULL"  /* 성과실적-평가자 최종평가의견 */
			      ]; 
	//Grid Vaild 체크
	var sCheckGroupTitle = this.div_DetailInput.form.tab_Detail.tpg_01.text;//체크그룹 타이틀
	if (!this.gfn_CheckGridValid(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10, arrArgs, sCheckGroupTitle) ) {
	 
		return false;
	}
	
	
	//trace("2");
	

	//Grid Vaild 체크
 	var checkYn= false;

	//PLRC_CD1
	for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
		checkYn= false;
		//매우부족
		if(this.ds_Detail20.getColumn(iLoop, "PLRC_CD1") == "1" ){
			var checkYn= true;
		}
		//부족
		if(this.ds_Detail20.getColumn(iLoop, "PLRC_CD2") == "2" ){
			var checkYn= true;
		}
		//충족
		if(this.ds_Detail20.getColumn(iLoop, "PLRC_CD3") == "3" ){
			var checkYn= true;
		}
		//초과
		if(this.ds_Detail20.getColumn(iLoop, "PLRC_CD4") == "4" ){
			var checkYn= true;
		}
		//탁월
		if(this.ds_Detail20.getColumn(iLoop, "PLRC_CD5") == "5" ){
			var checkYn= true;
		}
		
		if (!checkYn){
			var arrMsgArg =["역량평가의 성과실적-피평가자"];
			this.gfn_Alert("SM040", arrMsgArg);//&ITM0을(를) 선택하세요.
			
			this.div_DetailInput.form.tab_Detail.set_tabindex(1);
			this.ds_Detail20.set_rowposition(iLoop);
			
			return false;
		}
		
	}
	
	
	
	
	return true;
}
 
