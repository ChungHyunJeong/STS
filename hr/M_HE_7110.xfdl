/***********************************************************************************************
 * 15.06. 평가기준관리
 ***********************************************************************************************/
this.fn_He7020 = function(baseCd) 
{
	
	//trace("baseCd  = " + baseCd);
	
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Input.clearData();
	this.ds_Input.addRow();
	//this.ds_Input.setColumn(0, "GRCM_CD", '8010');	//성과목표_구분
	this.ds_Input.setColumn(0, "BASM_CD", baseCd);	//평가년도
	
	this.ds_He7020.clearData();
	this.ds_He7021.clearData();
	this.ds_He7022.clearData();
	

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "hr:M_HE_7020_D_SL20";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	
	svcAddObj.sSelectSql = "hr:M_HE_7020_D_SL30";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	
	svcAddObj.sSelectSql = "hr:M_HE_7020_D_SL10";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "FUNC_He7020";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_SM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_He7020=ds_Detail20 ds_He7021=ds_Detail30 ds_He7022=ds_Detail10";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);
}


/***********************************************************************************************
 * 15.06. 평가점수합계
 ***********************************************************************************************/
this.fn_He7021 = function() 
{

	this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MBOM_PPLN",this.ds_Detail10.getSum("EVPO_CT"));
	//this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MBOM_PLPL",this.ds_Detail10.getSum("EVPO_CT"));

    //this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MBOM_PLYR", this.ds_Detail20.getSum("EVPO_CT"));
	this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MBOM_PYRN", this.ds_Detail20.getSum("EVPO_CT"));


	//평가점수(조정전)
	//업적평가
	var mbomPpln = nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MBOM_PPLN"));
	//역량평가
	var mbomPyrn = nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MBOM_PYRN"));
	
	//평가점수(최종)
	//업적평가
	//var mbomPlPl = nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MBOM_PLPL"));
	//역량평가
	//var mbomPlyr = nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MBOM_PLYR"));
	
	//직급
	var rankCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RANK_CD"); // 직위
	
	//trace("mbomPlPl = " + mbomPlPl);
	//trace("mbomPlyr = " + mbomPlyr);
	//trace("rankCd = " + rankCd);
	//trace("mbomPyrn = " + mbomPyrn);
	//trace("mbomPlPl = " + mbomPlPl);
	//trace("mbomPlyr = " + mbomPlyr);
	
	var findRow = this.ds_He7021.findRowExpr("RANK_CD == '"+ rankCd +"'" );
	
	trace("findRow = " + findRow);
	
	if(findRow > -1){
		//업적평가
		var evalCt = nexacro.toNumber(this.ds_He7021.getColumn(findRow, "EVAL_CT")) / 100;
		//역량평가
		var capaCt = nexacro.toNumber(this.ds_He7021.getColumn(findRow, "CAPA_CT")) / 100;
		
		
		
		//trace("evalCt = " + evalCt);
		//trace("capaCt = " + capaCt);
		
		//평가점수(조정전)
		var mbomPsum = (mbomPpln * evalCt) + (mbomPyrn * capaCt);
		
		//평가점수(최종)
		//var mbomPljs = mbomPlPl * evalCt + mbomPlyr * capaCt;
		
		if(this.gfn_IsNull(mbomPsum)){
			mbomPsum = 0;
		}		
		
		//if(this.gfn_IsNull(mbomPljs)){
		//	mbomPljs = 0;
		//}
		
		trace("mbomPsum = " + mbomPsum);
		//trace("mbomPljs = " + mbomPljs);
		
		
		
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MBOM_PSUM", mbomPsum); // 
		//this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MBOM_PLJS", mbomPljs); //
		
		findRow = this.ds_He7022.findRowExpr("OVER_CT <= "+ mbomPsum +" && UNDE_CT >= "+ mbomPsum );
		//trace("findRow1 = " + findRow);
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MBOM_PPDG", this.ds_He7022.getColumn(findRow, "BASB_CD"));
		//trace("findRow2 = " + findRow);
		//findRow = this.ds_He7022.findRowExpr("OVER_CT <= "+ mbomPsum +" && UNDE_CT >= "+ mbomPljs );
		//this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MBOM_PLDG", this.ds_He7022.getColumn(findRow, "BASB_CD"));
	}

}


