	var objCondList = [
						"HR720|ds_Hr720", "HR700|ds_Hr700"		//역량항목 필수/선택,  평가관리자
					  ];
	
	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, "");
	
  	//콤보 인덱스 설정
	//this.div_Search.form.cbo_UsedYn.set_index(0);	
	this.ds_Hr700.find
	//trace(this.ds_Hr700.findRowExpr("CODM_CD == "+ this.gfn_GetSessionValue("SYST_ID") ));
	//trace(this.gfn_GetSessionValue("SYST_ID") );
	//trace("CODM_CD == "+ this.gfn_GetSessionValue("SYST_ID") );
	if (this.ds_Hr700.findRowExpr("CODM_CD == '"+ this.gfn_GetSessionValue("SYST_ID") + "'" )  > -1 ){
		this.div_Search.form.edt_EmpmCd.set_enable(true);
		this.div_Search.form.edt_EmpmNm.set_enable(true);
		this.div_Search.form.btn_SystId.set_enable(true);
