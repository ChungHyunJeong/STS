/***********************************************************************************************
 * 02.DG0.07. [Detail 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	

	if(e.columnid == "EVEX_CT"){
	
	
		//성과실적-평가자 실적
		var weigCt = nexacro.toNumber(obj.getColumn(e.row, "WEIG_CT"));
		var nPlabCt = nexacro.toNumber(obj.getColumn(e.row, "PLAB_CT"));
		var evexCt = nexacro.toNumber(obj.getColumn(e.row, "EVEX_CT"));
		var sScopCd = nexacro.toNumber(obj.getColumn(e.row, "SCOP_CD"));
		
		if(obj.getColumn(e.row, "PLAB_GU") =="0020" ){
			//팀평가시 
			// 비중보다 클수 없다.
			if(weigCt < evexCt){
				obj.setColumn(e.row, "EVEX_CT", 0);
				this.gfn_Alert("HR260");//팀평가시 비중보다 클 수 없습니다.
				return;
			}
		}	
		
		
		//var findRow = this.ds_He7020.findRowExpr("OVER_CT <= "+ evexCt +" && UNDE_CT >= "+ evexCt );
		//if (findRow > -1){
			//등급

			if(obj.getColumn(e.row, "PLAB_GU") =="0020"){
				//팀평가시
				obj.setColumn(e.row, "EVPO_CT", evexCt);
			}else{
				//달설율
				trace("weigCt = " + weigCt);
				trace("evexCt = " + evexCt);
				
				
				
				if(sScopCd == "0020" || sScopCd == "0030" || sScopCd == "0050"){
					 
					if (nPlabCt == 0){
						if  (evexCt <= 0){				
							//obj.setColumn(e.row, "EVWI_CT",  1);
							nRet = 100;
						}else if(evexCt==1){
							//obj.setColumn(e.row, "EVWI_CT",  0.9);
							nRet = 90;
						}else if(evexCt>=2){	
							//obj.setColumn(e.row, "EVWI_CT",  0.8);				
							nRet = 80;
						}
					} else {
						nRet =  (nPlabCt + (nPlabCt - evexCt)) / nPlabCt * 100;
					}
					 
					//obj.setColumn(e.row, "PLRB_RT",  nRet);
					
				}else{
					 nRet = evexCt / nPlabCt * 100;
					 
					 //obj.setColumn(e.row, "PLRB_RT",  nRet);
				}			
				nRet = nexacro.round(nRet,1);
				
				if (nRet > 9999){	
					var iLoop_1 = e.row + 1
					var arrMsgArg = ["업적평가탭 "+iLoop_1+"행 달성율","9999"];	
					this.gfn_Alert("SM056", arrMsgArg); //[&ITM0]이(가) [&ITM1]을(를) 초과할 수 없습니다.	
					obj.setColumn(e.row, "EVEX_CT", 0);
					nRet = 0;
					//return false;
				}
		
				obj.setColumn(e.row, "EVEX_RT",  nRet.toFixed(8));
				trace(nRet);
				findRow = this.ds_He7020.findRowExpr("OVER_CT <= "+ nRet +" && UNDE_CT >= "+ nRet );
				//obj.setColumn(e.row, "EVEX_RT", (weigCt / 100) * evexCt);
				trace("BASC_CD = "   + this.ds_He7020.getColumn(findRow, "BASC_CD"));
				//등급
				obj.setColumn(e.row, "EVGD_CD", this.ds_He7020.getColumn(findRow, "BASC_CD"));
				//가중치
				var weigCt7020 =  nexacro.toNumber(this.ds_He7020.getColumn(findRow, "WEIG_CT"));
				obj.setColumn(e.row, "EVWI_CT", weigCt7020);
				//평가점수
				obj.setColumn(e.row, "EVPO_CT", weigCt * weigCt7020);
			}
			
			
			
		//}
		
		

		//this.ds_Master10.setColumn(this.ds_Master10.rowposition, "crudType", "U");
		//master 합계
		this.fn_He7021();
	}
	


};
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail20] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	//trace("obj.getColumn(e.row, e.columnid)   = "  + obj.getColumn(e.row, e.columnid) );
	
	if(e.columnid == "EVGD_CD1" || e.columnid == "EVGD_CD2" || e.columnid == "EVGD_CD3" || e.columnid == "EVGD_CD4" || e.columnid == "EVGD_CD5"){

		var nWeigCtCT;
		
		nWeigCtCT  =  obj.getColumn(e.row,  "WEIG_CT");
		
		var nValue =  obj.getColumn(obj.rowposition, "GRED_C"+e.newvalue);
		nValue	   =  nexacro.round((nexacro.toNumber(nValue) * nWeigCtCT / 100), 1); 		
		
		obj.setColumn(obj.rowposition, "EVGD_CD", e.newvalue);
		//trace("------>"+this.gfn_Nvl(obj.getColumn(obj.rowposition, "GRAD_C"+e.newvalue),0))
		//obj.setColumn(obj.rowposition, "EVPO_CT", this.gfn_Nvl(obj.getColumn(obj.rowposition, "GRED_C"+e.newvalue),0));
		obj.setColumn(obj.rowposition, "EVPO_CT", this.gfn_Nvl(nValue,0));
		//평가등급
		if (e.columnid != "EVGD_CD1") obj.setColumn(obj.rowposition, "EVGD_CD1", 0);
		if (e.columnid != "EVGD_CD2") obj.setColumn(obj.rowposition, "EVGD_CD2", 0);
		if (e.columnid != "EVGD_CD3") obj.setColumn(obj.rowposition, "EVGD_CD3", 0);
		if (e.columnid != "EVGD_CD4") obj.setColumn(obj.rowposition, "EVGD_CD4", 0);
		if (e.columnid != "EVGD_CD5") obj.setColumn(obj.rowposition, "EVGD_CD5", 0);
		
		//master 합계
		this.fn_He7021();
	}
};

