/**
* 파일다이얼로그 VirtualFile 로드 성공 이벤트 - 사진
*/
this.fn_FileDialog_VirtualFilePo_onsuccess = function(obj, e) {

	var size_table = ["KB", "MB", "GB", "TB", "PB"];
	
	function cutFileSize(filesize, rate, count) {
		var ret = (filesize / (Math.pow(rate, count + 1))).toFixed(2);
		if (ret < rate) {
			if (size_table[count]) {
				return ret + " " + size_table[count];
			} else {
				return filesize + " Byte";
			}
		} else {
			return cutFileSize(filesize, rate, ++count);
		}
	};

	var sFileName = obj.filename;
	var iLastIndex = sFileName.lastIndexOf(".") + 1;
	var sExt = sFileName.substring(iLastIndex);
	var sSaveFileName = this.fvUsemId + "." + sExt;
	this.fvFileName = sSaveFileName;
	   
	   trace("this.fvSystId==" + sSaveFileName);

	if (e.reason == 1) {
		if (this.ds_Master10.getRowCount() > 0) {
			this.ds_Master10.setColumn(idx, "PATH_NM", this.fvFilePath);
			/*this.ds_Master10.setColumn(idx, "EMPO_NM", obj.filename);*/
			this.ds_Master10.setColumn(idx, "EMPO_NM", sSaveFileName);
			this.ds_Master10.setColumn(idx, "uploadYn", "N");
			this.ds_Master10.setColumn(idx, "SYST_ID", this.fvSystId);
			this.ds_Master10.setColumn(idx, "FILE_UL", "");	
			this.ds_Master10.setColumn(idx, "crudType", "U");//수정	
		} else {
			var idx = this.gfn_GridInsertProcess(this, this.ds_Master10);//crud_flag 변경-입력(I)

			this.ds_Master10.setColumn(idx, "PATH_NM", this.fvFilePath);
			//this.ds_Master10.setColumn(idx, "EMPO_NM", obj.filename);
			this.ds_Master10.setColumn(idx, "EMPO_NM", sSaveFileName);
			this.ds_Master10.setColumn(idx, "uploadYn", "N");
			this.ds_Master10.setColumn(idx, "SYST_ID", this.fvSystId);
			this.ds_Master10.setColumn(idx, "FILE_UL", "");
		}
		

		this.fn_FileUploadPo();//파일업로드 및 저장 - 사진
	}
		
};



/**
* 파일 업로드 처리 - 사진
*/
this.fn_FileUploadPo = function() {

	if (this._vFiles_po.length > 0) {

		var vfile = this._vFiles_po[this._vFiles_idx_po];
		if (vfile)
		{	
			this.fut_FilePo.addFile("file00", vfile);	
		}

		//this.fut_FilePo.upload(nEnv.services["svcUrl"].url + "FileUploader?mode=upload&subDir=" + this.fvFilePath);
		this.fut_FilePo.upload(nEnv.services["svcUrl"].url + "fileUpload?mode=upload&subDir=" + this.fvFilePath + "&sFileName=" + this.fvFileName);
		
	} else {

		this.gfn_Alert("SM035");//파일을 선택하세요.
	}
	
};



/**
* 파일다이얼로그 Close 이벤트 - 사진
*/
this.fdl_FilePo_onclose = function(obj:nexacro.FileDialog,e:nexacro.FileDialogEventInfo)
{

	for (var i = 0, len = e.virtualfiles.length, vFile; i < len; i++) {
		vFile = e.virtualfiles[i];
		vFile.addEventHandler("onsuccess", this.fn_FileDialog_VirtualFilePo_onsuccess, this);
		this._vFiles_po.push(vFile);
		vFile.open(null, 1);
		vFile.getFileSize();	
			
	}
};


/**
* 파일 업로드 성공 이벤트 - 사진
*/
this.fut_FilePo_onsuccess = function(obj:nexacro.FileUpTransfer,e:nexacro.FileUpTransferEventinfo)
{	

	this._vFiles_idx_po++; 
    var vfile = this._vFiles_po[this._vFiles_idx_po];

	if (vfile) {		
		//초기화 해줘야됨
		this.fut_FilePo.clearFileList();		
		this.fut_FilePo.addFile("file00",vfile);
		//this.fut_FilePo.upload(nEnv.services["svcUrl"].url+"FileUploader?mode=upload&subDir=" + this.fvFilePath);
		this.fut_FilePo.upload(nEnv.services["svcUrl"].url+"FileUploader?mode=upload&subDir=" + this.fvFilePath + "&sFileName=" + this.fvFileName);

	}
	

	trace("\n========FileUpTransfer00_onsuccess-Po========");
	trace("\ne.eventid : "+e.eventid);
	trace("\ne.fromobject : "+e.fromobject);
	trace("\ne.fromreferenceobject : "+e.fromreferenceobject);
	trace("\ne.url : "+e.url);
	trace("\ne.code : "+e.code);
	trace("\ne.message : "+e.message);
	trace("\n==============================================");

	//파일업로드 결과 데이셋이 있으면
	if (e.code == 0 && e.datasets[0] !=null && e.datasets[0].getRowCount() > 0) {

		var iFindRow = this.ds_Master10.findRowExpr("EMPO_NM == '" + e.datasets[0].getColumn(0, "FILE_CNAME") + "' && uploadYn == 'N'");

		this.ds_Master10.setColumn(iFindRow, "EMPO_ID", e.datasets[0].getColumn(0, "FILE_NAME"));
		this.ds_Master10.setColumn(iFindRow, "uploadYn", "Y");//파일업로드 성공여부
		this.ds_Master10.setColumn(iFindRow, "PATH_NM", this.fvFilePath);
	}

	if (this.ds_Master10.getCaseCount("uploadYn=='N'") < 1) {
	
		this._vFiles_idx_po = 0;
		this._vFiles_po = [];
		this.fut_FilePo.clearFileList();
		
		this.fn_ComBtnSave("PO");//저장
	}
};



