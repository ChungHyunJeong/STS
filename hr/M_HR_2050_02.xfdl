this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회	
			this.ds_Master10.addColumn("uploadYn", "string");	
			this.ds_Master10.addColumn("crudType", "string");	
			
			if (this.ds_Master10.getRowCount() > 0) {
				var sEMPO_ID = this.ds_Master10.getColumn(0, "EMPO_ID");	
				var sEncode = encodeURI(sEMPO_ID.replace(/\+/,"%2B"));
				this.img_Empo.set_image(this.ds_Master10.getColumn(0, "FILE_UL") + sEncode);				
			}
			
			break;		
			
		case "SAVE": //저장	
			this.div_PopupTitle_btn_Search_onclick();//조회
			break;			
			
		case "DELETE": //삭제	
			this.div_PopupTitle_btn_Search_onclick();//조회
			break;					
	}
};


