	if(strCerhGu == "0070"){
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FromMm.set_enable(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosMm.set_enable(true);
