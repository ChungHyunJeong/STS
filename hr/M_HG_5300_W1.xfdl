	if(strCerhGu == "0010"){
		strTermNm	=	this.ds_Master15.getColumn(0,"A01");
	//0020:경력증명서
	}else if(strCerhGu == "0020") {
		strTermNm	=	this.ds_Master15.getColumn(0,"A02");	
	//0030:퇴직증명서
	}else if(strCerhGu == "0030") {
		strTermNm	=	this.ds_Master15.getColumn(0,"A03");
	//0060:원천징수부
	}else if(strCerhGu == "0060") {
		strTermNm	=	this.ds_Master15.getColumn(0,"A04");	
	//0040:원천징수영수증
	}else if(strCerhGu == "0040") {
		strTermNm	=	this.ds_Master15.getColumn(0,"A05");	
	//0070:소득세원천징수증명서
	}else if(strCerhGu == "0070") {
		strTermNm	=	this.ds_Master15.getColumn(0,"A06");
	}	
	
