this.fn_EmpmInfoChk = function() 
{

	var strCerhGu	=	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CerhGu.value;
	var strRetiYn	=	this.ds_Master15.getColumn(0, "RETI_YN");
