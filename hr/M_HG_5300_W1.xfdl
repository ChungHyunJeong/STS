this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
	
		case "COMBO_SEARCH" : // 공통코드 조회	  
 			
			break;
			
		case "MST_SEARCH": //조회
			
			if (this.ds_Master10.getRowCount() > 0) {
			
				var strCerhNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CERH_NO");
				var iCerhLen  = 0
				if(!this.gfn_IsNull(strCerhNo))
				{
					iCerhLen = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CERH_NO").length;
				}
				
// 				if (iCerhLen == 10) {
// 					this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CerhNo.set_format("@@@@@@-@@@@");	
// 				} else {
// 					this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CerhNo.set_format("@@@@@@@@-@@@@");	
// 				}			

				if (iCerhLen == 10) {
					this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CerhNo.set_format("@@@@@@-@@@@");	
				} else if (iCerhLen == 8) {
					this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CerhNo.set_format("@@@@-@@@@");	
				} else {			
					this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CerhNo.set_format("@@@@@@@@-@@@@");	
				}
			
				
				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {				
					var iFvFindRow = this.ds_Master10.findRowExpr("CERH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ=='" + this.fvSchKey02 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}			
				
				
				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
				
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
				
				this.fvInserMode = false; //입력상태여부 : 조회
				
			} else {
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			
				//입력영역 비활성화
				this.div_MasterInput.set_enable(false);				
				
				//[공통]그리드NoData Text설정
				//this.gfn_SetGridNoDataText(this.div_MasterView.form.grd_Master10);
				
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = false;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);					
			}
			
			//저장후 재조회여부-true
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}
			
			break;
			
			
		case "MST_SEARCH_40": //조회(원천징수영수증)
		
			break;
		

		case "MST_SEARCHS":
// 			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_MM",  this.ds_Master15.getColumn(0,"ENTR_DD"));
