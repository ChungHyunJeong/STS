// 			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "ADDR_NM",  this.ds_Master15.getColumn(0,"ADDR_NM"));
// 			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "ADDC_NM",  this.ds_Master15.getColumn(0,"ADDC_NM"));

			this.fn_EmpmInfoChk();
			
			break;
		case "MST_SEARCHS_CHK": //조회		
			//첨부파일 정보 조회
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
