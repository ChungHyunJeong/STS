		this.ds_Master10.setColumn(this.ds_Master10.rowposition,  "");
	
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FromMm.set_enable(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_ClosMm.set_enable(false);
	}

