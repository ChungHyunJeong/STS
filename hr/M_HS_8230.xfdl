 * 15.05. 귀속년월에 맞는 근태기간(마감일자) 구하기
 ***********************************************************************************************/ 
this.fn_GetDate = function ()
{
// 	var sMpamMm = this.div_Search.form.cal_MpamMm.value.substr(0, 6);
// 	
// 	if (!this.gfn_IsNull(sMpamMm)) {
// 		sMpamMm = sMpamMm + "20"; //귀속년월 + 20일
// 		var sAddMonth = this.gfn_AddMonth(sMpamMm, -1).substr(0, 6) + "21"//귀속년월-1월 + 21일
// 		this.div_Search.form.cal_FromDd.set_value(sAddMonth);
// 		this.div_Search.form.cal_ClosDd.set_value(sMpamMm);
// 	
// 	} else {
// 		this.div_Search.form.cal_FromDd.set_value("");
// 		this.div_Search.form.cal_ClosDd.set_value("");
// 	}


	var sMpamMm = this.div_Search.form.cal_MpamMm.value; //귀속년월
	this.div_Search.form.cal_FromDd.set_value("");
	this.div_Search.form.cal_ClosDd.set_value("");

	if (this.gfn_IsNull(sMpamMm)) {
		var arrMsgArg = ["귀속년월"];
		this.gfn_Alert("SM008", arrMsgArg); //&ITM0은(는) 필수 입력 항목입니다.
		return;
	}
	
	if(sMpamMm.trim().length < 6 || sMpamMm.substr(0,4) > "2999" || sMpamMm.substr(0,4) < "1990" || sMpamMm.substr(4,2) < "01" || sMpamMm.substr(4,2) > "12"){
		var arrMsgArg = ["귀속년월("+ sMpamMm.substr(0,4) + "-" + sMpamMm.substr(4,2) +")"];
		this.gfn_Alert("SM017", arrMsgArg); //귀속년월은 유효하지 않은 날짜 형식입니다.
		return;
	}
	
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_InputSch.clearData();
	
	//[업무] 조회조건 입력
	this.ds_InputSch.addRow();
	this.ds_InputSch.setColumn(0, "FROM_DD", this.div_Search.form.cal_MpamMm.value.substr(0, 6));//귀속년월

	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "hr:M_HS_8230_M_SL40";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "DATE_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_InputSch=ds_InputSch" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Master40=ds_Master40";;//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
};

/***********************************************************************************************
 * 15.05. [그리드 버튼] keystring 정렬
 ***********************************************************************************************/ 
this.div_MasterView_tab_Master_tpg_03_grd_Master30_onheadclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	//DEPM_CD

	/*
	 그룹소계가 있고 정렬시 그룹소계는 해제 한다.
	*/
	var sortInfos = obj.sortInfos;
	var sortItems = obj.sortItems;
	
	
	//그룹초기화
	this.ds_Master30.set_keystring("");
	
	if ( this.gfn_IsNull(sortInfos) || this.gfn_IsNull(sortItems) ) return;

	// 정렬상태 초기화.
	for( var j=0; j<sortItems.length;j++){
		var col = sortItems[j];
		var sortInfo = sortInfos[col];

	}
	
	//정렬상태값 0: 없음, 1 ASC, 2 DESC 
	//이지만 상태값은 전단계상태값만 가져온다.
	if(sortInfo.status != "2"){
		this.ds_Master30.set_keystring("");
	}else{
		//정렬이 없을시 다시 그룹화 한다.
		this.ds_Master30.set_keystring("G:DEPM_CD");
	}
};



this.div_MasterView_tab_Master_tpg_01_grd_Master10_onheadclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{

	/*
	 그룹소계가 있고 정렬시 그룹소계는 해제 한다.
	*/
	var sortInfos = obj.sortInfos;
	var sortItems = obj.sortItems;
	
	
	//그룹초기화
	this.ds_Master10.set_keystring("");
	
	if ( this.gfn_IsNull(sortInfos) || this.gfn_IsNull(sortItems) ) return;

	// 정렬상태 초기화.
	for( var j=0; j<sortItems.length;j++){
		var col = sortItems[j];
		var sortInfo = sortInfos[col];

	}
	
	//정렬상태값 0: 없음, 1 ASC, 2 DESC 
	//이지만 상태값은 전단계상태값만 가져온다.
	if(sortInfo.status != "2"){
		this.ds_Master10.set_keystring("");
	}else{
		//정렬이 없을시 다시 그룹화 한다.
		this.ds_Master10.set_keystring("G:DEPM_CD");
	}
};]]></Script>
