this.div_MasterView_tab_Master_onchanged = function(obj:nexacro.Tab,e:nexacro.TabIndexChangeEventInfo)
{
	var iTabIndex = e.postindex;
	
	this.div_MasterView.form.sta_remk.set_visible(false);
	this.div_MasterView.form.btn_DetailView.set_visible(false);//상세보기
	
	this.div_Search.form.cal_FromDd.set_readonly(true);//근태시작일자
	this.div_Search.form.cal_ClosDd.set_readonly(true);//근태종료일자
	this.div_Search.form.cbo_PaboGu.set_readonly(false);//급상여구분
	this.div_Search.form.sta_PaboGu.set_visible(true);//급상여구분텍스트
	this.div_Search.form.cbo_PaboGu.set_visible(true);//급상여구분
	this.div_Search.form.sta_SameYn.set_visible(true);//일치여부텍스트
	this.div_Search.form.cbo_SameYn.set_visible(true);//일치여부
	
	//최근3개월간 급여변동내역
	if (iTabIndex == 0) {
		this.div_MasterView.form.sta_remk.set_visible(true);
		this.div_Search.form.sta_SameYn.set_visible(false);
		this.div_Search.form.cbo_SameYn.set_visible(false);
		
	//휴일및특근수당 체크
	} else if (iTabIndex == 1) {
		//this.div_MasterView.form.btn_DetailView.set_visible(true);
		this.div_Search.form.cbo_PaboGu.set_readonly(true);
		this.div_Search.form.cbo_PaboGu.set_index(0);
		
	//근태(일수)
	} else if (iTabIndex == 2) {
		//this.div_MasterView.form.btn_DetailView.set_visible(true);
		this.div_Search.form.cal_FromDd.set_readonly(false);
		this.div_Search.form.cal_ClosDd.set_readonly(false);
		this.div_Search.form.sta_PaboGu.set_visible(false);
		this.div_Search.form.cbo_PaboGu.set_visible(false);
	}
	
	//this.fn_ComBtnSearch();
};

