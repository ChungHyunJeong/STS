		//sWofhNm = "["+this.fvMenuNm+"]  " + this.ds_Master10.getColumn(iRow, "EMPM_NM") + " (" + this.gfn_StringFormat(this.ds_Master10.getColumn(iRow, "CORH_DD"),"@@@@-@@-@@")+ ") 외 " + (iCheckCnt - 1) + "건";//결재명
	
	}	
	
	
	
	
// 	결재요청 ORG
// 	var iRow = this.ds_Master10.rowposition;
// 	var sPks = this.ds_Master10.getColumn(iRow, "COMM_CD");//회사코드
// 	sPks += "ⓐ" + this.ds_Master10.getColumn(iRow, "CORH_NO");//관리번호
// 	sPks += "ⓐ" + this.ds_Master10.getColumn(iRow, "WOFH_SQ");//결재차수
