 ***********************************************************************************************/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{
		
	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 
	
	 //팝업창에서 조회시 부분선택, 해지 invisible
	if(!this.gfn_IsNull(this.getOwnerFrame().paramWkdhNo)){
		this.div_MasterView.form.btn_AllCheck.set_visible(false);
		this.div_MasterView.form.btn_AllUncheck.set_visible(false);	
		
		this.paramWkdhNo = this.getOwnerFrame().paramWkdhNo;
