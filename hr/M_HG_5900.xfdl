// 	sHr += " ~ "+ this.ds_Master10.getColumn(iRow, "CLOS_DD") + " " + this.ds_Master10.getColumn(iRow, "CLOS_HR") +":"+this.ds_Master10.getColumn(iRow, "CLOS_MI");
	
	
	
	var sPopupId = "C_WF_REQUEST";//팝업ID - 결재요청
	var oArg = { CommCd:this.gfn_GetSessionValue("COMM_CD")//회사코드
