			var iFvFindRow = this.ds_Detail10.findRowExpr("crudType=='D'");//업무Key Row 찾기
			var iDetailRow = this.ds_Detail10.rowposition;
			this.gfn_GridDelProcess(this, this.ds_Detail10);//crud_flag 변경-삭제(D)
			
			if (this.ds_Detail10.getRowCount() > iDetailRow+1) {
				this.ds_Detail10.set_rowposition(iDetailRow+1);
			} else if (iFvFindRow < 0) {
				this.ds_Detail10.set_rowposition(0);
			}
		} 
	}
};

