 ***********************************************************************************************/
this.fn_MasterGrd01DeleteRow = function()
{
	if (this.ds_Master10.rowposition > -1) {

		if (this.ds_Master10.getCaseCount("s_chk=='1'") > 1) {
			var arrMsgArg = ["하나"];
			this.gfn_Alert("HR004", arrMsgArg);//[&ITM0]의 행만 삭제할 수 있습니다.
			
		} else if (this.ds_Master10.getCaseCount("s_chk=='1'") > 0) {
			var iFvFindRow = this.ds_Master10.findRowExpr("s_chk=='1'");
// 			trace(iFvFindRow );
// 			trace(this.ds_Master10.rowposition);

			//체크되어있는 행과 현재position이 가 있는 행의 위치 동일여부
			if (this.ds_Master10.rowposition == iFvFindRow) { //동일할때
				if (this.ds_Detail10.getRowCount() > 0) {
					this.gfn_Alert("SM043");//상세정보가 존재합니다.\n상세정보를 삭제하신 후 작업하세요.
					
				} else {
					this.gfn_GridDelProcess(this, this.ds_Master10);//crud_flag 변경-삭제(D)	
				}
				this.fvDelRowChk = "";
			} else { //동일하지 않으면 position을 이동
				this.ds_Master10.set_rowposition(iFvFindRow);
				this.fvDelRowChk = "D";
			}
		}
	}

};

