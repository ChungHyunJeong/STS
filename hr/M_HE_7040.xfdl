/***********************************************************************************************
 * 13.03. [메세지] CallBack 함수 처리
 ***********************************************************************************************/  
this.fn_MsgCallback = function (strId, strVal)
{
	//삭제여부
	if (strId == "CONF.DELETE") {
		if(strVal )
		{
			this.fn_SvcDelete();//삭제 서비스 호출
		}
	//MASTER 변경사항 저장여부
	} else if (strId == "CONF.MAST_CHANGE") {
		if(strVal )
		{
			this.fn_ComBtnSave();//저장 서비스 호출
		} else {
			//데이터 변경여부 체크
			if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
				for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();) {
					var sCrudType = this.ds_Detail10.getColumn(iLoop, "crudType");
					if (sCrudType == "I") {
						this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10, iLoop);
						iLoop = iLoop-1;
					} else {
						this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10, iLoop);
						iLoop = iLoop+1;
					}
					
				}
			}		
		}
	} else if (strId == "CONF.CREATE") {
		if(strVal )
		{
			this.fn_AllCreate(); //일괄생성
		}
	} else if (strId == "CONF.ROWDELETE") {
		if(strVal )
		{
		}
	} else if (strId == "CONF.fvComPrgmId") {
		if(strVal) {
			this.fn_SendSMS();
		}
	}
}; 
 
