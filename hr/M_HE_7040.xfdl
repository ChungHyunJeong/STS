/***********************************************************************************************
 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회
			this.ds_Master10.addColumn("s_chk", "string");
			this.ds_Master10.addColumn("crudType", "string");
	
			if (this.ds_Master10.getRowCount() > 0) {

				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master10.findRowExpr("BASM_CD=='" + this.fvSchKey01 + "' && SYST_ID=='" + this.fvSchKey02 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}				
				
				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
				this.div_DetailInput.form.div_ComGrdBtn.set_url("");	
				this.fn_SetGridChoiceButton();//[공통] 검색 Tab 버튼 설정
				this.div_DetailInput.form.btn_SMS1.set_enable(true);
				this.div_DetailInput.form.btn_SMS2.set_enable(true);
				
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.grd_Master10);
				
			} else {
				
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterInput.form.grd_Master10);
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			}
			
			//저장후 재조회여부-true
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}			
	
			break;
		case "DTL_SEARCH": //평가자목록
			this.ds_Detail10.addColumn("s_chk", "string");
			this.ds_Detail10.addColumn("s_conf", "string");
			this.ds_Detail10.addColumn("crudType", "string");
			this.ds_Detail10.addColumn("BALSIN_TEL", "string");
			
			if (this.fvDelRowChk == "D") {
			
				if (this.ds_Detail10.getRowCount() > 0) {
					this.gfn_Alert("SM043");//상세정보가 존재합니다.\n상세정보를 삭제하신 후 작업하세요.
				} else {
					var iDelRow = this.gfn_GridDelProcess(this, this.ds_Master10);//crud_flag 변경-삭제(D)	
				}
				this.fvDelRowChk = "";
			}

			break;	
		case "SAVE":   //저장
			this.fvSchKeyYn = true;//저장후 재조회여부
			this.fn_ComBtnSearch();//저장후 재조회

			break;
		case "MSTS_SEARCH": //일괄생성
			this.ds_Master10.addColumn("s_chk", "string");
			this.ds_Master10.addColumn("crudType", "string");
		
 			for (var iLoop=0; iLoop<this.ds_Master10.getRowCount(); iLoop++) {
 				if(this.gfn_IsNull(this.ds_Master10.getColumn(iLoop, "COMM_CD"))){
 					this.ds_Master10.setColumn(iLoop, "crudType", 'I');
 				}
 			}
			
			if (this.ds_Master10.getRowCount() > 0) {
				//[공통]그리드NoData Text 초기화 설정
