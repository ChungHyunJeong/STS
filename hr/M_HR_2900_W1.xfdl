	if (sTmsuId == this.gfn_GetSessionValue("SYST_ID")) {
		//데이터 변경여부 체크
		//ds_Detail50 데이터 유효성 체크
		if (!this.fn_VaildDetail50Save()) return;

		//ds_Detail60 데이터 유효성 체크
		if (this.gfn_DsIsUpdated(this.ds_Detail60)) {
			if (!this.fn_VaildDetail60Save()) return;
		}	

		if (this.gfn_DsIsUpdated(this.ds_Detail50) || this.gfn_DsIsUpdated(this.ds_Detail60)) {
			this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

			return;
		}		

	}
	
	return true;
}

