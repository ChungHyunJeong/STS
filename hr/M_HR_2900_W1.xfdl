this.fn_SetSignImage = function()
{
	//사인 이미지 처리
	var sRethYn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RETH_YN");	//사직서 확인
	var sSecuYn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SECU_YN");	//비밀보호서약 확인
	var sSystId = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SYST_ID");	//등록자
	
	var sImgUrl = this.gfn_GetCommEmsi(sSystId);//서명이미지URL							
	eval("this.div_MasterInput.form.tab_Master.tpg_01.form.img_ApprSign0").set_image(sImgUrl);//사직서 서명
	eval("this.div_MasterInput.form.tab_Master.tpg_04.form.div_Cont.form.img_ApprSign1").set_image(sImgUrl);//비밀보호서약서 서명

	// 사직서 확인
	if (sRethYn == "1") {
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_ResignFix.set_visible(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.img_ApprSign0.set_visible(true);
		
	} else {
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_ResignFix.set_visible(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.img_ApprSign0.set_visible(false);
	}
	// 비밀보호서약 확인
	if (sRethYn == "1") {
		this.div_MasterInput.form.tab_Master.tpg_04.form.div_Cont.form.btn_SecretFix.set_visible(false);
		this.div_MasterInput.form.tab_Master.tpg_04.form.div_Cont.form.img_ApprSign1.set_visible(true);
		
	} else {
		this.div_MasterInput.form.tab_Master.tpg_04.form.div_Cont.form.btn_SecretFix.set_visible(true);
		this.div_MasterInput.form.tab_Master.tpg_04.form.div_Cont.form.img_ApprSign1.set_visible(false);
	}
};

