/***********************************************************************************************
 * 5. 저장
 ***********************************************************************************************/
/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Detail50) && !this.gfn_DsIsUpdated(this.ds_Detail60)) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}

	//ds_Detail50 데이터 유효성 체크
	if (!this.fn_VaildDetail50Save()) return;

	//ds_Detail60 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail60)) {
		if (!this.fn_VaildDetail60Save()) return;
	}	


	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	//[공통]서비스 호출 변수
	var sInputDaset = "" //입력 데이터셋
	var sOutDatset = "";//출력 데이터셋
	var sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2

	//Detail50 변경건이 있으면 Detail50 저장 (정보보안준수)
	if (this.gfn_DsIsUpdated(this.ds_Detail50)) {
		//최초 저장여부 확인
		for (var iLoop=0; iLoop<this.ds_Detail50.getRowCount(); iLoop++) {		
			if (this.ds_Detail50.getColumn(iLoop, "createYn") == '1') {
				this.ds_Detail50.setColumn(iLoop, "crudType", "I");//crudType
			}
		}

		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object	
		svcAddObj.sInsertSql = "hr:M_HR_2900_D_IN50";//저장Sql
		svcAddObj.sUpdateSql = "hr:M_HR_2900_D_UP50";//수정Sql
		svcAddObj.sDeleteSql = "";//삭제Sql
		svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		
		sInputDaset = "ds_Detail50=ds_Detail50:U" //입력 데이터셋		
	}

	//Detail60 변경건이 있으면 Detail60 저장 (인수인계)
	if (this.gfn_DsIsUpdated(this.ds_Detail60)) {
		
		//[공통]서비스 정보 추가
		svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object	
		svcAddObj.sInsertSql = "hr:M_HR_2900_D_IN60";//저장Sql
		svcAddObj.sUpdateSql = "hr:M_HR_2900_D_UP60";//수정Sql
		svcAddObj.sDeleteSql = "";//삭제Sql
		svcAddObj.sSaveFlagColumn = "";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
								 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.
		
		this.gfn_AddComSaveService(this, svcAddObj);
		
		if (this.gfn_IsNull(sInputDaset)) {
			sInputDaset = "ds_Detail60=ds_Detail60:U" //입력 데이터셋
		} else {
			sInputDaset = sInputDaset + " ds_Detail60=ds_Detail60:U" //입력 데이터셋
		}
	}
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "SAVE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = sInputDaset //입력 데이터셋
	svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
	svcCallObj.sOtherArg = sOtherArg;//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명		
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	

}

/***********************************************************************************************
 * 05.02. [Vaildation] Detail50 저장 필수값 체크 - 그리드항목
 ***********************************************************************************************/ 
this.fn_VaildDetail50Save = function()
{
	this.div_MasterInput.form.tab_Master.set_tabindex(5); //정보보안준수 Tab 이동
	
	//[저장] Validation 설정
	var arrArgs = ["RETD_YN^NULL"
			      ,"EMPM_NM^NULL"
			      ]; 

	var sCheckGroupTitle = this.div_MasterInput.form.tab_Master.tpg_05.text;//체크그룹 타이틀
				  
	//Grid Vaild 체크
	if (!this.gfn_CheckGridValid(this.div_MasterInput.form.tab_Master.tpg_05.form.grd_Detail50, arrArgs, sCheckGroupTitle) ) {		
		return false;
	}
	
	var iChkCnt = this.ds_Detail50.getCaseCount("RETD_YN == '1'");
	if (this.ds_Detail50.getRowCount() != iChkCnt) {
		this.div_MasterInput.form.tab_Master.set_tabindex(5); //정보보안준수 Tab 이동
		
		var arrMsgArg = ["정보보안준수"];
		this.gfn_Alert("SM008", arrMsgArg);//정보보안준수는 필수입력 항목입니다.
	
		return false;
	}	
	

	return true;
}

