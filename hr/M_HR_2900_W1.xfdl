this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {
	
		//조회된 정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RETH_NO"))) {
			//사직일자, 제출일자, 서명일자 셋팅
			var sRetiDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition,"RETI_DD");	//퇴사일자
			var sRethDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition,"RETH_DD");	//등록일자
			
			//사직일자
			var sRetiValue = sRetiDd.substr(0,4)+"년 "+sRetiDd.substr(4,2)+"월 "+sRetiDd.substr(6,2)+"일"; 		
			//제출일자, 서명일자
			var sRethValue = sRethDd.substr(0,4)+"년 "+sRethDd.substr(4,2)+"월 "+sRethDd.substr(6,2)+"일"; 		
			
			//[화면]일자 처리
			this.div_MasterInput.form.tab_Master.tpg_01.form.sta_RetiDd.set_text(sRetiValue);	//[사직서] 사직일자			     
			this.div_MasterInput.form.tab_Master.tpg_01.form.sta_RethDd.set_text(sRethValue);	//[사직서] 제출일자		
			this.div_MasterInput.form.tab_Master.tpg_04.form.div_Cont.form.sta_RethDd.set_text(sRethValue);	//[비밀보호서약서] 서명일자

