/***********************************************************************************************
 * 02.DG0.07. [Detail 데이터셋] 변경 이벤트
 ***********************************************************************************************/
this.ds_Detail50_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	// 권한부여여부, 점검내용
	if (e.columnid == "RETD_YN") {
		sRetdYn = this.ds_Detail50.getColumn(e.row, "RETD_YN");
		
		if (sRetdYn == '1') {	//권한부여여부 체크시
			this.ds_Detail50.setColumn(e.row, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//결재자사번	
			this.ds_Detail50.setColumn(e.row, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//결재자명		
			this.ds_Detail50.setColumn(e.row, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//결재자ID
			this.ds_Detail50.setColumn(e.row, "DUTY_CD", this.gfn_GetSessionValue("DUTY_CD"));//결재자직책
			this.ds_Detail50.setColumn(e.row, "POSI_CD", this.gfn_GetSessionValue("POSI_CD"));//결재자직위
			this.ds_Detail50.setColumn(e.row, "RETD_DD", this.gfn_GetCommDate("toDay"));//점검일자(오늘일자)		
			
			//최초 저장여부 확인
			if (this.ds_Detail50.getColumn(e.row, "createYn") == '1') {
				this.ds_Detail50.setColumn(e.row, "crudType", "I");//crudType
			}
		} else {
			this.ds_Detail50.setColumn(e.row, "EMPM_CD", "");//결재자사번	
			this.ds_Detail50.setColumn(e.row, "EMPM_NM", "");//결재자명		
			this.ds_Detail50.setColumn(e.row, "SYST_ID", "");//결재자ID
			this.ds_Detail50.setColumn(e.row, "DUTY_CD", "");//결재자직책
			this.ds_Detail50.setColumn(e.row, "POSI_CD", "");//결재자직위
			this.ds_Detail50.setColumn(e.row, "RETD_DD", "");//점검일자(오늘일자)		
			this.ds_Detail50.setColumn(e.row, "crudType", "");//crudType
		}
		
	}
};

