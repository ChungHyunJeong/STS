/***********************************************************************************************
 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
	
			if (this.ds_Master10.getRowCount() > 0) {
			
				//사인 이미지 처리
				this.fn_SetSignImage();

				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
					
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
								
			} else {
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			
				//입력영역 비활성화
				this.div_MasterInput.set_enable(false);
					
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = false;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);					
			}
			
			break;
		case "SAVE":   //저장		
			this.fn_ComBtnSearch(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_NO"));//저장후 재조회용 결재번호
				
			break;	
		case "DTL_SEARCH": //조회		
			this.ds_Detail50.addColumn("crudType", "string");
			this.ds_Detail50.addColumn("createYn", "string");

			//첨부파일 정보 조회
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
			oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RETH_NO");//PK번호2-관리번호
			oFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수						
			
			this.fvFileDiv.fn_AtchFileSearch(oFileInfo);			
			
			//[결재상태 신규, 로그인 조직장]정보보안준수, 퇴직상담 신규입력/수정이 가능해야 한다.
			var sWofhSt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST");//결재상태
			var sTmsuId = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "TMSU_ID");//조직장사번

			//Detail20 (설문조사)
			if (this.ds_Detail20.getRowCount() == 0) {
				this.ds_Detail20.addRow();	//설문조사
			}

			//Detail30 (인수인계)
			if (this.ds_Detail30.getRowCount() == 0) {
				this.ds_Detail30.addRow();	//인수인계
			}

			//Detail50 (정보보안준수)
			if (this.ds_Detail50.getRowCount() > 0) {
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_05.form.grd_Detail50);
			} else {
				//기 저장된 정보보안준수 내용이 없고 결재상태가 신청일 경우 공통코드의 정보보안 대상 포멧을 가져온다.
				if ((sWofhSt == '0020' || sWofhSt == '0030') && sTmsuId == this.gfn_GetSessionValue("SYST_ID")) {
					//공통코드 대상 조회 함수 호출
					this.SetInfoSecSearch();
				} else {
					//[공통]그리드NoData Text설정
					this.gfn_SetGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_05.form.grd_Detail50);			
				}
			}

			//Detail60 (퇴직면담)
			if (this.ds_Detail60.getRowCount() == 0) {
				//결재상태가 신청, 로그인 사용자가 조직장일 경우  결재자 정보를 넣는다.
				if ((sWofhSt == '0020' || sWofhSt == '0030') && sTmsuId == this.gfn_GetSessionValue("SYST_ID")) {
					this.ds_Detail60.addRow();	//퇴직면담
					this.ds_Detail60.setColumn(0, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));//회사코드
					this.ds_Detail60.setColumn(0, "RETH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "RETH_NO"));//관리번호
					this.ds_Detail60.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));//결재차수				
					this.ds_Detail60.setColumn(0, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//결재자사번	
					this.ds_Detail60.setColumn(0, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//결재자명		
					this.ds_Detail60.setColumn(0, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//결재자ID
				}
			}
			
			//Detail70 (보유전산자산)
			if (this.ds_Detail70.getRowCount() > 0) {
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_07.form.grd_Detail70);
			} else {
				this.gfn_SetGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_07.form.grd_Detail70);
			}

			//결재상태가 신청(0020)일 경우  입력/수정 활성화 모드, 아닌경우 비활성화 모드
			if ((sWofhSt == '0020' || sWofhSt == '0030') && sTmsuId == this.gfn_GetSessionValue("SYST_ID")) {
				//정보보안준수
				this.div_MasterInput.form.tab_Master.tpg_05.form.grd_Detail50.set_enable(true);
				//퇴직상담
				this.div_MasterInput.form.tab_Master.tpg_06.form.div_IntvView.form.txt_Rt01Nm.set_readonly(false);
				this.div_MasterInput.form.tab_Master.tpg_06.form.div_IntvView.form.txt_Rt02Nm.set_readonly(false);
				this.div_MasterInput.form.tab_Master.tpg_06.form.div_IntvView.form.txt_Rt03Nm.set_readonly(false);
				this.div_MasterInput.form.tab_Master.tpg_06.form.div_IntvView.form.txt_Rt04Nm.set_readonly(false);				
			} else {						//비활성화
				//정보보안준수, 퇴직상담
				this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력			
				this.div_MasterInput.form.btn_CallMainScrn.set_enable(true); //상세보기 버튼
			}

			break;		

		case "INFO_SEARCH": //조회		
			this.ds_Detail50.addColumn("RETD_NM", "string");
			this.ds_Detail50.addColumn("SYST_ID", "string");
			this.ds_Detail50.addColumn("EMPM_CD", "string");
			this.ds_Detail50.addColumn("EMPM_NM", "string");
			this.ds_Detail50.addColumn("DUTY_CD", "string");
			this.ds_Detail50.addColumn("POSI_CD", "string");
			this.ds_Detail50.addColumn("RETD_DD", "string");
			this.ds_Detail50.addColumn("REMK_NM", "string");
			this.ds_Detail50.addColumn("crudType", "string");

			break;		
	}
};


