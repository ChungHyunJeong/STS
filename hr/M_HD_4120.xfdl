 * 1. 조회 영역
 ***********************************************************************************************/
/***********************************************************************************************
 * 01.01. [공통버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.fn_ComBtnSearch = function()
{ 
	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;	
	
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Input.clearData();
		
	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "FROM_YM", this.gfn_StringFormat(this.div_Search.form.cal_FromDd.value.substr(0,6), "@@@@-@@")); /* 년월 YYYY-MM으로 변경*/
	if (this.div_Search.form.chk_TimeYn.value == "1") {
		this.ds_Input.setColumn(0, "TIME_CT", 1);
	} else {
		this.ds_Input.setColumn(0, "TIME_CT", 60);
	}	
	
	this.ds_Input.setColumn(0, "SAUM_CD", "0010");	//사업장 0010

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "hr:M_HR_CALM_M_SL10";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "COMBO_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_SM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_HrCalm=ds_HrCalm";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.isSync = true;//동기식으로 설정할지 여부

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);
	 
	var sCalmYm = "";
	var sCalmDy = "";
	var iNum = 0;
	
	for(var iLoop = 0; iLoop < this.ds_HrCalm.rowcount ; iLoop++){
		if(iLoop < 9) {
			iNum = '0' + (iLoop + 1).toString();					
		}else{
			iNum =  (iLoop + 1).toString();
		}	
				
		sCalmYm  =  this.ds_HrCalm.getColumn(iLoop, "CALM_YM");
		sCalmDy  =  this.ds_HrCalm.getColumn(iLoop, "CALM_DY");
		
		this.ds_Input.setColumn(0, "A" + iNum, sCalmYm + "-" + sCalmDy);
	}
	
	var sFromYm = this.ds_HrCalm.getColumn(0,"CALM_YM").toString();
	var sFromDy = this.ds_HrCalm.getColumn(0,"CALM_DY").toString();
	var sClosYm = this.ds_HrCalm.getColumn(this.ds_HrCalm.rowcount - 1,"CALM_YM").toString();
	var sClosDy = this.ds_HrCalm.getColumn(this.ds_HrCalm.rowcount - 1,"CALM_DY").toString();	
	
	var sFromDd = nexacro.replaceAll(sFromYm,"-","") + sFromDy;	
	var sClosDd = nexacro.replaceAll(sClosYm,"-","") + sClosDy;		
	
	this.ds_Input.setColumn(0, "FROM_DD", sFromDd);
	this.ds_Input.setColumn(0, "CLOS_DD", sClosDd);
	
	this.ds_Input.setColumn(0, "DEPM_CD", this.div_Search.form.edt_DepmCd.value);
	this.ds_Input.setColumn(0, "SYST_ID", this.div_Search.form.edt_SystId.value);
	this.ds_Input.setColumn(0, "PRGM_ID", this.fvComPrgmId);//프로그램ID

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);

	var selectedTabIdx = this.div_MasterView.form.tab_Master.tabindex;
	var sSqlId = "";
	var sOutDatset = "";

	if (selectedTabIdx == 0) {
		sSqlId = "hr:M_HD_4120_M_SL10";
		sOutDatset = "ds_Master10=ds_Master10";
		this.ds_Master10.clearData();
	} else if (selectedTabIdx == 1) {
		sSqlId = "hr:M_HD_4120_M_SL20";
		sOutDatset = "ds_Master20=ds_Master20";
		this.ds_Master20.clearData();
	} else if (selectedTabIdx == 2) {
		sSqlId = "hr:M_HD_4120_M_SL30";
		sOutDatset = "ds_Master30=ds_Master30";
		this.ds_Master30.clearData();
	}

	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = sSqlId;
	this.gfn_AddComSearchService(this, svcAddObj);

	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MST_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋	
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);			
}

