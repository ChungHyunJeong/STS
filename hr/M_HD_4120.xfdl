/***********************************************************************************************
 * 15. [사용자정의 함수]
 ***********************************************************************************************/
/***********************************************************************************************
 * 15.01. [초기화] 월별근무시간현황 Title 그리드 년월기준 주/일 셋팅
 ***********************************************************************************************/ 
this.fn_SetMonthWorkTitleGrd = function()
{ 
	var grdMst10 = this.div_MasterView.form.tab_Master.tpg_01.form.grd_Master10; //주 그리드
	var iGrdCol = 12; //주 그리드 명칭 위치 (시작위치 = head12)
	var iSizeCol = 6; //주 그리드 사이즈 위치 (시작위치 = col6)
	var iLastCol = 49; //5주 총 49컬럼
	var iCntCol = 0; //소계, 누계, 평균 실제 건수 Count
	var sTitle = ""; //제목
		
	//타이틀 값 넣기, 컬럼사이즈 40
	for(var iLoop=0; iLoop<this.ds_HrCalm.getRowCount()+14; iLoop++){

		//소계, 누계, 평균시 한칸 증가
		if (iLoop == 7 || iLoop == 8 || iLoop == 16 || iLoop == 17 || iLoop == 18 || iLoop == 26 || iLoop == 27 || iLoop == 28 || iLoop == 36 || iLoop == 37 || iLoop == 38 || iLoop == 46 || iLoop == 47 || iLoop == 48) {
			grdMst10.setFormatColProperty(iSizeCol + iLoop, "size", 50);			//소계, 누계, 평균은 size 50
			iCntCol = iCntCol + 1; //소계, 누적, 평균 개수만큼 증가
		} else {		
			sTitle = this.ds_HrCalm.getColumn(iLoop - iCntCol, "CALM_DY");	//주별 날짜 (소계, 누적, 평균 개수는 제외함)
			grdMst10.setCellProperty("head", iGrdCol + iLoop, "text", sTitle);		
			grdMst10.setFormatColProperty(iSizeCol + iLoop, "size", 40);	//날짜칸
		}
		
	}

	//5주 대상은 제외
	if (this.ds_HrCalm.getRowCount() != 35) {
		//타이틀 값 제거, 컬럼사이즈 0 (소계, 누계, 평균만큼 우측으로 이동)
		for(var iLoop=this.ds_HrCalm.getRowCount()+11; iLoop<iLastCol; iLoop++){
			grdMst10.setFormatColProperty(iSizeCol + iLoop, "size", 0);	//날짜, 소계, 누계, 평균 칸
		}
	}
	
};

/***********************************************************************************************
 * 15.02. [초기화] 출퇴근시간 Title 그리드 년월기준 주/일 셋팅
