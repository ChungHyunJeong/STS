this.fn_ComboSearch = function() 
{
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Input.clearData();
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "FROM_YM", this.gfn_StringFormat(this.div_Search.form.cal_FromDd.value.substr(0,6), "@@@@-@@")); /* 년월 YYYY-MM으로 변경*/
