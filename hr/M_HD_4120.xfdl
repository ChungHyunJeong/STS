this.fn_SetCommuteTitleGrd = function()
{ 
	var grdMst30 = this.div_MasterView.form.tab_Master.tpg_03.form.grd_Master30; //주 그리드
	var iGrdCol = 12; //주 그리드 명칭 위치 (시작위치 = head12)
	var iSizeCol = 7; //주 그리드 사이즈 위치 (시작위치 = col7)
	var iLastCol = 35; //5주 총 35컬럼
	var sTitle = ""; //제목
	
	//타이틀 값 넣기, 컬럼사이즈 40
	for(var iLoop=0; iLoop<this.ds_HrCalm.getRowCount(); iLoop++){
		sTitle = this.ds_HrCalm.getColumn(iLoop, "CALM_DY");	//주별 날짜
		
		grdMst30.setCellProperty("head", iGrdCol + iLoop, "text", sTitle);
		grdMst30.setFormatColProperty(iSizeCol + iLoop, "size", 40);	//날짜칸
	}

	//타이틀 값 제거, 컬럼사이즈 0
	for(var iLoop=this.ds_HrCalm.getRowCount(); iLoop<iLastCol; iLoop++){
		grdMst30.setFormatColProperty(iSizeCol + iLoop, "size", 0);	//날짜칸
	}
};			



]]></Script>
