/***********************************************************************************************
 * 02.MG0.07. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
 /*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{	
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position
	
	// 사용여부
	if (e.columnid == "USED_YN") {
		if(this.ds_Master10.getColumn(iMasterRowPos, "USED_YN") == "0"){
			var sToDay = this.gfn_GetCommDate("toDay");//현재일자
			var sToDayFull = this.gfn_GetCommDate("toDayFull");
			this.ds_Master10.setColumn(iMasterRowPos, "DISC_DD", sToDay);
			//폐기시 삭제자 세팅
 			this.ds_Master10.setColumn(iMasterRowPos, "DELE_ID", this.gfn_GetSessionValue("SYST_ID"));
 			this.ds_Master10.setColumn(iMasterRowPos, "DELE_NM", this.gfn_GetSessionValue("USEM_NM"));
 			this.ds_Master10.setColumn(iMasterRowPos, "DELE_DT", sToDayFull);
		} else {
			this.ds_Master10.setColumn(iMasterRowPos, "DISC_DD", null);
 			this.ds_Master10.setColumn(iMasterRowPos, "DELE_ID", null);
 			this.ds_Master10.setColumn(iMasterRowPos, "DELE_NM", null);
 			this.ds_Master10.setColumn(iMasterRowPos, "DELE_DT", null);
		}
		
		this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	//직군명
	} else if (e.columnid == "JOGM_CD") {
	
	
	//마스킹하면 중간에 빈칸이 생길수도 있어서
	//직렬코드
	} else if (e.columnid == "SERM_CD") {
 		var sSermCd = nexacro.replaceAll(e.newvalue, " ", "");
 		obj.setColumn(e.row, "SERM_CD", sSermCd);
		
	//정렬순서
	} else if (e.columnid == "SORT_NO") {
		var sSortNo = nexacro.replaceAll(e.newvalue, " ", "");
		obj.setColumn(e.row, "SORT_NO", sSortNo);
	}
};


