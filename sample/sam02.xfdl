    <Script type="xscript5.1"><![CDATA[include "comLib::ComSvcUtil.xjs";

this.pdv_MultiCalendar_btn_Confirm_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var sFromDate = this.pdv_MultiCalendar.form.cal_FromDate.value;
	var sToDate   = this.pdv_MultiCalendar.form.cal_ToDate.value;
	
	if (sFromDate > sToDate) {
		this.alert("시작일자가 종료일자 보다 큽니다.");
		return;
	}
	
	this.cal_FromDt.set_value(sFromDate);
	this.cal_ToDt.set_value(sToDate);
	
	this.pdv_MultiCalendar.closePopup();
	
};

this.pdv_MultiCalendar_btn_Cancel_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.pdv_MultiCalendar.closePopup();
};

