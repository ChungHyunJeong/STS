/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	//검증여부
	if (e.columnid == "VERB_YN") {
		var sVerbYn = this.ds_Detail10.getColumn(e.row, "VERB_YN");	//검증여부
		if (sVerbYn == '0') {	//검증여부 해제시 초기화
			this.ds_Detail10.setColumn(e.row, "APPL_DT", "");	//적용일자
			this.ds_Detail10.setColumn(e.row, "VERB_DT", "");	//검증일자
			this.ds_Detail10.setColumn(e.row, "VERB_CT", "");	//검증기간
			this.ds_Detail10.setColumn(e.row, "VERB_NM", "");	//검증내용
			this.ds_Detail10.setColumn(e.row, "VERB_CD", "");	//검증결과
			this.ds_Detail10.setColumn(e.row, "ACTI_NM", "");	//조치사항
			this.ds_Detail10.setColumn(e.row, "COSU_ID", "");	//확인자ID
			this.ds_Detail10.setColumn(e.row, "COSU_NM", "");	//확인자명
			this.ds_Detail10.setColumn(e.row, "COSU_CD", "");	//확인자사번
			this.ds_Detail10.setColumn(e.row, "POSI_CD", "");	//직위
			this.ds_Detail10.setColumn(e.row, "PLAN_DT", "");	//향후검증일자
			this.ds_Detail10.setColumn(e.row, "CLOS_DT", "");	//검증종료일자
			this.ds_Detail10.setColumn(e.row, "REMK_NM", "");	//비고
		}
	//적용일자, 검증일자 (검증기간 계산)
	} else if (e.columnid == "APPL_DT" || e.columnid == "VERB_DT") {
		var sApplDt = this.ds_Detail10.getColumn(e.row, "APPL_DT");	//적용일자
		var sVerbDt = this.ds_Detail10.getColumn(e.row, "VERB_DT");	//검증일자

		if (!this.gfn_IsNull(sApplDt) && !this.gfn_IsNull(sVerbDt)) {
			var iDiffDate = this.gfn_GetDiffDate(sApplDt, sVerbDt);	//적용일자와 검증일자 차이
			this.ds_Detail10.setColumn(e.row, "VERB_CT", iDiffDate);	//검증기간 적용
		}

	
	//확인자
	} else if (e.columnid == "COSU_CD") {
		var arrArgs = ["POSI_CD"];
		this.gfn_GetCommEmpmGrid(obj, "COSU_CD", "COSU_NM", "COSU_ID", arrArgs);//사원명 검색
	
	}

}

