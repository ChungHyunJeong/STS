				this.div_Search.form.edt_EmpmNm.set_value("");
				this.div_Search.form.edt_SystId.set_value("");				
			}
		}	

	//[Master영역]

	//[Detail영역]
	//확인자		
	} else if (strId == "B_SYST_ID_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.EMPM_CD)) {
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "COSU_CD", arrRtnVal.EMPM_CD);
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "COSU_NM", arrRtnVal.EMPM_NM);
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "COSU_ID", arrRtnVal.SYST_ID);
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "POSI_CD", arrRtnVal.POSI_CD);
			}
		} else {
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "COSU_CD", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "COSU_NM", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "COSU_ID", "");			
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "POSI_CD", "");			
