this.div_MasterInput_btn_Apply_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var sIpqcCd 	= this.div_MasterInput.form.cbo_IpqcAll.value;
	var sChk    	= "";
	var sDateTime 	= this.gfn_GetCommDate("toDayFull");
	
	this.ds_Master10.set_enableevent(false);
	
	
	for(var iLoop = 0; iLoop < this.ds_Master10.rowcount; iLoop++){
		sChk = this.ds_Master10.getColumn(iLoop, "s_chk");
		
		if(sChk=="1"){			
			this.ds_Master10.setColumn(iLoop, "IPQC_CD",  sIpqcCd);
			this.ds_Master10.setColumn(iLoop, "IPQC_ID",  this.gfn_GetSessionValue("SYST_ID"));
			this.ds_Master10.setColumn(iLoop, "EMPM_NM1", this.gfn_GetSessionValue("USEM_NM"));		
			this.ds_Master10.setColumn(iLoop, "IPQC_DT",  sDateTime);				
			
			this.ds_Master10.setColumn(iLoop, "crudType" ,"U");
		}		
	}
	
	this.ds_Master10.set_enableevent(true);	
};
