/***********************************************************************************************
 * 02.MG0.07. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
 this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
			
	// 반납일자 (변경)시
	if (e.columnid == "MIDD_DD") {
		var sMiddDd  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MIDD_DD");//반납일자
		var sOrgMiddDd  = this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "MIDD_DD");//이전반납일자

		if (!this.gfn_IsNull(sMiddDd)) {	//반납일자 Null  여부
			if (sMiddDd != sOrgMiddDd) {	//이전반납일자와 변경이 있을 경우
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MIST_YN", "1");	//반납여부 설정

				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MIDP_CD", this.gfn_GetSessionValue("DEPM_CD"));//처리부서코드	
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MIDP_NM", this.gfn_GetSessionValue("DEPM_NM"));//처리부서명	
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MISU_CD", this.gfn_GetSessionValue("USEM_ID"));//처리자사번	
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MISU_NM", this.gfn_GetSessionValue("USEM_NM"));//처리자명		
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MISU_ID", this.gfn_GetSessionValue("SYST_ID"));//처리자ID	
			}
		} else {	//반납일자가 Null이면 반납여부가 없음
		
			if (!this.gfn_IsNull(sMiddDd)) {	//이전반납일자 Null  여부
				var arrMsgArg = ["계측기 반납처리", "반납일자"];
				this.gfn_Alert("QM002", arrMsgArg); //대여가능 대상(을)를 선택하세요. 			
			}

			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MIDD_DD", sOrgMiddDd);//반납일자 원복

			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MIST_YN", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "MIST_YN"));	//반납여부 설정
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MIDP_CD", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "MIDP_CD"));//처리부서코드	
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MIDP_NM", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "MIDP_NM"));//처리부서명	
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MISU_CD", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "MISU_CD"));//처리자사번	
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MISU_NM", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "MISU_NM"));//처리자명		
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MISU_ID", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "MISU_ID"));//처리자ID	

			return;
		}
		
	}
};		

