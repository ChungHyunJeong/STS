this.ds_Detail30_cancolumnchange = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	trace(e.columnid);
	if (e.columnid == "PROC_QT" || e.columnid == "PROQ_QT"){
		
		if(this.gfn_IsNull(obj.getColumn(e.row,"PROC_GU"))){
			var arrMsgArg = ["평가기준"];
			this.gfn_Alert("SM040", arrMsgArg); //&ITM0을(를) 선택하세요.
			return false;
		}
		
		var sVisiGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "VISI_GU");
		if(sVisiGu =="0010"){//유형
			iRowPosition = obj.rowposition;
		}else if(sVisiGu =="0020"){//무형
			iRowPosition = obj.rowposition+5;
		}
		
		//alert(this.fvArrRows[iRowPosition]);
		var sObj = eval("this."+this.fvArrRows[iRowPosition]);
		//alert( obj.getColumn(obj.rowposition, "PROC_GU"));
		//var iFindRow = sObj.findRowExpr("CODM_CD=='" + obj.getColumn(obj.rowposition, "PROC_GU") + "'");//Key Row 찾기
		//var iOp2Nm = nexacro.toNumber(sObj.getColumn(iFindRow,"OP02_NM"));
		var iOp2Nm = nexacro.toNumber(sObj.getColumn(0,"OP02_NM"));
		
		var newVal = nexacro.toNumber(this.gfn_Nvl(e.newvalue,0));
		var sGrcmNm = this.ds_SmGrcm.getColumn( this.ds_SmGrcm.findRowExpr("GRCM_CD=='" + obj.getColumn(e.row,"GRCM_CD") + "'") ,"GRCM_NM");
		//trace("입력점수"+newVal);
		//trace("최대점수"+iOp2Nm);
		//trace(iOp2Nm < newVal);
		if(newVal > 0){
			if (e.columnid == "PROC_QT") { //실행부서 평가점수 제어
				if(iOp2Nm < newVal){
					var arrMsgArg = [sGrcmNm, "최대점수("+ iOp2Nm +")", "점수"];
					this.gfn_Alert("SM060", arrMsgArg); //[&ITM0]은(는) [&ITM1] 이하의 [&ITM2]만 입력 가능합니다.
