/***********************************************************************************************
 * 1. 조회 영역
 ***********************************************************************************************/
/***********************************************************************************************
 * 01.01. [공통버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.fn_ComBtnSearch = function(sWofhNo)
{ trace("=======================fn_ComBtnSearch=============================" + sWofhNo);
	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;	
	
	//저장후 재조회시-마스터 데이터셋 이벤트 비활성화
	if (this.fvSchKeyYn) {
		this.ds_Master10.set_enableevent(false);
	}

	this.fvComWofhNo = sWofhNo;//[결재]결재PID
	
	//결재시 "협조"자 일 경우 수정가능해야 함 (2022-07-15)
	//1. 문제점발생내역(INPUT)   : 발생공정, 발생지역, 사업군, 사내/외
	//2. 문제점발생상세(그리드) : 품질비용, 대분류, 소분류, 증상명, 4M항목, 귀책내역
	
	//trace(this.getOwnerFrame().paramTestDs.saveXML());
	
	if(this.gfn_IsNull(this.fvParentDs)){
		var iHelpCnt = this.parent.parent.ds_Detail11Help.getCaseCount("WOFH_NO=='" + sWofhNo + "' && SYST_ID=='" + this.gfn_GetSessionValue("SYST_ID") +"'");//협조자 여부(결재번호 + 로그인사번)	
	}else{	
		var iHelpCnt = this.fvParentDs.getCaseCount("WOFH_NO=='" + sWofhNo + "' && SYST_ID=='" + this.gfn_GetSessionValue("SYST_ID") +"'");//협조자 여부(결재번호 + 로그인사번)	
	}
 	
 	if (iHelpCnt > 0) {
 		this.fvHelpYn = true;	//[결재]협조자여부
 	}

	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Master10.clearData();
	this.ds_Detail10.clearData();
	this.ds_Input.clearData();
