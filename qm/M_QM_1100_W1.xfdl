 ***********************************************************************************************/ 
/*---------------------------------------------------------------------------------------------
 *    01. [그리드-grd_Detail10] 셀클릭 이벤트
  ---------------------------------------------------------------------------------------------*/
this.div_DetailInput_tab_Detail_tpg_01_grd_Detail10_oncellclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	//그리드 콤보 및 캘린더 클릭시 펼치기
	obj.dropdownCombo();
	obj.dropdownCalendar();

	//[결재]협조자의 경우 수정컬럼 활성화(2022-07-15)
	var sWofhSt = this.ds_Master10.getColumn(0, "WOFH_ST"); //결재상태가 신청(0020), 진행중(0030) 경우				
	if (sWofhSt == '0020' || sWofhSt == '0030') {
		if(this.fvHelpYn) {
			//원인구분 - 대분류 (콤보 클릭시 품질비용 조건으로 변경됨)
			if (e.col == 7) {		
				var sCostCd = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "COST_CD");	//품질비용 선택값
				var iFindRow = this.ds_Qm125.findRowExpr("CODM_CD == '"+sCostCd+"'"); //공통코드에서 품질비용 선택 대상 Row확인 
				var sPrr1Cd = this.ds_Qm125.getColumn(iFindRow, "OP01_NM"); //품질비용으로 대분류 값 검색
				
				this.ds_Qm010.filter("");	//원인구분 - 대분류 필터 초기화
				this.ds_Qm010.filter("OP01_NM == '"+sPrr1Cd+"'");	//초기화 후 대분류값 필터링
				
			//원인구분 - 소분류 (콤보 클릭시 대분류 조건으로 변경됨)
			} else if (e.col == 8) {		
				this.ds_Qm020.filter("");	//원인구분 - 소분류
				this.ds_Qm020.filter("OP01_NM == '"+ this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "PRR1_CD") +"'");

			//증상코드 (2022-04-01 추가)
			} else if (e.col == 10) {
				var sPopupId = "B_SYMM_CD_01";//팝업ID - 증상코드 팝업

				var oArg = { PrgmId:this.fvComPrgmId};//Argument처리
				this.gfn_SymmPop(sPopupId, oArg);		

			//귀책내용 명(귀책구분[내부] - 조직코드, 귀책구분[협력사/고객] - 거래처)
			} else if (e.col == 15) {		
				var sReasGu = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "REAS_GU");

				if (sReasGu == '0010') { //내부 (0010)
					var sPopupId = "B_DEPM_CD_01";//팝업ID - 부서코드
					var oArg = { DepmNm:""};  // Argument처리
					this.gfn_DeptPop(sPopupId, oArg);
				
				} else if (sReasGu == '0020'){ //협력사 (0020)
					var sPopupId = "B_CUSM_CD_01";//팝업ID - 거래처정보
					var sCusmNm = "";//거래처명
					var sCusmTp = "H";//업체유형 - C : 고객사, H : 협력사, E - 기타, A - 전체선택
					var oArg = { CusmNm:sCusmNm, CusmTp:sCusmTp};//Argument처리
					this.gfn_CusmPop(sPopupId, oArg);//거래처팝업 호출 	
					
				} else if (sReasGu == '0030'){ // 고객 (0030)
					var sPopupId = "B_CUSM_CD_01";//팝업ID - 거래처정보
					var sCusmNm = "";//거래처명
					var sCusmTp = "C";//업체유형 - C : 고객사, H : 협력사, E - 기타, A - 전체선택
					var oArg = { CusmNm:sCusmNm, CusmTp:sCusmTp};//Argument처리
					this.gfn_CusmPop(sPopupId, oArg);//거래처팝업 호출 	

				}
			}
		}
		
		//담당자
		if (e.col == 28) {
			var sPbrbYn =  this.ds_Detail10.getColumn(e.row, "PBRB_YN");		//필수여부 
		
			if (sPbrbYn == "1") {
				var sPopupId = "B_SYST_ID_01";//팝업ID - 사용자정보
				var oArg = {  EmpmNm:""};//Argument처리
				this.gfn_UserPop(sPopupId, oArg);		
			}
						
		}
	}
};

/***********************************************************************************************
 * 02.DG0.07. [Detail 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)

	//원인구분 - 대분류
	if (e.columnid == "PRR1_CD") {
		//원인구분 - 소분류 초기화
		var sPrr1CdOrg = this.ds_Detail10.getOrgColumn(e.row, "PRR1_CD");
		var sPrr1CdNew = this.ds_Detail10.getColumn(e.row, "PRR1_CD");
		
		if (sPrr1CdOrg != sPrr1CdNew) {
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "PRR2_CD", "");
		}
	
	//필수여부
	} else if (e.columnid == "PBRB_YN") {
		var sPbrbYn = this.ds_Detail10.getColumn(e.row, "PBRB_YN");		//필수여부
		
		if (sPbrbYn == "0") {	//필수여부 해제시 초기화
			//유효성검증 초기화(2021-12-15 추가요청)
			this.ds_Detail10.setColumn(e.row, "PBRB_CT","");
			this.ds_Detail10.setColumn(e.row, "DEPM_CD","");
			this.ds_Detail10.setColumn(e.row, "DEPM_NM","");
			this.ds_Detail10.setColumn(e.row, "EMPM_CD","");
			this.ds_Detail10.setColumn(e.row, "EMPM_NM","");
			this.ds_Detail10.setColumn(e.row, "SYST_ID","");
		}

	//담당자
	} else if (e.columnid == "EMPM_CD") {
		var arrArgs = ["DEPM_CD","DEPM_NM"];
		this.gfn_GetCommEmpmGrid(obj, "EMPM_CD", "EMPM_NM", "SYST_ID", arrArgs);//사원명 검색

	}


}

