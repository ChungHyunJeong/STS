*************************************************************************************************
*/

/***********************************************************************************************
 * 0. 폼로드
 ***********************************************************************************************/
/***********************************************************************************************
 * 00.01. Form Include 선언
 ***********************************************************************************************/
//include "comLib::CommonUtil.xjs";//공통함수

/***********************************************************************************************
 * 00.02. Form 변수 선언
 ***********************************************************************************************/
this.fvComPrgmId = "";//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID
this.fvFormComBtnAuthYn = false;//그리드 공통버튼 권한 유무
this.fvFileDiv = this.div_MasterInput.form.tab_Master.tpg_06.form.div_CommFile.form;//[첨부파일]Div
this.fvPathNm = "QM";//[첨부파일]업무파일경로
this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
this.fvSchKeyYn = false;//저장후 재조회여부
this.fvSchKey01 = "";//조회Key01
this.fvSchKey02 = "";//조회Key02

var fvFileInfo = new Object();//[첨부파일]정보
this.fvComWofhNo = "";//[결재]결재PID
this.fvHelpYn = false;//[결재]협조자여부

this.fvImgFileDiv = this.div_MasterInput.form.tab_Master.tpg_04.form.div_CommFile.form;//[이미지-첨부파일]Div
this.fvImgPathNm = "PN";//[이미지-첨부파일]업무파일경로
var fvImgFileInfo = new Object();//[이미지-첨부파일]정보
this.fvParentDs = "";

this.fvParentDs = this.getOwnerFrame().paramTestDs;
/***********************************************************************************************
 * 00.03. Form Onload 이벤트
 ***********************************************************************************************/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{
	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 

}

/***********************************************************************************************
 * 00.04. Form [공통] 초기설정 함수 - 필수
 ***********************************************************************************************/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	this.fvComPrgmId = "M_QM_1100";//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID - 결재화면인 경우에는 메뉴에서 클릭한게 아니기 때문에 해당 프로그램ID로 설정
	this.gfn_SetGrid(this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10);//[공통]그리드 이벤트 설정 - 문제점발생상세
	this.gfn_SetGrid(this.div_MasterInput.form.tab_Master.tpg_05.form.grd_Detail20);//[공통]그리드 이벤트 설정 - 구매현황
	
	this.fn_SetTabComAttachFile();//[공통] 첨부파일 Tab 설정
	this.fn_SetTabComImageAttachFile();//[공통] 이미지-첨부파일 Tab 설정
}

/***********************************************************************************************
 * 00.05. Form [업무] 초기설정 함수
 ***********************************************************************************************/
this.fn_FormBizInit = function() 
{ 
	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	var objCondList = ["QM010|ds_Qm010","QM020|ds_Qm020","WF110|ds_Wf110"
					  ,"QM110|ds_Qm110","QM115|ds_Qm115","QM120|ds_Qm120"
					  ,"QM125|ds_Qm125","QM130|ds_Qm130","QM135|ds_Qm135"
					  ,"QM140|ds_Qm140","QM145|ds_Qm145","QM160|ds_Qm160"
					  ,"QM170|ds_Qm170","PP125|ds_Pp125","MM110|ds_Mm110"
					  ];
	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, "");
	
	this.fvParentDs = this.getOwnerFrame().paramTestDs;

	//[업무] 일반콤보조회
	this.fn_ComboSearch();
	
    //[결재]결재함 화면에 결재번호가 있는 경우 조회 호출
    if (!this.gfn_IsNull(this.parent.parent.fvNowWofhNo)) {
       this.fn_ComBtnSearch(this.parent.parent.fvNowWofhNo); 
    }
}

