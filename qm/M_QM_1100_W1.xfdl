	}

	return true;
}

/***********************************************************************************************
 * 05.03. [Vaildation] Detail10 저장 필수값 체크 - 그리드항목
 ***********************************************************************************************/ 
this.fn_VaildDetailSave = function()
{
	//[저장] Validation 설정
	var arrArgs = ["COST_CD^NULL"
			      ,"PRR1_CD^NULL"
			      ,"PRR2_CD^NULL"
			      ,"SYMM_NM^NULL"
			      ,"FORM_CD^NULL"
			      ]; 

	var sCheckGroupTitle = this.div_MasterInput.form.tab_Master.tpg_02.text;//체크그룹 타이틀
				  
	//Grid Vaild 체크
	if (!this.gfn_CheckGridValid(this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10, arrArgs, sCheckGroupTitle) ) {		
		return false;
	}

	var sSymmNm = ""; //증상명	
	var sReasGu = ""; //귀책구분
	var sReasNm = ""; //귀책명
	var sPbrbYn = ""; //필수여부
	var iPbrbCt = ""; //검증기간(월)
	var sEmpmCd = ""; //담당자
	var alertMsg = "";	//팝업메시지
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		//증상명, 귀책내역
		sSymmNm = this.ds_Detail10.getColumn(iLoop, "SYMM_NM"); //증상명	
		sReasGu = this.ds_Detail10.getColumn(iLoop, "REAS_GU"); //귀책구분
		sReasNm = this.ds_Detail10.getColumn(iLoop, "REAS_NM"); //귀책명

		if (this.gfn_IsNull(sSymmNm)) {
			this.div_MasterInput.form.tab_Master.set_tabindex(1);			
			
			if (this.gfn_IsNull(sSymmNm)) {		
				alertMsg = "증상명";
			}
		}	
		
		if (sReasGu != '0090' && !this.gfn_IsNull(sReasGu) && this.gfn_IsNull(sReasNm)) {		
			if (this.gfn_IsNull(alertMsg)) {		
				alertMsg = "귀책명";
			} else {
				alertMsg = alertMsg + ", 귀책명";
			}
		}

		//검증기간 필수체크
		sPbrbYn = this.ds_Detail10.getColumn(iLoop, "PBRB_YN"); //필수여부	
		//필수여부 대상시
		if (sPbrbYn == '1') {
			iPbrbCt = this.ds_Detail10.getColumn(iLoop, "PBRB_CT");	//검증기간(월)
			sEmpmCd = this.ds_Detail10.getColumn(iLoop, "EMPM_CD");	//담당자
			
			if (this.gfn_IsNull(iPbrbCt) || this.gfn_IsNull(sEmpmCd)) {		
				//탭, 위치 이동
				this.div_MasterInput.form.tab_Master.set_tabindex(1);			
				
				if (this.gfn_IsNull(iPbrbCt)) {		
					if (this.gfn_IsNull(alertMsg)) {		
						alertMsg = "검증기간(월)";
					} else {
						alertMsg = alertMsg + ", 검증기간(월)";
					}
				}
				
				if (this.gfn_IsNull(sEmpmCd)) {		
					if (this.gfn_IsNull(alertMsg)) {		
						alertMsg = "담당자";
					} else {
						alertMsg = alertMsg + ", 담당자";
					}
				}
			}

		}
		
		//필수값 누락 발생시
		if (!this.gfn_IsNull(alertMsg)) {		
			//포커스 이동
			if (this.gfn_IsNull(sSymmNm)) {		
				this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setCellPos(10,iLoop);
			} else if (this.gfn_IsNull(sReasNm)) {		
				this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setCellPos(15,iLoop);
			} else if (this.gfn_IsNull(iPbrbCt)) {				
				this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setCellPos(23,iLoop);
				this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setFocus();		
			} else if (this.gfn_IsNull(sEmpmCd)) {		
				this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setCellPos(26,iLoop);
				this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setFocus();					
			}
			
			var iRow = iLoop + 1;
			var arrMsgArg = [iRow+"번째행 "+alertMsg];
			this.gfn_Alert("SM008", arrMsgArg);//검증기간(월), 담당자는 필수 입력 항목입니다.
			return false;
			
			break;			
		}
	}
	
	return true;
}
