/***********************************************************************************************
 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
	
			if (this.ds_Master10.getRowCount() > 0) {
			
				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master10.findRowExpr("PBRH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ=='" + this.fvSchKey02 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}				
				
				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
					
				//이미지-첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
				this.fvImgFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
				
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
				
				//[결재]협조자의 경우 수정컬럼 활성화(2022-07-15)
				var sWofhSt = this.ds_Master10.getColumn(0, "WOFH_ST"); //결재상태가 신청(0020), 진행중(0030) 경우				
				if(this.fvHelpYn && (sWofhSt == '0020' || sWofhSt == '0030')) {
					this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_PbrhCd.set_enable(true);	//발생공정
					this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_AreaCd.set_enable(true);	//발생지역
					this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_BusiCd.set_enable(true);	//사업군
					this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_InouGu.set_enable(true);	//사내/외
				}
				
				
			} else {
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			
				//입력영역 비활성화
				this.div_MasterInput.set_enable(false);
					
				//이미지-첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = false;//권한 유무	
				this.fvImgFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);		
				
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = false;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);					
			}
			
			//저장후 재조회여부-true
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}			
	
			break;
		case "SAVE":   //저장		
			for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
				this.ds_Detail10.setColumn(iLoop, "crudType", ""); //초기화
			}
		
			break;	
		case "DTL_SEARCH": //조회		
			this.ds_Detail10.addColumn("s_chk", "string");
			this.ds_Detail10.addColumn("crudType", "string");
			
			//이미지-첨부파일 정보 조회
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.PICH_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PBRH_NO");//관리번호-발행번호
			this.fvImgFileDiv.fn_AtchFileSearch(oFileInfo);			
			
			//첨부파일 정보 조회
			var oFileInfo = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
			oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PBRH_NO");//PK번호2-발생번호
			oFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수						
			
			this.fvFileDiv.fn_AtchFileSearch(oFileInfo);			
			
			//Detail10
			if (this.ds_Detail10.getRowCount() > 0) {
				//[결재]협조자의 경우 수정컬럼 활성화(2022-07-15)
				var sWofhSt = this.ds_Master10.getColumn(0, "WOFH_ST"); //결재상태가 신청(0020), 진행중(0030) 경우				
				if(this.fvHelpYn && (sWofhSt == '0020' || sWofhSt == '0030')) {
					this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setCellProperty("body", 6, "edittype", "combo");				
					this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setCellProperty("body", 7, "edittype", "combo");				
					this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setCellProperty("body", 8, "edittype", "combo");				
					this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setCellProperty("body", 11, "edittype", "combo");				
					this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10.setCellProperty("body", 12, "edittype", "combo");								
				}

				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10);
			} else {
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Detail10);			
			}

			//Detail20
			if (this.ds_Detail10.getRowCount() > 0) {
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_05.form.grd_Detail20);
			} else {
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_05.form.grd_Detail20);			
			}

			break;				
	}
};

/***********************************************************************************************
 * 13.02. [팝업] CallBack 함수 처리
          strId : 팝업호출시 지정한 팝업ID
	      rtnValue : 팝업창에서 close시 지정한 반환값
          주의 : Open(popuptype=modeless)창은 팝업콜백을 지원하지 않음
 ***********************************************************************************************/ 
this.fn_PopupCallback = function(strId, rtnValue)
{

	//[조회영역]
	
	//[Master영역]
		
	//[Detail영역]	
	//증상코드		
	if (strId == "B_SYMM_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	

			if (!this.gfn_IsNull(arrRtnVal.SYMM_CD)) {
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "SYMM_CD", arrRtnVal.SYMM_CD);
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "SYMM_NM", arrRtnVal.SYMM_NM);
			}
		} else {
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "SYMM_CD", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "SYMM_NM", "");
		}		

	//부서		
	} else if (strId == "B_DEPM_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.DEPM_CD)) {
