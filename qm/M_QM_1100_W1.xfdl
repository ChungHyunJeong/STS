			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "REAS_NM", "");
		}	
	//거래처 (고객, 협력사)
	} else if (strId == "B_CUSM_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			if (!this.gfn_IsNull(arrRtnVal.CUSM_CD)) {
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "REAS_CD", arrRtnVal.CUSM_CD);
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "REAS_NM", arrRtnVal.CUSM_NM);
			}
		} else {
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "REAS_CD", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "REAS_NM", "");
		}		

	//담당자		
	} else if (strId == "B_SYST_ID_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.EMPM_CD)) {
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "EMPM_CD", arrRtnVal.EMPM_CD);
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "EMPM_NM", arrRtnVal.EMPM_NM);
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "SYST_ID", arrRtnVal.SYST_ID);
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "DEPM_CD", arrRtnVal.DEPM_CD);
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "DEPM_NM", arrRtnVal.DEPM_NM);
			}
		} else {
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "EMPM_CD", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "EMPM_NM", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "SYST_ID", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "DEPM_CD", "");
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "DEPM_NM", "");
		}			
	}
	
	//[Detail영역-검색]

	
};
 
