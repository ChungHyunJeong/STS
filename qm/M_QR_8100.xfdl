	if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PROH_GU")=="0010"){
		//첨부파일 유무 체크
		//alert(this.fvFileDiv.fn_GetFileCount("0110"));//0100제안서 0110 실행증빙
		if (this.fvFileDiv.fn_GetFileCount("0110") < 1 ) {
