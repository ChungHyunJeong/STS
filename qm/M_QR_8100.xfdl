				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrdpNm.set_value(arrRtnVal.DEPM_NM);					
				
				this.fn_EmpmNm2(arrRtnVal.SYST_ID);
				
				
				//var sPopupId = "M_DEPM_CD_01";//팝업ID - 부서코드
				//this.gfn_GetCommDepm("C", this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrdpCd, this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrdpNm, sPopupId);//코드로 검색
			}
		}else{
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrsuCd.set_value("");
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrsuNm.set_value("");
