/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail20] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	//금액
	if (e.columnid == "PROB_AM") {
		var iNum = obj.getColumn(e.row,"PROB_AM") ;
		if(this.gfn_IsNull(iNum)){
			obj.setColumn(e.row,"PROB_AM",0);
		}
		var iCount = obj.rowcount;
		var iInvestAm = 0;
		//var sSum  = 0;
		for (var iLoop=0; iLoop< iCount-1; iLoop++) {		
			if (iLoop ==0) {
				iInvestAm =  this.gfn_Nvl(nexacro.toNumber(obj.getColumn(iLoop,"PROB_AM")),0) ;
			}else{
				iInvestAm = iInvestAm - this.gfn_Nvl(nexacro.toNumber(obj.getColumn(iLoop,"PROB_AM")),0) ;
			}
		}
		/*
		var sSum = this.gfn_Nvl(obj.getColumn(0,"PROB_AM"),0) 
				 - this.gfn_Nvl(obj.getColumn(1,"PROB_AM"),0) 
				 - this.gfn_Nvl(obj.getColumn(2,"PROB_AM"),0) 
				 - this.gfn_Nvl(obj.getColumn(3,"PROB_AM"),0) 
				 - this.gfn_Nvl(obj.getColumn(4,"PROB_AM"),0);
		trace("--------------------------------------------------");
		trace(this.gfn_Nvl(obj.getColumn(0,"PROB_AM"),0) );
		trace(this.gfn_Nvl(obj.getColumn(1,"PROB_AM"),0) );
		trace(this.gfn_Nvl(obj.getColumn(2,"PROB_AM"),0) );
		trace(this.gfn_Nvl(obj.getColumn(3,"PROB_AM"),0) );
		trace(this.gfn_Nvl(obj.getColumn(4,"PROB_AM"),0) );
		trace(" 합계"+sSum);
		*/
		obj.setColumn(iCount-1, "PROB_AM", iInvestAm);
		obj.setColumn(iCount-1, "crudType", "U");
		
		//마스터 유형효과금액 업데이트
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PROH_AM", iInvestAm);
		//expr:(PROB_GU =='0900')? PROB_AM =dataset.getSum("PROB_AM")-PROB_AM :PROB_AM
		
	}
	
	/*
	//부서
	if (e.columnid == "DEPM_CD") {
		this.gfn_GetCommDepmGrid(obj, "DEPM_CD", "DEPM_NM");//부서명 검색
	} 
	*/
};
