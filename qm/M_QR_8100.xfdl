			}
		}else{
			//조회조건영역 - 자신의자료 권한여부 체크		
			if (this.gfn_CheckOwnAuth(this.fvComPrgmId)) {
				this.div_Search.form.edt_EmpmCd.set_value(this.gfn_GetSessionValue("EMPM_CD"));
				this.div_Search.form.edt_EmpmNm.set_value(this.gfn_GetSessionValue("EMPM_NM"));
				this.div_Search.form.edt_SystId.set_value(this.gfn_GetSessionValue("SYST_ID"));	
			} else {
				this.div_Search.form.edt_EmpmCd.set_value("");
				this.div_Search.form.edt_EmpmNm.set_value("");
				this.div_Search.form.edt_SystId.set_value("");				
			}
		}	
		
	//[Master영역]
	//실행부서		
	} else if (strId == "M_PRDP_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.DEPM_CD)) {
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrdpCd.set_value(arrRtnVal.DEPM_CD);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrdpNm.set_value(arrRtnVal.DEPM_NM);
			}
		}else{
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrdpCd.set_value("");
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrdpNm.set_value("");
		}	
	//실행자		
	} else if (strId == "M_PRSU_ID_01") {
		
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.EMPM_CD)) {
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrsuCd.set_value(arrRtnVal.EMPM_CD);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PrsuNm.set_value(arrRtnVal.EMPM_NM);
