		//[저장] Validation 설정
		var arrArgs = [   "this.div_Search.form.edt_DepmCd^제안부서|NULL"
						, "this.div_Search.form.edt_DepmCd^제안자|NULL"
					  ]; 
					  
		var sCheckGroupTitle = "검색조건";//체크그룹 타이틀

		//컴포넌트 Vaild 체크
		if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
			return false;
		}
	}
	
	return true;
}

