 * 09.01. [공통버튼] 결재요청 버튼 클릭 이벤트
 ***********************************************************************************************/  
this.fn_ComBtnDecide = function()
{ 
	//[공통] 결재요청 Vaildation 체크
	if (!this.fn_VaildWfDecide()) return;
	
	this.ds_Input.clearData();
	this.ds_HeadDepm.clearData();
	
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "SESU_ID", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PRSU_ID")); //실행자

	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
		
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	//svcAddObj.sSelectSql = "hr:M_HE_7030_M_SL13";		//실행자의 부서장 조회
	svcAddObj.sSelectSql = "qm:M_QR_8100_M_SL99";		//실행자의 부서장 조회
	this.gfn_AddComSearchService(this, svcAddObj);
	
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_HR;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_HeadDepm=ds_HeadDepm";//출력 데이터셋	
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.isSync = true;//동기식으로 설정할지 여부
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);
	
	var sRowCount = this.ds_HeadDepm.getRowCount();
	var sWfUser = "";//결재자정보
	
	var nTypeCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "TYPE_CD");
	
	var nRow    = this.ds_Qm810.findRowExpr("CODM_CD=='" + nTypeCd + "'");
	var sSystId = this.ds_Qm810.getColumn(nRow, "OP01_NM"); 
	
	if (sRowCount > 0) {
		var sWFGU_CD = "0020";//결재구분-결재(협조)
		
		sPRSU_ID = this.ds_HeadDepm.getColumn(0,"EVSU_ID");//부서장ID
		sWfUser = sWfUser + sWFGU_CD + ";" + sPRSU_ID;
		
		//-- 공통코드의 제안분류의 사번을 협조로 자동 생성
		sWfUser = sWfUser + "ⓐ" + sWFGU_CD + ";" + sSystId;		
		
		/*
		sWFGU_CD = "0070";//결재구분-결재(주관부서)
		sPRSU_CD = "";//참석자
		sPRSU_ID = this.ds_Master10.getColumn(iRow, "HR_PRSU_ID");//담당자ID
				
		//담당자 추가
		//sWfUser = sWfUser + "ⓐ" + sWFGU_CD + ";" + sPRSU_CD;
		sWfUser = sWfUser + "ⓐ" + sWFGU_CD + ";" + sPRSU_ID;
		*/
		
	} 
	
	
	var iRow = this.ds_Master10.rowposition;
	var sPks = this.ds_Master10.getColumn(iRow, "COMM_CD");//회사코드
	var sProhNo = this.ds_Master10.getColumn(iRow, "PROH_NO");
	
	sPks += "ⓐ" + sProhNo;//제안번호
	sPks += "ⓐ" + this.ds_Master10.getColumn(iRow, "WOFH_SQ");//결재차수
	
		
	sProhNo= this.gfn_StringFormat(sProhNo, "@@@@@@-@@@@");
	var sWofhNm = "[제안등록:"+ sProhNo +"] "+this.ds_Master10.getColumn(iRow, "PROH_NM");
	
	var sPopupId = "C_WF_REQUEST";//팝업ID - 결재요청
	var oArg = { CommCd:this.gfn_GetSessionValue("COMM_CD")//회사코드
	           , ModuCd:"QM"//모듈구분
			   , WctmId:"WF_M_QR_8100_W1"//결재업무ID
			   , SystId:this.gfn_GetSessionValue("SYST_ID")//결재자_시스템사번
			   , WofhNm: sWofhNm //결재명
			   , WofhNo:this.ds_Master10.getColumn(iRow, "WOFH_NO")//결재PID
			   , InnrGb:"1"//내부결재구분(1:신규, 2:내부)
			   , CharSystId:""//담당자_시스템사번
			   , CharWfguCd:""//담당자 결재구분
			   , SworqVr:"3"//결재요청버전(1,2,3)
			   , WfamAmtr:""//금액
			   , WfamOpid:""//원천PIID
			   , WfPkvl:sPks//PK값 정보 - PK 값을 순서대로 전달, Row구분자는 ⓡ, 조건 구분자는 ⓐ
			   , WfUser:sWfUser//결재자구분
	           };//Argument처리
			    trace("-------->"+oArg.WfUser);
	this.gfn_WfRequestPop(sPopupId, oArg);	
	
}

/***********************************************************************************************
 * 09.02. [공통버튼] 결재요청 유효성체크
 ***********************************************************************************************/  
this.fn_VaildWfDecide = function() {
	
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail20) && !this.fvFileDiv.fn_CheckFileAddYn()) {
		var sWOFH_ST = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST");//결재진행상태

		//결재상태에 따른 결재요청 가능여부 체크
		if (!this.gfn_CheckDecideWf(sWOFH_ST)) {
			return false;
		}
		
	} else {
		var arrMsgArg = ["결재요청"];
		this.gfn_Alert("SM064", arrMsgArg);//변경된 내용이 있습니다.\n저장 후 &ITM0하시기 바랍니다.
		return false;
	}
	
	//결재진행내역체크
	if (this.ds_Master10.getRowCount() < 1 || this.ds_Master10.rowposition < 0) {
		this.gfn_Alert("WF016", arrMsgArg);//결재내역을 선택한 후 결재요청하세요.
		return false;
	}	
	
	return true;
}

