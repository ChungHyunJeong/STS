/***********************************************************************************************
 * 09.01. [공통버튼] 결재요청 버튼 클릭 이벤트
 ***********************************************************************************************/  
this.fn_ComBtnDecide = function()
{ 
	//[공통] 결재요청 Vaildation 체크
	if (!this.fn_VaildWfDecide()) return;
	
	var iRow = this.ds_Master10.rowposition;
	var sPks = this.ds_Master10.getColumn(iRow, "COMM_CD");//회사코드
	sPks += "ⓐ" + this.ds_Master10.getColumn(iRow, "PBRH_NO");//발생번호
	sPks += "ⓐ" + this.ds_Master10.getColumn(iRow, "WOFH_SQ");//결재차수

	//계약번호 기존/신규 마스킹포멧 처리
	var sCnthNo = this.ds_Master10.getColumn(iRow, "CNTH_NO");
	if(sCnthNo.length > 8)
		sCnthNo= this.gfn_StringFormat(sCnthNo, "@@@@-@-@@@@");
	else
		sCnthNo= this.gfn_StringFormat(sCnthNo, "@@@-@@@@");

	//결재명
	var sWofhNm = "[문제점발생:"+sCnthNo+"] "+this.ds_Master10.getColumn(iRow, "PBRH_NM");
	
	//자동전표관련 파라메타-자동전표를 사용하는 화면에만 추가 
	var sAtobKey = "@COMM_CD=" + this.ds_Master10.getColumn(iRow, "COMM_CD");//회사코드
	sAtobKey += "ⓐ@PBRH_NO=" + this.ds_Master10.getColumn(iRow, "PBRH_NO");//발생번호
	sAtobKey += "ⓐ@WOFH_SQ=" + this.ds_Master10.getColumn(iRow, "WOFH_SQ");//결재차수


	//[협조추가]귀책내역 "사내"일 경우 선택한 부서의 부서장 협조로 생성(2022-07-19)
	var sWfUser = "";//결재자정보
	var sWFGU_CD = "0020";//결재구분-결재(협조)
	//var sWFGU_CD = "0070";//결재구분-결재(주관부서)

	//귀책내역 "사내" 대상 부서확인
	var sReasGu = "";	//귀책구분
	var sReasCd = "";	//귀책내역 "사내"의 조직코드
	var sDepmCd = "";	//검색대상 조직
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		sReasGu = this.ds_Detail10.getColumn(iLoop, "REAS_GU");	//귀책구분
		sReasCd = this.ds_Detail10.getColumn(iLoop, "REAS_CD");	//귀책내용(부서코드)
		//귀책구분이 "사내"인 경우 귀책내용(부서코드)를 검색조건으로 추가
		if (sReasGu == "0010") {
			if (this.gfn_IsNull(sDepmCd)) {
				sDepmCd = sReasCd;
			} else {
				sDepmCd = sDepmCd + ", "+sReasCd;
			}	
		}
	}

	//부서코드가 존재하는 경우 
	if (!this.gfn_IsNull(sDepmCd)) {
		//귀책내역 부서코드의 부서장 사번 DB에서 조회
		var sReasYn = this.fn_DempHeadSearch(sDepmCd);	//부서장 조회
	
		//조회된 결과가 존재시 부서장사번을 협조자로 넣음
		if (sReasYn) {
			var sTmsuId = "";	//조직장 사번
			for (var iLoop=0; iLoop<this.ds_TmsuId.getRowCount(); iLoop++) {
				sTmsuId = this.ds_TmsuId.getColumn(iLoop, "TMSU_ID");//부서장사번
				
				if (this.gfn_IsNull(sWfUser)) {	//최초의 경우
					sWfUser = sWfUser + sWFGU_CD + ";" + sTmsuId;
				} else {
					sWfUser = sWfUser + "ⓐ" + sWFGU_CD + ";" + sTmsuId;
				}
			}
		}
				
	}
	//[협조추가] 끝
	
	var sPopupId = "C_WF_REQUEST";//팝업ID - 결재요청
	var oArg = { CommCd:this.gfn_GetSessionValue("COMM_CD")//회사코드
	           , ModuCd:"QM"//모듈구분
			   , WctmId:"WF_M_QM_1100_W1"//결재업무ID
			   , SystId:this.gfn_GetSessionValue("SYST_ID")//결재자_시스템사번
			   , WofhNm:sWofhNm//결재명
			   , WofhNo:this.ds_Master10.getColumn(iRow, "WOFH_NO")//결재PID
			   , InnrGb:"1"//내부결재구분(1:신규, 2:내부)
			   , CharSystId:""//담당자_시스템사번
			   , CharWfguCd:""//담당자 결재구분
			   , SworqVr:"3"//결재요청버전(1,2,3)
			   , WfamAmtr:"0"//금액
			   , WfamOpid:""//원천PIID
			   , WfPkvl:sPks//PK값 정보 - PK 값을 순서대로 전달, Row구분자는 ⓡ, 조건 구분자는 ⓐ
			   , WfUser:sWfUser//결재자구분
	           };//Argument처리
	this.gfn_WfRequestPop(sPopupId, oArg);	
}

/***********************************************************************************************
 * 09.02. [공통버튼] 결재요청 유효성체크
 ***********************************************************************************************/  
this.fn_VaildWfDecide = function() {
	
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.fvImgFileDiv.fn_CheckFileAddYn() && !this.fvFileDiv.fn_CheckFileAddYn()) {
		var sWOFH_ST = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST");//결재진행상태
		//결재상태에 따른 결재요청 가능여부 체크		
		if (!this.gfn_CheckDecideWf(sWOFH_ST)) {			
			return false;		
		}
		
	} else {
		var arrMsgArg = ["결재요청"];
		this.gfn_Alert("SM064", arrMsgArg);//변경된 내용이 있습니다.\n저장 후 &ITM0하시기 바랍니다.
		return false;
	}

	//결재진행내역체크
	if (this.ds_Master10.getRowCount() < 1 || this.ds_Master10.rowposition < 0) {
		this.gfn_Alert("WF016", arrMsgArg);//결재내역을 선택한 후 결재요청하세요.
		return false;
	}

	//[월마감제어-결재요청]
	var sBUDG_MM = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PBRH_DD"), "");//마감년월
	
	if (!this.gfn_IsNull(sBUDG_MM)) {
		//마감여부 체크
		var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
		var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
		paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
		paramObj.MODM_CD = "QM";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
		paramObj.ACUM_CD = "";//[선택] 회계단위
		paramObj.BUSM_CD  = "";//[선택] 사업부문
		paramObj.SAUM_CD  = "";//[선택] 사업장코드
		paramObj.DEPM_CD  = "";//[선택] 부서코드
		paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
		paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
		
		var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		
		
		//월마감된 경우
		if (bCheck) {
			this.gfn_Alert("CA027");//해당월의 정보가 마감이 되어서 결재요청 할 수 없습니다.
			return false;
		}	
	}

	return true;
}


