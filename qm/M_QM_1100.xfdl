			var sPopupId = "B_WO_NO_01";//팝업ID - W/O 정보

			var oArg = { CnthNo:this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.value, PrgmId:this.fvComPrgmId};//Argument처리
			this.gfn_WorhPop(sPopupId, oArg);

		//품목코드(자재코드)
		} else if (e.col == 9) {	
			var sPopupId = "B_ITEM_CD_01";//팝업ID - 품목정보
			var oArg = { ItemCd:"", ItemNm:"" };//Argument처리
			this.gfn_ItemPop(sPopupId, oArg);		

		//원인구분 - 대분류 (콤보 클릭시 품질비용 조건으로 변경됨)
