{
	//[저장] Validation 설정
	var arrArgs = ["this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PbrhDd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_PbrhDd.text + "|NULL" //발생일자
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cal_OccuDd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_OccuDd.text + "|NULL" //등록일자
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_DepmCd.text + "|NULL" //등록부서
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_EmpmCd.text + "|NULL" //등록자
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_CnthNo.text + "|NULL" //계약번호
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_OccuGu^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_OccuGu.text + "|NULL" //발생구분
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_PbrhCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_PbrhCd.text + "|NULL" //발생공정
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_AreaCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_AreaCd.text + "|NULL" //발생지역
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PbrhQt^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_PbrhQt.text + "|NULL" //발생수량
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_PbqtCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_PbrhQt.text + "단위|NULL" //발생수량단위
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_BusiCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_BusiCd.text + "|NULL" //사업군
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_InouGu^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_InouGu.text + "|NULL" //사내/외
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PbrhNm^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_PbrhNm.text + "|NULL" //보고제목
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_RemkNm.text + "|NULL" //문제점내용
			      ]; 


	var sCheckGroupTitle = this.div_MasterInput.form.tab_Master.tpg_01.text;//체크그룹 타이틀

	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
		return false;
	}

	var sOccuGu =  this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OCCU_GU");
	
	if(sOccuGu=="0090"){
		var OccuNm = this.div_DetailInput.form.tab_Detail.tpg_02.form.txt_OccuNm.value;
		
		if(!this.gfn_IsNull(OccuNm)){
			OccuNm = OccuNm.trim();
		}
		
		if(this.gfn_IsNull(OccuNm) || OccuNm == ""){
			this.div_DetailInput.form.tab_Detail.set_tabindex(1);
			this.div_DetailInput.form.tab_Detail.tpg_02.form.txt_OccuNm.setFocus();
			
			var arrMsgArg = ["발생원인상세"];
			this.gfn_Alert("SM008", arrMsgArg);//[필수 입력 항목입니다
			return false;
		}
		
		var ActiNm_H = this.div_DetailInput.form.tab_Detail.tpg_02.form.txt_ActiNm.value;
		
		if(!this.gfn_IsNull(ActiNm_H)){
			ActiNm_H = ActiNm_H.trim();
		}
		
		if(this.gfn_IsNull(ActiNm_H) || ActiNm_H == ""){
			this.div_DetailInput.form.tab_Detail.set_tabindex(1);
			this.div_DetailInput.form.tab_Detail.tpg_02.form.txt_ActiNm.setFocus();
			
			var arrMsgArg = ["대책/조치사항"];
			this.gfn_Alert("SM008", arrMsgArg);//[필수 입력 항목입니다
			return false;
		}
	}


	return true;
}

/***********************************************************************************************
 * 05.03. [Vaildation] Detail10 저장 필수값 체크 - 그리드항목
 ***********************************************************************************************/ 
this.fn_VaildDetailSave = function()
{
	//[저장] Validation 설정
	this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 3, "edittype", "readonly");				
	var arrArgs = ["WORB_NO^NULL"
			      ,"COST_CD^NULL"
			      ,"PRR1_CD^NULL"
			      ,"PRR2_CD^NULL"
			      ,"SYMM_NM^NULL"
			      ,"FORM_CD^NULL"
			      ,"ACTI_CD^NULL"				  
			      ,"PBRB_AM^NULL"
			      ]; 
	
	var sCheckGroupTitle = this.div_DetailInput.form.tab_Detail.tpg_01.text;//체크그룹 타이틀				  
	
	//Grid Vaild 체크
	if (!this.gfn_CheckGridValid(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10, arrArgs, sCheckGroupTitle) ) {		
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 3, "edittype", "none");	
		return false;
	} else {
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 3, "edittype", "none");	
	}

	var sSymmNm = ""; //증상명	
	var sReasGu = ""; //귀책구분
	var sReasNm = ""; //귀책명
	var sPbrbYn = ""; //필수여부
	var iPbrbCt = ""; //검증기간(월)
	var sEmpmCd = ""; //담당자
	var alertMsg = "";	//팝업메시지
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		//증상명, 귀책내역
		sSymmNm = this.ds_Detail10.getColumn(iLoop, "SYMM_NM"); //증상명	
		sReasGu = this.ds_Detail10.getColumn(iLoop, "REAS_GU"); //귀책구분
		sReasNm = this.ds_Detail10.getColumn(iLoop, "REAS_NM"); //귀책명

		if (this.gfn_IsNull(sSymmNm)) {
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);			
			
			if (this.gfn_IsNull(sSymmNm)) {		
				alertMsg = "증상명";
			}
		}	
		
		if (sReasGu != '0090' && !this.gfn_IsNull(sReasGu) && this.gfn_IsNull(sReasNm)) {		
			if (this.gfn_IsNull(alertMsg)) {		
				alertMsg = "귀책명";
			} else {
				alertMsg = alertMsg + ", 귀책명";
			}
		}

		//검증기간 필수체크
		sPbrbYn = this.ds_Detail10.getColumn(iLoop, "PBRB_YN"); //필수여부	
		//필수여부 대상시
		if (sPbrbYn == '1') {
			iPbrbCt = this.ds_Detail10.getColumn(iLoop, "PBRB_CT");	//검증기간(월)
			sEmpmCd = this.ds_Detail10.getColumn(iLoop, "EMPM_CD");	//담당자
			
			if (this.gfn_IsNull(iPbrbCt) || this.gfn_IsNull(sEmpmCd)) {		
				//탭, 위치 이동
				this.div_DetailInput.form.tab_Detail.set_tabindex(0);			
				
				if (this.gfn_IsNull(iPbrbCt)) {		
					if (this.gfn_IsNull(alertMsg)) {		
						alertMsg = "검증기간(월)";
					} else {
						alertMsg = alertMsg + ", 검증기간(월)";
					}
				}
				
				if (this.gfn_IsNull(sEmpmCd)) {		
					if (this.gfn_IsNull(alertMsg)) {		
						alertMsg = "담당자";
					} else {
						alertMsg = alertMsg + ", 담당자";
					}
				}
			}

		}
		
		//필수값 누락 발생시
		if (!this.gfn_IsNull(alertMsg)) {		
			//포커스 이동
			if (this.gfn_IsNull(sSymmNm)) {		
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(13,iLoop);
			} else if (this.gfn_IsNull(sReasNm)) {		
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(18,iLoop);
			} else if (this.gfn_IsNull(iPbrbCt)) {				
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(28,iLoop);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();		
			} else if (this.gfn_IsNull(sEmpmCd)) {		
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(31,iLoop);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();					
			}
			
			var iRow = iLoop + 1;
			var arrMsgArg = [iRow+"번째행 "+alertMsg];
			this.gfn_Alert("SM008", arrMsgArg);//검증기간(월), 담당자는 필수 입력 항목입니다.
			return false;
			
			break;			
		}
	}
	
	
	var sOccuGu =  this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OCCU_GU");
	
	if(sOccuGu=="0090"){
		var ActiNm = this.div_DetailInput.form.tab_Detail.tpg_02.form.txt_ActiNm.value;
		
		if(!this.gfn_IsNull(ActiNm)){
			ActiNm = ActiNm.trim();
		}
		
		if(this.gfn_IsNull(ActiNm) || ActiNm == ""){
			this.div_DetailInput.form.tab_Detail.set_tabindex(1);
			this.div_DetailInput.form.tab_Detail.tpg_02.form.txt_ActiNm.setFocus();
			
			var arrMsgArg = ["대책/조치사항"];
			this.gfn_Alert("SM008", arrMsgArg);//[필수 입력 항목입니다
			return false;
		}
	}
	
	return true;
}

