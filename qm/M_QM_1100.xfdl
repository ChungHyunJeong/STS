		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PBRH_NO"))) {		
			//[월마감제어-버튼/상세그리드]
			var sBUDG_MM = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PBRH_DD"), "");//마감년월
			
			//마감여부 체크
			var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
			var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
			paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
			paramObj.MODM_CD = "QM";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
			paramObj.ACUM_CD = "";//[선택] 회계단위
			paramObj.BUSM_CD  = "";//[선택] 사업부문
			paramObj.SAUM_CD  = "";//[선택] 사업장코드
			paramObj.DEPM_CD  = "";//[선택] 부서코드
			paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
			paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
			
			var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		

			//월마감된 경우
			if (bCheck) {		
				this.fvMmCloseYn = true;//월마감여부-Tab버튼 제어용
			} else {
				this.fvMmCloseYn = false;//월마감여부-Tab버튼 제어용
			}		
			
			//계약번호 자리수 확인
			var iCnthLen = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO").length;

			//계약번호 format 변경 (9자리, 8자리)
			if (iCnthLen > 8) 
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_format("@@@@-@-@@@@");		
			else 
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_format("@@@-@@@@");				

			//발생원인상세 발생구분 옵션1인 경우 필수, 아닌경우 필수해제(2022-04-29)
			var sOccuGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OCCU_GU");

			var iFindRow = this.ds_Qm110.findRowExpr("CODM_CD=='" + sOccuGu +"'");
			var sOp01Nm = this.ds_Qm110.getColumn(iFindRow,"OP01_NM"); //발생구분 (ECR 돋보기 활성화) 

			if (sOp01Nm == "1") {	//발생구분 옵션1이 1인경우 
				this.div_DetailInput.form.tab_Detail.tpg_02.form.txt_OccuNm.set_cssclass("essential");
				this.div_DetailInput.form.tab_Detail.tpg_02.form.txt_ActiNm.set_cssclass("essential");				
			} else {
				this.div_DetailInput.form.tab_Detail.tpg_02.form.txt_OccuNm.set_cssclass("");
				this.div_DetailInput.form.tab_Detail.tpg_02.form.txt_ActiNm.set_cssclass("");							
			}

			//결재상태가 아닌경우
			if (!this.fvWfSearchMode && !this.fvWofhStatus) {
				//ECR 버튼 활성화/비활성화 및 셋팅값 처리 (2022-04-04)
				var sOccuGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OCCU_GU");

				var iFindRow = this.ds_Qm110.findRowExpr("CODM_CD=='" + sOccuGu +"'");
				var sOp01Nm = this.ds_Qm110.getColumn(iFindRow,"OP01_NM"); //발생구분 (ECR 돋보기 활성화) 
				
				if (sOp01Nm == "1") {	//발생구분 옵션1이 1인경우 
					//ECR 번호 검색버튼 활성화
					this.div_MasterInput.form.tab_Master.tpg_01.form.btn_EcrhNo.set_enable(true);
					this.fvOccuGu = "0080";	//발주구분 옵션1일 경우 ECR(0080)로 검색함
					
				} else {
					//ECR 번호 검색버튼 비활성화
					this.div_MasterInput.form.tab_Master.tpg_01.form.btn_EcrhNo.set_enable(false);
					this.fvOccuGu = "";	//발주구분 옵션1이 아닌경우 초기화
				}
			}

			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "PBRH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PBRH_NO"));
			this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
			this.ds_Input.setColumn(0, "PRGM_ID", this.fvComPrgmId);					
			
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "qm:M_QM_1100_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);

			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "qm:M_QM_1100_D_SL20";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			
// 			svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
// 			svcAddObj.sSelectSql = "qm:M_QM_1100_D_SL12";
// 			this.gfn_AddComSearchService(this, svcAddObj);		
			
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_QM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10 ds_Detail20=ds_Detail20 ";//출력 데이터셋			
			//svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10 ds_Detail20=ds_Detail20 ds_Detail12=ds_Detail12";//출력 데이터셋			
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);		
		}
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master10] Row 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10) || this.gfn_DsIsUpdated(this.ds_Detail10) || this.fvImgFileDiv.fn_CheckFileAddYn() || this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	

	return true;
};  

/*---------------------------------------------------------------------------------------------
 *    03. [데이터셋-ds_Master10] 컬럼 변경 전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_cancolumnchange = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//[월마감제어-등록/수정]
	if (e.columnid == "PBRH_DD") {
		var sBUDG_MM = "";//마감년월
		
		if (!this.gfn_IsNull(this.fvMmCloseDay)) {
			sBUDG_MM = this.gfn_Nvl(this.fvMmCloseDay, "");//마감년월
		} else {
			sBUDG_MM = this.gfn_Nvl(e.newvalue, "");//마감년월
		}
		this.fvMmCloseDay = "";

		//마감여부 체크
		var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
		var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
		paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
		paramObj.MODM_CD = "QM";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
		paramObj.ACUM_CD = "";//[선택] 회계단위
		paramObj.BUSM_CD  = "";//[선택] 사업부문
		paramObj.SAUM_CD  = "";//[선택] 사업장코드
		paramObj.DEPM_CD  = "";//[선택] 부서코드
		paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
		paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
		
		var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		

		//월마감된 경우
		if (bCheck) {
			this.gfn_Alert("CA025");//해당월의 정보가 마감이 되어서 등록/수정 할 수 없습니다.
			
			var sOrgDate = this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "PBRH_DD");//변경전값
			
			//변경전값이 없을경우
			if (this.gfn_IsNull(sOrgDate)) {				
				var sToDay = this.gfn_GetCommDate("toDay");//현재일자
				if (sBUDG_MM.substr(0,6) == sToDay.substr(0,6)) {
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PBRH_DD", "");//입력날짜가 마감된경우 오늘날짜로 세팅				
				} else {
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PBRH_DD", sToDay);//입력날짜가 마감된경우 오늘날짜로 세팅
				}
			} else {
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PBRH_DD", sOrgDate);//입력날짜가 마감된경우 변경전 값으로 세팅
			}
			
			return false;
		} else {
			return true;
		}

	}
};

 /*---------------------------------------------------------------------------------------------
 *    04. [데이터셋-ds_Master10] 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{

	//  발생구분 선택시
	if (e.columnid == "OCCU_GU") {
		var sOccuGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "OCCU_GU");

		var iFindRow = this.ds_Qm110.findRowExpr("CODM_CD=='" + sOccuGu +"'");
		var sOp01Nm = this.ds_Qm110.getColumn(iFindRow,"OP01_NM"); //발생구분 (ECR 돋보기 활성화) 

		if (sOp01Nm == "1") {	//발생구분 옵션1이 1인경우 
			//ECR 번호 검색버튼 활성화
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_EcrhNo.set_enable(true);
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "ECRH_NO", this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "ECRH_NO"));
			this.fvOccuGu = "0080";	//발주구분 옵션1일 경우 ECR(0080)로 검색함
			
			//Detail 상세 발생원인상세, 대책/조치사항 입력 필수사항(2022-04-29)
			this.div_DetailInput.form.tab_Detail.tpg_02.form.txt_OccuNm.set_cssclass("essential");
			this.div_DetailInput.form.tab_Detail.tpg_02.form.txt_ActiNm.set_cssclass("essential");

		} else {
			//ECR 번호 검색버튼 비활성화
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_EcrhNo.set_enable(false);
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "ECRH_NO", "");
			this.fvOccuGu = "0090";	//발주구분 옵션1이 아닌경우 초기화
			
			//Detail 상세 발생원인상세, 대책/조치사항 입력 필수사항 제외(2022-04-29)
			this.div_DetailInput.form.tab_Detail.tpg_02.form.txt_OccuNm.set_cssclass("");
			this.div_DetailInput.form.tab_Detail.tpg_02.form.txt_ActiNm.set_cssclass("");			

		}
		
	}
};


