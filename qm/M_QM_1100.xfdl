/***********************************************************************************************
 * 1. 조회 영역
 ***********************************************************************************************/
/***********************************************************************************************
 * 01.01. [공통버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.fn_ComBtnSearch = function(sWofhNo) //[결재]
{ 

	//[결재] 결재 PID가 있으면-결재Popup으로 호출시
	if (!this.gfn_IsNull(sWofhNo)) {
		this.fvWfSearchMode = true;//[결재]결재내역 조회모드
		this.fvWfPopupMode = true;//[결재]결재내역 팝업화면으로 호출여부
	}

	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;	
	
	//저장후 재조회시-마스터 데이터셋 이벤트 비활성화
	if (this.fvSchKeyYn) {
		this.ds_Master10.set_enableevent(false);
	}

	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Master10.clearData();
	this.ds_Detail10.clearData();
	this.ds_Input.clearData();
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	this.fvImgFileDiv.fn_ClearAtchFile();//이미지-첨부파일 정보 초기화

	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	var sSqlId = "";//[결재]쿼리ID

	//[결재] 결재내역 조회가 아니면 (if문)
	if (!this.fvWfSearchMode) {		
		this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_FromDd.value);
		this.ds_Input.setColumn(0, "CLOS_DD", this.div_Search.form.cal_ClosDd.value);
		this.ds_Input.setColumn(0, "CNTH_NO", this.div_Search.form.mae_CnthNo.value);
		this.ds_Input.setColumn(0, "OCCU_GU", this.div_Search.form.cbo_OccuGu.value);
		this.ds_Input.setColumn(0, "PBRH_NO", this.div_Search.form.edt_PbrhNo.value);
		this.ds_Input.setColumn(0, "DEPM_CD", this.div_Search.form.edt_DepmCd.value);
		this.ds_Input.setColumn(0, "SESU_ID", this.div_Search.form.edt_SystId.value);
		this.ds_Input.setColumn(0, "WOFH_YN", this.div_Search.form.chk_WofhYn.value);
		sSqlId = "qm:M_QM_1100_M_SL10";//쿼리ID
	} else {
		this.ds_Input.setColumn(0, "WOFH_NO", sWofhNo);//결재PID
		sSqlId = "qm:M_QM_1100_M_SL11";//쿼리ID
	}
	
	if(this.fvOpenChk=="Y"){
		this.ParamPbrhNo = this.getOwnerFrame().paramPbrhNo;
		this.ParamWofhSq = this.getOwnerFrame().paramWofhSq;
		
		this.ds_Input.setColumn(0, "PBRH_NO", this.ParamPbrhNo);//결재PID
		this.ds_Input.setColumn(0, "WOFH_SQ", this.ParamWofhSq);//결재PID
		sSqlId = "qm:M_QM_1100_M_SL12";//쿼리ID
	}	

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = sSqlId;
	this.gfn_AddComSearchService(this, svcAddObj);
	
	svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "qm:M_QM_1100_D_SL12";
	this.gfn_AddComSearchService(this, svcAddObj);		
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MST_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_QM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Master10=ds_Master10 ds_Detail12=ds_Detail12";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
}

