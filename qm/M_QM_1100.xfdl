/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	//품목코드
	if (e.columnid == "ITEM_CD") {
		this.gfn_GetCommItemGrid(obj, "ITEM_CD", "ITEM_NM");//품목코드,명 검색							

	// 품질비용 선택시
	} else if (e.columnid == "COST_CD") {
		var sCostCd = this.ds_Detail10.getColumn(e.row, "COST_CD");	//품질비용 선택값
		var iFindRow = this.ds_Qm125.findRowExpr("CODM_CD == '"+sCostCd+"'"); //공통코드에서 품질비용 선택 대상 Row확인 
		var sPrr1Cd = this.ds_Qm125.getColumn(iFindRow, "OP01_NM"); //품질비용으로 대분류 값 검색

		//Detail10 대분류/중분류 값 초기화
		this.ds_Detail10.setColumn(e.row, "PRR1_CD", ""); //대분류
		this.ds_Detail10.setColumn(e.row, "PRR2_CD", ""); //중분류

		this.ds_Qm010.filter("");	//대분류 필터 초기화
		this.ds_Qm020.filter("");	//중분류 필터 초기화
		this.ds_Qm010.filter("OP01_NM == '"+sPrr1Cd+"'");	//초기화 후 대분류값 필터링
		
		//품질비용
		if (sCostCd == '0050') {	//품질비용 = 실패비용(0050)
			this.ds_Detail10.setColumn(e.row, "REAS_GU", "0010");	//사내로 Default(2020-03-03 요청)
			this.ds_Detail10.setColumn(e.row, "REAS_CD","");	    //초기화
			this.ds_Detail10.setColumn(e.row, "REAS_NM","");	    //초기화
		} else {	//품질비용 = 추가비용(0020)
			this.ds_Detail10.setColumn(e.row, "REAS_GU", "");	    //초기화(필수값 체크 제외하기 위해 공백 넣음)
			this.ds_Detail10.setColumn(e.row, "REAS_CD","");	    //초기화
			this.ds_Detail10.setColumn(e.row, "REAS_NM","");	    //초기화(필수값 체크 제외하기 위해 공백 넣음)
		}

		//유효성검증 초기화(2021-12-15 추가요청)
		this.ds_Detail10.setColumn(e.row, "PBRB_YN","0");
		this.ds_Detail10.setColumn(e.row, "PBRB_CT","");
		this.ds_Detail10.setColumn(e.row, "DEPM_CD","");
		this.ds_Detail10.setColumn(e.row, "DEPM_NM","");
		this.ds_Detail10.setColumn(e.row, "EMPM_CD","");
		this.ds_Detail10.setColumn(e.row, "EMPM_NM","");
		this.ds_Detail10.setColumn(e.row, "SYST_ID","");

		
	//원인구분 - 대분류
	} else if (e.columnid == "PRR1_CD") {
		this.ds_Qm020.filter("");
		this.ds_Qm020.filter("OP01_NM == '"+ this.ds_Detail10.getColumn(e.row, "PRR1_CD") +"'");

		//원인구분 - 소분류 초기화
		var sPrr1CdOrg = this.ds_Detail10.getOrgColumn(e.row, "PRR1_CD");
		var sPrr1CdNew = this.ds_Detail10.getColumn(e.row, "PRR1_CD");
		
		if (sPrr1CdOrg != sPrr1CdNew) {
			this.ds_Detail10.setColumn(e.row, "PRR2_CD","")
		}		

		//유효성검증 초기화(2021-12-15 추가요청)
		this.ds_Detail10.setColumn(e.row, "PBRB_YN","0");
		this.ds_Detail10.setColumn(e.row, "PBRB_CT","");
		this.ds_Detail10.setColumn(e.row, "DEPM_CD","");
		this.ds_Detail10.setColumn(e.row, "DEPM_NM","");
		this.ds_Detail10.setColumn(e.row, "EMPM_CD","");
		this.ds_Detail10.setColumn(e.row, "EMPM_NM","");
		this.ds_Detail10.setColumn(e.row, "SYST_ID","");

	//원인구분 - 중분류 (2021-12-06 추가 : 옵션값 유효성평가여부 "1"인 경우 필요유무 자동체크)
	} else if (e.columnid == "PRR2_CD") {
		var sPrr2Cd = this.ds_Detail10.getColumn(e.row, "PRR2_CD");	//중분류 
		var iFindRow = this.ds_Qm020.findRowExpr("CODM_CD == '"+sPrr2Cd+"'"); //공통코드에서 중분류 선택 대상 Row확인 
		var fvPbrbYnFlag = this.ds_Qm020.getColumn(iFindRow, "OP03_NM"); //유효성평가여부(옵션값3)

		//유효성평가여부가 1인 경우
		if (fvPbrbYnFlag == "1") {
			this.ds_Detail10.setColumn(e.row, "PBRB_YN","1");		//필수여부 체크
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 27, "edittype", "none");			
		} else {
			this.ds_Detail10.setColumn(e.row, "PBRB_YN","0");		//필수여부 체크해제
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 27, "edittype", "checkbox");						
		}

		//유효성검증 초기화(2021-12-15 추가요청)
		this.ds_Detail10.setColumn(e.row, "PBRB_CT","");
		this.ds_Detail10.setColumn(e.row, "DEPM_CD","");
		this.ds_Detail10.setColumn(e.row, "DEPM_NM","");
		this.ds_Detail10.setColumn(e.row, "EMPM_CD","");
		this.ds_Detail10.setColumn(e.row, "EMPM_NM","");
		this.ds_Detail10.setColumn(e.row, "SYST_ID","");

	//귀책구분
	} else if (e.columnid == "REAS_GU") {
		var sReasGu = this.ds_Detail10.getColumn(e.row, "REAS_GU");
		if (sReasGu == '0090') {
			this.ds_Detail10.setColumn(e.row, "REAS_NM"," ");	//초기화(필수값 체크 제외하기 위해 공백 넣음)
		} else {
			this.ds_Detail10.setColumn(e.row, "REAS_NM","");	//초기화
		}
		
		this.ds_Detail10.setColumn(e.row, "REAS_CD","");
	
	//귀책내용 코드
	} else if (e.columnid == "REAS_CD") {
		var sReasGu = this.ds_Detail10.getColumn(e.row, "REAS_GU");
		
		if (sReasGu == '0010') { //내부 (0010)
			this.gfn_GetCommDepmGrid(obj, "REAS_CD", "REAS_NM");//부서명 검색
			
		} else if (sReasGu == '0020'){ //협력사(0020)
			this.gfn_GetCommCusmGrid(obj, "REAS_CD", "REAS_NM", "H");//거래처명 검색		

		} else if (sReasGu == '0030'){ //고객 (0030)
			this.gfn_GetCommCusmGrid(obj, "REAS_CD", "REAS_NM", "C");//거래처명 검색		
			
		}

	//예상소요비용 -> 총소요비용 처리
	} else if (e.columnid == "PBRB_AM") {
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PBRH_AM",this.ds_Detail10.getSum("PBRB_AM"));	//Master에 총소요비용 표시

	//처리자
	} else if (e.columnid == "ACSU_CD") {
		var arrArgs = ["ACDP_CD","ACDP_NM"];
		this.gfn_GetCommEmpmGrid(obj, "ACSU_CD", "ACSU_NM", "ACSU_ID", arrArgs);//사원명 검색

	//필수여부
	} else if (e.columnid == "PBRB_YN") {
		var sPbrbYn = this.ds_Detail10.getColumn(e.row, "PBRB_YN");		//필수여부
		
		if (sPbrbYn == "0") {	//필수여부 해제시 초기화
			//유효성검증 초기화(2021-12-15 추가요청)
			this.ds_Detail10.setColumn(e.row, "PBRB_CT","");
			this.ds_Detail10.setColumn(e.row, "DEPM_CD","");
			this.ds_Detail10.setColumn(e.row, "DEPM_NM","");
			this.ds_Detail10.setColumn(e.row, "EMPM_CD","");
			this.ds_Detail10.setColumn(e.row, "EMPM_NM","");
			this.ds_Detail10.setColumn(e.row, "SYST_ID","");
		}

	//담당자
	} else if (e.columnid == "EMPM_CD") {
		var arrArgs = ["DEPM_CD","DEPM_NM"];
		this.gfn_GetCommEmpmGrid(obj, "EMPM_CD", "EMPM_NM", "SYST_ID", arrArgs);//사원명 검색

	}
	
}

/***********************************************************************************************
 * 02.DG0.08. [Detail Tab] 변경 이벤트
 ***********************************************************************************************/
this.div_DetailInput_tab_Detail_onchanged = function(obj:nexacro.Tab,e:nexacro.TabIndexChangeEventInfo)
{
	var iTabIndex = e.postindex;
	
	if (!this.fvWfSearchMode && !this.fvWofhStatus) {	
		this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(true);
	}
	
	if(this.fvOpenChk == "Y"){
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);
	} else {
		//Tab-문제점발생상세
		if (iTabIndex == 0) {
			this.fn_SetGridInputButton();//[공통] 그리드 입력 버튼 설정
		//Tab-발생원인 및 대책
		} else if (iTabIndex == 1) {
			this.div_DetailInput.form.div_ComGrdBtn.set_visible(false); //비활성화		
		//Tab-문제점발생사진[이미지]
		} else if (iTabIndex == 2) {
			this.fn_SetGridImageFileButton();//[공통] 그리드 파일 버튼 설정[이미지]
		//Tab-구매현황
		} else if (iTabIndex == 3) {
			this.div_DetailInput.form.div_ComGrdBtn.set_visible(false); //비활성화		
		//Tab-첨부파일
		} else if (iTabIndex == 4) {	
			this.fn_SetGridFileButton();//[공통] 그리드 파일 버튼 설정
		}
	}	
};

