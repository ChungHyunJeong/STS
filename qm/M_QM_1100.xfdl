			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 12, "edittype", "none") ; //소분류
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 13, "edittype", "none") ; //증상명
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 15, "edittype", "none") ; //4M항목
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 16, "edittype", "none") ; //귀책구분
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 18, "edittype", "none") ; //명
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 20, "edittype", "none") ; //조치방안
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 21, "edittype", "none") ; //처리방안상세
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 23, "edittype", "none") ; //처리사번
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 26, "edittype", "none") ; //예상소요비용
	 		
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 27, "edittype", "checkbox") ; //필요유무
		//	this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 28, "edittype", "text") ; //검증기간
			//this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 31, "edittype", "text") ; //사번		
			
		}



