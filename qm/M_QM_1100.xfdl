		if (this.ds_Detail12.getRowCount() > 0) {
			
			this.gfn_SetCompGroupEnable(this.div_DetailInput.form.tab_Detail.tpg_01.form, true); 		
			
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 7, "edittype", "none") ; //자재코드
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 10, "edittype", "none") ; //품질비용
