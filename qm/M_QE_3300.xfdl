			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_MrthNm.setFocus();		   //포커스
		} else { 	//반납이상여부 N : 반납이상사유 비활성화
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_MrthNm.set_readonly(true); //비활성화
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "MRTH_NM", "");	   //반납이상사유 초기화
		}
	}
};


