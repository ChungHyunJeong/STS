/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Detail20] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{

	//체크시 해당 프로그램 전체선택
	if (e.columnid == "s_chk") {	
		if (this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "s_chk") == '1') {		
			var objSearchList = ["MEAM_NO|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "MEAM_NO") //계측기번호
								];		
			//[공통] Detail10 중복건 체크후 해당건 Skip
			if (this.gfn_DsSchDupcheck(this.ds_Detail10, objSearchList)) {
				var arrMsgArg = ["반납", "계측기번호"];
				this.gfn_Alert("QM003", arrMsgArg); //반납대상 계측기번호가 이미 존재합니다.					
				this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "s_chk","0");// Check 해제
				return;
			}			
			
		}	
	}
	
};


