 ***********************************************************************************************/
this.fn_DetailGrd01DeleteRow = function()
{
	var s_chk = "";//체크
	//계측기대여요청 내역 체크 가능하게 변경
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		s_chk = this.ds_Detail10.getColumn(iLoop, "s_chk");
		
		//체크
		if (s_chk == "1") {
		
			//검색값설정 - 컬럼명:검색값
			var objSearchList = ["MERH_NO|" + this.ds_Detail20.getColumn(iLoop, "MERH_NO") //대여요청번호
							   , "MERH_SQ|" + this.ds_Detail20.getColumn(iLoop, "MERH_SQ") //대여요청차수
							   , "MERB_SE|" + this.ds_Detail20.getColumn(iLoop, "MERB_SE") //대여요청순번
							   , "MERH_DD|" + this.ds_Detail20.getColumn(iLoop, "MERH_DD") //대여요청일자
							   , "MEAM_NO|" + this.ds_Detail20.getColumn(iLoop, "MEAM_NO") //계측기번호
								];		

			//[공통] 행삭제시 검색Tab 선택Row 해제 처리
			this.gfn_DeleteRowSchTabUncheck(this.ds_Detail20, objSearchList);
						
		}
	}

	this.gfn_GridDelProcess(this, this.ds_Detail10);//crud_flag 변경-삭제(D)	
};

