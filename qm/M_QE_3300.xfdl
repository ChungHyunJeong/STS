 this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
			
	//  반납이상여부 선택시
	if (e.columnid == "MRTH_YN") {
		var sMrthYn  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MRTH_YN");//반납이상여부
		
		if (sMrthYn == '1') {	//반납이상여부 Y : 반납이상사유 활성화, 포커스
