/***********************************************************************************************
 * 15.05. [예산정보] 예산업데이트 정보 생성
 ***********************************************************************************************/ 
this.fn_SetBGCM = function(sACCH_NO, sWOFH_SQ, sBGCM_GB)
{
	var sBGCM_YN = "";//예산통제계정여부
	this.ds_Input.clearData();
	this.ds_Master11.clearData();
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "ACCH_NO", sACCH_NO);//발의번호
	this.ds_Input.setColumn(0, "WOFH_SQ", sWOFH_SQ);//결재차수
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "fa:M_FA_1130_M_SL11";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "CHK_BGCM";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_SM;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Master11=ds_Master11";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.isSync = true;//동기식으로 설정할지 여부

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
	
	//예산통제 계정이 있는경우
	if (this.ds_Master11.getRowCount() > 0 && this.ds_Master11.getCaseCount("BGCM_YN == 'Y'") > 0) {
		
		var sDECR_GU = "";//차대구분
		var dBGAC_AM = 0;//금액
		
		for (var jLoop=0; jLoop<this.ds_Master11.getRowCount(); jLoop++) {
			sBGCM_YN = this.ds_Master11.getColumn(jLoop, "BGCM_YN");//예산통제계정여부
			if (sBGCM_YN == "Y") {
				iAddRow = this.ds_Master12.addRow();
				this.ds_Master12.copyRow(iAddRow, this.ds_Master11, jLoop);
				this.ds_Master12.setColumn(iAddRow, "crudType", "U");
				sDECR_GU = this.ds_Master12.getColumn(iAddRow, "DECR_GU");//차대구분
				
				//차대구분-차변
				if (sDECR_GU == "D") {
					dBGAC_AM = this.ds_Master12.getColumn(iAddRow, "DEBI_AM");//금액-차변금액
				} else {
					dBGAC_AM = this.ds_Master12.getColumn(iAddRow, "CRED_AM");//금액-대변금액
				}
				
				this.ds_Master12.setColumn(iAddRow, "BGAC_AM", dBGAC_AM);//금액
				this.ds_Master12.setColumn(iAddRow, "BGCM_GB", sBGCM_GB);//구분 : S-결의,A-발의
				
			}
		}
	}
}

