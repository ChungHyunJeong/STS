this.fn_SetWfCompEnable = function()
{
	this.div_MasterInput.form.btn_FiahY.set_enable(false);
	this.div_MasterInput.form.btn_FiahN.set_enable(false);
		
	//[결재]결재내역 팝업화면으로 호출여부
	if (!this.fvWfPopupMode) {
		//[결재] 공통버튼 제어
		var bCommSaveButton = false;//저장버튼 활성화 여부-기본값은 false로 해야함. - 결재승인후 수정일 필요한 화면에서만 true로 설정해야함. 
		//this.fvWfSearchMode = this.gfn_SetCommButtonWf(this, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"), bCommSaveButton);//결재진행상태		
		this.fvWofhStatus = this.gfn_SetCommButtonWf(this, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"), bCommSaveButton);//결재진행상태

	}
	
	//[결재]결재내역 조회모드이면 Visble false
	if (this.fvWfSearchMode || this.fvWofhStatus) { // 결재 진행된거
		//[결재]결재내역 팝업화면으로 호출여부
		if (this.fvWfPopupMode) {
			this.div_Search.set_enable(false);//조회영역 Enable처리
		}
		
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);
		//[결재] Div하위의 컴포넌트 Enable 처리
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
 		this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 입력	
 		this.div_DetailInput.form.btn_DivMoveDi.set_enable(true);
		
		//[전표보기]
		//결재상태-승인인경우 => [전표보기] 버튼 활성화
		if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST") == "0040") {
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(true);
		} else {
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(false);
		}
		
	} else { // 결재 진행안한거
		//[결재]결재내역 팝업화면으로 호출여부
		if (this.fvWfPopupMode) {	//h2팝업으로 열었을때
			this.div_Search.set_enable(true);//조회영역 Enable처리
		}
		
		//Master 입력/수정/삭제 상태 체크
		var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
		
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(true);
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
		
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcchNm.set_readonly(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_RemkPop.set_enable(true);
		//[전표보기] 결재진행 상태가 승인이 아닌경우 => [전표보기] 버튼 비활성화
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(false);
		
		this.div_MasterInput.form.btn_FiahY.set_enable(false);
		this.div_MasterInput.form.btn_FiahN.set_enable(false);
			
		if("I" == sChkRowType){
			this.div_MasterInput.form.btn_FiahY.set_enable(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_LeasYn.set_enable(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumCd.set_readonly(false);
			this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FiahMm.set_enable(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcumCd.set_enable(true);
		}
		else if(this.ds_Detail10.rowcount > 0){
			this.div_MasterInput.form.btn_FiahN.set_enable(true);
		}
		else if(this.ds_Detail10.rowcount == 0){
			this.div_MasterInput.form.btn_FiahY.set_enable(true);
		}
	}
	
}
