 * 02.MG0.03. [Master그리드-버튼] 부분선택 클릭 이벤트
 ***********************************************************************************************/
this.fn_MasterGrd01SomeCheck = function()
{
	var arrRows = this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.getSelectedRows();
	for (var iLoop=0; iLoop < arrRows.length; iLoop++) {
		//이미 선택한내용은 부분선택에서 제외 처리
		if (this.ds_Master10.getColumn(arrRows[iLoop], "s_conf") != "1") {
			this.ds_Master10.setColumn(arrRows[iLoop], "s_chk", "1");
		}
	}	
};

