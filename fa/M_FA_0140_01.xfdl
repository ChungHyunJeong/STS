 ***********************************************************************************************/
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "DTL_SEARCH": //조회		
			//파라메타 내용 추출
			this.fn_GetParam();
			break;		
			
		case "QUERY_MAKE_TEMP": //쿼리파일 생성		
			//생성된 쿼리조회
			this.fn_GetTempQuery();
			break;			
		case "MAKE_TEMP_LIST":
			
			var objGrd;
			var objDs;

			if (this.tab_Master.tabindex == 0) {
				objGrd = this.tab_Master.tpg_01.form.grd_QueryResultList;
				objDs = this.ds_FreeQueryResultList;
			} else {
				objGrd = this.tab_Master.tpg_02.form.grd_QueryResultList;
				objDs = this.ds_FreeQueryResultList2;
			}
		
			objGrd.set_formats("");
			
			if (objDs.getRowCount() > 0) {

				objGrd.set_enableredraw(false);
				objGrd.set_binddataset("");
				
				objGrd.createFormat();
				objGrd.appendContentsRow("head");
				objGrd.appendContentsRow("body");

				var iCol = objDs.getColCount();
				var indexCol = 0;
				var sColName = "";
				
				for (var iLoop = 0; iLoop < iCol; iLoop++) {
				
					if (iLoop > 0) {
						indexCol = objGrd.appendContentsCol();
					}

					sColName = objDs.getColID(iLoop);

					objGrd.setFormatColProperty(indexCol, "size", 100);
					objGrd.setCellProperty("head", indexCol, "text", sColName);
					objGrd.setCellProperty("body", indexCol, "text", "bind:" + sColName);					
					
				}				
				
				if (this.tab_Master.tabindex == 0) {
					objGrd.set_binddataset("ds_FreeQueryResultList");
				} else {
					objGrd.set_binddataset("ds_FreeQueryResultList2");
				}
				objGrd.set_enableredraw(true);	

			}

			break;			
	}
};

