this.fn_BodySearch = function()
{  
	if (this.ds_Master10.getRowCount() > 0) {

		//상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "AUTM_CD"))) {
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "AUTM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "AUTM_CD"));
					
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "fa:M_FA_0140_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_FA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);		
		}
	}
}  


//파라메타 내용 추출
this.fn_GetParam = function()
{

	if (this.ds_Master10.getRowCount() > 0) {
		var sQuery = this.ds_Master10.getColumn(0, "AUTM_GU");
		this.fn_CheckParam(sQuery, "@", 0);
	}

	if (this.ds_Detail10.getRowCount() > 0) {
		var sQuery = this.ds_Detail10.getColumn(0, "AUTD_GU");
		this.fn_CheckParam(sQuery, "@", 0);
	}
};

//파라메타 조회
this.fn_CheckParam = function(sQuery, sSearchWord, index) {

	var iChkIndex = sQuery.indexOf(sSearchWord, index);
	
	if (iChkIndex < 0) {
		return;
	} else {
		var sParamName = sQuery.substr(iChkIndex, 8);		
		var iFindRow = this.ds_Param.findRow("PARAM_ID", sParamName);
		
		if (iFindRow < 0) {
			var iAddRow = this.ds_Param.addRow();
			this.ds_Param.setColumn(iAddRow, "PARAM_ID", sParamName);
		}

		this.fn_CheckParam(sQuery, sSearchWord, (iChkIndex+1));
	}
	var sValue = "";
	for (var iLoop=0; iLoop < this.ds_Param.getRowCount(); iLoop++) {
		
		if (iLoop == 0) {
			sValue = "ST";
		} else if (iLoop == 1) {
			sValue = "2108030003";
		} else {
			sValue = "3";
		}
		
		this.ds_Param.setColumn(iLoop, "PARAM_VALUE", sValue);
	}
}

