this.fn_Search = function()
{ 


	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Input.clearData();
	this.ds_Item.clearData();	
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "ITEM_NM", this.div_Search.form.edt_ItemNm.value);
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "fa:" + this.fvFormId + "_SL00"; 
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid 		= "MST_SEARCH";			//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName 	= this.G_SV_FA;			//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset 	= "ds_Input=ds_Input" 	//입력 데이터셋
	svcCallObj.sOutDatset 	= "ds_Item=ds_Item";//출력 데이터셋
	svcCallObj.sOtherArg 	= "";					//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";		//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
} 


 /***********************************************************************************************
 *  ▶ 클릭 이벤트 
 ***********************************************************************************************/
this.btn_Add_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	//선택된 줄 추가
	var arrRows = this.grd_Master.getSelectedRows();
	var iAddRow = 0;
	
	for (var iLoop=0; iLoop < arrRows.length; iLoop++) {
		
		
		if(this.ds_ItemSelected.getRowCount() > 4){
			//선택 할 수 있는 관리항목은 5개 까지 입니다.
			this.gfn_Alert("FA006");
			return;
		}
	
		this.fn_copyRow(arrRows[iLoop]);
	}	
	
	
};


/***********************************************************************************************
 * 15.05. [ds_Detail20] 선택 copyRow
 ***********************************************************************************************/ 

this.fn_copyRow = function(row)
{
	
	if(this.ds_ItemSelected.getRowCount() > 4){
		//선택 할 수 있는 관리항목은 5개 까지 입니다.
		this.gfn_Alert("FA006");
		return;
	}
	
	
	var itemCd = this.ds_Item.getColumn(row, "ITEM_CD");
	var itemNm = this.ds_Item.getColumn(row, "ITEM_NM");
	
	//이미 선택된 관리항목이 있는지 찾기
	var iFvFindRow = this.ds_ItemSelected.findRowExpr("ITEM_CD=='" + itemCd+ "'");//업무Key Row 찾기
	
	if(iFvFindRow<=-1){
	// 선택된 관리항목이 없으면
		var iAddRow = this.ds_ItemSelected.addRow();
		this.ds_ItemSelected.setColumn(iAddRow, "ITEM_CD", itemCd);
		this.ds_ItemSelected.setColumn(iAddRow, "ITEM_NM", itemNm);
	}

}



 /***********************************************************************************************
 *  ◀ 버튼 클릭 이벤트 
 ***********************************************************************************************/
this.btn_Del_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var arrRows = this.grd_Detail.getSelectedRows();
	var iAddRow = 0;
	
	
	for (var iLoop=arrRows.length-1; iLoop >=0; iLoop--) {
		this.fn_delRow(arrRows[iLoop]);
	}
};


/***********************************************************************************************
 * 15.05. [ds_Detail] 선택 deleterow
 ***********************************************************************************************/ 
this.fn_delRow = function(row)
{
	this.ds_ItemSelected.deleteRow(row); 
}
