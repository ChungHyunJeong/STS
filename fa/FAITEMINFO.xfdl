this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
		
			if (!this.gfn_IsNull(arrRtnValNm)) {
				if (arrRtnValNm.length > 0) {
					for (var iLoop=0; iLoop < arrRtnValNm.length; iLoop++) {
						//row = this.ds_ItemSelected.addRow();
						var iFvFindRow = this.ds_Item.findRowExpr("ITEM_NM=='" + arrRtnValNm[iLoop]+ "'");//관리항목Key Row 찾기
						this.fn_copyRow(iFvFindRow);
					}
				}
			}
			

			break;
		case "DTL_SEARCH": //상세조회
		    break;
	}
};

