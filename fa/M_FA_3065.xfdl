				this.ds_Detail20.setColumn(iAddRow, "crudType", "I");
				this.fn_ComBtn20Save();
			}
		}else if(s_chk =="0" && sCrudType == "U"){
			//확정취소건
			this.ds_DetailDel.clearData();
			iAddRow = this.ds_DetailDel.addRow();
			this.ds_DetailDel.setColumn(iAddRow, "FIAM_NO", sFiamNo);//고정자산번호
			this.ds_DetailDel.setColumn(iAddRow, "crudType", "U");
