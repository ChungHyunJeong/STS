this.fn_MasterGrd01SomeCheck = function()
{
	var cntRows = 0;
	var arrRows = this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.getSelectedRows();
	for (var iLoop=0; iLoop < arrRows.length; iLoop++) {
		//이미 선택한내용은 부분선택에서 제외 처리
			
		var sSlipNo = this.ds_Master10.getColumn(arrRows[iLoop], "SLIP_NO");//결의일자
		
		if(this.gfn_IsNull(sSlipNo) || sSlipNo == ""){
			//this.gfn_Alert("FA048");//전표확정 자산만 신규/변동자산확정 할 수 있습니다.
			this.ds_Master10.setColumn(arrRows[iLoop], "s_chk", "0");
			cntRows = cntRows + 1;
			//return;
		}else{
			this.ds_Master10.setColumn(arrRows[iLoop], "s_chk", "1");
		}

		if (cntRows > 0 ){
			this.gfn_Alert("FA048");//전표확정 자산만 신규/변동자산확정 할 수 있습니다.
		}

	}	
};

