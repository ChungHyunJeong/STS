 * 02.MG0.02. [Master그리드-버튼] 전체해지 클릭 이벤트
 ***********************************************************************************************/
this.fn_MasterGrd01AllUnCheck = function()
{
	for (var iLoop=0; iLoop < this.ds_Master10.getRowCount(); iLoop++) {
		this.ds_Master10.setColumn(iLoop, "s_chk", "0");
	}
};

