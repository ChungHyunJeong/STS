/***********************************************************************************************
 * 02.MG0.01. [Master그리드-버튼] 전체선택 클릭 이벤트
 ***********************************************************************************************/
this.fn_MasterGrd01AllCheck = function()
{
	var cntRows = 0;
	for (var iLoop=0; iLoop < this.ds_Master10.getRowCount(); iLoop++) {
		
		var sSlipNo = this.ds_Master10.getColumn(iLoop, "SLIP_NO");//결의일자
		
		if(this.gfn_IsNull(sSlipNo) || sSlipNo == ""){
			//this.gfn_Alert("FA048");//전표확정 자산만 신규/변동자산확정 할 수 있습니다.
			this.ds_Master10.setColumn(iLoop, "s_chk", "0");
			cntRows = cntRows + 1;
			//return;
		}else{
			this.ds_Master10.setColumn(iLoop, "s_chk", "1");
		}


		if (cntRows > 0 ){
			this.gfn_Alert("FA048");//전표확정 자산만 신규/변동자산확정 할 수 있습니다.
		}
		
		
	}
};

