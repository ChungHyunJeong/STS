this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid == "VA54_VA"
	|| e.columnid == "VA55_VA"
	|| e.columnid == "VA56_VA"
	|| e.columnid == "VA57_VA"
	|| e.columnid == "VA58_VA"
	|| e.columnid == "VA59_VA"
	|| e.columnid == "VA60_VA")
	{
		//trace("1");
		this.setSum();
		//trace("2");
	}
};

this.setSum = function(){

	var va54Va = this.ds_Master10.getColumn(0, "VA54_VA");
	var va55Va = this.ds_Master10.getColumn(0, "VA55_VA");
	var va56Va = this.ds_Master10.getColumn(0, "VA56_VA");
	var va57Va = this.ds_Master10.getColumn(0, "VA57_VA");
	var va58Va = this.ds_Master10.getColumn(0, "VA58_VA");
	var va59Va = this.ds_Master10.getColumn(0, "VA59_VA");
	
	if(this.gfn_IsNull(va54Va)){va54Va=0};
	if(this.gfn_IsNull(va55Va)){va55Va=0};
	if(this.gfn_IsNull(va56Va)){va56Va=0};
	if(this.gfn_IsNull(va57Va)){va57Va=0};
	if(this.gfn_IsNull(va58Va)){va58Va=0};
	if(this.gfn_IsNull(va59Va)){va59Va=0};
	
	this.ds_Master10.setColumn(0, "VA60_VA", va54Va + va55Va + va56Va +  va57Va +  va58Va +  va59Va);
// 	
// 	this.ds_Master10.setColumn(0, "VA18_VA", va54Va + va55Va + va56Va +  va57Va +  va58Va +  va59Va);
		
}
