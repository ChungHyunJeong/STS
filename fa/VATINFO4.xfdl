this.fn_SelectRow = function(iRow)
{
	if (iRow > -1) {	
	
        var rtnValue = "({" +
						"VA54_VA:'" + this.ds_Master10.getColumn(0, "VA54_VA")  +"'"+ //
						",VA55_VA:'" + this.ds_Master10.getColumn(0, "VA55_VA") +"'"+ //
						",VA56_VA:'" + this.ds_Master10.getColumn(0, "VA56_VA") +"'"+ //
						",VA57_VA:'" + this.ds_Master10.getColumn(0, "VA57_VA") +"'"+ //
						",VA58_VA:'" + this.ds_Master10.getColumn(0, "VA58_VA") +"'"+ //
						",VA59_VA:'" + this.ds_Master10.getColumn(0, "VA59_VA") +"'"+ //
						",VA60_VA:'" + this.ds_Master10.getColumn(0, "VA60_VA") +"'"+ //
						
                "})";

					
