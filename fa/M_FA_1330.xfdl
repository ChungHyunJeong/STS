this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
this.fvSchKeyYn = false;//저장후 재조회여부
this.fvSchKey01 = "";//조회Key01
this.fvSchKey02 = "";//조회Key02
this.fvSchKey03 = "";//조회Key03

/***********************************************************************************************
 * 00.03. Form Onload 이벤트
 ***********************************************************************************************/
this.fn_FormOnload = function (obj:Form, e:LoadEventInfo)
{
	this.fn_FormComInit();//[공통] 화면로드시 초기 설정 함수
	this.fn_FormBizInit();//[업무] 화면로드시 초기 설정 함수 
}

/***********************************************************************************************
 * 00.04. Form [공통] 초기설정 함수 - 필수
 ***********************************************************************************************/
this.fn_FormComInit = function() 
{
	this.gfn_CommFormOnLoad(this);//[공통] 화면로드시 처리 함수(다국어등)
	this.fvComPrgmId = this.gfn_CommLoadFormPrgmId(this);//[필수]프로그램ID-메인화면기준, 팝업일경우 해당 메인화면의 프로그램ID

	this.gfn_SetGrid(this.div_MasterInput.form.tab_Detail.tpg_01.form.grd_Master10);//[공통]그리드 이벤트 설정
	
}

/***********************************************************************************************
 * 00.05. Form [업무] 초기설정 함수
 ***********************************************************************************************/
this.fn_FormBizInit = function() 
{ 
	//년도설정
	var rtnValue = this.gfn_GetCommDateObj();
	var toDay = rtnValue.toDay;
	this.div_Search.form.cal_CrbhMm.set_value(toDay.substr(0,6));
	
	this.div_Search.form.edt_AcumCd.set_value(this.gfn_GetSessionValue("ACUM_CD"));
	this.div_Search.form.edt_AcumNm.set_value(this.gfn_GetSessionValue("ACUM_NM"));

	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	var objCondList = ["FA310|ds_Fa310","FA312|ds_Fa312","SD102|ds_Sd102|Y|선택"];
	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, "");
		
	//[업무] 일반콤보조회
	this.fn_ComboSearch();
}

/***********************************************************************************************
 * 00.06. Form [업무] 일반콤보 조회 함수
 ***********************************************************************************************/
this.fn_ComboSearch = function() 
{

	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Input.clearData();
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0,"AGIN_YN","1");

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
		
	//[공통]서비스정보 추가
	svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "fa:M_FA_ACOM_M_SL11";	//계정코드 조회
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj 			= new Object();				//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid 		= "COMBO_SEARCH";		//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName 	= this.G_SV_FA;				//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sActionName 	= "";				//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조	
	svcCallObj.sInputDaset 	= "ds_Input=ds_Input" 		//입력 데이터셋
	svcCallObj.sOutDatset 	= "ds_FaAcom=ds_Master10";	//출력 데이터셋
	svcCallObj.sOtherArg 	="";						// "key=" + this.gfn_GetSessionValue("COMM_CD");//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";			//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);

}

/***********************************************************************************************
 * 00.07. Form OnClose 이벤트
 ***********************************************************************************************/
this.fn_FormClose = function(obj:nexacro.Form,e:nexacro.CloseEventInfo)
{
	return true;
};

/***********************************************************************************************
 * 1. 조회 영역
 ***********************************************************************************************/
/***********************************************************************************************
 * 01.01. [공통버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.fn_ComBtnSearch = function()
{ 
	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;	
	
	//keystring 이벤트 활성/비활성화
	var paboGu = this.div_Search.form.cbo_PaboGu00
	if(paboGu.index == 0 ) {
		this.ds_Master10.set_keystring("G:+ACOM_CD");
	} else {		
		this.ds_Master10.set_keystring("");
	}
	
	//그리드CELL TEXT 변경
	for(var i=0; i<=36; i++){
		var sMM = this.gfn_AddMonth(this.div_Search.form.cal_CrbhMm.value, (i*-1));
		this.div_MasterInput.form.tab_Detail.tpg_01.form.grd_Master10.setCellProperty( "head", (i+24), "text", sMM.substr(0,4)+"년"+sMM.substr(4,2)+"월");
	}
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Master10.clearData();
	this.ds_Input.clearData();
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_CrbhMm.value);
	this.ds_Input.setColumn(0, "CLOS_DD", this.div_Search.form.cal_CrbhMm.value);
	this.ds_Input.setColumn(0, "SESU_ID", this.gfn_GetSessionValue("SYST_ID"));
	this.ds_Input.setColumn(0, "CRBH_MM", this.div_Search.form.cal_CrbhMm.value.substr(0,6));
	this.ds_Input.setColumn(0, "ACUM_CD", this.div_Search.form.edt_AcumCd.value);
	this.ds_Input.setColumn(0, "CNTH_NO", this.div_Search.form.mae_CnthNo.value);
	this.ds_Input.setColumn(0, "CUSM_CD", this.div_Search.form.edt_CusmCd.value);
	this.ds_Input.setColumn(0, "ACOM_CD", this.div_Search.form.cbo_PaboGu00.value);
	this.ds_Input.setColumn(0, "CRCS_GU", this.div_Search.form.cbo_CrcsGu.value);
	
	this.ds_Input.setColumn(0, "PRGM_ID", "M_FA_1330");//프로그램ID	

	//[공통]서비스정보 초기화
