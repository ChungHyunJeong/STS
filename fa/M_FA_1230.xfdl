this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{


	
	//trace("nErrorCode = " + nErrorCode);
	//trace("sSvcId = " + sSvcId);
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "SEARCH":
			break;
		case "MST_SEARCH": //조회
		
			//조회
			var sMaCnt = parseInt(this.ds_Master10.getColumn(0, "SMA_CNT"));
			var arrColMaster = ["SMA01_NM", "SMA02_NM", "SMA03_NM", "SMA04_NM", "SMA05_NM"];
			var arrColDetailCd = ["SDE01_CD", "SDE02_CD", "SDE03_CD", "SDE04_CD", "SDE05_CD"];
			var arrColDetailNm = ["SDE01_NM", "SDE02_NM", "SDE03_NM", "SDE04_NM", "SDE05_NM"];
			var nIdxBody=0;
			var grdObj =this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10;
			var sText  = "";
			
			

			for (var idx=0; idx < 5 ; idx++) 
			{
				nIdxBody = 20 + idx;//grdObj.getBindCellIndex("Body",arrColDetail[idx]);
				
				
				//SM01_NM, SM02_NM, SM03_NM, SM04_NM, SM05_NM
				
				sText = this.ds_Master10.getColumn(0, "SMA0"+(idx + 1)+"_NM");
				
				//trace("("idx = " + idx);
				//trace("("sMaCnt = " + sMaCnt);
				//trace("("sText = " + sText);
				//0~4 < 3
				if(idx < sMaCnt){
					//trace("("nIdxBody =="+ nIdxBody);
					//trace("("idx ===== " + idx);
					grdObj.setCellProperty("head", nIdxBody, "text", sText);
					grdObj.setRealColSize("body",nIdxBody + 22 + idx, 80, false );  //44
					grdObj.setRealColSize("body",nIdxBody + 23 + idx, 100, false ); //45
				}else{
					grdObj.setRealColSize("body",nIdxBody + 22 + idx, 0, false );
					grdObj.setRealColSize("body",nIdxBody + 23 + idx, 0, false );
				}
			}
			
			//전월이월
			for (var iLoop = 0; iLoop < this.ds_Master10.getRowCount(); iLoop++) {
				var sRowLevel = this.ds_Master10.getRowLevel(iLoop);
				var sSlipMm = this.ds_Master10.getColumn(iLoop, "SLIP_MM");
				if (sRowLevel == 1 && (this.gfn_IsNull(sSlipMm) || sSlipMm=="")) { //전월이월
					
					this.ds_Master10.deleteRow(iLoop);
				}
			}
			
		/*
		
			this.ds_Master10.addColumn("s_chk", "string");
			this.ds_Master10.addColumn("crudType", "string");
	

			if (this.ds_Master10.getRowCount() > 0) {

				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
				    var iFvFindRow = this.ds_Master10.findRowExpr("COMM_CD=='" + this.fvSchKey01 +  "' && SYST_ID=='" + this.fvSchKey02+  "' && PABO_GU=='" + this.fvSchKey03+ +  "' && DEDD_CD=='" + this.fvSchKey04+ "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}				
	
				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
				
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10);
				
		
			} else {
				
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10);
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			
				//입력영역 비활성화
				//this.div_MasterInput.set_enable(false);
			}
			
			
			
			
			
			//저장후 재조회여부-true
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}	
			
			*/

			this.sortInfo = 0;
			this.fn_Ds_Master10GridSort01();
			
			break;
		case "SAVE":   //저장
			this.fvSchKeyYn = true;//저장후 재조회여부 
			this.fn_ComBtnSearch();
			
			break;
			
		case "DELETE": //삭제		
			this.fn_ComBtnSearch();//조회
			break;	
			
		case "EXCEL_DOWN":   //엑셀다운로드
			//trace("(this.ds_ExcelResult.saveXML());
			if (this.ds_ExcelResult.getRowCount() > 0) {
				var sRSLT_CD = this.ds_ExcelResult.getColumn(0, "RSLT_CD");//결과코드 : S-성공, E-에러
				var sRSLT_MG = this.ds_ExcelResult.getColumn(0, "RSLT_MG");//에러메시지 / 파일명
				
				//성공이면서 파일명이 있으면
				if (sRSLT_CD == "S" && !this.gfn_IsNull(sRSLT_MG)) {
					this.fn_ExcelFileDownload(sRSLT_MG);//엑셀파일 다운로드
				}
				
			} else {
			}
			break;			
	}
};

/***********************************************************************************************
 * 13.02. [팝업] CallBack 함수 처리
          strId : 팝업호출시 지정한 팝업ID
	      rtnValue : 팝업창에서 close시 지정한 반환값
          주의 : Open(popuptype=modeless)창은 팝업콜백을 지원하지 않음
 ***********************************************************************************************/ 
this.fn_PopupCallback = function(strId, rtnValue)
{
    //[조회영역]
	//부서
	if (strId == "S_DEPM_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);
			
			if (!this.gfn_IsNull(arrRtnVal.DEPM_CD)) {
				this.div_Search.form.edt_DepmCd.set_value(arrRtnVal.DEPM_CD);
				this.div_Search.form.edt_DepmNm.set_value(arrRtnVal.DEPM_NM);
			}
		} else {
				this.div_Search.form.edt_DepmCd.set_value("");
				this.div_Search.form.edt_DepmNm.set_value("");
			}
	//사원
	} else if (strId == "S_SYST_ID_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.EMPM_CD)) {
				this.div_Search.form.edt_EmpmCd.set_value(arrRtnVal.EMPM_CD);
				this.div_Search.form.edt_EmpmNm.set_value(arrRtnVal.EMPM_NM);
				this.div_Search.form.edt_SystId.set_value(arrRtnVal.SYST_ID);				
			}
		} else {
				this.div_Search.form.edt_EmpmCd.set_value("");
				this.div_Search.form.edt_EmpmNm.set_value("");
				this.div_Search.form.edt_SystId.set_value("");				
			}
	//회계단위
	} else if(strId == "S_ACUM_CD_01"){
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.ACUM_CD)) {
				this.div_Search.form.edt_AcumCd.set_value(arrRtnVal.ACUM_CD);
				this.div_Search.form.edt_AcumNm.set_value(arrRtnVal.ACUM_NM);
			}
		} else {
				this.div_Search.form.edt_AcumCd.set_value("");
				this.div_Search.form.edt_AcumNm.set_value("");
			}
		
	//계정코드1
	} else if(strId == "S_ACOM_CD_01"){
	
		
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			
			trace("=========S_ACOM_CD_01" + arrRtnVal.ACOM_CD);
			
			if (!this.gfn_IsNull(arrRtnVal.ACOM_CD)) {
			
				this.div_Search.form.edt_AcomCdFr.set_value(arrRtnVal.ACOM_CD);
				this.div_Search.form.edt_AcomNmFr.set_value(arrRtnVal.ACOM_NM);
				this.div_Search.form.edt_AcomCdTo.set_value(arrRtnVal.ACOM_CD);
				this.div_Search.form.edt_AcomNmto.set_value(arrRtnVal.ACOM_NM);
				 
				}
		}else{
			if (!this.gfn_IsNull(this.div_Search.form.edt_AcomNmFr.value)){
				this.div_Search.form.edt_AcomCdTo.set_value(this.div_Search.form.edt_AcomCdFr.value);
				this.div_Search.form.edt_AcomNmto.set_value(this.div_Search.form.edt_AcomNmFr.value);
			}
		}
		
		
		
		
	//계정코드2
	} else if(strId == "S_ACOM_CD_02"){
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.ACOM_CD)) {
				this.div_Search.form.edt_AcomCdTo.set_value(arrRtnVal.ACOM_CD);
				this.div_Search.form.edt_AcomNmto.set_value(arrRtnVal.ACOM_NM);
			}
		} else {
				this.div_Search.form.edt_AcomCdTo.set_value("");
				this.div_Search.form.edt_AcomNmto.set_value("");
			}
	//[Master영역]
	//사번
	} else if (strId == "B_SYST_ID_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.EMPM_CD)) {
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EMPM_CD", arrRtnVal.EMPM_CD);
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EMPM_NM", arrRtnVal.EMPM_NM);
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "SYST_ID", arrRtnVal.SYST_ID);
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DEPM_CD", arrRtnVal.DEPM_CD);
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DEPM_NM", arrRtnVal.DEPM_NM);
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DUTY_CD", arrRtnVal.DUTY_CD);
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "POSI_CD", arrRtnVal.POSI_CD);
			}
		} else {
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EMPM_CD", "");
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EMPM_NM", "");
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "SYST_ID", "");
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DEPM_CD", "");
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DEPM_NM", "");
