/***********************************************************************************************
 * 02.DG0.09. [Detail그리드 엑셀버튼]  클릭 이벤트
 ***********************************************************************************************/
this.div_DetailInput_btn_XlsxDown_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	// 엑셀버튼 이벤트	

	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;
	
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_ExcelInput.clearData();

	//[업무] 조회조건 입력
	this.ds_ExcelInput.addRow();
	this.ds_ExcelInput.setColumn(0, "G_COMM", this.gfn_GetSessionValue("COMM_CD"));//회사코드
	this.ds_ExcelInput.setColumn(0, "FROM_DD", this.div_Search.form.cal_TemhDtFr.value);
	this.ds_ExcelInput.setColumn(0, "CLOS_DD", this.div_Search.form.cal_TemhDtTo.value);
	//this.ds_ExcelInput.setColumn(0, "FROM_DD", "20180101");
	//this.ds_ExcelInput.setColumn(0, "CLOS_DD", "20211103");	
	this.ds_ExcelInput.setColumn(0, "ACUM_CD", this.div_Search.form.edt_AcumCd.value);
	this.ds_ExcelInput.setColumn(0, "ACOM_CD1", this.div_Search.form.edt_AcomCdFr.value);
	this.ds_ExcelInput.setColumn(0, "ACOM_CD2", this.div_Search.form.edt_AcomCdTo.value);
	//this.ds_ExcelInput.setColumn(0, "ACOM_CD1", "101010101000");
	//this.ds_ExcelInput.setColumn(0, "ACOM_CD2", "601010105000");	
	this.ds_ExcelInput.setColumn(0, "SHET_GB", this.div_MasterInput.form.rdo_ShetGb.value);//시트출력구분:A-한Sheet에 전체 출력, C-계정별로 Sheet 출력
	this.ds_ExcelInput.setColumn(0, "MA_LIST", this.div_Search.form.edt_SelItemCd.value);
	
	this.ds_ExcelInput.setColumn(0, "EXCEL_YN", "1"); //엑셀변환
	
	this.ds_ExcelResult.clearData();	
	
   	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);	
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "EXCEL_DOWN";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = "ExcelAction";//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sMethodName = "makeExcelFile";// Action 안에서 호출해야 하는 메소드 명, 기본은 execute를 호출하도록 되어있음	
	svcCallObj.sInputDaset = "ds_ExcelInput=ds_ExcelInput" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_ExcelResult=ds_ExcelResult";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명	
	
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);		

	/* 
	  프로시져 FAS..S_FA_1230_01
	
		@G_COMM  NVARCHAR(10),          // 회사코드 
		@ACUM_CD NVARCHAR(20),          // 회계단위 
