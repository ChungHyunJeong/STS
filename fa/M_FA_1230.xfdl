				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "POSI_CD", "");
			}
	//관리항목
	} else if(strId == "S_ITEM_NM_01"){	
		if (!this.gfn_IsNull(rtnValue)) {
			//trace("rtnValue  ===  " + rtnValue);
			
			var arrRtnVal01 = rtnValue.split("@^&");
			var arrRtnVal02;
			var itemCd = "", itemNm = "";
			
			for(var iLoop = 0; iLoop < arrRtnVal01.length; iLoop++) {
				
				arrRtnVal02 = arrRtnVal01[iLoop].split(";;");
				
				if(iLoop==0){
					itemCd = arrRtnVal02[0];
					itemNm = arrRtnVal02[1];
				}else{
					itemCd = itemCd + "," + arrRtnVal02[0];
					itemNm = itemNm + "," + arrRtnVal02[1];
				}
				
			}
			this.div_Search.form.edt_SelItemCd.set_value(itemCd);
			this.div_Search.form.edt_SelItemNm.set_value(itemNm);
			
		}
		
	}else{
			this.div_Search.form.edt_SelItemCd.set_value("");
			this.div_Search.form.edt_SelItemNm.set_value("");
		}
};
 
