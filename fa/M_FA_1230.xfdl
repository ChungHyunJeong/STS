this.fn_Ds_Master10GridSort = function (obj){

	trace("fn_Ds_Master10GridSort============");
	var sortInfos = obj.sortInfos;
	var sortItems = obj.sortItems;
	
	//그룹초기화
	this.ds_Master10.set_keystring("");
	
	if ( this.gfn_IsNull(sortInfos) || this.gfn_IsNull(sortItems) ) return;

	// 정렬상태 초기화.
	trace("sortItems.length = " + sortItems.length);
	var col;
	var sortInfo;
	for( var j=0; j<sortItems.length;j++){
		col = sortItems[j];
		trace("col = " + col);
		sortInfo = sortInfos[col];
		trace("sortInfo = " + sortInfo);
	}
	
	//정렬상태값 0: 없음, 1 ASC, 2 DESC 
	//이지만 상태값은 전단계상태값만 가져온다.
	trace("sortInfo.status = " + sortInfo.status);
	if(sortInfo.status != "1"){
		this.ds_Master10.set_keystring("");
	}else{
		//정렬이 없을시 다시 그룹화 한다.
		this.ds_Master10.set_keystring("S:ACOM_CD+NUM,G:ACOM_CD,G:ACOM_CD+SLIP_MM");
	}
}


