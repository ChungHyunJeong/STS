/***********************************************************************************************
 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
			if (this.ds_Master10.getRowCount() > 0) {
				//[결재] 결재상태에 따른 컴포넌트 제어			
				this.fn_SetWfCompEnable();	
			
				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master10.findRowExpr("NFAH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ=='" + this.fvSchKey02 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}				

				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
									
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterView.form.grd_Master10);

				//조회 후 신규자산내역 Tab이동
				this.div_MasterInput.form.tab_Master.set_tabindex(0);
				
				//조회시 구매입고검색 비활성화(10/12 설계자확인) - 입력시 활성화
				this.div_MasterInput.form.tab_Master.tpg_02.set_enable(false);

			} else {
				
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterView.form.grd_Master10);
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			
				//입력영역 비활성화
				this.div_MasterInput.set_enable(false);
					
			}
			
			//저장후 재조회여부-true
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}			
	
			break;
		case "SAVE":   //저장		

			this.fvSchKeyYn = true;//저장후 재조회여부
			this.fn_ComBtnSearch();//저장후 재조회
			
			break;	
			
		case "MSTS_SEARCH": //신규자산관리대상검색		
			this.ds_Master20.addColumn("s_chk", "string");		
			this.ds_Master20.addColumn("s_conf", "string");
			
			if (this.ds_Master20.getRowCount() > 0) {
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Master20);
			} else {
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterInput.form.tab_Master.tpg_02.form.grd_Master20);			
			}
			
			break;			

		case "DELETE": //삭제		
			this.fn_ComBtnSearch();//조회
			break;	

		case "WF_RECIDE": //재기안처리		
			if (nApp.gds_WfRst.getRowCount() > 0) {
				if (nApp.gds_WfRst.getColumn(0, "O_RET_CODE") == "TRUE") {
					//1. 고정자산마스터에 데이터 유무 확인
					//2. 발주구분 전산자산 대상 및 존재유무 체크
					//3. 재기안시 고정자산마스터 데이터DELE_YN 처리
					//4. 발주구분 전산자산 대상시 전산자산마스터, 자산운영관리 USED_YN 처리
					
					var sFaFiamYn = this.fn_GetFaFiamDupCheck();	//고정자산마스터에 데이터 유무 확인

					if (sFaFiamYn) { //고정자산마스터 데이터 존재시		
						this.fn_GetEdmmEdahDupCheck();	//발주구분 전산자산(0060) 대상 및 존재유무(고정자산 처리전에 확인해야 함)					
						this.fn_RecideFiamDelete();	//고정자산마스터 처리함수 호출
						
					} else {
						this.fvSchKeyYn = true;//저장후 재조회여부
						this.fn_ComBtnSearch();//재기안 재조회					
					}

					this.gfn_Alert("WF012");//재기안 처리가 완료되었습니다.

				} else {
					var arrMsgArg = [nApp.gds_WfRst.getColumn(0, "O_RET_CMSG")];
					this.gfn_Alert("WF013", oArg);//재기안 처리시 오류가 발생하였습니다.\n&ITM0				
				}
			} else {
				var arrMsgArg = [""];
				this.gfn_Alert("WF013", arrMsgArg);//재기안 처리시 오류가 발생하였습니다.\n&ITM0
			}
			break;					

		case "RECIDE_FIAM": //재기안시 고정자산마스터(DELE_YN = 1) 처리
			if (this.ds_SmEdmmDup.getRowCount() > 0) {
				this.fn_RecideEdmmDelete();	//전산자산마스터, 자산운영관리 처리
				
			} else {
				this.fvSchKeyYn = true;//저장후 재조회여부
				this.fn_ComBtnSearch();//재기안 재조회
			}
			
			break;				

		case "RECIDE_EDMM": //재기안시  전산자산마스터, 자산운영관리 삭제(USDE_YN = 0) 처리
		
