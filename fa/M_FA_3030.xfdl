/***********************************************************************************************
 *    04. [적용]
 ***********************************************************************************************/
this.fn_MasterGrd02Choice = function()
{
	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Master20.getCaseCount("s_chk=='1'") > 0) {
		
			//1. Master20 --> Master10 복사
			var arrCheckRow = [];//체크된 Row
			var nRow = this.ds_Master10.rowposition;
			var iIpgoQt = 0;	//입고수량
			var iWoneAm = 0;	//입고금액
			var iFsgtAm = 0;	//취득원가
			for (var iLoop=0; iLoop<this.ds_Master20.getRowCount(); iLoop++) {
			
				if (this.ds_Master20.getColumn(iLoop, "s_chk") == "1") {
					//Master10에 적용되어 있는지 체크
					var objSearchList = ["PCWH_NO|" + this.ds_Master20.getColumn(iLoop, "PCWH_NO") //입고번호
										,"PCWB_SE|" + this.ds_Master20.getColumn(iLoop, "PCWB_SE") //입고순번
										];		
										
					//[공통] Master10 중복건 체크후 해당건 Skip
					//수량 개별 등록으로 변경되어 중복체크 제외처리.
					//if (!this.gfn_DsSchDupcheck(this.ds_Master10, objSearchList)) {
						
						//선택항목 비활성화처리 (색상변경 및 체크박스 Lock)
						this.ds_Master20.setColumn(iLoop, "s_conf", "1");
						this.ds_Master20.setColumn(iLoop, "s_chk", "0");//Uncheck로 변경

						//적용 대상 복사 (Master20 -> Master10)
						this.ds_Master10.copyRow(nRow, this.ds_Master20, iLoop);	//Row 복사
						 
						//Master10과 Master20의 컬럼값이 다른 대상 복사
						iIpgoQt = this.gfn_Nvl(this.ds_Master20.getColumn(iLoop, "IPGO_QT"), 1);
						iWoneAm = this.gfn_Nvl(this.ds_Master20.getColumn(iLoop, "WONE_AM"), 0);
						iFsgtAm = nexacro.floor(nexacro.toNumber(iWoneAm) / nexacro.toNumber(iIpgoQt), 0);	//취득원가 = 입고금액/입고수량
						this.ds_Master10.setColumn(nRow, "NFAH_DD", this.ds_Master20.getColumn(iLoop, "PCWH_DD")); //취득일자 = 입고번호
						this.ds_Master10.setColumn(nRow, "NFAH_NM", this.ds_Master20.getColumn(iLoop, "ITEM_NM")); //신규자산명(품목명)
						this.ds_Master10.setColumn(nRow, "FSGT_AM", iFsgtAm); //최초취득가액(입고금액)

						//상각구분, 법정내용연수 적용
						var sAcomCd = this.ds_Master20.getColumn(iLoop, "ACOM_CD");
						if (!this.gfn_IsNull(sAcomCd)) {			
							var iFindRow = this.ds_FaAcom.findRowExpr("ACOM_CD=='" + sAcomCd + "'"); //해당계정코드의 Row
							var sFiamGu = this.ds_FaAcom.getColumn(iFindRow, "FABM_CD");	//상각구분
							var iFasmCt = this.ds_FaAcom.getColumn(iFindRow, "FASM_CT");	//법정내용연수

							if (sFiamGu == '*') sFiamGu = ''; //상각구분이 '*'이면 null			
							
							this.ds_Master10.setColumn(nRow, "FIAM_GU",sFiamGu);	//상각구분
							this.ds_Master10.setColumn(nRow, "FASM_CT",iFasmCt);	//법정내용연수
						}

						var sAcchNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition,"ACCH_NO");	//발의번호
						this.fn_AcchNoFormat(sAcchNo);
						
						//상각구분에 따른 활성화/비활성화 (11/29)						
						this.fn_SetFiamGuCompEnable();
						
						arrCheckRow.push(iLoop );

						//선택항목 비활성화처리
						for (var jLoop=0; jLoop<this.ds_Master20.getRowCount(); jLoop++) {	//이전 선택대상 비활성화
							this.ds_Master20.setColumn(jLoop, "s_conf", "0");
						}
						
						this.ds_Master20.setColumn(iLoop, "s_conf", "1");
						this.ds_Master20.setColumn(iLoop, "s_chk", "0");//Uncheck로 변경
					//수량 개별 등록으로 변경되어 중복체크 제외처리.
					//}	
					
					break; //체크된 건이 1건이므로 1건 처리 후 종료
				}
			}
			
			this.ds_Master20.set_rowposition(-1);
			
			this.fn_GetFabm(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ACOM_CD"));
			
			//적용 후 신규자산내역 Tab이동
			this.div_MasterInput.form.tab_Master.set_tabindex(0);
			
		}
	}
};

/***********************************************************************************************
 * 02.DG1.06. [Detail그리드2-버튼] 취소 클릭 이벤트
 ***********************************************************************************************/
this.fn_MasterGrd02Cancel = function()
{
	var nRow = this.ds_Master10.rowposition;
	this.ds_Master20.reset();

	//Master10 최초정보 초기화
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자

	this.ds_Master10.setColumn(nRow, "NFAH_DD", sToDay); //취득일자 = 입고번호
