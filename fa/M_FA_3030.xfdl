this.fn_ComBtnAdd = function()
{
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
		
	var iAddRow = this.ds_Master10.addRow();
	this.fvMmCloseDay = sToDay;//[월마감] 마감일자
	
	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", "1");//결재차수
	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");//최종여부	
	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//관리부서코드	
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//관리부서명	
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//관리자사번	
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//관리자명		
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//관리자ID	
	this.ds_Master10.setColumn(iAddRow, "ACUM_CD", this.gfn_GetSessionValue("ACUM_CD"));//회계단위
	this.ds_Master10.setColumn(iAddRow, "ACUM_NM", this.gfn_GetSessionValue("ACUM_NM"));//회계단위명	
	this.ds_Master10.setColumn(iAddRow, "LEAS_YN", "0");//리스구분 - 일반(0 Default)
	this.ds_Master10.setColumn(iAddRow, "ACQU_GU", "1");//취득구분 - 구매취득(1 Default)
	this.ds_Master10.setColumn(iAddRow, "NFAH_DD", sToDay);//취득일자-오늘날자로 설정

	//신규자산내역 Tab이동
	this.div_MasterInput.form.tab_Master.set_tabindex(0);

	//입력시 구매입고검색 활성화(10/12 설계자확인) - 조회시 비활성화
	this.div_MasterInput.form.tab_Master.tpg_02.set_enable(true);

	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	this.fn_SetWfCompEnable();	

	//입력버튼 클릭시 구매입고검색 Tab이동
	this.div_MasterInput.form.tab_Master.set_tabindex(1);

	//처음 입력 항목에 포커스
	this.div_MasterInput.form.tab_Master.tpg_01.form.cal_NfahDd.setFocus();

	this.ds_Master20.clearData();//검색정보 초기화

	//신규자산관리대상검색 초기화/활성화
	/* 로그인시 부서/사용자로 기본셋팅으로 초기화 제외
	this.div_MasterInput.form.tab_Master.tpg_02.form.div_Search.form.edt_DepmCd.set_value(""); //입고부서코드
	this.div_MasterInput.form.tab_Master.tpg_02.form.div_Search.form.edt_DepmNm.set_value(""); //입고부서명
	this.div_MasterInput.form.tab_Master.tpg_02.form.div_Search.form.edt_EmpmCd.set_value(""); //입고자코드
	this.div_MasterInput.form.tab_Master.tpg_02.form.div_Search.form.edt_EmpmNm.set_value(""); //입고자명
	*/
	
	this.div_MasterInput.form.tab_Master.tpg_02.form.div_Search.form.edt_DepmCd.set_readonly(false); //입고부서코드
	this.div_MasterInput.form.tab_Master.tpg_02.form.div_Search.form.btn_DepmCd.set_enable(true); //입고부서버튼
	this.div_MasterInput.form.tab_Master.tpg_02.form.div_Search.form.edt_EmpmCd.set_readonly(false); //입고자코드
	this.div_MasterInput.form.tab_Master.tpg_02.form.div_Search.form.edt_EmpmNm.set_readonly(false); //입고자명
	this.div_MasterInput.form.tab_Master.tpg_02.form.div_Search.form.btn_SystId.set_enable(true); //입고자버튼
	
	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
}

