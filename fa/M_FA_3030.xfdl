			//[공통] Master10 중복건 체크후 해당건 Skip
			/*
			if (this.gfn_DsSchDupcheck(this.ds_Master10, objSearchList)) {
				this.gfn_Alert("SM041"); //중복된 데이터가 존재합니다.
				this.ds_Master20.setColumn(this.ds_Master20.rowposition, "s_chk", '0');// Check 해제
				return false;
			}
