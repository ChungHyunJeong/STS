		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FIAM_RT", 0);
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "TEMP_RT", 0);
		
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_FiamRt.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_TempRt.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_FiamAm.set_readonly(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_TempAm.set_readonly(false);	
	}
	
