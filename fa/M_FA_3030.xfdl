		}
		
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_AcchNo.set_format(sFormat);
	} else {
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(false);
