// 상각방법 계산 
this.fn_Calc = function()
{
		
	//정율법
	if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FIAM_GU")=="1")
    {
		var rt = nexacro.toNumber(this.fn_rateSearch(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FASM_CT")));
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FIAM_RT", rt);
    }
	//정액법
	else if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FIAM_GU")=="2")
    {
		if(this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FASM_CT")) 
			|| nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FASM_CT")) <= 0 ) return;
			
		var am = nexacro.round(nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FSGT_AM"))/nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FASM_CT")));
			
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FIAM_AM", am);
    }
	
}
