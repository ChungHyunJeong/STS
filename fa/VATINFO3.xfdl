this.setSum = function(){

	var va50Am = this.ds_Master10.getColumn(0, "VA50_AM");
	var va50Va = this.ds_Master10.getColumn(0, "VA50_VA");
	var va51Am = this.ds_Master10.getColumn(0, "VA51_AM");
	var va51Va = this.ds_Master10.getColumn(0, "VA51_VA");
	var va52Am = this.ds_Master10.getColumn(0, "VA52_AM");
	var va52Va = this.ds_Master10.getColumn(0, "VA52_VA");
	
	if(this.gfn_IsNull(va50Am)){va50Am=0};
	if(this.gfn_IsNull(va50Va)){va50Va=0};
	if(this.gfn_IsNull(va51Am)){va51Am=0};
	if(this.gfn_IsNull(va51Va)){va51Va=0};
	if(this.gfn_IsNull(va52Am)){va52Am=0};
	if(this.gfn_IsNull(va52Va)){va52Va=0};
	
	this.ds_Master10.setColumn(0, "VA53_AM", va50Am + va51Am + va52Am);
	this.ds_Master10.setColumn(0, "VA53_VA", va50Va + va51Va + va52Va);
	
