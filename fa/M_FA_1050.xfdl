/***********************************************************************************************
 * 8. 인쇄
 ***********************************************************************************************/
/***********************************************************************************************
 * 08.01. [공통버튼] 인쇄 버튼 클릭 이벤트
 ***********************************************************************************************/  
this.fn_ComBtnPrint = function()
{ 
	var sMultiAcchNo = "";//발의번호-다건
	var sAcchNo = "";//발의번호

	//체크된 내역이 있으면
	if (this.ds_Master10.getCaseCount("s_chk=='1'") > 0) {
		//일괄인쇄 건수 제한      
		if (this.ds_Master10.getCaseCount("s_chk=='1'") > 100 ) {
			this.gfn_Alert("SM121");//일괄인쇄는 최대 100건까지 가능합니다.
			return;
		}
				
		this.div_MasterInput.form.pgb_PrintState.set_min(0);		
		
		for (var iLoop=0; iLoop<this.ds_Master10.getRowCount(); iLoop++) {		 

			//선택대상만 전표보기 화면으로 이동
			if (this.ds_Master10.getColumn(iLoop, "s_chk") == "1") {					
						
				sAcchNo = this.ds_Master10.getColumn(iLoop, "ACCH_NO");//발의번호				
				if (this.gfn_IsNull(sMultiAcchNo)) {
					sMultiAcchNo = sAcchNo;
				} else {
					sMultiAcchNo = sMultiAcchNo + ";" + sAcchNo;
				}  
			} 
			
		}  			 
		
		this.fvMultPrintYn = true;//멀티인쇄여부
		this.fvArrAcchNo = new Array();//발의번호 Array
		this.fvArrReportParam = new Array();//레포트파라메타 Array
		
		if (sMultiAcchNo.indexOf(";") > 0) {
			var arrAcchNo = sMultiAcchNo.split(";");
			
			for (var iLoop=0; iLoop <arrAcchNo.length; iLoop++) {
				this.fvArrAcchNo.push(arrAcchNo[iLoop]);//발의번호 Array
			}
		} else {
			this.fvArrAcchNo.push(sMultiAcchNo);//발의번호 Array
		}
		
		if (this.fvArrAcchNo.length > 0) {
		
			this.div_MasterInput.form.pgb_PrintState.set_visible(true);
			this.div_MasterInput.form.pgb_PrintState.set_pos(0);			
			this.div_MasterInput.form.pgb_PrintState.set_min(0);
			this.fvPrintCnt = this.fvArrAcchNo.length;//출력건수
			this.div_MasterInput.form.pgb_PrintState.set_max(this.fvPrintCnt);

			var sAcchNo = this.fvArrAcchNo.shift();//배열에서 처음내용 꺼냄
			this.fvMultiSearchYn = true;//멀티인쇄조회중여부
			// 조회 
			this.fn_ComAcchSearch(sAcchNo);	
		
			this.setTimer(900, 200);
		}
	}
}

