/***********************************************************************************************
 * 레포트 출력용 파라메타 생성
 ***********************************************************************************************/  
this.fn_ComMakeReportParam = function()
{	

	this.ds_ComWfLine.clearData();//발의부서 결재라인
	this.ds_ComWfDrLine.clearData();//재경팀 결재라인
	this.ds_ComWfAtLine.clearData();//감사 결재라인
	this.ds_Master10MultiRt.clearData();
	this.ds_Detail10MultiRt.clearData();
	this.ds_Master10MultiRt.copyData(this.ds_Master10Multi);
	this.ds_Detail10MultiRt.copyData(this.ds_Detail10Multi);

	var sACCH_GU = this.ds_Master10MultiRt.getColumn(0, "ACCH_GU");//발의구분
	var sReportFile = "";//레포트파일ID
	//발의구분 - 지출
	if (sACCH_GU == "2") {
		sReportFile = "M_FA_1000_01";//레포트파일ID
	//발의구분 - 대체
	} else if (sACCH_GU == "3") {
		sReportFile = "M_FA_1000_02";//레포트파일ID
	//발의구분 - 결산
	} else if (sACCH_GU == "4") {
		sReportFile = "M_FA_1000_03";//레포트파일ID	
	//발의구분 - 수입
	} else {
		sReportFile = "M_FA_1000_04";//레포트파일ID		
	}	

	var dsObj = new Object();//출력 데이터셋 정보
	dsObj.ds_Master10 = this.ds_Master10MultiRt;
	dsObj.ds_Detail10 = this.ds_Detail10MultiRt;
	dsObj.ds_ComWfLine = this.ds_ComWfLine;
	dsObj.ds_ComWfDrLine = this.ds_ComWfDrLine;
	dsObj.ds_ComWfAtLine = this.ds_ComWfAtLine;	

	var objWfFileUrl = nApp.gds_WfFileUrl;
	var iRowCnt = objWfFileUrl.getRowCount();
	var sApprCode = "";//결재자시스템사번

	//결재라인 정보가 있으면
	if (iRowCnt > 0) {
		this.ds_ComWfLine.addRow();
		
		var iNowCol = 1;//현재컬럼번호
		var sEMSI_ID = "";//저장파일명	
		var sFILE_UL = "";//URL		
		var sFULL_FILE_UL = "";//Full URL	
		var sEncode = "";
		var sPOSI_NM = "";//직위명
			
		for (var iLoop=0; iLoop < objWfFileUrl.getRowCount(); iLoop++) {
			
			sEMSI_ID = objWfFileUrl.getColumn(iLoop, "EMSI_ID");//저장파일명	
			sFILE_UL = objWfFileUrl.getColumn(iLoop, "FILE_UL");//URL
			sPOSI_NM = objWfFileUrl.getColumn(iLoop, "POSI_NM");//직위명
			sFULL_FILE_UL = "";//Full URL	

			if (!this.gfn_IsNull(sEMSI_ID)) {
				sEncode = encodeURI(sEMSI_ID.replace(/\+/,"%2B"));
				sFULL_FILE_UL = sFILE_UL + sEncode;//Full URL
			}				

			//직위명이 감사인경우
			if (sPOSI_NM == "감사") {
				this.ds_ComWfAtLine.addRow();
				this.ds_ComWfAtLine.setColumn(0, "POSI_NM1", sPOSI_NM);//직위
				this.ds_ComWfAtLine.setColumn(0, "APPR_NM1", objWfFileUrl.getColumn(iLoop, "EMPM_NM"));//성명
				this.ds_ComWfAtLine.setColumn(0, "SIGN_UL1", sFULL_FILE_UL);//사인
				this.ds_ComWfAtLine.setColumn(0, "APPR_TM1", objWfFileUrl.getColumn(iLoop, "WOFH_DD"));//날짜				
			} else {
				this.ds_ComWfLine.setColumn(0, "POSI_NM" + iNowCol, sPOSI_NM);//직위
				this.ds_ComWfLine.setColumn(0, "APPR_NM" + iNowCol, objWfFileUrl.getColumn(iLoop, "EMPM_NM"));//성명
				this.ds_ComWfLine.setColumn(0, "SIGN_UL" + iNowCol, sFULL_FILE_UL);//사인
				this.ds_ComWfLine.setColumn(0, "APPR_TM" + iNowCol, objWfFileUrl.getColumn(iLoop, "WOFH_DD"));//날짜
				iNowCol++;
			}
			
		}			

	}	
	
	objWfFileUrl = nApp.gds_WfSlipFileUrl;
	iRowCnt = objWfFileUrl.getRowCount();	
	
	//결의자 결재라인 정보가 있으면
	if (iRowCnt > 0) {
		this.ds_ComWfDrLine.addRow();
		
		var iNowCol = 1;//현재컬럼번호
		var sEMSI_ID = "";//저장파일명	
		var sFILE_UL = "";//URL		
		var sFULL_FILE_UL = "";//Full URL	
		var sEncode = "";

		for (var iLoop=0; iLoop < objWfFileUrl.getRowCount(); iLoop++) {

			sEMSI_ID = objWfFileUrl.getColumn(iLoop, "EMSI_ID");//저장파일명	
			sFILE_UL = objWfFileUrl.getColumn(iLoop, "FILE_UL");//URL
			sFULL_FILE_UL = "";//Full URL	

			if (!this.gfn_IsNull(sEMSI_ID)) {
				sEncode = encodeURI(sEMSI_ID.replace(/\+/,"%2B"));
				sFULL_FILE_UL = sFILE_UL + sEncode;//Full URL
			}				

			this.ds_ComWfDrLine.setColumn(0, "POSI_NM" + iNowCol, objWfFileUrl.getColumn(iLoop, "POSI_NM"));//직위
			this.ds_ComWfDrLine.setColumn(0, "APPR_NM" + iNowCol, objWfFileUrl.getColumn(iLoop, "EMPM_NM"));//성명
			this.ds_ComWfDrLine.setColumn(0, "SIGN_UL" + iNowCol, sFULL_FILE_UL);//사인
			this.ds_ComWfDrLine.setColumn(0, "APPR_TM" + iNowCol, objWfFileUrl.getColumn(iLoop, "WOFH_DD"));//날짜
			iNowCol++;

		}			

	}		
	
	var paObj = new Object();//출력 파라메타 정보	
	paObj.PRNT_NM = this.gfn_GetSessionValue("USEM_NM");//출력자
	paObj.LOGO_UL = this.gfn_GetReportImageUrl("LOGO_UL_01");//이미지-로고
	   
	var _projectNm = "RPT^FA";//프로젝트명
	var _formNm = sReportFile;//레포트명
	var _datasetList = dsObj;//데이터셋
	var _paramList = paObj;//파라미터
	var _params = this.gfn_getPostData(_projectNm, _formNm, _datasetList, _paramList);//파라메타 설정		
	
	this.fvArrReportParam.push(_params);//레포트파라메타 Array
		
	//멀티인쇄로 호출한경우
	if (this.fvMultPrintYn) {
		this.fvMultiSearchYn = false;//멀티인쇄조회중여부;
	}	
		
}
