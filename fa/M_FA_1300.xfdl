		var AcomNm = this.ds_Master10.getColumn(e.row, "ACOM_NM");  //명
       

		var sPrgmId = "M_FA_1310";//호출프로그램ID  
		var sCallFunc = "fn_FormCall";//호출함수
		
		//[공통]특정화면 호출 파라메타 정보 Object
		var oParam = new Object();
		oParam.FromDd = FromDd;
		oParam.ClosDd = ClosDd;
		oParam.AcumCd = AcumCd;
		oParam.AcumNm = AcumNm;
		oParam.CusmCd = CusmCd;
