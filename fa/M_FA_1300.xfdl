/***********************************************************************************************
 * 1. 조회 영역
 ***********************************************************************************************/
/***********************************************************************************************
 * 01.01. [공통버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.fn_ComBtnSearch = function()
{ 

	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;
	
    //저장후 재조회시-마스터 데이터셋 이벤트 비활성화
	if (this.fvSchKeyYn) {
		this.ds_Master10.set_enableevent(false);
	}		
	
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Master10.clearData();
	this.ds_Input.clearData();
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_FromDd.value);//조회일자
	this.ds_Input.setColumn(0, "ACUM_CD", this.div_Search.form.edt_AcumCd.value);//회계단위
	this.ds_Input.setColumn(0, "CUSM_CD", this.div_Search.form.edt_CusmCd.value);//거래처코드

	this.ds_Input.setColumn(0, "PRGM_ID", "M_FA_1300");//프로그램ID
	
	var arrCode = this.div_Search.form.edt_AcomCd.code.split(",");//계정코드
	
	if (arrCode.length > 0) {
		var sComma = arrCode.toString().substr(0,1);
		if (sComma == ","){
			arrCode = arrCode.toString().replace(",","");
		}
		if (this.gfn_IsNull(arrCode[0])) {
			this.ds_Input.setColumn(0, "ACOM_CD", "''");
		} else {
			this.ds_Input.setColumn(0, "ACOM_CD", arrCode.toString());
		}
	} else {
		this.ds_Input.setColumn(0, "ACOM_CD", "''");
	}

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);

	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "fa:M_FA_1300_M_SL10";
	this.gfn_AddComSearchService(this, svcAddObj);

	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MST_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_FA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Master10=ds_Master10";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);
	
}

/***********************************************************************************************
 * 01.02. [Vaildation] 조회 필수값 체크
 ***********************************************************************************************/
this.fn_VaildSearch = function()
{
	//[검색]NULL 체크
	var arrArgs = ["this.div_Search.form.cal_FromDd^" + this.div_Search.form.sta_FromDd.text + "|NULL"
				  ,"this.div_Search.form.edt_AcumCd^" + this.div_Search.form.sta_AcumCd.text + "|NULL"
				  //,"this.div_Search.form.edt_CusmCd^" + this.div_Search.form.sta_CusmCd.text + "|NULL"
				  ]; 
	
	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs) ) {
		return false;
	}
	
	//날짜체크
	var sFromDd = this.div_Search.form.cal_FromDd.value;
	if(sFromDd.trim().length < 8 || sFromDd.substr(0,4) > "2999" || sFromDd.substr(0,4) < "1990" || sFromDd.substr(4,2) < "01" || sFromDd.substr(4,2) > "12" || sFromDd.substr(6,2) < "01" || sFromDd.substr(6,2) > "31"){
		this.div_Search.form.cal_FromDd.setFocus();
		
		var arrMsgArg = [this.div_Search.form.sta_FromDd.text + " ("+ sFromDd.substr(0,4) + "-" + sFromDd.substr(4,2) + "-" + sFromDd.substr(6,2) +")"];
		this.gfn_Alert("SM017", arrMsgArg); // 유효하지 않은 날짜 형식입니다.
		return false;
	}
		
	return true;
}

