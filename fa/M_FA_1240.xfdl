 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
			if (this.ds_Master10.getRowCount() > 0) {

				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterView.form.tab_Master.tpg_01.form.grd_Master10);
				
			} else {
				
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterView.form.tab_Master.tpg_01.form.grd_Master10);
		
			}		
			break;
		case "DTL10_SEARCH":
			if (this.ds_Detail10.getRowCount() > 0) {
				this.gfn_InitGridNoDataText(this.div_MasterView.form.tab_Master.tpg_01.form.grd_Detail10);
			} else {
				this.gfn_SetGridNoDataText(this.div_MasterView.form.tab_Master.tpg_01.form.grd_Detail10);
			}
			
			break;
		case "DTL20_SEARCH":
			if (this.ds_Detail20.getRowCount() > 0) {
				this.gfn_InitGridNoDataText(this.div_MasterView.form.tab_Master.tpg_01.form.grd_Detail20);
			} else {
				this.gfn_SetGridNoDataText(this.div_MasterView.form.tab_Master.tpg_01.form.grd_Detail20);
			}	
			
			break;
	}
};

/***********************************************************************************************
 * 13.02. [팝업] CallBack 함수 처리
          strId : 팝업호출시 지정한 팝업ID
	      rtnValue : 팝업창에서 close시 지정한 반환값
          주의 : Open(popuptype=modeless)창은 팝업콜백을 지원하지 않음
 ***********************************************************************************************/ 
this.fn_PopupCallback = function(strId, rtnValue)
{
    //[조회영역]
	//회계단위
	if(strId == "S_ACUM_CD_01"){
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.ACUM_CD)) {
				this.div_Search.form.edt_AcumCd.set_value(arrRtnVal.ACUM_CD);
				this.div_Search.form.edt_AcumNm.set_value(arrRtnVal.ACUM_NM);
			}
		} else {
				this.div_Search.form.edt_AcumCd.set_value("");
				this.div_Search.form.edt_AcumNm.set_value("");
			}
		
	//계정코드1
	} else if(strId == "S_ACOM_CD_01"){
		//trace("=========S_ACOM_CD_01========="  + this.gfn_IsNull(rtnValue));
		if (!this.gfn_IsNull(rtnValue)) {
		
			var arrRtnVal = eval(rtnValue);	
			
			
			//trace("=========S_ACOM_CD_01" + arrRtnVal.ACOM_CD);
			
			
			if (!this.gfn_IsNull(arrRtnVal.ACOM_CD)) {
				this.div_Search.form.edt_AcomCdFr.set_value(arrRtnVal.ACOM_CD);
				this.div_Search.form.edt_AcomNmFr.set_value(arrRtnVal.ACOM_NM);
				this.div_Search.form.edt_AcomCdTo.set_value(arrRtnVal.ACOM_CD);
				this.div_Search.form.edt_AcomNmto.set_value(arrRtnVal.ACOM_NM);
				 
			}
		} else {
			if (!this.gfn_IsNull(this.div_Search.form.edt_AcomNmFr.value)){
			
				this.div_Search.form.edt_AcomCdTo.set_value(this.div_Search.form.edt_AcomCdFr.value);
				this.div_Search.form.edt_AcomNmto.set_value(this.div_Search.form.edt_AcomNmFr.value);
				
			}
		}
	//계정코드2
	} else if(strId == "S_ACOM_CD_02"){
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.ACOM_CD)) {
				this.div_Search.form.edt_AcomCdTo.set_value(arrRtnVal.ACOM_CD);
				this.div_Search.form.edt_AcomNmto.set_value(arrRtnVal.ACOM_NM);
				 
			}
		} else {
				this.div_Search.form.edt_AcomCdTo.set_value("");
				this.div_Search.form.edt_AcomNmto.set_value("");
			}
	} 
};
 
 
