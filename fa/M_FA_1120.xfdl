/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{	
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	if (e.columnid == "CMSB_CD") {	 
		if(e.newvalue == "0010"){
			this.ds_Detail10.setColumn( this.ds_Detail10.rowposition, "DRAM_GU", null );
			this.ds_Detail10.setColumn( this.ds_Detail10.rowposition, "ISSU_NM", null );
			this.ds_Detail10.setColumn( this.ds_Detail10.rowposition, "DRAM_NO", null );
			this.ds_Detail10.setColumn( this.ds_Detail10.rowposition, "FROM_DD", null );
			this.ds_Detail10.setColumn( this.ds_Detail10.rowposition, "EXPI_DD", null );
		}	
	} else if (e.columnid == "BANM_CD") {	 
		this.gfn_GetCommBanmGrid(obj, "BANM_CD", "BANM_NM");
	} else if (e.columnid == "CURR_CD") { //화폐코드
		var nRatm = this.gfn_GetCommRatm(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CMSH_DD"), e.newvalue);//환율정보		
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "EXCH_RT", nRatm);
		
		this.fn_AmtSet();
	} else if (e.columnid == "ACNM_NO") {	 		
		var strAcnmNo = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "ACNM_NO");
		var nRow      = this.ds_FaAcnm.findRowExpr("ACNM_NO == '" + strAcnmNo + "'");
		var strBanmCd = this.ds_FaAcnm.getColumn(nRow, "BANM_CD");
		
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "BANM_CD", strBanmCd);		
	
	} else if (e.columnid == "EXCH_RT") {	//환율정보
		this.fn_AmtSet();
	} else if (e.columnid == "CMSB_AM") {	//입금액	
		this.fn_AmtSet();
	}	
	
	//증감
	if (e.columnid == "WONE_AD") {		
			
		if(obj.getColumn(e.row,"CURR_CD") != "0090"){//외화인 경우만			
			obj.setColumn(e.row, "WONE_AM", nexacro.toNumber(obj.getOrgColumn(e.row,"WONE_AM")) + nexacro.toNumber(e.newvalue));// 매출금액
		
		}
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "WONE_AM", obj.getCaseSum("crudType!='D'","WONE_AM") );				
		
		trace("증감+금액-->"+obj.getColumn(e.row, "WONE_AM"));
	}
};

