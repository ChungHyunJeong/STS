		paramObj.ACUM_CD = sACUM_CD;//[선택] 회계단위
		paramObj.BUSM_CD  = "";//[선택] 사업부문
		paramObj.SAUM_CD  = "";//[선택] 사업장코드
		paramObj.DEPM_CD  = "";//[선택] 부서코드
		paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
		paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
		
		var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		

		//월마감된 경우
		if (bCheck) {
			this.gfn_Alert("CA025");//해당월의 정보가 마감이 되어서 등록/수정 할 수 없습니다.
			
			var sOrgDate = this.ds_Detail10.getOrgColumn(this.ds_Detail10.rowposition, "CMSH_DD");//변경전값
			
			//변경전값이 없을경우
			if (this.gfn_IsNull(sOrgDate)) {				
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "CMSH_DD", "");//입력날짜가 마감된경우 오늘날짜로 세팅
			} else {
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "CMSH_DD", sOrgDate);//입력날짜가 마감된경우 변경전 값으로 세팅
			}
			
			return false;
		} else {
			return true;
		}

	}		
		
};

/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  

this.fn_AmtSet = function() {
	var nRow = this.ds_Detail10.rowposition;
	
	var strCurrCd	= this.ds_Detail10.getColumn(nRow, "CURR_CD");	//화폐코드
	var nExchRt		= this.ds_Detail10.getColumn(nRow, "EXCH_RT");	//환율정보
	//var nCmsbAm		= nexacro.floor(this.ds_Detail10.getColumn(nRow, "CMSB_AM"));	//입금액
	var nCmsbAm		= this.ds_Detail10.getColumn(nRow, "CMSB_AM");	//입금액
	
	var nAm			= nexacro.floor(nexacro.floor(nExchRt * nCmsbAm) / 10) * 10;	
	
	if (strCurrCd == "0090") { //원화이면
		this.ds_Detail10.setColumn(nRow, "WONE_AM",	nexacro.floor(nCmsbAm));
		this.ds_Detail10.setColumn(nRow, "FORE_AM",	0);
	} else {		
		this.ds_Detail10.setColumn(nRow, "WONE_AM",	nAm);	
		
		this.ds_Detail10.setColumn(nRow, "FORE_AM",	nCmsbAm);		
	}
	
	 this.fn_GetSum();
};
