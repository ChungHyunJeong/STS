 ***********************************************************************************************/ 
this.fn_VaildDetailSave = function()
{
	//[저장] Validation 설정
	var arrArgs = [ "CMSH_NO^NULL"				  
				   ,"WOFH_SQ^NULL"
				   ,"CMSB_CD^NULL"
				   ,"BANM_CD^NULL"
				   ,"ACNM_NO^NULL"
				   ,"CMSH_DD^NULL"
				   ,"CURR_CD^NULL"
				   ,"EXCH_RT^NULL"
				   ,"CMSB_AM^NULL"
			      ]; 
				  
	var sCheckGroupTitle = this.div_DetailInput.form.tab_Detail.tpg_01.text;//체크그룹 타이틀
				  
	//Grid Vaild 체크
	if (!this.gfn_CheckGridValid(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10, arrArgs, sCheckGroupTitle) ) {
		return false;
	}
	
	for(var iLoop = 0; iLoop < this.ds_Detail10.getRowCount(); iLoop++){
		
		var strCmsbCd = this.ds_Detail10.getColumn(iLoop, "CMSB_CD");
		
		var strAcnmNo = this.ds_Detail10.getColumn(iLoop, "ACNM_NO");
		var strBanmCd = this.ds_Detail10.getColumn(iLoop, "BANM_CD");
		
		var strDramGu = this.ds_Detail10.getColumn(iLoop, "DRAM_GU");
		var strIssuNm = this.ds_Detail10.getColumn(iLoop, "ISSU_NM");
		var strDramNo = this.ds_Detail10.getColumn(iLoop, "DRAM_NO");
		var strFromDd = this.ds_Detail10.getColumn(iLoop, "FROM_DD");
		var strExpiDd = this.ds_Detail10.getColumn(iLoop, "EXPI_DD");


		if(this.gfn_IsNull(strAcnmNo) || strAcnmNo ==""){
			var arrMsgArg = ["계좌번호"];				
			this.gfn_Alert("SM008", arrMsgArg);// 계좌번호 필수 입력 항목입니다.				
			this.ds_Detail10.set_rowposition(iLoop);
			return false;
		}
		
		if(this.gfn_IsNull(strBanmCd) || strBanmCd == ""){
			var arrMsgArg = ["은행명"];				
			this.gfn_Alert("SM008", arrMsgArg);// 은행명 필수 입력 항목입니다.				
			this.ds_Detail10.set_rowposition(iLoop);
			return false;
		}


		if(strCmsbCd == "0020"){
			
			if(this.gfn_IsNull(strDramGu)){
				var arrMsgArg = ["어음구분"];				
				this.gfn_Alert("SM008", arrMsgArg);// 어음구분은 필수 입력 항목입니다.				
				this.ds_Detail10.set_rowposition(iLoop);
				return false;
			}
// 			if(this.gfn_IsNull(strIssuNm)){
// 				var arrMsgArg = ["발행인"];				
// 				this.gfn_Alert("SM008", arrMsgArg);// 발행인 필수 입력 항목입니다.				
// 				this.ds_Detail10.set_rowposition(iLoop);
// 				return false;
// 			}		
			if(this.gfn_IsNull(strDramNo)){
				var arrMsgArg = ["어음번호"];				
				this.gfn_Alert("SM008", arrMsgArg);// 어음번호 필수 입력 항목입니다.				
				this.ds_Detail10.set_rowposition(iLoop);
				return false;
			}		
			if(this.gfn_IsNull(strFromDd)){
				var arrMsgArg = ["발행일자"];				
				this.gfn_Alert("SM008", arrMsgArg);// 발행일자 필수 입력 항목입니다.				
				this.ds_Detail10.set_rowposition(iLoop);
				return false;
			}		
			if(this.gfn_IsNull(strExpiDd)){
				var arrMsgArg = ["만기일자"];				
				this.gfn_Alert("SM008", arrMsgArg);// 만기일자 필수 입력 항목입니다.				
				this.ds_Detail10.set_rowposition(iLoop);
				return false;
			}		
			
			
		}		
	}	 
	 
	return true;
}
 
