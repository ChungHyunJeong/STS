	this.div_MasterInput.form.tab_Master.tpg_01.form.mae_WoneAm.set_value( woneAm );
			
	//trace("합계금액>>>>>>>>>>>>>>> : " + woneAm);
	
 };
 

this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{	
	//선택	
	if (e.columnid == "s_chk") {
		var cnthNo = this.ds_Detail20.getColumn(e.row, "CMSH_NO");
		var value = "0";
		for (var iLoop = 0; iLoop < this.ds_Detail20.getRowCount(); iLoop++) {
