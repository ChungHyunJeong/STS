 ***********************************************************************************************/
this.fn_VaildSearch = function()
{
	//[결재] 결재내역 조회가 아니면
	if (!this.fvWfSearchMode) {
		if (!this.gfn_CheckFromToDate(this.fvComPrgmId, this.div_Search.form.cal_PdrhDtFr, this.div_Search.form.cal_PdrhDtTo)) return false;//조회기간 범위 체크
		//조회조건 필수입력값 있으면 체크
			//[저장] Validation 설정
		var arrArgs = ["this.div_Search.form.edt_DepmCd^등록부서|NULL"				  
