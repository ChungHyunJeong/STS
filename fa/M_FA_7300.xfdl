this.fn_ComBtnAdd = function()
{
	//this.div_MasterView.form.grd_Master10.updateToDataset();
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	
	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	
	//var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Master10);//crud_flag 변경-입력(I)
	var iAddRow = this.ds_Master10.addRow();
	
	//this.div_MasterInput.form.tab_Master.tpg_01.form.mae_DebmCd.set_enable(true);	
	
	//-------------------------------------------------------------------------------------------------------------//
	
	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//등록부서코드
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//등록부서명
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//등록자사번
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//등록자명
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//등록자ID	
	this.ds_Master10.setColumn(iAddRow, "DEBM_DD", this.gfn_GetCommDate("toDay"));//등록일자-오늘날짜로 설정
	this.ds_Master10.setColumn(iAddRow, "ACUM_CD", this.gfn_GetSessionValue("ACUM_CD"));
	this.ds_Master10.setColumn(iAddRow, "ACUM_NM", this.gfn_GetSessionValue("ACUM_NM"));
	
	this.ds_Master10.setColumn(iAddRow, "USED_YN", "1");//사용여부
	this.ds_Master10.setColumn(iAddRow, "AUTO_YN", "0");//원리금자동이체
	this.ds_Master10.setColumn(iAddRow, "LOAN_YN", "0");//사채여부	
	this.ds_Master10.setColumn(iAddRow, "FROM_MM", "");//지급시작월
	this.ds_Master10.setColumn(iAddRow, "CLOS_MM", "");//지급종료월
	
	//처음 입력 항목에 포커스
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DebmNm.setFocus();
		
	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
	
}

