/***********************************************************************************************
 * 02.MG0.02. [Master그리드-버튼] 행추가 클릭 이벤트
 ***********************************************************************************************/
this.fn_MasterGrd01AddRow = function()
{
	//체크버튼 활성화
	this.fn_SetGridVisible(true);
	
	var iAddRow = this.ds_Master10.addRow();
	this.ds_Master10.setColumn(iAddRow, "s_chk", "1"); // 선택 체크 
	this.ds_Master10.setColumn(iAddRow, "FSTM_YY", this.div_Search.form.edt_FstmYy.value); // 적용년도
	this.ds_Master10.setColumn(iAddRow, "FSTM_GU", this.div_Search.form.cbo_FstmGu.value); // 양식구분
	
	//윗줄의 표시순서에 10을 더해준다.
	if(iAddRow > 0){
		var sortNo = this.ds_Master10.getColumn(iAddRow -1, "SORT_NO");
		var sortNoInput = sortNo.substr(0, sortNo.length - 2);
		var sortNoTail = sortNo.substring(sortNo.length -2, sortNo.length);
		sortTail = (Number(sortNoTail) + 10);
		this.ds_Master10.setColumn(iAddRow, "SORT_NO", sortNo); // 양식구분
	}
	
	
};

/***********************************************************************************************
 * 02.MG0.03. [Master그리드-버튼] 행삽입 클릭 이벤트
 ***********************************************************************************************/
this.fn_MasterGrd01InsertRow = function()
{

	//체크버튼 활성화
	this.fn_SetGridVisible(true);
	
	var iAddRow = this.ds_Master10.insertRow(this.ds_Master10.rowposition);
	this.ds_Master10.setColumn(iAddRow, "s_chk", "1"); // 선택 체크 
	this.ds_Master10.setColumn(iAddRow, "FSTM_YY", this.div_Search.form.edt_FstmYy.value); // 적용년도
	this.ds_Master10.setColumn(iAddRow, "FSTM_GU", this.div_Search.form.cbo_FstmGu.value); // 양식구분
	
	//윗줄의 표시순서에 10을 더해준다.
	if(iAddRow > 0){
		var sortNo = this.ds_Master10.getColumn(iAddRow -1, "SORT_NO");
		var sortNoInput = sortNo.substr(0, sortNo.length - 2);
		var sortNoTail = sortNo.substring(sortNo.length -2, sortNo.length);
		sortTail = (Number(sortNoTail) + 10);
		
		this.ds_Master10.setColumn(iAddRow, "SORT_NO", sortNoInput + sortTail); // 양식구분
	}
	
	

};

