/***********************************************************************************************
 * 15.05. [ds_Mster10] 선택 value 변경시 처리 함수
 ***********************************************************************************************/ 

this.fn_setMaster10Checked = function(row, checked)
{
	var acomCd="";
	var acomNm = "";
	var printGu = "";
	var leftYn = "";
	var rightYn = "";
	var lvCnt = 0;
	 
	
	trace("e.row = "+ row + "   checked = " + checked);
	
	//if(e.columnid =="s_chk"){
		
		if(this.gfn_IsNull(this.ds_Master10.getColumn(row, "FSTM_ID"))){
			//계정
			acomCd = this.ds_Master10.getColumn(row, "ACOM_CD");
			//계정명
			acomNm = this.ds_Master10.getColumn(row, "ACOM_NM");
			//출력구분
			printGu = this.ds_Master10.getColumn(row, "PRIN_GU");
			//좌측출력여부
			leftYn = this.ds_Master10.getColumn(row, "LEFT_YN");
			//우측출력여부
			rightYn = this.ds_Master10.getColumn(row, "RIGH_YN");
			//레벨
			lvCnt = this.ds_Master10.getColumn(row, "LV_CNT");
			
			//trace("lvCnt" + lvCnt);
			
			
			// 출력명 레벨별 들여쓰기
			acomNm = this.gfn_Space((lvCnt - 1) * 3) + acomNm;
			
			if(this.gfn_IsNull(acomNm)){
				acomNm = "";
			}
			
			if(checked =="1"){
				// 체크 true
				this.ds_Master10.setColumn(row, "SORT_NO", acomCd);
				this.ds_Master10.setColumn(row, "FSTM_NM", acomNm);
				this.ds_Master10.setColumn(row, "PRIN_GU", "0010"); // 금액 > 0
				
				//
				if(leftYn =="0" && rightYn == "0"){
					this.ds_Master10.setColumn(row, "RIGH_YN", "1");
				}
			}else{
				// 체크 false
				this.ds_Master10.setColumn(row, "SORT_NO", "");
				this.ds_Master10.setColumn(row, "FSTM_NM", "");
				this.ds_Master10.setColumn(row, "PRIN_GU", ""); // 금액 > 0
				this.ds_Master10.setColumn(row, "LEFT_YN", "0");
				this.ds_Master10.setColumn(row, "RIGH_YN", "0");
			}
		}
	
	//}
	
};


/***********************************************************************************************
 * 15.05. [ds_Detail20] 선택 copyRow
 ***********************************************************************************************/ 

this.fn_copyRow = function(row)
{
	//trace("row = " + row);
	
	var arrMsgArg;
	
	
	if(this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SORT_NO"))){
	
		arrMsgArg = ["계정"];
		this.gfn_Alert("SM040", arrMsgArg);//&ITM0을(를) 선택하세요.
		return
	}
	if(this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FSTM_ID"))){
		this.gfn_Alert("FA003");//먼저 저장을 하신 후 계산항목공식을 입력 하십시오.
	}
	
	
	var acomCd = this.ds_Detail20.getColumn(row, "ACOM_CD");
	var acomNm = this.ds_Detail20.getColumn(row, "ACOM_NM");
	var fasmYy = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FSTM_YY");
	var fasmGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FSTM_GU");
	var fasmId = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FSTM_ID");
	var usedYn = this.ds_Detail20.getColumn(row, "USED_YN");
	if(usedYn =="1"){
		arrMsgArg = [acomNm + " 계정"];
		this.gfn_Alert("FA001", arrMsgArg);//&ITM0을(를) 선택하세요.
		return
	
	}
	this.ds_Detail20.setColumn(row, "USED_YN", "1");
	
	
	
	//줄추가
	var iAddRow = this.ds_Detail10.addRow();
	
	
	this.ds_Detail10.setColumn(iAddRow, "ACOM_CD", acomCd);
	this.ds_Detail10.setColumn(iAddRow, "ACOM_NM", acomNm);
	this.ds_Detail10.setColumn(iAddRow, "FSTM_YY", fasmYy);
	this.ds_Detail10.setColumn(iAddRow, "FSTM_GU", fasmGu);
	this.ds_Detail10.setColumn(iAddRow, "FSTM_ID", fasmId);
	this.ds_Detail10.setColumn(iAddRow, "crudType", "I");
	this.ds_Detail10.setColumn(iAddRow, "LEFT_YN", "1");
	
	// 금액구분 = 잔액
	this.ds_Detail10.setColumn(iAddRow, "FSTD_GU", "4");
	// 기호구분 = +
	this.ds_Detail10.setColumn(iAddRow, "FSTD_CD", "0010");
	
	
	//trace("acomCd.substr(0,1) = " + acomCd.substr(0,1));
	
	if (acomCd.substr(0,1) == "$"){
		this.ds_Detail10.setColumn(iAddRow, "FSTD_GU", "4");
	}
	
}


/***********************************************************************************************
 * 15.05. [ds_Detail10] 선택 deleterow
 ***********************************************************************************************/ 

this.fn_delRow = function(row)
{
	
	if(this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SORT_NO"))){
	
		var arrMsgArg = ["계정"];
		this.gfn_Alert("SM040", arrMsgArg);//&ITM0을(를) 선택하세요.
	}
	if(this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FSTM_ID"))){
		this.gfn_Alert("FA003");//먼저 저장을 하신 후 계산항목공식을 입력 하십시오.
	}

	
	//줄삭제
	var acomCd = this.ds_Detail10.getColumn(row, "ACOM_CD");
	
	var iFvFindRow = this.ds_Detail20.findRowExpr("ACOM_CD=='" + acomCd+ "'");//업무Key Row 찾기
	trace("row = " + row + " == iFvFindRow ="+ iFvFindRow);
	if (iFvFindRow > -1) {
		//
		this.ds_Detail20.setColumn(iFvFindRow, "USED_YN", "0");
	
	}
	
	//줄삭제
	var delRow = this.ds_Detail10_del.addRow();
	this.ds_Detail10_del.copyRow(delRow, this.ds_Detail10,row);
	this.ds_Detail10_del.setColumn(delRow, "crudType", "D");
	
	this.ds_Detail10.deleteRow(row); 
	
	
}




