this.fn_ComBtnCopy = function()
{ 

	if(this.gfn_IsNull(this.div_Search.form.edt_CopyYy.value)){
		//복사년도 필수체크	
		var arrMsgArg = [this.div_Search.form.sta_CopyYy.text];
		this.gfn_Alert("SM008", arrMsgArg);//결재가 진행중일때는 &ITM0 하실수 없습니다.
		this.div_Search.form.edt_CopyYy.setFocus();	
		return
	}
	
// 	if(this.gfn_IsNull(this.div_Search.form.cbo_CopyFstmGu.value)){
// 		//복사양식 필수체크	
// 		var arrMsgArg = [this.div_Search.form.sta_CopyFstmGu.text];
// 		this.gfn_Alert("SM008", arrMsgArg);//결재가 진행중일때는 &ITM0 하실수 없습니다.
// 		this.div_Search.form.sta_CopyFstmGu.setFocus();	
// 		return
// 	}
	
	this.ds_Input.clearData();
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "FSTM_YY", this.div_Search.form.edt_FstmYy.value);//회의번호
	this.ds_Input.setColumn(0, "FSTM_GU", this.div_Search.form.cbo_FstmGu.value);//결재차수
	this.ds_Input.setColumn(0, "FSTM_COPY_YY", this.div_Search.form.edt_CopyYy.value);//프로그램ID
	//복사양식은 양식종류 그대로 가져온다.
	this.ds_Input.setColumn(0, "FSTM_COPY_GU", this.div_Search.form.cbo_FstmGu.value);//this.div_Search.form.cbo_CopyFstmGu.value);// 복사양식(TO)
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "fa:M_FA_0110_M_UP11";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid 		= "MST_COPY";			//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName 	= this.G_SV_FA;			//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset 	= "ds_Input=ds_Input" 	//입력 데이터셋
	svcCallObj.sOutDatset 	= "ds_return=ds_return";//출력 데이터셋
	svcCallObj.sOtherArg 	= "";					//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";		//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);
	

}

