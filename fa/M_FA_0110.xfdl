{
	//체크 값 변경시
	//true : 입력상태값, 선택, 계정코드, 계정명 visible = true
	//fasle :입력상태값, 선택, 계정코드, 계정명 visible = false
	
	if(e.postvalue){
		//true
		this.fn_SetGridVisible(true);
		this.fn_ComBtnSearch();
		// 회계계정참조인 체크 경우 autofittype = "none"	
		this.div_MasterView.form.grd_Master10.set_autofittype( "none" );
	}else{
		//false
		this.fn_SetGridVisible(false);
		this.fn_ComBtnSearch();
		// 회계계정참조인 체크 안 한 경우 autofittype = "col"		
		//this.div_MasterView.form.grd_Master10.set_autofittype( "col" );
	}
	
	
	
};



