/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] rowposchange 이벤트
  ---------------------------------------------------------------------------------------------*/  

this.ds_Detail10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	// ds_detail20 row 찾기
	var acomCd = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "ACOM_CD");
	if(!this.gfn_IsNull(acomCd)){
		var iFvFindRow = this.ds_Detail20.findRowExpr("ACOM_CD=='" + acomCd+ "'");// 계정찾기
		
		//trace("iFvFindRow = " + iFvFindRow);
		
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail20.clearSelect()
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail20.setFocus();
		
		if (iFvFindRow > -1) {
			if (iFvFindRow == 0) {
				this.ds_Detail20.set_rowposition(-1);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail20.selectRow(-1);
			} 
			this.ds_Detail20.set_rowposition(iFvFindRow);
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail20.selectRow(iFvFindRow);
		} else {
		
			this.ds_Detail20.set_rowposition(0);
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail20.selectRow(iFvFindRow);
		}
	}
	
};


