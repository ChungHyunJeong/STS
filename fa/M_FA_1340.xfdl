};

/***********************************************************************************************
 * 1. 조회 영역
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    [조회영역-년월달력] dropdown 이벤트
  ---------------------------------------------------------------------------------------------*/
 this.div_Search_cal_CrdhMm_ondropdown = function(obj:nexacro.Calendar,e:nexacro.EventInfo)
{
	this.gfn_CallYmCalendar(this, "this.div_Search.form.cal_CrbhMm", "fn_YmCallBack");	
};

/***********************************************************************************************
 * 01.01. [공통버튼] 조회 버튼 클릭 이벤트
 ***********************************************************************************************/
this.fn_ComBtnSearch = function()
{ 
	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;	
	
	//그리드CELL TEXT 변경	
	for(var i=0; i<=36; i++){
		var sMM = this.gfn_AddMonth(this.div_Search.form.cal_CrbhMm.value, (i*-1));
		this.div_MasterInput.form.tab_Detail.tpg_01.form.grd_Master10.setCellProperty( "head", (i+1), "text", sMM.substr(0,4)+"년"+sMM.substr(4,2)+"월");
		this.div_MasterInput.form.tab_Detail.tpg_02.form.grd_Master20.setCellProperty( "head", (i+1), "text", sMM.substr(0,4)+"년"+sMM.substr(4,2)+"월(%)");
		this.div_MasterInput.form.tab_Detail.tpg_03.form.grd_Master30.setCellProperty( "head", (i+3), "text", sMM.substr(0,4)+"년"+sMM.substr(4,2)+"월");
	}
		
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	//this.ds_Master10.clearData();
	this.ds_Input.clearData();
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_CrbhMm.value);
	this.ds_Input.setColumn(0, "CLOS_DD", this.div_Search.form.cal_CrbhMm.value);
	this.ds_Input.setColumn(0, "SESU_ID", this.gfn_GetSessionValue("SYST_ID"));
	this.ds_Input.setColumn(0, "CRBH_MM", this.div_Search.form.cal_CrbhMm.value.substr(0,6));
	this.ds_Input.setColumn(0, "ACUM_CD", this.div_Search.form.edt_AcumCd.value);
	this.ds_Input.setColumn(0, "CRCS_GU", this.div_Search.form.cbo_CrcsGu.value);
	
	this.ds_Input.setColumn(0, "PRGM_ID", "M_FA_1340");//프로그램ID	

