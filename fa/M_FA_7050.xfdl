/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{

	if (e.newrow > -1) {
		
		//[결재] 결재상태에 따른 컴포넌트 제어
		this.fn_SetWfCompEnable();		
		
		//미지급정산정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MIJH_NO"))) {
			
			//미지급정산코드 포멧 설정 변경 (기존데이터용)
			var strMijhNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MIJH_NO");

			if(strMijhNo.length == 10){
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_MijhNo.set_format("######-####");
			} else {
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_MijhNo.set_format("########-###");
			}
			
			//입력영역 버튼 비활성화
			this.fn_SetGridInputButtonEnable();
			
			/*
			//22.06.02 주석
			if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ANAC_CD") == "0010"){
				//보통예금
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFormatColProperty(14,"size",120);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFormatColProperty(15,"size",150);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFormatColProperty(16,"size",0);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFormatColProperty(17,"size",0);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFormatColProperty(18,"size",0);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFormatColProperty(19,"size",0);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFormatColProperty(20,"size",0);

			}else{
				//지급어음
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFormatColProperty(14,"size",0);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFormatColProperty(15,"size",0);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFormatColProperty(16,"size",80);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFormatColProperty(17,"size",150);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFormatColProperty(18,"size",90);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFormatColProperty(19,"size",90);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFormatColProperty(20,"size",120);

			}
			*/
			
			
			
			//[월마감제어-버튼/상세그리드]
			var sBUDG_MM = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CRDH_DD"), "");//마감년월
			var sACUM_CD = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ACUM_CD"), "");//회계단위
			
			if (!this.gfn_IsNull(sBUDG_MM)) {
			
				//마감여부 체크
				var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
				var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
				paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
				paramObj.MODM_CD = "FA";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
				paramObj.ACUM_CD = sACUM_CD;//[선택] 회계단위
				paramObj.BUSM_CD  = "";//[선택] 사업부문
				paramObj.SAUM_CD  = "";//[선택] 사업장코드
				paramObj.DEPM_CD  = "";//[선택] 부서코드
				paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
				paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
				
				var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크
				
				//월마감된 경우
				if (bCheck) {
					this.fvMmCloseYn = true;//월마감여부-Tab버튼 제어용
				} else {
					this.fvMmCloseYn = false;//월마감여부-Tab버튼 제어용
				}
			}
			
			//결재상태면
			if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST") == "0040") {
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(true);
				eval(this.fvAtobDiv).fn_ScrnEnable(false);
			//등록상태면
			} else if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST") == "0010") {
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(false);
				eval(this.fvAtobDiv).fn_ScrnEnable(true);
			} else {
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(false);
				eval(this.fvAtobDiv).fn_ScrnEnable(false);
			}
			
			
			//관리번호 포맷세팅
			var sLeng = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MIJH_NO").length;
			if (sLeng > 10){
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_MijhNo.set_format("@@@@@@@@-@@@@");
			}else{
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_MijhNo.set_format("@@@@@@-@@@@");
			}
			
			
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "MIJH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MIJH_NO"));
			this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
					
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "fa:M_FA_7050_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);

			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_FA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			//svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10 ds_Detail20=ds_Detail20";//출력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);
		}
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master10] Row 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10) /*|| this.gfn_DsIsUpdated(this.ds_Detail10)*/) {
		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	

	return true;
};

