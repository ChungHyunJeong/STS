				this.div_MasterInput.form.tab_Master.tpg_02.form.edt_RankNm.set_value(arrRtnVal.RANK_NM);
			}
		}	
		
	//[Detail영역]
			
	//첨부파일		
	} else if (strId == "B_FILE_01") {
