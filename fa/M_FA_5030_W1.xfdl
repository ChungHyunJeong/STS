/***********************************************************************************************
 * 13.02. [팝업] CallBack 함수 처리
          strId : 팝업호출시 지정한 팝업ID
	      rtnValue : 팝업창에서 close시 지정한 반환값
          주의 : Open(popuptype=modeless)창은 팝업콜백을 지원하지 않음
 ***********************************************************************************************/ 
this.fn_PopupCallback = function(strId, rtnValue)
{

	//[조회영역]

	
	//[Master영역]
	//등록부서		
	if (strId == "M_DEPM_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.DEPM_CD)) {
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_value(arrRtnVal.DEPM_CD);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmNm.set_value(arrRtnVal.DEPM_NM);
			}
		}	
	//등록자		
	} else if (strId == "M_SYST_ID_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.EMPM_CD)) {
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_value(arrRtnVal.EMPM_CD);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm.set_value(arrRtnVal.EMPM_NM);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SystId.set_value(arrRtnVal.SYST_ID);				
			}
		}	
	//계약번호		
	} else if (strId == "M_CNTH_NO_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = rtnValue.split(";;");	
			//alert(arrRtnVal);
			if (arrRtnVal.length > 0) {
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_value(arrRtnVal[0]);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CnthNm.set_value(arrRtnVal[1]);
			}
		}		
	//계약처		
	} else if (strId == "M_CNCS_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);
			
			if (!this.gfn_IsNull(arrRtnVal.CUSM_CD)) {
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CncsCd.set_value(arrRtnVal.CUSM_CD);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CncsNm.set_value(arrRtnVal.CUSM_NM);
			}
		}	
	
	//거래처	
	} else if (strId == "M_CUSM_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			//trace(rtnValue);
			var arrRtnVal = eval(rtnValue);
			
			if (!this.gfn_IsNull(arrRtnVal.CUSM_CD)) {
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_value(arrRtnVal.CUSM_CD);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmNm.set_value(arrRtnVal.CUSM_NM);
				//공급받는자 설정
				this.div_MasterInput.form.tab_Master.tpg_02.form.mae_RsauNo.set_value(arrRtnVal.SAUM_NO);   //사업자등록번호
				this.div_MasterInput.form.tab_Master.tpg_02.form.edt_ReprNm.set_value(arrRtnVal.REPR_NM);   //대표자명
				this.div_MasterInput.form.tab_Master.tpg_02.form.edt_BusiCo.set_value(arrRtnVal.BUSI_CO);   //업태
				this.div_MasterInput.form.tab_Master.tpg_02.form.edt_ItemNm.set_value(arrRtnVal.ITEM_NM);   //종목
				
			}
		}		
	//고객		
	} else if (strId == "M_CSPM_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			//trace(rtnValue);
			var arrRtnVal = eval(rtnValue);
			
			if (!this.gfn_IsNull(arrRtnVal.CSPM_CD)) {
				this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CspmCd.set_value(arrRtnVal.CSPM_CD);
