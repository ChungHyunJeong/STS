/***********************************************************************************************
 * 02.MG0.07. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	//trace(obj.id);
	//법정내용년수/취득경과년수
	if(e.columnid == "FASM_CT" || e.columnid == "BFPR_CT"){
		var ct = nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FASM_CT"))-nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BFPR_CT"));
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "RMPR_CT", ct);
		this.fn_Calc();
	}
	//최초취득가액/재평가가치액
	//취득원가계
	else if(e.columnid == "FSGT_AM"){
		this.fn_Calc();
	}
	//계정과목 
	else if(e.columnid =="ACOM_CD"){
		var sAcomCd = this.ds_Master10.getColumn(e.row,"ACOM_CD");	//계정코드
		//this.fn_GetFabm(sAcomCd);
		if (!this.gfn_IsNull(sAcomCd)) {			

			var iFindRow = this.ds_FaAcom.findRowExpr("ACOM_CD=='" + sAcomCd + "'"); //해당계정코드의 Row
			var sFiamGu = this.ds_FaAcom.getColumn(iFindRow, "FABM_CD");	//상각구분
			var iFasmCt = this.ds_FaAcom.getColumn(iFindRow, "FASM_CT");	//법정내용연수

			if (sFiamGu == '*') sFiamGu = ''; //상각구분이 '*'이면 null			
			
			//건설중인자산인경우 비상각
			if(this.ds_Master10.getColumn(e.row,"CONS_YN") == "1"){
				this.ds_Master10.setColumn(e.row, "FIAM_GU","0");	//비상각구분
			
			}else{
				this.ds_Master10.setColumn(e.row, "FIAM_GU",sFiamGu);	//상각구분
			}
			this.fn_SetFiamGuCompEnable();
			this.ds_Master10.setColumn(e.row, "FASM_CT",iFasmCt);	//법정내용연수
			this.ds_Master10.setColumn(e.row, "RMPR_CT",iFasmCt);	//법정내용연수
			
		}
		//건설중인자산
	}else if(e.columnid == "CONS_YN"){
		if(e.newvalue == "1"){
			this.ds_Master10.setColumn(e.row, "FIAM_GU", "0");	//상각구분
		}else{
			var sAcomCd = this.ds_Master10.getColumn(e.row,"ACOM_CD");	//계정코드
			var iFindRow = this.ds_FaAcom.findRowExpr("ACOM_CD=='" + sAcomCd + "'"); //해당계정코드의 Row
			var sFiamGu = this.ds_FaAcom.getColumn(iFindRow, "FABM_CD");	//상각구분
			
			this.ds_Master10.setColumn(e.row, "FIAM_GU", sFiamGu);	//상각구분
		}
		
		//상각구분 선택시
	
	} else if (e.columnid == "FIAM_GU") {
		this.fn_SetFiamGuCompEnable();		

	//선급금
	} else if (e.columnid == "ADVA_AM") {
	
		//미지급금 계산
		var iFsgtAm = this.ds_Master10.getColumn(e.row, "FSGT_AM"); //최초취득가액
		var iVtaxAm = this.ds_Master10.getColumn(e.row, "VTAX_AM"); //부가세
		var iAdvaAm = this.ds_Master10.getColumn(e.row, "ADVA_AM"); //선급금
		var iAdvrAm = this.ds_Master10.getColumn(e.row, "ADVR_AM"); //선급금잔액
		var iRpamAm = 0; //미지급금
		
		if (this.gfn_IsNull(iFsgtAm)) iFsgtAm = 0;	//NULL인 경우 0셋팅
		if (this.gfn_IsNull(iVtaxAm)) iVtaxAm = 0;	//NULL인 경우 0셋팅
		if (this.gfn_IsNull(iAdvaAm)) iAdvaAm = 0;	//NULL인 경우 0셋팅
		if (this.gfn_IsNull(iAdvrAm)) iAdvrAm = 0;	//NULL인 경우 0셋팅

		if (nexacro.toNumber(iAdvaAm) > nexacro.toNumber(iAdvrAm)) { //선급금 > 선급금잔액
			this.ds_Master10.setColumn(e.row, "ADVA_AM","0"); //선급금 0으로 셋팅			
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_AdvaAm.setFocus();
			
			var arrMsgArg = ["선급금", "선급금잔액"];
			this.gfn_Alert("SM056", arrMsgArg);//선급금은 선급금잔액을 초과할 수 없습니다.
			
			return false;
			
		}

		//미지급금 = 최초취득가액 + 부가세 - 선급금
		iRpamAm =  nexacro.toNumber(iFsgtAm) + nexacro.toNumber(iVtaxAm) - nexacro.toNumber(iAdvaAm);
		this.ds_Master10.setColumn(e.row, "RPAM_AM",iRpamAm); //미지급금 	
		
	//최초취득가액, 부가세 (12/30 최초취득가액 추가)
	} else if (e.columnid == "FSGT_AM" || e.columnid == "VTAX_AM") {
// 		//미지급금 계산
// 		var iFsgtAm = this.ds_Master10.getColumn(e.row, "FSGT_AM"); //최초취득가액
// 		var iVtaxAm = this.ds_Master10.getColumn(e.row, "VTAX_AM"); //부가세
// 		var iAdvaAm = this.ds_Master10.getColumn(e.row, "ADVA_AM"); //선급금
// 		var iRpamAm = 0; //미지급금
// 		
// 		if (this.gfn_IsNull(iFsgtAm)) iFsgtAm = 0;	//NULL인 경우 0셋팅
// 		if (this.gfn_IsNull(iVtaxAm)) iVtaxAm = 0;	//NULL인 경우 0셋팅
// 		if (this.gfn_IsNull(iAdvaAm)) iAdvaAm = 0;	//NULL인 경우 0셋팅
// 		
// 		//미지급금 = 최초취득가액 + 부가세 - 선급금
// 		iRpamAm =  nexacro.toNumber(iFsgtAm) + nexacro.toNumber(iVtaxAm) - nexacro.toNumber(iAdvaAm);
// 		this.ds_Master10.setColumn(e.row, "RPAM_AM",iRpamAm); //미지급금 	
		
	}	
	
};

/***********************************************************************************************
 * 02.DG0.01. [Detail Tab] 변경 이벤트
 ***********************************************************************************************/
this.div_MasterInput_tab_Master_onchanged = function(obj:nexacro.Tab,e:nexacro.TabIndexChangeEventInfo)
{
	var iTabIndex = e.postindex;
	
	this.div_MasterInput.form.div_ComGrdBtn.set_enable(true);
	
	//Tab-신규자산내역
	if (iTabIndex == 0) {	
		this.div_MasterInput.form.div_ComGrdBtn.set_visible(false);
	} 
	//Tab-자동전표
	else if (iTabIndex == 1) {	
		if (this.fvWfSearchMode || this.fvWofhStatus) {
			this.div_MasterInput.form.div_ComGrdBtn.set_visible(false);
		} else {
			this.div_MasterInput.form.div_ComGrdBtn.set_visible(true);
		}
		this.fn_SetGridAtobButton();//[공통] 자동전표상세 Div Url 및 입력 버튼 설정	

	//Tab-첨부파일
	} else if (iTabIndex == 2) {	
		if (this.fvWfSearchMode || this.fvWofhStatus) {
			this.div_MasterInput.form.div_ComGrdBtn.set_visible(false);
		} else {
			this.div_MasterInput.form.div_ComGrdBtn.set_visible(true);
		}
		
		this.fn_SetGridFileButton();//[공통] 그리드 파일 버튼 설정
	}

	
};

