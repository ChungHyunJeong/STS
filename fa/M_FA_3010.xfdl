this.fn_SetWfCompEnable = function()
{ 	
	//[결재]결재내역 팝업화면으로 호출여부
	if (!this.fvWfPopupMode) {
		//[결재] 공통버튼 제어
		var bCommSaveButton = true;//저장버튼 활성화 여부-기본값은 false로 해야함. - 결재승인후 수정일 필요한 화면에서만 true로 설정해야함. 
		this.fvWofhStatus = this.gfn_SetCommButtonWf(this, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"), bCommSaveButton);//결재진행상태		
	}
	
	//[결재]결재내역 조회모드이면 Visble false || 결재상태가 "등록"이 아닐경우
	if (this.fvWfSearchMode || this.fvWofhStatus) {
		//[결재]결재내역 팝업화면으로 호출여부
		if (this.fvWfPopupMode) {
			this.div_Search.set_enable(false);//조회영역 Enable처리
		}
		//[결재] Div하위의 컴포넌트 Enable 처리
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
		this.div_MasterInput.form.div_ComGrdBtn.set_visible(false);
		
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_RemkPop.set_enable(true);

		//[전표보기]
		//결재상태-승인인경우 => [전표보기] 버튼 활성화			
		if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST") == "0040") {
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(true);			
		} else {
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(false);
		}

		//선지급금이 0보다 큰 경우 필수체크(12/16 추가개발)		
// 		if(this.ds_Master10.getColumn(this.ds_Master10.rowposition,"ADVA_AM") > 0) {
// 			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AdvaCd.set_cssclass("essential");				
// 		//선지급금이 0인 경우 선급금계정코드 필수해제
// 		} else {
// 			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AdvaCd.set_cssclass("");
// 		}
				
	} else {
		//[결재]결재내역 팝업화면으로 호출여부
		if (this.fvWfPopupMode) {	
			this.div_Search.set_enable(true);//조회영역 Enable처리
		}
		//[결재] Div하위의 컴포넌트 Enable 처리
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, true);//Master 입력 프리폼
		//[결재] Div하위의 컴포넌트 Enable 처리 제외할 컴포넌트설정
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_NfahNo.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PalmNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_FiamGu.set_enable(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_FasmCt.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_AcchNo.set_readonly(true);
		//this.div_MasterInput.form.tab_Master.tpg_01.form.mae_AdvrAm.set_readonly(true);
		//this.div_MasterInput.form.tab_Master.tpg_01.form.mae_AntiNo.set_readonly(true);
		//this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AntiSe.set_readonly(true);
		//this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AdvaCd.set_readonly(true);
		//this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AdvaNm.set_readonly(true);
		//this.div_MasterInput.form.tab_Master.tpg_01.form.mae_RpamAm.set_readonly(true);

		//[전표보기] 결재진행 상태가 승인이 아닌경우 => [전표보기] 버튼 비활성화
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(false);

		var iTabIndex = this.div_MasterInput.form.tab_Master.tabindex;		

		//Tab-신규자산내역
		if (iTabIndex == 0) {	
			this.div_MasterInput.form.div_ComGrdBtn.set_visible(false);
		} 
		//Tab-자동전표
		else if (iTabIndex == 1) {	
			this.fn_SetGridAtobButton();//[공통] 자동전표상세 Div Url 및 입력 버튼 설정	
		//Tab-첨부파일
		} else if (iTabIndex == 2) {	
			this.div_MasterInput.form.div_ComGrdBtn.set_visible(true);
			this.fn_SetGridFileButton();//[공통] 그리드 파일 버튼 설정
		}
	
		//상각구분에 따른 활성화/비활성화 (11/29)
		this.fn_SetFiamGuCompEnable();
	}
}

