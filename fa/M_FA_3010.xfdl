 ***********************************************************************************************/ 
this.fn_GetNotDupCheck = function()
{
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Master20Check.clearData();
	this.ds_Input.clearData();

	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "NFAH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "NFAH_NO"));

