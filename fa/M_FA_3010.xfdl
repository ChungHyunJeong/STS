				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "ANTI_SE", arrRtnVal.ACCB_SE); //반제 발의순번
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "ADVR_AM", arrRtnVal.MIBAN_AM); //잔액
			}
		} else {
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "ADVA_CD", "");
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "ADVA_NM", "");
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "ANTI_NO", ""); //반제 발의번호
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "ANTI_SE", ""); //반제 발의순번
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "ADVR_AM", ""); //잔액
		}

	//[Detail영역]
		
	//[Detail영역-검색]
	}
	
};
 
/***********************************************************************************************
 * 13.03. [메세지] CallBack 함수 처리
 ***********************************************************************************************/  
this.fn_MsgCallback = function (strId, strVal)
{
	//삭제여부
	if (strId == "CONF.DELETE") {
		if(strVal )
		{
			this.fn_SvcDelete();//삭제 서비스 호출
		}
	//MASTER 변경사항 저장여부
	} else if (strId == "CONF.MAST_CHANGE") {
		if(strVal )
		{
			this.fn_ComBtnSave();//저장 서비스 호출
		} else {
			//데이터 변경여부 체크
			if (this.gfn_DsIsUpdated(this.ds_Master10)) {
				this.gfn_UndoColumn(this.div_MasterView.form.grd_Master10);
			}					
		}
	//[결재] 재기안여부
	} else if (strId == "CONF.RECIDE") {
		if(strVal) {
			this.fn_ComBtnRecideProc();//[결재] 재기안 서비스 호출
		}	
		
	}
}; 
 
