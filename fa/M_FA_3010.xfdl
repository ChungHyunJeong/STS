			this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PalmNm.set_value("");
		}	
	//선급금계정코드
	} else if(strId == "M_BANJE_CD_01"){
		//계정 popup
			rtnValue = rtnValue.split("@^&");
		if (!this.gfn_IsNull(rtnValue[0])) {
