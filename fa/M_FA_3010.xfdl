this.fn_ComBtnAdd = function()
{
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자

	var iAddRow = this.ds_Master10.addRow();
	this.fvMmCloseDay = sToDay;//[월마감] 마감일자

	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", "1");//결재차수
	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");//최종여부	
	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//관리부서코드	
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//관리부서명	
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//관리자사번	
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//관리자명		
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//관리자ID	
	this.ds_Master10.setColumn(iAddRow, "ACUM_CD", this.gfn_GetSessionValue("ACUM_CD"));//회계단위
	this.ds_Master10.setColumn(iAddRow, "ACUM_NM", this.gfn_GetSessionValue("ACUM_NM"));//회계단위명	
	this.ds_Master10.setColumn(iAddRow, "LEAS_YN", "0");//리스구분 (Default : 일반)
	this.ds_Master10.setColumn(iAddRow, "ACQU_GU", "1");//취득구분 (Default : 신규)
// 	this.ds_Master10.setColumn(iAddRow, "ADVA_AM", 0); //선급금 	
// 	this.ds_Master10.setColumn(iAddRow, "VTAX_AM", 0); //부가세 	
	this.ds_Master10.setColumn(iAddRow, "NFAH_DD", sToDay);//취득일자-오늘날자로 설정


	//자동전표생성시 디폴트는 행추가 버튼 안보이게 처리
	eval(this.fvAtobDiv).fn_ScrnEnable(false);
	
	//선지급금계정코드 상태 초기화(12/16 추가개발)
// 	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AdvaCd.set_readonly(true);
// 	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AdvaCd.set_cssclass("");

	//신규자산내역 Tab이동
	this.div_MasterInput.form.tab_Master.set_tabindex(0);

	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	this.fn_SetWfCompEnable();

	//처음 입력 항목에 포커스
	this.div_MasterInput.form.tab_Master.tpg_01.form.cal_NfahDd.setFocus();
	
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
	//첨부파일정보 권한설정
