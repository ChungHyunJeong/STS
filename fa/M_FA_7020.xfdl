this.fn_DataCreate = function()
{
	//유효성 체크
	if (!this.fn_VaildMasterSave()) return;
	
	if (this.ds_Master20.getRowCount() > 0) {
		var sFunhNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FUNH_NO");
		var sFunhNo2 = this.ds_Master20.getColumn(0, "FUNH_NO");
		if (sFunhNo != sFunhNo2) {
			//var sFunhNo = this.gfn_StringFormat(this.ds_Master20.getColumn(0, "FUNH_NO"), "@@@@@@-@@@@");
			var arrMsgArg = ["기준일자와 회계단위로 등록된 자료"] //해당 &ITM0(이)가 이미 존재합니다.
			this.gfn_Alert("SD008", arrMsgArg);
			return;
		}
	}
	
	if( this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FUNH_NO"))){
	
		this.fvSchKeyYnDCreate = true;
		//관리번호 없을 시 저장부터 한다.
		this.fn_ComBtnSave();
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FunhDd.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumCd.set_readonly(true);
	}
	
	
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_InputSch.clearData();
	this.ds_Detail10.clearData();
	this.ds_Detail20.clearData();
	this.ds_Detail30.clearData();
	this.ds_Detail40.clearData();
	this.ds_Detail50.clearData();
	this.ds_Detail60.clearData();
	this.ds_Detail70.clearData();
	
	this.ds_InputSch.addRow();
	this.ds_InputSch.setColumn(0, "FROM_DD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FUNH_DD"));//기준일자
	this.ds_InputSch.setColumn(0, "ACUM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ACUM_CD"));//회계단위
	this.ds_InputSch.setColumn(0, "FUNH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FUNH_NO"));//관리번호
	this.ds_InputSch.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));//결재차수
	
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	//var sToDay = "20210701"
	
	this.ds_InputSch.setColumn(0, "FR_DD", sToDay.substr(0,6));//당월+1
	this.ds_InputSch.setColumn(0, "CL_DD", this.gfn_AddMonth(sToDay,4).substr(0,6));//당월+5

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "fa:M_FA_7020_D_SL11";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	svcAddObj.sSelectSql = "fa:M_FA_7020_D_SL21";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	svcAddObj.sSelectSql = "fa:M_FA_7020_D_SL31";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	svcAddObj.sSelectSql = "fa:M_FA_7020_D_SL41";
	this.gfn_AddComSearchService(this, svcAddObj);
	//금일자금수지실적
	svcAddObj.sSelectSql = "fa:M_FA_7020_D_SL51";
	this.gfn_AddComSearchService(this, svcAddObj);
	//익일자금소요자금계획및재무대책
	svcAddObj.sSelectSql = "fa:M_FA_7020_D_SL61";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	svcAddObj.sSelectSql = "fa:M_FA_7020_D_SL71";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "DETAIL_CREATE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
