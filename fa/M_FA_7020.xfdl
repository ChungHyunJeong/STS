 ***********************************************************************************************/
/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 

	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.gfn_DsIsUpdated(this.ds_Detail20) && !this.gfn_DsIsUpdated(this.ds_Detail30)
	 && !this.gfn_DsIsUpdated(this.ds_Detail40) && !this.gfn_DsIsUpdated(this.ds_Detail51) && !this.gfn_DsIsUpdated(this.ds_Detail61) && !this.gfn_DsIsUpdated(this.ds_Detail70)
	 && !this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.
		return;
	}

	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		//첨부파일정보만 변경시
		if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.gfn_DsIsUpdated(this.ds_Detail20) && !this.gfn_DsIsUpdated(this.ds_Detail30)
		 && !this.gfn_DsIsUpdated(this.ds_Detail40) && !this.gfn_DsIsUpdated(this.ds_Detail51) && !this.gfn_DsIsUpdated(this.ds_Detail61) && !this.gfn_DsIsUpdated(this.ds_Detail70)
		 && this.ds_Master10.getRowCount() > 0) {
			//첨부파일정보 저장
			fvFileInfo = new Object();//[공통] 첨부파일정보
			fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, B-상세(그리드Row), E-기타
			fvFileInfo.CALLBACK_FUNC = "";//호출할 콜백함수
			fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
			fvFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FUNH_NO");//PK번호2-관리번호
			fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
			//첨부파일Div 화면에 Key세팅하게 변경
			this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);
			return;	
		}
	}

	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		if (!this.fn_VaildMasterSave()) return;
	}
	
	if (this.ds_Master20.getRowCount() > 0) {
		var sFunhNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FUNH_NO");
		var sFunhNo2 = this.ds_Master20.getColumn(0, "FUNH_NO");
		if (sFunhNo != sFunhNo2) {
			var arrMsgArg = ["기준일자와 회계단위로 등록된 자료"] //해당 &ITM0(이)가 이미 존재합니다.
			this.gfn_Alert("SD008", arrMsgArg);
			return;
		}
	}
	
	//Detail10 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		if (!this.fn_VaildDetailSave()) return;
	}
	//Detail20 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail20)) {
		if (!this.fn_VaildDetail20Save()) return;
	}
	//Detail30 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail30)) {
		if (!this.fn_VaildDetail30Save()) return;
	}
	//Detail40 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail40)) {
		if (!this.fn_VaildDetail40Save()) return;
	}
	//Detail50 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail51)) {
		if (!this.fn_VaildDetail50Save()) return;
	}
	//Detail60 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail61)) {
		if (!this.fn_VaildDetail60Save()) return;
	}
	//Detail70 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail70)) {
		if (!this.fn_VaildDetail70Save()) return;
	}
	
	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	//Master 입력 상태이면
	if (sChkRowType == "I") {
	
		//채번-관리번호
		var sFunhNo = this.gfn_GetTbnmNo(this.fvComPrgmId, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FUNH_DD"));//관리번호
		
		if (this.gfn_IsNull(sFunhNo)) {
			//관리번호를 가져오는데 실패했습니다.
			return;
		}
		var sChkCd = "";//체크용
		var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position
		
		//Master 데이터셋에 채번정보 입력-관리번호
		this.ds_Master10.setColumn(iMasterRowPos, "FUNH_NO", sFunhNo);
		var sWofhSq = this.ds_Master10.getColumn(iMasterRowPos, "WOFH_SQ");//결재차수
		
		//Detail10 데이터셋에 기본정보 입력-관리번호/결재차수
		for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
			sChkCd = this.ds_Detail10.getColumn(iLoop, "COMM_CD");
			
			if (this.gfn_IsNull(sChkCd)) {
				this.ds_Detail10.setColumn(iLoop, "FUNH_NO", sFunhNo);
				this.ds_Detail10.setColumn(iLoop, "WOFH_SQ", sWofhSq);
			}
		}
		//Detail20 데이터셋에 기본정보 입력
		for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
			sChkCd = this.ds_Detail20.getColumn(iLoop, "COMM_CD");
			
			if (this.gfn_IsNull(sChkCd)) {
				this.ds_Detail20.setColumn(iLoop, "FUNH_NO", sFunhNo);
				this.ds_Detail20.setColumn(iLoop, "WOFH_SQ", sWofhSq);
			}
		}
		//Detail30 데이터셋에 기본정보 입력
		for (var iLoop=0; iLoop<this.ds_Detail30.getRowCount(); iLoop++) {
			sChkCd = this.ds_Detail30.getColumn(iLoop, "COMM_CD");
			
			if (this.gfn_IsNull(sChkCd)) {
				this.ds_Detail30.setColumn(iLoop, "FUNH_NO", sFunhNo);
				this.ds_Detail30.setColumn(iLoop, "WOFH_SQ", sWofhSq);
			}
		}
		//Detail40 데이터셋에 기본정보 입력
		for (var iLoop=0; iLoop<this.ds_Detail40.getRowCount(); iLoop++) {
			sChkCd = this.ds_Detail40.getColumn(iLoop, "COMM_CD");
			
			if (this.gfn_IsNull(sChkCd)) {
				this.ds_Detail40.setColumn(iLoop, "FUNH_NO", sFunhNo);
				this.ds_Detail40.setColumn(iLoop, "WOFH_SQ", sWofhSq);
			}
		}
		//Detail50 데이터셋에 기본정보 입력
		for (var iLoop=0; iLoop<this.ds_Detail50.getRowCount(); iLoop++) {
			sChkCd = this.ds_Detail50.getColumn(iLoop, "COMM_CD");
			
			if (this.gfn_IsNull(sChkCd)) {
				this.ds_Detail50.setColumn(iLoop, "FUNH_NO", sFunhNo);
				this.ds_Detail50.setColumn(iLoop, "WOFH_SQ", sWofhSq);
			}
		}
		//Detail60 데이터셋에 기본정보 입력
		for (var iLoop=0; iLoop<this.ds_Detail60.getRowCount(); iLoop++) {
			sChkCd = this.ds_Detail60.getColumn(iLoop, "COMM_CD");
			
			if (this.gfn_IsNull(sChkCd)) {
				this.ds_Detail60.setColumn(iLoop, "FUNH_NO", sFunhNo);
				this.ds_Detail60.setColumn(iLoop, "WOFH_SQ", sWofhSq);
			}
		}
		//Detail70 데이터셋에 기본정보 입력
		for (var iLoop=0; iLoop<this.ds_Detail70.getRowCount(); iLoop++) {
			sChkCd = this.ds_Detail70.getColumn(iLoop, "COMM_CD");
			
			if (this.gfn_IsNull(sChkCd)) {
				this.ds_Detail70.setColumn(iLoop, "FUNH_NO", sFunhNo);
				this.ds_Detail70.setColumn(iLoop, "WOFH_SQ", sWofhSq);
			}
		}
	}
	
	
	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
