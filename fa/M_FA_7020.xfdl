	this.gfn_SetGrid(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);//1.일일자금수지현황
	this.gfn_SetGrid(this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20);//2.어음및차입금현황
	this.gfn_SetGrid(this.div_DetailInput.form.tab_Detail.tpg_03.form.grd_Detail30);//3.지급어음사용한도및할인현황
	this.gfn_SetGrid(this.div_DetailInput.form.tab_Detail.tpg_04.form.grd_Detail40);//4.어음수지현황
	this.gfn_SetGrid(this.div_DetailInput.form.tab_Detail.tpg_05.form.grd_Detail50);//5.금일자금수지실적
	this.gfn_SetGrid(this.div_DetailInput.form.tab_Detail.tpg_06.form.grd_Detail60);//6.익일자금소요자금계획및재무대책
	this.gfn_SetGrid(this.div_DetailInput.form.tab_Detail.tpg_07.form.grd_Detail70);//7.계좌현황
	//스플리터 초기화 함수 호출
	this.gfn_InitSpliter(this.sta_HSplitter, "horizontal", -1, -1);
	
	//스플리터 초기화 함수 호출
	this.gfn_InitSpliter(this.sta_VSplitter, "vertical", -1, -1);	

