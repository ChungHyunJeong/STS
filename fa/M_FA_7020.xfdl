/***********************************************************************************************
 * 15.08. [사용자함수] ds_Detail 세팅작업
 ***********************************************************************************************/ 
 
/* 저장 데이터셋, 복사 데이터 셋, 구분자가 끝나는 행의 순번 */
this.fn_SetDetail = function (sDatasetNm, sDatasetNm1, count, Gu)
{
	count = nexacro.toNumber(count);
	
	am = "FUN" + Gu + "_AM";
	am1 = "FUN" + Gu + "_AM1";
	
	/* 소계세팅  */
	var sSuSum = 0;//수입소계1
	var sJiSum = 0;//지출소계1
 	var mSuSum = 0;//수입소계2
 	var mJiSum = 0;//지출소계2
	var LSuSum = 0;//수입소계3
	var LJiSum = 0;//지출소계3
	
	for (var iLoop=0;iLoop<sDatasetNm1.getRowCount();iLoop++) {
		//구분1
		var sSogeGu = sDatasetNm1.getColumn(iLoop, "SOGE_GU");	 //소계구분자
		var dFunfAm = nexacro.toNumber(sDatasetNm1.getColumn(iLoop, am));	 //금액
		//구분2
		var sSogeGu1 = sDatasetNm1.getColumn(iLoop, "SOGE_GU1");//소계구분자
		var dFunfAm1 = nexacro.toNumber(sDatasetNm1.getColumn(iLoop, am1));//금액
		
		//구분1
		if (sSogeGu == 0) {//소계가 아니면
			sSuSum += dFunfAm;
			mSuSum += dFunfAm;//소계2
			LSuSum += dFunfAm;//소계3
		} else if (sSogeGu == 1) {//소계1이라면
			sDatasetNm1.setColumn(iLoop, am, sSuSum);
			sSuSum = 0;	//초기화
		} else if (sSogeGu == 2) {//소계2라면
			sDatasetNm1.setColumn(iLoop, am, mSuSum);
			mSuSum = 0;	//초기화
		} else if (sSogeGu == 3) {//소계3이라면
			sDatasetNm1.setColumn(iLoop, am, LSuSum);
		}
		
		//구분2
		if (sSogeGu1 == 0) {//소계가 아니면
			sJiSum += dFunfAm1;
			mJiSum += dFunfAm1;//소계2
			LJiSum += dFunfAm1;//소계3
		} else if (sSogeGu1 == 1) {//소계라면
			sDatasetNm1.setColumn(iLoop, am1, sJiSum);
			sJiSum = 0;	//초기화
		} else if (sSogeGu1 == 2) {//소계2라면
			sDatasetNm1.setColumn(iLoop, am1, mJiSum);
			mJiSum = 0;	//초기화
		} else if (sSogeGu1 == 3) {//소계3이라면
			sDatasetNm1.setColumn(iLoop, am1, LJiSum);
		}
		
		if (iLoop < count) {
			sDatasetNm.setColumn(iLoop, am, sDatasetNm1.getColumn(iLoop, am));//금액
			sDatasetNm.setColumn(iLoop, "REMK_NM", sDatasetNm1.getColumn(iLoop, "REMK_NM"));	//적요
		}
		
		sDatasetNm.setColumn(iLoop+count, am, sDatasetNm1.getColumn(iLoop, am1));//금액
		sDatasetNm.setColumn(iLoop+count, "REMK_NM", sDatasetNm1.getColumn(iLoop, "REMK_NM1"));	//적요
		
		sDatasetNm.setColumn(iLoop, "crudType", "I");	//수입
		sDatasetNm.setColumn(iLoop+count, "crudType", "I");	//수입
		
	}
};


