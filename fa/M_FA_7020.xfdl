	this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcumCd.set_enable(true);//회계버튼 - 새로입력 제외하고는 수정되면 안됨

	
	//처음 입력 항목에 포커스
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumCd.setFocus();
	
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
	//첨부파일정보 권한설정
