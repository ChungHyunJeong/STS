this.fn_VaildMasterSave = function()
{
	//[저장] Validation 설정
	var arrArgs = ["this.div_MasterInput.form.tab_Master.tpg_01.form.cal_FunhDd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_FunhDd.text + "|NULL" //기준일자
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_DepmCd.text + "|NULL" //등록부서
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmNm^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_DepmCd.text + "|NULL" //등록부서_명
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SystId^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_EmpmCd.text + "|NULL" //등록자
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_EmpmCd.text + "|NULL" //등록자_명
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_AcumCd.text + "|NULL" //회계단위
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumNm^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_AcumCd.text + "|NULL" //회계단위_명
			      ]; 
				  
	var sCheckGroupTitle = this.div_MasterInput.form.tab_Master.tpg_01.text;//체크그룹 타이틀

	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
		return false;
	}
	
	//날짜체크
	var sFunhDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FUNH_DD"); //기준일자

	if(sFunhDd.trim().length < 8 || sFunhDd.substr(0,4) > "2999" || sFunhDd.substr(0,4) < "1990" || sFunhDd.substr(4,2) < "01" || sFunhDd.substr(4,2) > "12"
	|| sFunhDd.substr(6,2) < "01" || sFunhDd.substr(6,2) > "31"){
		
		var arrMsgArg = [" 기준일자("+ sFunhDd.substr(0,4) + "-" + sFunhDd.substr(4,2) + "-" + sFunhDd.substr(6,2) +")"];
		this.gfn_Alert("SM017", arrMsgArg); //적용년월은 유효하지 않은 날짜 형식입니다.
		return false;
	}

	//해당 기준일자, 회계단위 되어있는 데이터 있는지 체크
	this.ds_Master20.clearData();
	this.ds_Input.clearData();
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "FROM_DD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FUNH_DD"));
	this.ds_Input.setColumn(0, "ACUM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ACUM_CD"));

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "fa:M_FA_7020_M_SL20";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MST20_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_FA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Master20=ds_Master20";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "";//공통버튼구분-조회버튼
