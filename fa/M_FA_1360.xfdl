this.fn_ComBtnAdd = function()
{
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	var sToLDay = this.gfn_GetCommDate("monthLastday");//마지막일자
	
	var iAddRow = this.ds_Master10.addRow();
	//월마감
	this.fvMmCloseDay = sToDay;//[월마감] 마감일자
	
	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", "1");//결재차수
	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");//최종여부
	this.ds_Master10.setColumn(iAddRow, "WOFH_ST", "0010");//결재상태-등록(0010)	
	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));
	this.ds_Master10.setColumn(iAddRow, "ACUM_CD", this.gfn_GetSessionValue("ACUM_CD"));
	this.ds_Master10.setColumn(iAddRow, "ACUM_NM", this.gfn_GetSessionValue("ACUM_NM"));
	this.ds_Master10.setColumn(iAddRow, "CRDH_DD", sToLDay);
	this.ds_Master10.setColumn(iAddRow, "CRDH_MM", sToDay.substr(0,6));
	
	eval(this.fvAtobDiv).fn_ClearAtobData();//자동전표 정보 초기화
	
	this.ds_Master10.setColumn(this.ds_Master10.rowposition, "ACCH_NM", this.fvMenuNm+ " " +sToDay.substr(0,4)+"-"+sToDay.substr(4,2));
	this.ds_Master10.setColumn(this.ds_Master10.rowposition, "REMK_NM", this.fvMenuNm+ " " +sToDay.substr(0,4)+"-"+sToDay.substr(4,2));
		
	//입력영역 활성화
 	this.div_MasterInput.set_enable(true);
 	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
 	this.div_DetailInput.form.tab_Detail.set_enable(true);

	//this.fn_SetWfCompEnable();
	//처음 입력 항목에 포커스
	this.div_MasterInput.form.tab_Master.tpg_01.form.cal_CrdhDD.setFocus();
	
	this.ds_Detail10.clearData();//상세정보 초기화
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
}

