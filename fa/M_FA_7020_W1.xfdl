 * 15.06. [사용자함수] ds_Detail50 세팅작업
 ***********************************************************************************************/ 
this.fn_SetDetail50 = function ()
{
	var sWofhYn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_YN");
	var sWofhSt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST");
	var sCommcd = this.ds_Detail50.getColumn(0, "COMM_CD");
	
	this.ds_Detail51.clearData();
	
	if ((sWofhYn == "1" && (sWofhSt == "0010"|| this.gfn_IsNull(sWofhSt))) || (sWofhYn == "1" && sWofhSt!= "0010" && !this.gfn_IsNull(sCommcd))) {
	} else {
		return;
	}
	
	this.ds_Detail50.addColumn("s_chk", "string");
	this.ds_Detail50.addColumn("crudType", "string");
	this.ds_Detail50.addColumn("FUNF_NM1", "string");
	this.ds_Detail50.addColumn("FUNF_AM1", "string");
	this.ds_Detail50.addColumn("REMK_NM1", "string");
	this.ds_Detail50.addColumn("GU1", "string");
	this.ds_Detail50.addColumn("SOGE_GU1", "string");
	
	for (var iLoop=0;iLoop<this.ds_Detail50.getRowCount();iLoop++) {
		var sFunfGu = this.ds_Detail50.getColumn(iLoop, "FUNF_GU");
		var sCommCd = this.ds_Detail50.getColumn(iLoop, "COMM_CD");
		//지출
		if (sFunfGu == "지출") {
			this.ds_Detail50.setColumn(iLoop-38, "FUNF_NM1", this.ds_Detail50.getColumn(iLoop, "FUNF_NM"));
			this.ds_Detail50.setColumn(iLoop-38, "FUNF_AM1", this.ds_Detail50.getColumn(iLoop, "FUNF_AM"));
			this.ds_Detail50.setColumn(iLoop-38, "REMK_NM1", this.ds_Detail50.getColumn(iLoop, "REMK_NM"));
			this.ds_Detail50.setColumn(iLoop-38, "GU1", this.ds_Detail50.getColumn(iLoop, "GU"));
			this.ds_Detail50.setColumn(iLoop-38, "SOGE_GU1", this.ds_Detail50.getColumn(iLoop, "SOGE_GU"));
		}
	}
	var jLoop = 0;//crudType 수정하기 위해
	//38-75행까지 지우기
	for (var iLoop=38;iLoop<76;iLoop++) {
		this.ds_Detail50.deleteRow(38);
		this.ds_Detail50.setColumn(jLoop, "crudType", "");
		jLoop++;
	}
	
	this.ds_Detail51.copyData(this.ds_Detail50);
	this.ds_Detail50.reset();
};


