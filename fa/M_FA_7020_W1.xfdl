                                <Cell col="3" text="bind:DEBI_AM" textAlign="right" displaytype="mask" maskeditformat="#,##0"/>
                                <Cell col="4" text="bind:CRED_AM" textAlign="right" displaytype="mask" maskeditformat="#,##0"/>
                                <Cell col="5" textAlign="right" displaytype="number" expr="PRED_AM + DEBI_AM - CRED_AM"/>
                                <Cell col="6" text="bind:REMK_NM"/>
                                <Cell col="7" expr="dataset.getRowLevel(currow)"/>
                              </Band>
                              <Band id="summary">
                                <Cell textAlign="center"/>
                                <Cell col="1" text="합계" textAlign="center"/>
                                <Cell col="2" expr="dataset.getSum(&quot;PRED_AM&quot;)" displaytype="number"/>
                                <Cell col="3" displaytype="number" expr="dataset.getSum(&quot;DEBI_AM&quot;)"/>
                                <Cell col="4" expr="dataset.getSum(&quot;CRED_AM&quot;)" displaytype="number"/>
                                <Cell col="5" expr="dataset.getSum(&quot;PRED_AM&quot;) + dataset.getSum(&quot;DEBI_AM&quot;) - dataset.getSum(&quot;CRED_AM&quot;)" displaytype="number"/>
                                <Cell col="6" expr="dataset.getSum(&quot;DEBI_AM&quot;) - dataset.getSum(&quot;CRED_AM&quot;)" displaytype="number"/>
                                <Cell col="7"/>
                              </Band>
                            </Format>
