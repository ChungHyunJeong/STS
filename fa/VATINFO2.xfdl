	var va42Am = this.ds_Master10.getColumn(0, "VA42_AM");
	var va42Va = this.ds_Master10.getColumn(0, "VA42_VA");
	var va43Am = this.ds_Master10.getColumn(0, "VA43_AM");
	var va43Va = this.ds_Master10.getColumn(0, "VA43_VA");
	var va44Am = this.ds_Master10.getColumn(0, "VA44_AM");
	var va44Va = this.ds_Master10.getColumn(0, "VA44_VA");
	var va45Va = this.ds_Master10.getColumn(0, "VA45_VA");
	var va46Va = this.ds_Master10.getColumn(0, "VA46_VA");
	var va47Va = this.ds_Master10.getColumn(0, "VA47_VA");
	var va48Va = this.ds_Master10.getColumn(0, "VA48_VA");
	
