	if(this.gfn_IsNull(va44Am)){va44Am=0};
	if(this.gfn_IsNull(va44Va)){va44Va=0};
	if(this.gfn_IsNull(va45Va)){va45Va=0};
	if(this.gfn_IsNull(va46Va)){va46Va=0};
	if(this.gfn_IsNull(va47Va)){va47Va=0};
	if(this.gfn_IsNull(va48Va)){va48Va=0};
	
	
	//trace("va41Am + va42Am + va43Am + va44Am = " + va41Am + va42Am + va43Am + va44Am);
	
	this.ds_Master10.setColumn(0, "VA49_AM", va41Am + va42Am + va43Am + va44Am  );
	this.ds_Master10.setColumn(0, "VA49_VA", va41Va + va42Va + va43Va + va44Va + va45Va + va46Va + va47Va + va48Va);
	
