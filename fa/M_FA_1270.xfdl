/***********************************************************************************************
 * 15.06. [그리드 버튼] keystring 정렬
 ***********************************************************************************************/ 
this.div_MasterView_tab_Master_tpg_01_grd_Master10_onheadclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{

	/*
	 그룹소계가 있고 정렬시 그룹소계는 해제 한다.
	*/
	var sortInfos = obj.sortInfos;
	var sortItems = obj.sortItems;
	
	
	//그룹초기화
	this.ds_Master10.set_keystring("");
	
	if ( this.gfn_IsNull(sortInfos) || this.gfn_IsNull(sortItems) ) return;

	// 정렬상태 초기화.
	for( var j=0; j<sortItems.length;j++){
		var col = sortItems[j];
		var sortInfo = sortInfos[col];

	}
	
	//정렬상태값 0: 없음, 1 ASC, 2 DESC 
	//이지만 상태값은 전단계상태값만 가져온다.
	if(sortInfo.status != "2"){
		this.ds_Master10.set_keystring("");
	}else{
		//정렬이 없을시 다시 그룹화 한다.
		this.ds_Master10.set_keystring("S:ACOM_CD+NUM,G:ACOM_CD,G:ACOM_CD+SLIP_MM");
	}

};




/***********************************************************************************************
 * 15.06. [계정코드 변경시]  datastore ds_acomMa 조회
 ***********************************************************************************************/ 
this.fn_dsAcomMa = function(acomCd)
{
	//acomCd : 계정코드
	//1. ds_acomMa 조회
	this.ds_Input.clearData();
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "ACOM_CD", acomCd);
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	
	//[공통]서비스정보 추가
