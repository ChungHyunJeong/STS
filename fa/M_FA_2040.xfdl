this.fn_DataCreate = function() 
{
	//자료생성전 유효성 체크
	if (!this.fn_VaildMasterSave()) return;	
	

	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Detail10.clearData();
	this.ds_Detail11.clearData();
	this.ds_Detail20.clearData();
	this.ds_Detail30.clearData();
	this.ds_InputSch.clearData();
	
	//[업무] 조회조건 입력
	this.ds_InputSch.addRow();	
	this.ds_InputSch.setColumn(0, "VATH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "VATH_NO"));//관리번호
	this.ds_InputSch.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));//차수
	this.ds_InputSch.setColumn(0, "FROM_DD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FROM_DD"));//조회기간
	this.ds_InputSch.setColumn(0, "CLOS_DD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CLOS_DD"));//조회기간
	this.ds_InputSch.setColumn(0, "SAUM_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SAUM_NO"));//사업자등록번호
	//this.ds_InputSch.setColumn(0, "TAXB_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "")); //세금계산서구분(0010:세금계산서,0020:계산서,0030:신용카드,0040:없음)
		
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	//세금계산서합계표[왼쪽]
	svcAddObj.sSelectSql = "fa:M_FA_2040_D_SL11";
	this.gfn_AddComSearchService(this, svcAddObj);
 	//세금계산서합계표[오른쪽]
 	svcAddObj.sSelectSql = "fa:M_FA_2040_D_SL12";
 	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "DETAIL_CREATE";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_FA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_InputSch=ds_InputSch" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10 ds_Detail11=ds_Detail11";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	

}


