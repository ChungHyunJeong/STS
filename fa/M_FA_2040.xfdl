		return false;
	}	

	return true;
};  

 /*---------------------------------------------------------------------------------------------
 *    03. [데이터셋-ds_Master10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	
	//생성구분
	if (e.columnid == "CREA_GU") {
		var sCreaGu = e.newvalue;
		var sToday = this.gfn_GetCommDate("toDay");
		
		
		//생성구분이 확정기수일때
		if (sCreaGu == "0010") {
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ConfCd.set_enable(true);//확정기수 활성화
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ConfCd.set_index(3);//확정기수
			
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_VathMm.set_value("");	  //귀속년월 값 세팅
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_VathMm.set_enable(false);//귀속년월 비활성화
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SingGu.set_value("");	  //신고구분 값 세팅
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SingGu.set_enable(false);//신고구분 비활성화
			
			//조회기간 세팅
			var sConfCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CONF_CD");
			var iFindRow = this.ds_Fa425.findRowExpr("CODM_CD=='" + sConfCd + "'");
			var FromDd = this.ds_Fa425.getColumn(iFindRow, "OP01_NM");
			var ClosDd = this.ds_Fa425.getColumn(iFindRow, "OP02_NM");
			
			FromDd = sToday.substr(0,4) + FromDd.substr(0,2) + FromDd.substr(3,2);
			ClosDd = sToday.substr(0,4) + ClosDd.substr(0,2) + ClosDd.substr(3,2);
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", FromDd);
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", ClosDd);
		
		//생성구분이 귀속년월일때
		} else if (sCreaGu == "0020") {
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_VathMm.set_value(sToday.substr(0,6));//귀속년월 값 세팅
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_VathMm.set_enable(true);			  //귀속년월 활성화
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SingGu.set_index(0);				  //신고구분
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SingGu.set_enable(true);			  //신고구분 활성화
			
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ConfCd.set_value("");	  //확정기수 값 세팅
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ConfCd.set_enable(false);//확정기수 비활성화
			
			//조회기간 세팅
			var sVathMm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "VATH_MM");//귀속년월
			var sSingGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SING_GU");//신고구분
			var sLastday = this.gfn_GetLastDate(sVathMm);

			var FromDd = this.gfn_AddDate(this.gfn_AddMonth(sLastday, -sSingGu), 1);
			var ClosDd = sLastday;
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", FromDd);
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", ClosDd);

		}
		
	
	//확정기수(1기확정, 2기예정...)
	} else if (e.columnid == "CONF_CD") {
		var sToday = this.gfn_GetCommDate("toDay");
		var sConfCd = e.newvalue;
		var iFindRow = this.ds_Fa425.findRowExpr("CODM_CD=='" + sConfCd + "'");
		var FromDd = this.ds_Fa425.getColumn(iFindRow, "OP01_NM");
		var ClosDd = this.ds_Fa425.getColumn(iFindRow, "OP02_NM");
		
		FromDd = sToday.substr(0,4) + FromDd.substr(0,2) + FromDd.substr(3,2);
		ClosDd = sToday.substr(0,4) + ClosDd.substr(0,2) + ClosDd.substr(3,2);
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", FromDd);
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", ClosDd);

	//귀속년월
	} else if (e.columnid == "VATH_MM") {
		var sVathMm = e.newvalue;//귀속년월
		var sSingGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SING_GU");//신고구분
		var sLastday = this.gfn_GetLastDate(sVathMm);//귀속년월 마지막 일자

		var FromDd = this.gfn_AddDate(this.gfn_AddMonth(sLastday, -sSingGu), 1);
		var ClosDd = sLastday;
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", FromDd);
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", ClosDd);
		
	//신고구분
	} else if (e.columnid == "SING_GU") {
		var sVathMm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "VATH_MM");//귀속년월
		var sSingGu = e.newvalue;//신고구분
		var sLastday = this.gfn_GetLastDate(sVathMm);//귀속년월 마지막 일자
	
		var FromDd = this.gfn_AddDate(this.gfn_AddMonth(sLastday, -sSingGu), 1);
		var ClosDd = sLastday;
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_DD", FromDd);
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_DD", ClosDd);
	}

}

