this.fn_ComBtnAdd = function()
{
	
	//데이터 변경여부 체크
	if (this.fn_IsUpdateDataset()) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	var sToday = this.gfn_GetCommDate("toDay");
	
	var iAddRow = this.ds_Master10.addRow();
	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", "1");//결재차수는 1부터 시작
	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");//최종여부
	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//등록부서코드
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//등록부서명
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//등록자사번
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//등록자명
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//등록자ID
	this.ds_Master10.setColumn(iAddRow, "VATH_DD", sToday);//등록일자-오늘날짜로 설정
	this.ds_Master10.setColumn(iAddRow, "VATH_YY", sToday.substr(0,4));//신고년도-올해
	
	this.ds_Master10.setColumn(iAddRow, "FILE_YN", 0);//파일작성여부

	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.div_DetailInput.form.tab_Detail.set_enable(true);
	
	//처음 입력 항목에 포커스
 	this.div_MasterInput.form.tab_Master.tpg_01.form.spn_VathYy.setFocus();
	
 	//콤보 인덱스 설정
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_VathCd.set_index(0);//신고분류
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CreaGu.set_index(0);//생성구분
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ConfCd.set_index(3);//확정기수

	//enable 설정
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ConfCd.set_enable(true);//확정기수 활성화
	this.div_MasterInput.form.tab_Master.tpg_01.form.mae_VathMm.set_enable(false);//귀속년월 비활성화
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SingGu.set_enable(false);//신고구분 비활성화
	
	this.ds_Detail10.clearData();//상세정보 초기화
	this.ds_Detail11.clearData();//상세정보 초기화
	this.ds_Detail20.clearData();//상세정보 초기화
	this.ds_Detail30.clearData();//상세정보 초기화
	
	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
}

