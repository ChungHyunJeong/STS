this.div_MasterInput_tab_Master_tpg_01_cbo_VathCd_onitemchanged = function(obj:nexacro.Combo,e:nexacro.ItemChangeEventInfo)
{
	var sVathCd = obj.value;
	var sVathNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "VATH_NO");
	
	//세금계산서
	if (sVathCd == 0) {
		this.div_DetailInput.form.tab_Detail.set_tabindex(0);
