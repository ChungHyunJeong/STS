			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CreaGu.set_index(0);//생성구분 값을 확정기수로 변경
		}
		
	//계산서
	} else if (sVathCd == 1) {
		this.div_DetailInput.form.tab_Detail.set_tabindex(1);
	
	//신용카드
	} else if (sVathCd == 2) {
	
	//수출실적명세서
	} else if (sVathCd == 3) {
		this.div_DetailInput.form.tab_Detail.set_tabindex(2);
		if (this.gfn_IsNull(sVathNo)) {
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CreaGu.set_index(1);//생성구분 값을 귀속년월로 변경
		}
	} 
};

  
