/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	//trace("ds_Master10_onrowposchanged 1 ");
	if (e.newrow > -1) {
		//trace("ds_Master10_onrowposchanged 2 ");
		//[결재] 결재상태에 따른 컴포넌트 제어
		this.fn_SetWfCompEnable();		
		//trace("ds_Master10_onrowposchanged 3 ");
		//미지급정산정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BILH_NO"))) {
			//trace("ds_Master10_onrowposchanged 4 ");
			//미지급정산코드 포멧 설정 변경 (기존데이터용)
			//어음구분
			var bilmGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "DRAM_GU");
	   	    this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_BilmGu.set_value(bilmGu);
			
			
			var strBilhNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BILH_NO")
			//trace(strSalhNo.length);
			//trace(strSalhNo.substr(0,1)=="S");
			if(strBilhNo.length == 10){
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_BilhNo.set_format("######-####");
			}else{
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_BilhNo.set_format("########-###");
			}
			//입력영역 버튼 비활성화
			this.fn_SetGridInputButtonEnable();
			
			if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST") == "0040") {
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(true);
			} else {
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(false);
			}
			//등록일때 날짜 활성화
			if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST") == "0010") {
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtFr.set_readonly(false);
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtTo.set_readonly(false);
			} else {
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtFr.set_readonly(true);
				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtTo.set_readonly(true);
			}
			
			//[월마감제어-버튼/상세그리드]
			var sBUDG_MM = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BILH_DD"), "");//마감년월
			var sACUM_CD = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ACUM_CD"), "");//회계단위
			
			if (!this.gfn_IsNull(sBUDG_MM)) {
			
				//마감여부 체크
				var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
				var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
				paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
				paramObj.MODM_CD = "FA";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
				paramObj.ACUM_CD = sACUM_CD;//[선택] 회계단위
				paramObj.BUSM_CD  = "";//[선택] 사업부문
				paramObj.SAUM_CD  = "";//[선택] 사업장코드
				paramObj.DEPM_CD  = "";//[선택] 부서코드
				paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
				paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
				
				var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크
				
				//월마감된 경우
				if (bCheck) {
					this.fvMmCloseYn = true;//월마감여부-Tab버튼 제어용
				} else {
					this.fvMmCloseYn = false;//월마감여부-Tab버튼 제어용
				}
			}
			
			
			//trace("ds_Master10_onrowposchanged 5 ");
			
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "BILH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BILH_NO"));
			this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
			//this.ds_Input.setColumn(0, "PRGM_ID", this.fvComPrgmId);
			
			//trace("ds_Master10_onrowposchanged 6 ");
			
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "fa:M_FA_7620_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			//[공통]서비스정보 추가
// 			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
// 			svcAddObj.sSelectSql = "fa:M_FA_7620_D_SL20";
