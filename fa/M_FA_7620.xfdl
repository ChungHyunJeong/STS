 * 02.01. [공통버튼] 입력 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnAdd = function()
{
	//trace("fn_ComBtnAdd 1 ");
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}

	//trace("fn_ComBtnAdd 1 ");
	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	var sAcumCd = this.gfn_GetSessionValue("ACUM_CD");//회계단위
	
	this.fvMmCloseDay = sToDay;//[월마감] 마감일자
	this.fvMmCloseAcum = sAcumCd;//[월마감] 마감회계단위
	
	var iAddRow = this.ds_Master10.addRow();
	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", "1");//결재차수
	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");//최종여부	
	this.ds_Master10.setColumn(iAddRow, "WOFH_ST", "0010");	//결재상태	
	
	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//어음부서코드	
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//어음부서명	
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//어음자사번	
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//어음자명		
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//어음자ID	
	
	this.ds_Master10.setColumn(iAddRow, "BILH_DD", sToDay); //어음일자-오늘날자로 설정
	this.ds_Master10.setColumn(iAddRow, "ACUM_CD", sAcumCd);//회계단위
	this.ds_Master10.setColumn(iAddRow, "ACUM_NM", this.gfn_GetSessionValue("ACUM_NM"));//미지급정산일자-오늘날자로 설정
	
	//콤보 인덱스 설정
 	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_DramGu.set_index(0); 	//상대계정		(보통예금) 
 	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_BilhGu.set_index(0); 	//생성구분	(합산)
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_DramGu.set_readonly(false);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_BilhGu.set_readonly(false);

	//trace("fn_ComBtnAdd 3 ");

	//[결재]
	this.div_MasterInput.form.tab_Master.tpg_01.set_enable(true);	

	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.fn_SetGridInputButtonEnable();
	this.div_DetailInput.form.tab_Detail.set_enable(true);
	//처음 입력 항목에 포커스
	this.div_MasterInput.form.tab_Master.tpg_01.form.cal_TemhDt.setFocus();
	this.div_DetailInput.form.tab_Detail.set_tabindex(1);
	
	this.ds_Detail10.clearData();//상세정보 초기화
	this.ds_Detail20.clearData();//검색정보 초기화
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
	
	//첨부파일정보 권한설정
	var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
	oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
	this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
// 	
	//trace("fn_ComBtnAdd 4 ");
	//검색초기화
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtFr.set_readonly(false);
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cal_TemhDtTo.set_readonly(false);
    this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_BilmGu.set_readonly(true);
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_AcnmNo.set_readonly(false);
	
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_AcnmNo.set_value(""); //계좌번호
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_AcnmNm.set_value(""); //계좌번호명
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_BilmGu.set_index(0);  //어음구분
	
	
	eval(this.fvAtobDiv).fn_ClearAtobData();
	
	
	//trace("fn_ComBtnAdd 5 ");
	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
	
	//발의내용요약 변경
	this.fn_SetAcchNm();
	//trace("fn_ComBtnAdd 6 ");
}

