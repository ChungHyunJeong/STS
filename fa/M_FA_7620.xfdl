/***********************************************************************************************
 * 02.DG1.05. [Detail그리드2-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Choice = function()
{
	var miAm=0;
	var calcAm = 0;
	var arrArgs ;
	arrArgs = ["this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_AcnmNo^계좌번호|NULL"
				  ];


	
	var sCheckGroupTitle = "적용체크";//체크그룹 타이틀
	
	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
		return;
	}

	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {


			// 정산금액 체크
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				//정산금액 체크 
				calcAm = this.ds_Detail20.getColumn(iLoop, "CALC_AM");
				if (this.gfn_IsNull(calcAm)){
					calcAm = 0;
				}
				if(calcAm == 0){
					var arrMsgArg = ["정산금액","1"];
					this.gfn_Alert("SM055", arrMsgArg); //[정산금액]을(를) 1 이상 입력해 주세요.
					return;
				}
				
				miAm = this.ds_Detail20.getColumn(iLoop, "BILB_AM") - this.ds_Detail20.getColumn(iLoop, "GI_AM") - this.ds_Detail20.getColumn(iLoop, "CALC_AM")
	
				if(miAm < 0 ){
					var arrMsgArg = ["정산금액", "미정산금액"];
					this.gfn_Alert("SM056", arrMsgArg); //[정산금액]이(가) [미정산금액]을(를) 초과할 수 없습니다.
					return;
				}
			}
 

			var arrCheckRow = [];//체크된 Row
			

			
			
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
			
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
				
					var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)
					
					trace("iAddRow = " + iAddRow + " iLoop = " + iLoop);
					
					this.ds_Detail10.copyRow(iAddRow, this.ds_Detail20, iLoop);
					this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
					this.ds_Detail10.setColumn(iAddRow, "BILH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BILH_NO"));
					this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
					this.ds_Detail10.setColumn(iAddRow, "BILB_SE", "");//어음순번
					this.ds_Detail10.setColumn(iAddRow, "BANM_CD", this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_BanmCd.value); // 지급은행코드
					this.ds_Detail10.setColumn(iAddRow, "PABK_NM", this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_BanmNm.value); // 지급은행명
					this.ds_Detail10.setColumn(iAddRow, "ACNM_NO", this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_AcnmNo.value); // 계좌번호
					this.ds_Detail10.setColumn(iAddRow, "ACNM_NM", this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_AcnmNm.value); // 계좌명
					arrCheckRow.push(iLoop );
					
					//선택항목 비활성화처리
					this.ds_Detail20.setColumn(iLoop, "s_conf", "1");
					this.ds_Detail20.setColumn(iLoop, "s_chk", "0");//Uncheck로 변경
				}
			}  

			this.ds_Detail20.set_rowposition(-1);
			//어음상세로 Tab이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);
			
			//어음구분,정산구분 비활성
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_DramGu.set_readonly(true);
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_BilhGu.set_readonly(true);
		}else{
			//어음구분,정산구분 활성
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_DramGu.set_readonly(false);
			this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_BilhGu.set_readonly(false);
		
		}
	}
};

/***********************************************************************************************
 * 02.DG1.06. [Detail그리드2-버튼] 취소 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Cancel = function()
{
	this.ds_Detail10.reset();
	this.ds_Detail20.reset();

	//기존 체크정보 초기화
	for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
	
		if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
			this.ds_Detail20.setColumn(iLoop, "s_chk", "0");//Uncheck로 변경
