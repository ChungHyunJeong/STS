/***********************************************************************************************
 * 02.DG5.03. [자동전표상세 그리드-버튼] 자동전표 미리보기 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd01Atob = function()
{
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10) || this.gfn_DsIsUpdated(this.ds_Detail10)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}
	
	if (this.ds_Detail10.getRowCount() > 0) {
		var iCurRow = this.ds_Master10.rowposition;//현재 선택된 행
		
		if (iCurRow > -1) {
			var atobObj = new Object();//[공통] 자동전표 파라메타 정보 Object
			atobObj.COMM_CD = this.ds_Master10.getColumn(iCurRow, "COMM_CD");//회사코드
			//atobObj.AUTM_CD = this.fvAutmCd;//분류코드-T_FA_AUTM(자동전표환경설정) 테이블 내용 참조
			atobObj.AUTM_CD = "FA_5030_10";//분류코드-T_FA_AUTM(자동전표환경설정) 테이블 내용 참조
			atobObj.PRGM_ID = this.fvComPrgmId;//프로그램ID
			atobObj.MANA_NO = this.ds_Master10.getColumn(iCurRow, "PEXH_NO");//관리번호-업무Key
			atobObj.WOFH_SQ = this.ds_Master10.getColumn(iCurRow, "WOFH_SQ");//결재차수
			atobObj.REGI_ID = this.gfn_GetSessionValue("SYST_ID");//등록자
			atobObj.REGI_NM = this.gfn_GetSessionValue("USEM_NM");//등록자명
			atobObj.KEY_01 = "@COMM_CD=" + this.ds_Master10.getColumn(iCurRow, "COMM_CD");//T_FA_AUTD-자동전표환경설정-D 테이블의 쿼리에 사용하는 파라메타 Key-Value정보
			atobObj.KEY_02 = "@PEXH_NO=" + this.ds_Master10.getColumn(iCurRow, "PEXH_NO");
			atobObj.KEY_03 = "@WOFH_SQ=" + this.ds_Master10.getColumn(iCurRow, "WOFH_SQ");
			atobObj.KEY_04 = "";
			atobObj.KEY_05 = "";
			atobObj.KEY_06 = "";
			atobObj.KEY_07 = "";
			atobObj.KEY_08 = "";
			atobObj.KEY_09 = "";
			atobObj.KEY_10 = "";
			
			eval(this.fvAtobDiv).fn_DetailGrd01Atob(atobObj);//자동전표 미리보기
		}
	} else {
		var arrMsgArg = ["선급비용상세"];
		this.gfn_Alert("FA034", arrMsgArg) //[&ITM0]이(가) 없어 해당 작업을 하실 수 없습니다.
		return;
	}
};
