this.fn_ComBtnAdd = function()
{
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	
	var iAddRow = this.ds_Master10.addRow();
	this.ds_Master10.setColumn(iAddRow, "USED_YN", "1");//사용여부
	this.ds_Master10.setColumn(iAddRow, "DECL_YN", "0");//신고여부
	this.ds_Master10.setColumn(iAddRow, "COST_YN", "0");//ERP_원가계산여부(0F_1T)
	this.ds_Master10.setColumn(iAddRow, "PROD_YN", "0");//ERP_생산사업장여부(0F_1T)
	this.ds_Master10.setColumn(iAddRow, "MATE_YN", "0");//ERP_자재사업장여부(0F_1T)
	this.ds_Master10.setColumn(iAddRow, "CYBE_YN", "0");//ERP_가상사업장여부(0F_1T)
	this.ds_Master10.setColumn(iAddRow, "FROM_DD", sToDay);//생성일자-오늘날짜로 설정

	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	
	//처음 입력 항목에 포커스
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SaumCd.setFocus();
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SaumCd.set_enable(true); //사업장코드 활성화
	
	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
}

