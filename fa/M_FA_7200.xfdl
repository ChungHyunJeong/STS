this.div_MasterInput_btn_Apply_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	if (this.ds_Master10.rowposition > -1) {	
		if (this.ds_Master20.getCaseCount("s_chk=='1'") > 0) {
		
			var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
			
			if(sChkRowType != 'I'){							
				this.gfn_Alert("PS001"); //입력일 경우만 적용 가능합니다.
				return;				
			} else {
			
				var nRow = this.ds_Master20.findRowExpr("s_chk=='1'");				
				
				this.ds_Master10.copyRow(this.ds_Master10.rowposition, this.ds_Master20, nRow);
				
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "DECR_GU",  "D" ) //차대구분
				
				var strSlipNo = this.ds_Master20.getColumn(nRow, "SLIP_NO");
				var strAccbSe = this.ds_Master20.getColumn(nRow, "ACCB_SE");				
				
				for(var i=0 ; i<this.ds_Master20.getRowCount(); i++){
					var strSlipNoNext = this.ds_Master20.getColumn(i, "SLIP_NO");
					var strAccbSeNext = this.ds_Master20.getColumn(i, "ACCB_SE");
					
					if(strSlipNo != strSlipNoNext || strAccbSe != strAccbSeNext) {
						this.ds_Master20.setColumn(i,"s_conf", "0");						
					}
				}
				
			}
			
			//선택항목 비활성화처리			
			this.ds_Master20.setColumn(nRow, "s_conf", "1");//선급비용대상검색 컬러 변경 구분자 업데이트						
			this.ds_Master20.setColumn(nRow, "s_chk", "0");	//Uncheck로 변경			
			
			this.ds_Master20.set_rowposition(-1);
			//선급비용 마스터내역 Tab이동			
			this.div_MasterInput.form.tab_Master.set_tabindex(0);
		}
	}		
};


 

this.ds_Master20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
 	//선택	
 	if (e.columnid == "s_chk") {
 		var SlipNo = this.ds_Master20.getColumn(e.row, "SLIP_NO");
		var AccbSe = this.ds_Master20.getColumn(e.row, "ACCB_SE");		
		
 		var value = "0";
 		for (var iLoop = 0; iLoop < this.ds_Master20.getRowCount(); iLoop++) {
 			if (this.ds_Master20.getColumn(iLoop, "s_conf") != "1") {
 				value = (this.ds_Master20.getColumn(iLoop, "SLIP_NO") == SlipNo && this.ds_Master20.getColumn(iLoop, "ACCB_SE") == AccbSe) ? e.newvalue : "0";				
 				this.ds_Master20.setColumn(iLoop, "s_chk", value);
 			}
 		}
 	}	
};

