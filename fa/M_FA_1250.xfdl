/***********************************************************************************************
 * 15.13 [fn_ComMaPopupCallback ] 관리항목 팝업관련 콜백함수
 ***********************************************************************************************/ 
this.fn_ComMaPopupCallback = function(strId, rtnValue)
{
	// pop창 콜백 id
	trace("fn_ComMaPopupCallback strart");
	trace("fn_ComMaPopupCallback strId = " + strId);
	trace("fn_ComMaPopupCallback rtnValue = " + rtnValue);
	if (!this.gfn_IsNull(rtnValue)) {
		var arrRtnVal = eval(rtnValue);
		
		trace("this.fvCmMaRtnColCd == " + this.fvCmMaRtnColCd);
		trace("this.fvCmMaRtnColNm == " + this.fvCmMaRtnColNm);
		
		
		if (!this.gfn_IsNull(eval("arrRtnVal." + this.fvCmMaRtnColCd))) {
		
			
			trace("this.fvCmMaRtnColCd == " + eval("arrRtnVal." + this.fvCmMaRtnColCd));
			trace("this.fvCmMaRtnColNm == " + eval("arrRtnVal." + this.fvCmMaRtnColCd));
			this.div_Search.form.div_detacd.form.DeCd.set_value(eval("arrRtnVal." + this.fvCmMaRtnColCd));
			this.div_Search.form.div_detacd.form.DeNm.set_value(eval("arrRtnVal." + this.fvCmMaRtnColNm));
		}
	}	
	trace("fn_ComMaPopupCallback end");
	
}

/***********************************************************************************************
 * 15.13 [fn_AcomMaInsertItem ] 관리항목 콤보에 전체  관리항목을 넣는다
 ***********************************************************************************************/ 
this.fn_AcomMaInsertItem = function(){
		//계정없을시 관리항목을 다 넣는다.
		var iRow = -1;
		this.ds_AcomMa.clearData();
		
		for (var iLoop = 0; iLoop < this.ds_ItemCd.getRowCount(); iLoop++) {
			iRow = this.ds_AcomMa.addRow();
			this.ds_AcomMa.setColumn(iRow, "MA_CD", this.ds_ItemCd.getColumn(iLoop, "ITEM_CD"));
			this.ds_AcomMa.setColumn(iRow, "MA_NM", this.ds_ItemCd.getColumn(iLoop, "ITEM_NM"));
			
			this.ds_AcomMa.setColumn(iRow, "TYPE_CD", this.ds_ItemCd.getColumn(iLoop, "TYPE_CD"));
			this.ds_AcomMa.setColumn(iRow, "TYPE_NM", this.ds_ItemCd.getColumn(iLoop, "TYPE_NM"));
			this.ds_AcomMa.setColumn(iRow, "LENG_CT", this.ds_ItemCd.getColumn(iLoop, "LENG_CT"));
			this.ds_AcomMa.setColumn(iRow, "ITEM_GU", this.ds_ItemCd.getColumn(iLoop, "ITEM_GU"));
			this.ds_AcomMa.setColumn(iRow, "DETA_CD", this.ds_ItemCd.getColumn(iLoop, "DETA_CD"));
			this.ds_AcomMa.setColumn(iRow, "CODM_CD", this.ds_ItemCd.getColumn(iLoop, "CODM_CD"));
			this.ds_AcomMa.setColumn(iRow, "CD01_NM", this.ds_ItemCd.getColumn(iLoop, "CD01_NM"));
			this.ds_AcomMa.setColumn(iRow, "CD02_NM", this.ds_ItemCd.getColumn(iLoop, "CD02_NM"));
			this.ds_AcomMa.setColumn(iRow, "FUNC_NM", this.ds_ItemCd.getColumn(iLoop, "FUNC_NM"));
			this.ds_AcomMa.setColumn(iRow, "BALA_YN", this.ds_ItemCd.getColumn(iLoop, "BALA_YN"));
		}
}



