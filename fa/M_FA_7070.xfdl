 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "COMBO_SEARCH":
			this.ds_FaAcom.addColumn("SORT_NO", "string");
	
			//체크 콤보 설정값 만들기
			this.objConfig = {
					dropbutton	 : this.div_Search.form.btn_AcomCd, //Button 컴포너트ID
					innerdataset : this.ds_FaAcom, //데이터셋
					codecolumn	 : "ACOM_CD",   //코드컬럼
					datacolumn	 : "ACOM_NM", //코드값컬럼
					displayRowCount : 10 //화면표시건수
			};

			this.ds_FaAcom.filter("ACOM_GU=='0040'");	//미지급계정만
 			var iAddRow = this.ds_FaAcom.addRow();
 			this.ds_FaAcom.setColumn(iAddRow, "ACOM_NM", "ALL");
 			this.ds_FaAcom.setColumn(iAddRow, "SORT_NO", "1");
 			this.ds_FaAcom.set_keystring("S:+SORT_NO+ACOM_CD");

			//체크 콤보 초기화 함수 호출
			this.gfn_InitMultiCombo(this.div_Search.form.edt_AcomCd, this.objConfig);	
			//체크 콤보의 Code 값을 입력하는 함수 실행
			this.gfn_SetMultiComboCode(this.div_Search.form.edt_AcomCd, "ALL");//전체
			
			break;

		case "MST_SEARCH": //조회
			if (this.ds_Master10.getRowCount() > 0) {
			
				//입력영역 활성화
				this.div_DetailInput.set_enable(true);
								
				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_MasterView.form.tab_Master.tpg_01.form.grd_Master10);
				
			} else {
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
				
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_MasterView.form.tab_Master.tpg_01.form.grd_Master10);
			}		
			
			
			if (this.ds_Master20.getRowCount() > 0) {
			
				//입력영역 활성화
				this.div_DetailInput.set_enable(true);
								
				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_MasterView.form.tab_Master.tpg_02.form.grd_Master20);
				
			} else {
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master20.enableevent) {
					this.ds_Master20.set_enableevent(true);
				}
				
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_MasterView.form.tab_Master.tpg_02.form.grd_Master20);
			}	
			
			if (this.ds_Master30.getRowCount() > 0) {
			
				//입력영역 활성화
				this.div_DetailInput.set_enable(true);
								
				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_MasterView.form.tab_Master.tpg_03.form.grd_Master30);
				
			} else {
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master30.enableevent) {
					this.ds_Master30.set_enableevent(true);
				}
				
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_MasterView.form.tab_Master.tpg_03.form.grd_Master30);
			}	
			
			this.ds_FaAcom.addColumn("SORT_NO", "string");
	
			//체크 콤보 설정값 만들기
			this.objConfig = {
					dropbutton	 : this.div_Search.form.btn_AcomCd, //Button 컴포너트ID
					innerdataset : this.ds_FaAcom, //데이터셋
					codecolumn	 : "ACOM_CD",   //코드컬럼
					datacolumn	 : "ACOM_NM", //코드값컬럼
					displayRowCount : 10 //화면표시건수
			};

			this.ds_FaAcom.filter("ACOM_GU=='0040'");	//미지급계정만
 			var iAddRow = this.ds_FaAcom.addRow();
 			this.ds_FaAcom.setColumn(iAddRow, "ACOM_NM", "ALL");
 			this.ds_FaAcom.setColumn(iAddRow, "SORT_NO", "1");
 			this.ds_FaAcom.set_keystring("S:+SORT_NO+ACOM_CD");

			//체크 콤보 초기화 함수 호출
			this.gfn_InitMultiCombo(this.div_Search.form.edt_AcomCd, this.objConfig);	
			//체크 콤보의 Code 값을 입력하는 함수 실행
			this.gfn_SetMultiComboCode(this.div_Search.form.edt_AcomCd, "ALL");//전체
			
			break;
		case "SAVE":   //저장		
			this.fvSchKeyYn = true;//저장후 재조회여부
			this.fn_ComBtnSearch();//저장후 재조회

			break;	
		case "DTL_SEARCH": //조회
			if (this.ds_Detail10.getRowCount() > 0) {
				
				//조회결과(초기화) 메시지 표시
				this.gfn_InitGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);
				
			} else {
				//조회결과(데이터없음) 메시지 표시
				this.gfn_SetGridNoDataText(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10);		
			}
			
			
			this.fvDtlSearchYn = false;//상세조회여부
			
			break;							
	}
};


/***********************************************************************************************
 * 13.02. [팝업] CallBack 함수 처리
          strId : 팝업호출시 지정한 팝업ID
	      rtnValue : 팝업창에서 close시 지정한 반환값
          주의 : Open(popuptype=modeless)창은 팝업콜백을 지원하지 않음
 ***********************************************************************************************/ 
this.fn_PopupCallback = function(strId, rtnValue)
{
    //[조회영역]
	//회계단위
	if(strId == "S_ACUM_CD_01"){
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);	
			
			if (!this.gfn_IsNull(arrRtnVal.ACUM_CD)) {
				this.div_Search.form.edt_AcumCd.set_value(arrRtnVal.ACUM_CD);
				this.div_Search.form.edt_AcumNm.set_value(arrRtnVal.ACUM_NM);
			}
		} else {
				this.div_Search.form.edt_AcumCd.set_value("");
				this.div_Search.form.edt_AcumNm.set_value("");
		}
	} 
};

