 ***********************************************************************************************/ 
this.div_MasterInput_tab_Master_onchanged = function(obj:nexacro.Tab,e:nexacro.TabIndexChangeEventInfo)
{
	var iTabIndex = e.postindex;
	
	if (iTabIndex == 0) {
		this.div_MasterInput.form.btn_XlsxUp.set_visible(false);
		
		
	}else{
		//엑셀업로드 버튼
		this.div_MasterInput.form.btn_XlsxUp.set_visible(true);
	}
};



/***********************************************************************************************
 * 15.06. [e세로   ] 전체삭제
 ***********************************************************************************************/ 
this.fn_eSeroDataDeleteAll = function() 
{ 
	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_Input.clearData();
	this.ds_Input.addRow();
	
	var sFirstDd = this.ds_Detail20.getCaseMin("crudType=='I'", "ESEH_DD");
	var sLastDd  = this.ds_Detail20.getCaseMax("crudType=='I'", "ESEH_DD");
	
	
// 	this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_FromDd.value);
// 	this.ds_Input.setColumn(0, "CLOS_DD", this.div_Search.form.cal_ClosDd.value);
	
	this.ds_Input.setColumn(0, "FROM_DD", sFirstDd.substring(0,6));
	this.ds_Input.setColumn(0, "CLOS_DD", sLastDd.substring(0,6));
	
		
	trace("====="+sFirstDd.substring(0,6));
	trace("====="+sLastDd.substring(0,6));
	

	
	this.ds_Input.setColumn(0, "crudType", "D");//삭제-Flag

	

	var sTaxbCd = this.div_Search.form.cbo_TaxbCd.value;
	
	if(sTaxbCd=="1"){
		//세금계산서 매출
		this.ds_Input.setColumn(0, "VATM_GU", "1");//매출매입구분(0:매입,1:매출)
		this.ds_Input.setColumn(0, "TAXB_CD", "0010");//세금계산서구분0010:세금계산서/0020:계산서구분
	}else if(sTaxbCd=="2"){
		//세금계산서 매입
		this.ds_Input.setColumn(0, "VATM_GU", "0");//매출매입구분(0:매입,1:매출)
		this.ds_Input.setColumn(0, "TAXB_CD", "0010");//세금계산서구분0010:세금계산서/0020:계산서구분
	}else if(sTaxbCd=="3"){
		//계산서매출
		this.ds_Input.setColumn(0, "VATM_GU", "1");//매출매입구분(0:매입,1:매출)
		this.ds_Input.setColumn(0, "TAXB_CD", "0020");//세금계산서구분0010:세금계산서/0020:계산서구분
	}else if(sTaxbCd=="4"){
		//계산서매입
		this.ds_Input.setColumn(0, "VATM_GU", "0");//매출매입구분(0:매입,1:매출)
		this.ds_Input.setColumn(0, "TAXB_CD", "0020");//세금계산서구분0010:세금계산서/0020:계산서구분
	}
	
	
	
	//[공통]서비스 정보 초기화
	this.gfn_InitComService(this);
  	
  	//[공통]서비스정보 추가
  	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	
	//[공통]서비스정보 추가
	svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sInsertSql = "";//저장Sql
	svcAddObj.sUpdateSql = "";//수정Sql
	svcAddObj.sDeleteSql = "fa:M_FA_2130_D_DL20";//삭제Sql
	svcAddObj.sSaveFlagColumn = "crudType";//insertSql, updateSql, deleteSql이 존재하여 신규, 수정, 삭제를 처리할 경우 기본적으로 Dataset의 getRowStates()로 판단하여 처리하며
							 //이를 Flag 컬럼으로 대체하고 싶은 경우 사용하는 [컬럼명], 해당 Flag 컬럼의 값은 신규 - I , 수정 - U , 삭제 - D의 값을 가져야 한다.

	this.gfn_AddComSaveService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "DELETE_ALL";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_FA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.isSync = true;//동기식으로 설정할지 여부
	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
	
}

