 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {	
		//this.gfn_SetGridSelectRowColor(this.div_MasterView.form.grd_Master10, e.newrow);//선택한 Row BackgroundColor 설정		
		//입력가능모드 확인....
		this.fn_SetWfCompEnable();
		
		//고정자산번호가 있으면
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FIAM_NO"))) {
			var sSctrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SCTR_CD");//전표손익제어(FA012)
			var sFiamGu = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FIAM_GU");//상각구분(0:비상각, 1:정률법, 2:정액법)

			this.ds_FaAnac.filter("");
			if (sFiamGu != "0") {
				this.ds_FaAnac.filter("CTRL_CD == '" + sSctrCd + "'");
			}
		}
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master10] Row 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	

	return true;
};  
/*---------------------------------------------------------------------------------------------
 *    03. [데이터셋-ds_Master10] 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{	//상각 계산처리.
	//this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)

	//계정코드
	if(e.columnid == "ACOM_CD"){
		this.gfn_GetCommAcomGrid(obj, "ACOM_CD", "ACOM_NM"); // 계정명검색
	} else if (e.columnid == "ANAC_CD"){
		this.gfn_GetCommAcomGrid(obj, "ANAC_CD", "ANAC_NM"); // 계정명검색
	} else if (e.columnid == "ACUM_CD"){
		this.gfn_GetCommAcumGrid(obj, "ACUM_CD", "ACUM_NM"); // 회계단위명검색
	} 
};
