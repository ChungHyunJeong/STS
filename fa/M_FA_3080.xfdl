this.fn_ConsYnOnCheck = function(obj:nexacro.CheckBox,e:nexacro.CheckBoxChangedEventInfo)
{
	//건설중인자산 선택
	if(e.postvalue == "1"){
        //건설중인자산 일 경우만 적격자산여부가 활성화
		this.div_MasterInput.form.tab_Master.tpg_01.form.chk_AmynYn.set_enable(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_FiamGu.set_enable(false);
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FIAM_GU", "0");
        //상각구분도 비상각이 됨
		this.fn_SetFiamGuCompEnable();
	}
	else{
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "AMYN_YN", "0");
		this.div_MasterInput.form.tab_Master.tpg_01.form.chk_AmynYn.set_enable(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_FiamGu.set_enable(true);

		var sAcomCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition,"ACOM_CD");	//계정코드
		if (!this.gfn_IsNull(sAcomCd)) {			

			var iFindRow = this.ds_AcomCd.findRowExpr("ACOM_CD=='" + sAcomCd + "'"); //해당계정코드의 Row
			var sFiamGu = this.ds_AcomCd.getColumn(iFindRow, "FABM_CD");	//상각구분
			var iFasmCt = this.ds_AcomCd.getColumn(iFindRow, "FASM_CT");	//법정내용연수

			if (sFiamGu == '*') sFiamGu = ''; //상각구분이 '*'이면 null			
			
			//건설중인자산인경우 비상각
			if(this.ds_Master10.getColumn(this.ds_Master10.rowposition,"CONS_YN") == "1"){
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FIAM_GU","0");	//비상각구분
			
			}else{
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FIAM_GU",sFiamGu);	//상각구분
			}
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FASM_CT",iFasmCt);	//법정내용연수
			this.fn_SetFiamGuCompEnable();
			
		}
	}
};
