	this.ds_Input.setColumn(0, "ACOM_GU", "0120"); //손익비용구분(ACOM_GU : 0120) - M_FA_ACOM_C_SL00
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);

	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
