// 내용년수에 따른 감가상각율 조회
this.fn_GetFabm = function(sAcom)
{

	this.ds_Input.clearData();
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "ACOM_CD", sAcom);

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "fa:M_FA_3070_M_SL10";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "FABM_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_FA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Fabm=ds_Master10";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.isSync = true;//동기식으로 설정할지 여부
	svcCallObj.sCallBackFnc = "_gfn_Callback_ComFunc";//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
	
	var objCommDs = this.ds_Fabm;
	var iRowCnt = objCommDs.getRowCount();

	if (iRowCnt > 0) {
	
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FASM_CT",  objCommDs.getColumn(0, "FASM_CT"));
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FIAM_GU",  objCommDs.getColumn(0, "FABM_CD"));
	
		var ct = nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FASM_CT"))-nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BFPR_CT"));
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "RMPR_CT", ct);
		this.fn_Calc();
		
		this.fn_SetFiamGuCompEnable();
		
	}	
}
