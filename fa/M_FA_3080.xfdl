// 상각방법 계산 
this.fn_Calc = function()
{
		
	//정율법
	if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FIAM_GU")=="1")
    {
		var rt = nexacro.toNumber(this.fn_rateSearch(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FASM_CT")));
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FIAM_RT", rt);
    }
	//정액법
	else if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FIAM_GU")=="2")
    {
		if(this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FASM_CT")) 
			|| nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FASM_CT")) <= 0 ) return;
			
		var am = nexacro.round(nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "GTCT_AM"))/nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FASM_CT")));
			
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FIAM_AM", am);
    }
	
}
// 내용년수에 따른 감가상각율 조회
this.fn_rateSearch = function(sYear)
{

	this.ds_Input.clearData();
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "FASM_CT", sYear);

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "fa:M_FA_3075_M_SL10";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "FASM_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_FA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Fasm=ds_Master10";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.isSync = true;//동기식으로 설정할지 여부
	svcCallObj.sCallBackFnc = "_gfn_Callback_ComFunc";//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
	
	var objCommDs = this.ds_Fasm;
	var iRowCnt = objCommDs.getRowCount();

	if (iRowCnt > 0) {
		return objCommDs.getColumn(0, "FASM_RT");
	}
	
	return 0;
	
}
//컴포넌트 변경시 처리
this.fn_CompChanged = function(obj, e)
{

	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	
	//trace(obj.id);
	//법정내용년수/취득경과년수
	//if(obj.id == "edt_FasmCt" || obj.id == "edt_BfprCt"){
	if(e.columnid == "FASM_CT" || e.columnid == "BFPR_CT"){
		var ct = nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FASM_CT"))-nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BFPR_CT"));
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "RMPR_CT", ct);
		this.fn_Calc();
	}
	//최초취득가액/재평가가치액
	//else if(obj.id == "mae_FsgtAm" || obj.id == "mae_RevaAm"){
	else if(e.columnid == "FSGT_AM" || e.columnid == "REVA_AM"){
		var am = nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "FSGT_AM"))-nexacro.toNumber(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "REVA_AM"));
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "GTCT_AM", am);
	}
	//취득원가계
	//else if(obj.id == "mae_GtctAm"){
	else if(e.columnid == "GTCT_AM"){
		this.fn_Calc();
	}
	//계정과목 
	//else if(obj.id=="cbo_AcomCd"){
	else if(e.columnid =="ACOM_CD"){
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FASM_CT", 0);
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BFPR_CT",0);
		
		this.fn_GetFabm(e.newvalue);
	}
	//재평가
	//else if(obj.id=="chk_RevaYn"){
	else if(e.columnid =="REVA_YN"){
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_RevaMm.set_value("");
		if(e.postvalue == "1"){
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_RevaMm.set_value(sToDay.substr(0,6));
		}
	}
	//상각완료여부
	//else if(obj.id=="chk_CompYn"){
	else if(e.columnid=="COMP_YN"){
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_CompDd.set_value("");
		if(e.newvalue == "1"){
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_CompDd.set_value(sToDay);
		}
	}
	//매각예정
	//else if(obj.id=="chk_SellYn"){
	else if(e.columnid=="SELL_YN"){
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_SellMm.set_value("");
		//if(e.postvalue == "1"){
		if(e.newvalue == "1"){
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_SellMm.set_value(sToDay.substr(0,6));
		}
	}
	//재무확정
	//else if(obj.id=="chk_AcchYn"){
	else if(e.columnid=="ACCH_YN"){
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_AcchDd.set_value("");
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcsuId.set_value("");
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcsuCd.set_value("");
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcsuNm.set_value("");
		//if(e.postvalue == "1"){
		if(e.newvalue == "1"){
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_AcchDd.set_value(sToDay);
