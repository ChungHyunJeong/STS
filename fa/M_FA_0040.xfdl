/***********************************************************************************************
 * 15.14 [ ] 항목코드추가 추가시  *(없음)을 찾아 넣는다.
 ***********************************************************************************************/ 
this.fn_MaCdAdd  = function()
{

	var arrMsgArg;
	var maNum; //관리항목 num
	var itemCd = this.ds_Item.getColumn(this.ds_Item.rowposition, "ITEM_CD")
	var itemNm = this.ds_Item.getColumn(this.ds_Item.rowposition, "ITEM_NM")
	var balaYn = this.ds_Item.getColumn(this.ds_Item.rowposition, "BALA_YN")

	//등록된 항목코드가 있는지 확인
	for (var iLoop=0; iLoop < this.ds_Master20.rowcount ; iLoop++) {
		
		if (this.ds_Master20.getColumn(iLoop, "MA_CD") == itemCd ){
			// [&ITM0]은(는) 이미 선택한 항목입니다. FA001
			var arrMsgArg = [itemNm];
			this.gfn_Alert("FA001", arrMsgArg);  //[&ITM0]은(는) 이미 선택한 항목입니다 
			return;
		}
	}
	
	
	for (var iLoop=0; iLoop < this.ds_Master20.rowcount ; iLoop++) {
		//항목코드 "*"를 찾아 넣는다.
		if (this.ds_Master20.getColumn(iLoop, "MA_CD") == "*" || this.ds_Master20.getColumn(iLoop, "MA_CD") == ""){
		
			this.ds_Master20.setColumn(iLoop, "MA_CD", itemCd);
			this.ds_Master20.setColumn(iLoop, "MA_NM", itemNm);
			this.ds_Master20.setColumn(iLoop, "BALA_YN", balaYn);
			
			maNum = this.ds_Master20.getColumn(iLoop, "NUM")
			//trace("fn_MaCdAdd maNum = " + maNum);
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "MA"+maNum+"_CD", itemCd);
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "MA"+maNum+"_YN", "0");
			
			break;
			
		}
		
		if(iLoop == this.ds_Master20.rowcount - 1){
			if (this.ds_Master20.getColumn(iLoop, "MA_CD") != "*" && this.ds_Master20.getColumn(iLoop, "MA_CD") != ""){
				this.gfn_Alert("FA002");  //관리항목 등록은 8개 까지 입니다.
			}
		}
	}
}




/***********************************************************************************************
 * 15.14 [ ] 항목코드추가 선택위치에 넣는다
 ***********************************************************************************************/
this.fn_MaCdSetAdd  = function()
{
    /**
	1. 추가시 *(없음)을 찾아 넣는다.
	**/
	var arrMsgArg;
	var maNum; //관리항목 num
	var itemCd = this.ds_Item.getColumn(this.ds_Item.rowposition, "ITEM_CD") //항목코드
	var itemNm = this.ds_Item.getColumn(this.ds_Item.rowposition, "ITEM_NM") //항목명
	var balaYn = this.ds_Item.getColumn(this.ds_Item.rowposition, "BALA_YN") //필수여부
	
	// 위치 지정 관리항목  row
	var dsMaster20RowPosition = this.ds_Master20.rowposition;
	var iLoop=0;
	
	//선택한 위치가 * or '' 인지 체크
	if (this.ds_Master20.getColumn(dsMaster20RowPosition, "MA_CD") == "*" || this.ds_Master20.getColumn(dsMaster20RowPosition, "MA_CD") == ""){

		/*
			선택한 위치가 '*' or '' 이면
			최초 * 을 찾는다.
		*/
		for (iLoop=0; iLoop < this.ds_Master20.rowcount; iLoop++){
			
			if(this.ds_Master20.getColumn(iLoop, "MA_CD") == "*" || this.ds_Master20.getColumn(iLoop, "MA_CD") == ""){
				dsMaster20RowPosition = iLoop;
				break;
			}
		}
	}
	
	//등록된 항목코드가 있는지 확인
	for (var iLoop=0; iLoop < this.ds_Master20.rowcount; iLoop++) {
		if (this.ds_Master20.getColumn(iLoop, "MA_CD") == itemCd && !(itemCd == "*")){ //'없음'은 여러개 넣을수있음
			// [&ITM0]은(는) 이미 선택한 항목입니다. FA001
			var arrMsgArg = [itemNm];
			this.gfn_Alert("FA001", arrMsgArg);  //[&ITM0]은(는) 이미 선택한 항목입니다 
			return;
		}
	}
	

	this.ds_Master20.setColumn(dsMaster20RowPosition, "MA_CD", itemCd);
	this.ds_Master20.setColumn(dsMaster20RowPosition, "MA_NM", itemNm);
	this.ds_Master20.setColumn(dsMaster20RowPosition, "MA_YN", "0");
	this.ds_Master20.setColumn(dsMaster20RowPosition, "BALA_YN", balaYn);
	
	maNum = this.ds_Master20.getColumn(dsMaster20RowPosition, "NUM")
	this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "MA"+maNum+"_CD", itemCd);
	this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "MA"+maNum+"_YN", "0");
	
	//대표관리항목여부 CHECKED
	if(this.ds_Master20.getColumn(dsMaster20RowPosition, "REPR_YN")=="1"){
		//체크를 풀어준다.
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "REPR_CD", "");
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "REPR_YN", "0");
		this.ds_Master20.setColumn(dsMaster20RowPosition, "REPR_YN", "0");
	}
}

/***********************************************************************************************
 * 15.14 [ ] 항목코드삭제
 ***********************************************************************************************/ 
this.fn_MaCdDel  = function()
{
	var maNum; //관리항목 num
	var rowPosition= this.ds_Master20.rowposition; // 현재 row
	
	for (var iLoop=0; iLoop < this.ds_Master20.rowcount; iLoop++) {
		if(iLoop >= rowPosition){
			// 
			
			maNum = this.ds_Master20.getColumn(iLoop, "NUM")
			
			if (iLoop == this.ds_Master20.rowcount -1){
				//마지막 줄은 없음으로 처리
				this.ds_Master20.setColumn(iLoop, "MA_CD", "*");
				this.ds_Master20.setColumn(iLoop, "MA_NM", "없음");
				this.ds_Master20.setColumn(iLoop, "MA_YN", "0");
				this.ds_Master20.setColumn(iLoop, "BALA_YN", "0");
				this.ds_Master20.setColumn(iLoop, "REPR_YN", "0");
				
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "MA"+maNum+"_CD", "*");
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "MA"+maNum+"_YN", "0");
				
			}else{
				// 다음줄을 복사해 이전줄로 붙여넣는다.
				this.ds_Master20.copyRow(iLoop, this.ds_Master20, iLoop + 1);
				this.ds_Master20.setColumn(iLoop, "NUM", "0"+(iLoop + 1));
				
				//trace("MA"+maNum+"_CD");
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "MA"+maNum+"_CD", this.ds_Master20.getColumn(iLoop, "MA_CD"));
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "MA"+maNum+"_YN", this.ds_Master20.getColumn(iLoop, "MA_YN"));

			}
		}
	}
}


