/***********************************************************************************************
 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	var AddRow;
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회
			if (this.ds_Master10.getRowCount() > 0) {
				this.div_MasterView.form.grd_Master10.setTreeStatus(0, true);
				//this.div_MasterView.form.grd_Master10.set_treeinitstatus("expand,all")
				
				
				//저장후 재조회여부-true
				trace("this.fvSchKeyYn =" + this.fvSchKeyYn);
				if (this.fvSchKeyYn) {
					
				
					var iFvFindRow = this.ds_Master10.findRowExpr("ACOM_CD=='" + this.fvSchKey01 + "'");//업무Key Row 찾기
					
					trace("iFvFindRow = " + iFvFindRow);
					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
					
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						trace("iFvFindRow 1= " + iFvFindRow);
						this.div_MasterView.form.grd_Master10.set_treeinitstatus("expand,all")
						this.fvSchKeyYn = false;
						this.ds_Master10.set_rowposition(iFvFindRow);
						
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}
				
				
				
			}
			
			break;
		case "DTL_SEARCH": //하위계정조회
			this.ds_Detail10.addColumn("crudType", "string");
			
  			if (this.ds_Detail10.getRowCount() > 0) {
  				this.div_MasterInput.set_enable(true);
  			} else {
  				this.div_MasterInput.set_enable(false);
				this.ds_Master20.clearData();
  			}
			this.ds_Input.clearData();
			this.ds_Input.addRow();
			
			break;
		case "SAVE":   //저장
			this.fn_ComBtnSearch();
			
			break;	
			
		case "DTL10_SEARCH":
			this.ds_Master20.addColumn("crudType", "string");
			if (this.ds_Master20.rowcount <= 0) {
				
				this.ds_Master20.clearData();
				for(var iLoop=0; iLoop < 8; iLoop++){
				
					AddRow = this.ds_Master20.addRow();
					//trace("AddRow= "+ AddRow);
 					this.ds_Master20.setColumn(AddRow, "NUM", "0" + (iLoop+1));
 					this.ds_Master20.setColumn(AddRow, "MA_CD", "*");
 					this.ds_Master20.setColumn(AddRow, "MA_NM", "없음");
 					this.ds_Master20.setColumn(AddRow, "MA_YN", "0");
 					this.ds_Master20.setColumn(AddRow, "BALA_YN", "0");
 					this.ds_Master20.setColumn(AddRow, "REPR_YN", "0");
					
				}
			}
			
			break;
	}
};
 
