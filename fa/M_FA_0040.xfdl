 ***********************************************************************************************/ 
this.fn_DupCheck = function() 
{ 
	var iCnt = 0;
	
	for (var iLoop=0; iLoop < 8; iLoop++) {
		for (var jLoop=0; jLoop < 8; jLoop++) {
			if (iLoop != jLoop && this.ds_Master20.getColumn(0, "MA0" + (iLoop+1) + "_CD") == this.ds_Master20.getColumn(0, "MA0" + (jLoop+1) + "_CD")) {
				if (this.ds_Master20.getColumn(0, "MA0" + (jLoop+1) + "_CD") != "*"){
					iCnt++;
					break;
				}
			}
		}
	}
	if (iCnt > 0) {
		this.gfn_Alert("SM041");//중복된 데이터가 존재합니다.
		return false;
	}
	return true;
}
 
/***********************************************************************************************
 * 15.02. [사용자함수] 재조회
 ***********************************************************************************************/ 
this.fn_ReSearch = function()
{
	this.ds_Detail10.clearData();
 	//this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcomCd.set_enable(false);
 	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcomCd.set_readonly(true);
	
	// 그리드 Row가 존재하고, Row 포지션 이동이 있을 경우
	if (this.ds_Master10.getRowCount() > 0 && this.ds_Master10.rowposition > -1) {
		var sAcomCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ACOM_CD"); //계정코드
		this.ds_Input.setColumn(0, "ACOP_CD", sAcomCd);
		this.ds_Input.setColumn(0, "USED_YN", this.div_Search.form.cbo_UsedYn.value);
		
		//[공통]서비스정보 초기화
		this.gfn_InitComService(this);

		//[공통]서비스정보 추가
		var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
		svcAddObj.sSelectSql = "fa:M_FA_0040_D_SL10";
		this.gfn_AddComSearchService(this, svcAddObj);

		//[공통]서비스 호출 변수
		var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
		svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
		svcCallObj.sActionName = this.G_SV_FA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
		svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
		svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10";//출력 데이터셋
		svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

		//[공통]서비스 호출
		this.gfn_CallComService(this, svcCallObj);
	}
}


