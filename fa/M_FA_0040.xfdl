 /*---------------------------------------------------------------------------------------------
 *    03-1. [Master영역-grd_Master20] oncolumnchanged event
  ---------------------------------------------------------------------------------------------*/
this.ds_Master20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
 	if (e.columnid == "REPR_YN") {//대표관리항목여부
		/*
		대표관리항목여부는 8개중 1개만 선택 가능
		*/
 		for(var iLoop = 0; iLoop < this.ds_Master20.rowcount; iLoop++){
 			//trace(e.newvalue);
 			if(iLoop != e.row){
 				this.ds_Master20.setColumn(iLoop, e.columnid, "0");
 			}
 		}
		
		// ds_detail10에 값을 넣는다.
		if (e.newvalue =="0") {
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "REPR_CD", "");
		} else {
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "REPR_CD", this.ds_Master20.getColumn(e.row, "MA_CD"));
		}
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "REPR_YN", e.newvalue);
		
 	} else if (e.columnid = "MA_YN") {//필수여부
		
		//관리항목순서
		var maNum = this.ds_Master20.getColumn(e.row, "NUM");
		
		this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "MA" + maNum+"_YN", this.ds_Master20.getColumn(e.row, "MA_YN"));
	}
};
