  ---------------------------------------------------------------------------------------------*/
this.div_MasterInput_tab_Master_tpg_01_grd_Master10_oncelldblclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{	
	if(e.col == 1 || e.col == 2 || e.col == 3 || e.col == 4 ){
	// 당기(좌/우), 전기(좌/우)
		var inputYn = this.ds_Master10.getColumn(e.row, "INPU_YN")
	
		//trace("inputYn===" + inputYn);
		if (inputYn =="1"){
	
			var sPrgmId = "M_FA_1230";//호출프로그램ID
			var sCallFunc = "fn_FormCall";//호출함수
			//[공통]특정화면 호출 파라메타 정보 Object
			var oParam = new Object();
			oParam.commCd = this.ds_Master10.getColumn(e.row, "COMM_CD");
			if(e.col == 1 || e.col == 2){
				//당기
				oParam.fromDd = this.ds_Master10.getColumn(e.row, "FROM_DD");
				oParam.closDd = this.ds_Master10.getColumn(e.row, "CLOS_DD");
			}else{
				//전기
				oParam.fromDd = this.ds_Master10.getColumn(e.row, "PFROM_DD");
				oParam.closDd = this.ds_Master10.getColumn(e.row, "PCLOS_DD");
			}
			oParam.acumCd = this.ds_Master10.getColumn(e.row, "ACUM_CD");
			oParam.acumNm = this.ds_Master10.getColumn(e.row, "ACUM_NM");
			oParam.acomCd = this.ds_Master10.getColumn(e.row, "ACOM_CD");
			oParam.acomNm = this.ds_Master10.getColumn(e.row, "ACOM_PNM");

			//trace("inputYn==2=" + inputYn);
			this.gfn_CallScrn(sPrgmId, sCallFunc, oParam, "");
			
		}
	
	}
};

