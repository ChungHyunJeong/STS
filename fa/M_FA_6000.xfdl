this.fn_FormBizInit = function() 
{ 

	//[공통] 날짜정보 설정
	var rtnValue = this.gfn_GetCommDateObj();
	var toDay = rtnValue.toDay;
	this.div_Search.form.cal_TemhDtFr.set_value(toDay);
	
	//[공통] 공통코드 설정 : 공통코드|데이터셋명|초기항목추가여부(Y/N)|추가항목
	var objCondList = ["SM015|ds_Sm015"];

	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, "");
	
	
	
	this.div_Search.form.edt_AcumCd.set_value(this.gfn_GetSessionValue("ACUM_CD"));
	this.div_Search.form.edt_AcumNm.set_value(this.gfn_GetSessionValue("ACUM_NM"));//회계단위	
	
	
	//콤보 인덱스 설정
	//this.div_Search.form.cbo_UsedYn.set_index(0);
		
	//[업무] 일반콤보조회
	//this.fn_ComboSearch();
	
	
	//[공통-Pivot] 그리드 검색/Pivot 설정
	this.div_Search.form.div_ComGridSearch.form.fn_SetCmmGridProc(this, this.div_MasterInput.form.tab_Master);


}

