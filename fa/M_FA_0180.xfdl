/***********************************************************************************************
 * 05.04. [Vaildation] Master 저장 필수값 체크 - 사업자번호/주민번호 유효성체크
 ***********************************************************************************************/ 
this.div_MasterInput_tab_Master_tpg_01_mae_SaumNo_onchanged = function(obj:nexacro.MaskEdit,e:nexacro.ChangeEventInfo)
{
	
	var sCutyCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUTY_CD"); //회사형태
	var sNatmCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "NATM_CD"); //국가
	
	//국가가 "대한민국"의 경우에만 사업자번호/주민번호 유효성 체크
	if (sNatmCd == "KR") {	
		var sSaumNo = "";
		if (sCutyCd == '0030') 
			sSaumNo = "RSRNO";
		else 
			sSaumNo = "BIZNO";
		
		//유효성체크
		var arrArgs = ["this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo^"+ this.div_MasterInput.form.tab_Master.tpg_01.form.sta_SaumNo.text +"|"+sSaumNo
					  ]; 				  
		var sCheckGroupTitle = this.div_MasterInput.form.tab_Master.tpg_01.text;//체크그룹 타이틀
		
		//컴포넌트 Vaild 체크
		if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.set_value("");
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.setFocus();	
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.setCaretPos(0);	
		}
	}

};

