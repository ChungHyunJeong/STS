this.fn_ComBtnAdd = function()
{
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10) || this.gfn_DsIsUpdated(this.ds_Master20)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	} 

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무

	this.div_DetailInput.form.tab_Detail.set_tabindex(0); //TabIndex 이동
	
	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.div_DetailInput.form.tab_Detail.set_enable(true);

	//Tab위치 확인
	var iTabIndex = this.div_MasterInput.form.tab_Master.tabindex;

	if (iTabIndex == 0) {					//거래처정보 Tab
	
		var iAddRow = this.ds_Master10.addRow();
		
		//필수값 셋팅 : 화면존재 && NOT NULL 대상
		this.ds_Master10.setColumn(iAddRow, "CUTY_CD", "0010");										/* 회사형태-법인(0010) */
		this.ds_Master10.setColumn(iAddRow, "TY03_YN", "1");										/* 거래처-기타 */
		this.ds_Master10.setColumn(iAddRow, "HDOF_YN", "0");										/* 본사여부-아니오(0) */
		this.ds_Master10.setColumn(iAddRow, "TAXB_GU", "0");										/* 세금계산서발행구분-정발행(0) */
		this.ds_Master10.setColumn(iAddRow, "USED_YN", "1");										/* 사용여부 */
		this.ds_Master10.setColumn(iAddRow, "NATM_CD", "KR");					
		this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", "1");										/* 결재차수(2022-03-10 추가) */
		this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");										/* 최종여부(2022-03-10 추가) */
		
		//처음 입력 항목에 포커스
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.setFocus();

		//사업자번호, 주민등록번호 포멧 셋팅
		this.fn_SetFormatChange();

		this.ds_Detail10.clearData();//구매담당자 초기화
		this.ds_Detail20.clearData();//거래처상담현황 초기화
		this.ds_Detail30.clearData();
		this.ds_Detail40.clearData();
		this.ds_Detail50.clearData();//계좌정보 초기화
		this.ds_Master20.clearData();
		this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
		
	} else if (iTabIndex == 1) {			//고객정보 Tab
		var iAddRow = this.ds_Master20.addRow();
		
		//필수값 셋팅 : 화면존재 && NOT NULL 대상
		this.ds_Master20.setColumn(iAddRow, "CUSM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD"));//거래처코드
		this.ds_Master20.setColumn(iAddRow, "CUSM_NM", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_NM"));//거래처명
		this.ds_Master20.setColumn(iAddRow, "TAXR_YN", "0");										/* 세금계산서수신여부-아니오(0) */
		
		//처음 입력 항목에 포커스
		this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CspmNm.setFocus();
		
		this.ds_Detail30.clearData();//구매담당자 초기화
		this.ds_Detail40.clearData();//거래처상담현황 초기화
		this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
		
	}
	
	//첨부파일정보 권한설정
	var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
	oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
	this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
	
	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
}

