/***********************************************************************************************
 * 05.02. [Vaildation] Master 저장 필수값 체크 - 입력항목 
 ***********************************************************************************************/ 
this.fn_VaildMasterSave = function()
{
	//Tab위치 확인
	var iTabIndex = this.div_MasterInput.form.tab_Master.tabindex;
	var sNatmCd	  = this.ds_Master10.getColumn(this.ds_Master10.rowposition,"NATM_CD");
	
	//[저장] Validation 설정
	if (iTabIndex == 0) {			//거래처정보 Tab
		var arrArgs = ["this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CutyCd^회사형태|NULL"
					 // ,"this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo^"+ this.div_MasterInput.form.tab_Master.tpg_01.form.sta_SaumNo.text +"|NULL"
					  ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmNm^거래처명(한글)|NULL"
					  ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ReprNm^대표자명|NULL"
					  ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_NatmCd^국가|NULL"
					  ]; 	
					  
		if(sNatmCd == "KR"){
			arrArgs.push("this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo^"+ this.div_MasterInput.form.tab_Master.tpg_01.form.sta_SaumNo.text +"|NULL");		
		}
		
		var sCheckGroupTitle = this.div_MasterInput.form.tab_Master.tpg_01.text;//체크그룹 타이틀
		
		//Master 입력/수정/삭제 상태 체크
		var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
		
		this.ds_Input.clearData();
		this.ds_Input.addRow();
		
		//Master 입력 상태이면
		if (sChkRowType == "I") {
			this.ds_Input.setColumn(0,	"INPUTCHK", "I");			
		}else{
			this.ds_Input.setColumn(0,	"INPUTCHK", "U");			
		}
		var sCusmCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD");			
		var sSaumNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SAUM_NO");			
			
		
		this.ds_Input.setColumn(0,	"CUSM_CD", sCusmCd);
		this.ds_Input.setColumn(0,	"SAUM_NO", sSaumNo);
		
		//[공통]서비스정보 초기화
		this.gfn_InitComService(this);
		
		//[공통]서비스정보 추가
		var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
		svcAddObj.sSelectSql = "fa:M_FA_0180_M_SL11";
		this.gfn_AddComSearchService(this, svcAddObj);
		
		//[공통]서비스 호출 변수
		var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
		svcCallObj.sTrid = "MST_SEARCH_EXISTS";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
		svcCallObj.sActionName = this.G_SV_SD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
		svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
		svcCallObj.sOutDatset = "ds_Exists=ds_Exists";//출력 데이터셋
		svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
		svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
		svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
		svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID		
		svcCallObj.isSync = true;//동기식으로 설정할지 여부

		//[공통]서비스 호출
		this.gfn_CallComService(this, svcCallObj);
		
		if(this.ds_Exists.rowcount > 0){
			var arrMsgArg = ["사업자번호"];
			
			this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.setFocus();
			this.gfn_Alert("SD008", arrMsgArg); //사업자번호 이미 존재합니다.			
			return false; 
		}
		
	
	} else if (iTabIndex == 1) {		//고객정보 Tab
		var arrArgs = ["this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CspmNm^고객명|NULL"
					  ,"this.div_MasterInput.form.tab_Master.tpg_02.form.edt_CusmCd^거래처|NULL"
					  
					  ,"this.div_MasterInput.form.tab_Master.tpg_02.form.edt_DepmNm^부서|NULL"
					  ,"this.div_MasterInput.form.tab_Master.tpg_02.form.edt_RankNm^직급|NULL"
					  ,"this.div_MasterInput.form.tab_Master.tpg_02.form.edt_OtelNo^사내전화|NULL"
					  
					  ,"this.div_MasterInput.form.tab_Master.tpg_02.form.edt_EmalNm^ |NULL|EMAIL"
					  ]; 
		
		var sCheckGroupTitle = this.div_MasterInput.form.tab_Master.tpg_02.text;//체크그룹 타이틀
	}	

	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
		return false;
	}	 
	
	var CutyCd = this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_CutyCd.value;
	
	if(CutyCd == "0030") {
		var SaumNo = this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.value;				
		var bSsn   = this.gfn_IsSSN(SaumNo);  //주민번호여부 체크
		if(!bSsn) {
			var arrMsgArg = ["주민등록번호"];
			this.gfn_Alert("SM042", arrMsgArg); //주임등록번호를 확인하세요
			return false;
		}	
	}	



	return true;
}

