this.fn_MsgCallback = function (strId, strVal)
{

	//MASTER 변경사항 저장여부 ### MASTER10 ###
	if (strId == "CONF.MAST10_CHANGE") {
		if(strVal )
		{
			this.fn_ComBtnSave();//저장 서비스 호출
		} else {
			//데이터 변경여부 체크
			if (this.gfn_DsIsUpdated(this.ds_Master10)) {
				this.gfn_UndoColumn(this.div_MasterView.form.grd_Master10);
			}		
			
			if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
				for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
					this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10, iLoop);
				}
			}				
		}
	//MASTER 변경사항 저장여부 ### MASTER20 ###
	} else if (strId == "CONF.MAST20_CHANGE") {
		if(strVal )
		{
			this.fn_ComBtnSave();//저장 서비스 호출
		} else {
			//데이터 변경여부 체크
			if (this.gfn_DsIsUpdated(this.ds_Master20)) {
				this.gfn_UndoColumn(this.div_MasterView.form.grd_Master20);
			}		
			
			if (this.gfn_DsIsUpdated(this.ds_Detail30)) {
				for (var iLoop=0;iLoop<this.ds_Detail30.getRowCount();iLoop++) {
					this.gfn_UndoColumn(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10, iLoop);
				}
			}				
		}
	//[결재] 재기안여부
	} else if (strId == "CONF.RECIDE") {
		if(strVal) {
			this.fn_ComBtnRecideProc();//[결재] 재기안 서비스 호출
		}	
	}
}; 
 
