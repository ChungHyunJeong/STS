		case "WF_RECIDE": //재기안처리		
			this.gfn_Alert("WF012");//재기안 처리가 완료되었습니다.
			
			this.fvSchKey01 = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD");//조회Key01
