/***********************************************************************************************
 * 02.MF0.03. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master] 데이터 Row 변경 이벤트 - ds_Master10
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{	
	if (e.newrow > -1) {
		//[결재] 결재상태에 따른 컴포넌트 제어			
		this.fn_SetWfCompEnable();					
		
		//조회된 정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD"))) {
			//탭포커스 
			this.div_DetailInput.form.tab_Detail.set_tabindex(2);
			
			//사업자번호, 주민등록번호 포멧 셋팅
			var sCutyCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUTY_CD"); //회사형태
			var sNatmCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "NATM_CD"); //국가

			//사업자번호 활성화/비활성화
			if (sNatmCd == "KR") {
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.set_cssclass("essential");
				
				//회사형태에 따른 포멧
				if (sCutyCd == '0030') {
					this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.set_format("######-#######");
				} else {
					this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.set_format("###-##-#####");
				}
				
			} else {
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.set_cssclass("");	
				
				//사업자번호 거래처정보로 저장
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_SaumNo.set_format("########");		
			}

			//회사형태에 따른 타이틀 제목
			if (sCutyCd == '0030') {
				this.div_MasterInput.form.tab_Master.tpg_01.form.sta_SaumNo.set_text("주민등록번호");
			} else {
				this.div_MasterInput.form.tab_Master.tpg_01.form.sta_SaumNo.set_text("사업자번호");
			}

			this.div_MasterInput.form.tab_Master.tpg_02.set_enable(true);	//거래처직원정보 Tab 활성화

			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();		//탭1-구매담당자
			this.ds_Detail20.clearData();		//탭1-거래처상담현황
			this.ds_Detail30.clearData();		//탭2-구매담당자
			this.ds_Detail40.clearData();		//탭2-거래처상담현황
			this.ds_Detail50.clearData();		//탭1-계좌정보
			this.ds_Master20.clearData();		//탭2-거래처직원정보
			
			this.ds_Input.clearData();
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "CUSM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD"));
					
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
// 			svcAddObj.sSelectSql = "fa:M_FA_0180_D_SL10";	//구매담당자
// 			this.gfn_AddComSearchService(this, svcAddObj);
			
// 			svcAddObj.sSelectSql = "fa:M_FA_0180_D_SL20";	//고객상담현황
// 			this.gfn_AddComSearchService(this, svcAddObj);

			svcAddObj.sSelectSql = "fa:M_FA_0180_D_SL50";	//계좌정보
			this.gfn_AddComSearchService(this, svcAddObj);
			
			svcAddObj.sSelectSql = "fa:M_FA_0180_M_SL20";	//거래처직원정보
			this.gfn_AddComSearchService(this, svcAddObj);

			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL10_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_FA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋			
			svcCallObj.sOutDatset = "  ds_Detail50=ds_Detail50 ds_Master20=ds_Master20";//출력 데이터셋
			//svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10 ds_Detail20=ds_Detail20 ds_Detail50=ds_Detail50 ds_Master20=ds_Master20";//출력 데이터셋
			
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);		
		}
	} else {
		this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);
	}
};

