this.fn_SetWfCompEnable = function()
{ 	
	//[결재]결재내역 팝업화면으로 호출여부
	if (!this.fvWfPopupMode) {
		//[결재] 공통버튼 제어
		var bCommSaveButton = true;//저장버튼 활성화 여부-기본값은 false로 해야함. - 결재승인후 수정일 필요한 화면에서만 true로 설정해야함. 
		//this.fvWfSearchMode = this.gfn_SetCommButtonWf(this, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"), bCommSaveButton);//결재진행상태		
		this.fvWofhStatus = this.gfn_SetCommButtonWf(this, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"), bCommSaveButton);//결재진행상태		
	}
	
	//trace("fn_SetWfCompEnable fvWfSearchMode= " + this.fvWfSearchMode + "  this.fvWofhStatus = " + this.fvWofhStatus);
		
	if (!this.gfn_IsNull(this.popCrehNo)) {
		this.fvWofhStatus = true;
	}
	
	//[결재]결재내역 조회모드이면 Visble false
	if (this.fvWfSearchMode || this.fvWofhStatus) {
		//[결재]결재내역 팝업화면으로 호출여부
		if (this.fvWfPopupMode) {
			this.div_Search.set_enable(false);//조회영역 Enable처리
		}
		
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);
		//[결재] Div하위의 컴포넌트 Enable 처리
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
		//this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 입력	//org
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form.tab_Detail.tpg_02.form, false);//Detail 입력 //카드영수증출력_선택0707
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form.tab_Detail.tpg_03.form, false);//Detail 입력 //카드영수증출력_선택0707
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form.tab_Detail.tpg_04.form, false);//Detail 입력 //카드영수증출력_선택0707
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_RemkPop.set_enable(true);
		
		
		
		//[전표보기]
		//결재상태-승인인경우 => [전표보기] 버튼 활성화
		
		//trace("fn_SetWfCompEnable = " + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"));
		if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST") == "0040") {
			
			//trace("fn_SetWfCompEnable true = " + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"));
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(true);
		} else {
			
			//trace("fn_SetWfCompEnable false = " + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"));
			this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(false);
		}
		
		
		
	} else {
	
		
		//trace("fn_SetWfCompEnable fvWfSearchMode= " + this.fvWfSearchMode + "  this.fvWofhStatus = " + this.fvWofhStatus);
		this.div_DetailInput.form.div_ComGrdBtn.set_visible(true);
		
		var iTabIndex = this.div_DetailInput.form.tab_Detail.tabindex;
		if (iTabIndex == 2) {		
			if (this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CREH_NO"))) {
				this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);
			}
		}
		
		//[결재] Div하위의 컴포넌트 Enable 처리
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, true);//Master 입력 프리폼
		//this.gfn_SetCompGroupEnable(this.div_DetailInput.form, true);//Detail 입력	//org
		
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form.tab_Detail.tpg_02.form, true);//Detail 입력(//카드영수증출력_선택0707)
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form.tab_Detail.tpg_03.form, true);//Detail 입력(//카드영수증출력_선택0707)
		this.gfn_SetCompGroupEnable(this.div_DetailInput.form.tab_Detail.tpg_04.form, true);//Detail 입력(//카드영수증출력_선택0707)
		
		//[결재] Div하위의 컴포넌트 Enable 처리 제외할 컴포넌트설정
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CrehNo.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmNm.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_readonly(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmNm.set_readonly(true);
		
	}
	  
	
	//발의번호
	this.div_MasterInput.form.tab_Master.tpg_01.form.mae_AcchNo.set_readonly(true);
	
	
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_BgdpNm.set_readonly(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumNm.set_readonly(true);
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_DepmNm.set_readonly(true);
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_AcomNm.set_readonly(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cal_BgdpMm.set_readonly(true);
	
	//trace("fn_SetWfCompEnable end" );
	
}
