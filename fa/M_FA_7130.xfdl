/***********************************************************************************************
 * 09.01. [공통버튼] 결재요청 버튼 클릭 이벤트
 ***********************************************************************************************/  
this.fn_ComBtnDecide = function()
{ 

	//결재요청시 자동전표상세가 없으면 결재 불가
	if(	eval(this.fvAtobDiv).fn_AtobRowCount() <= 0){
		this.gfn_Alert("FA011"); //자동전표 미리보기를 먼저 실행 하신 후 작업 하세요.
		this.div_DetailInput.form.tab_Detail.set_tabindex(2);
		return;
	}
	
	//this.fn_CheckAtobUpdate
	trace("eval(this.fvAtobDiv).fn_CheckAtobUpdate()==== " + eval(this.fvAtobDiv).fn_CheckAtobUpdate());
	if(eval(this.fvAtobDiv).fn_CheckAtobUpdate()){
		this.gfn_Alert("SM002"); //변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다..
		this.div_DetailInput.form.tab_Detail.set_tabindex(2);
		return;
	}
	
	
	
	//자동전표 체크
	if (!eval(this.fvAtobDiv).fn_AtobVaildSave(true)){
		this.div_DetailInput.form.tab_Detail.set_tabindex(2);
		return;
	}

	//[공통] 결재요청 Vaildation 체크
	if (!this.fn_VaildWfDecide()) return;
	
	//자동전표Body 금액체크
	if (!this.fn_AtobSelectAm()) return;
	
	var iRow = this.ds_Master10.rowposition;
	var sCREH_NO = this.ds_Master10.getColumn(iRow, "CREH_NO");//번호
	var sChkNo = this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CrehNo.value;
	
	if (sCREH_NO != sChkNo) {
		this.gfn_Alert("FA044"); //법인카드전표발의목록에서 선택된 관리번호와 결재요청할 관리번호가 일치하지 않습니다.\n재조회 하시거나 목록을 재선택 하신후 결재요청 하시기 바랍니다.
		return;
	}
	
	//결재요청시에 자동전표HEAD 업데이트
	this.fn_AtohUpdate(iRow);

