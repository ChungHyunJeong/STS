this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{	
	//선택	
	//trace("ds_Detail20_oncolumnchanged 1");
	//trace("ds_Detail20_oncolumnchanged 1 = "  + e.columnid);
	
	var acomCd = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_AcomCd.value;
	var acomNm = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_AcomNm.value;
	
	
	if(e.columnid =="s_chk"){
		if(this.ds_Detail20.getColumn(e.row, "s_chk") == "1"){
			this.ds_Detail20.setColumn(e.row, "ACOM_CD", acomCd);
			this.ds_Detail20.setColumn(e.row, "ACOM_NM", acomNm);
		}else{
			this.ds_Detail20.setColumn(e.row, "ACOM_CD", "");
			this.ds_Detail20.setColumn(e.row, "ACOM_NM", "");			
		}
	//계정코드
	} else if(e.columnid == "ACOM_CD"){
	    
		//trace("ds_Detail20_oncolumnchanged 2 = "  + e.columnid);
		var arrArgs = [""];
		this.gfn_GetCommAcomGrid(obj, "ACOM_CD", "ACOM_NM", arrArgs, "fn_PopupCallback"); // 계정명검색	

	//차대구분
	}	
	
	
};

/***********************************************************************************************
 * 02.DG0.08. [Detail Tab] 변경 이벤트
 ***********************************************************************************************/
this.div_DetailInput_tab_Detail_onchanged = function(obj:nexacro.Tab,e:nexacro.TabIndexChangeEventInfo)
{
	var iTabIndex = e.postindex;
	
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.div_DetailInput.form.btn_Cancel.set_visible(false);
	
	//Tab-법인카드전표발의상세
	if (iTabIndex == 0) {
		this.fn_SetGridInputButton();//[공통] 그리드 입력 버튼 설정
		this.fn_SetGridInputButtonEnable();
		
		 //카드매출전표 출력 버튼 활성화(결재상태 상관없이20220707)				  
		this.div_DetailInput.form.btn_CdUsePrt.set_visible(true);		
		this.div_DetailInput.form.btn_AllCheck.set_visible(true);
		this.div_DetailInput.form.btn_AllUnCheck.set_visible(true);
		 
		this.div_DetailInput.form.btn_CdUsePrt.set_enable(true);		
		this.div_DetailInput.form.btn_AllCheck.set_enable(true);
		this.div_DetailInput.form.btn_AllUnCheck.set_enable(true);					
		
		
	//Tab-전표발의대상검색
	} else if (iTabIndex == 1) {
		this.fn_SetGridChoiceButton();//[공통] 검색 Tab 버튼 설정
		//전표발의대상검색 부서명, 적용계정명 비활성화
		this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_DepmNm.set_enable(false);
		this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_AcomNm.set_enable(false);
		
		//카드매출전표 출력 버튼 활성화(결재상태 상관없이20220707)				  
		this.div_DetailInput.form.btn_CdUsePrt.set_visible(false);		
		this.div_DetailInput.form.btn_AllCheck.set_visible(false);
		this.div_DetailInput.form.btn_AllUnCheck.set_visible(false);	
		
	//Tab-자동전표상세
	} else if (iTabIndex == 2) {	
		this.fn_SetGridAtobButton();//[공통] 자동전표상세 Div Url 및 입력 버튼 설정	
		
		//결재상태가 등록인 경우만 이벤트 활성화 한다.
		var sWOFH_ST = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST");//결재진행상태
		var sCREH_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CREH_NO");//관리번호
		//trace("sWOFH_ST = " + sWOFH_ST);
		if (sWOFH_ST != "0010") {	
			eval(this.fvAtobDiv).fn_ScrnEnable(false);
		}else{
			eval(this.fvAtobDiv).fn_ScrnEnable(true);
		}
		
		//관리번호가 있는 저장상태라면
		if (!this.gfn_IsNull(sCREH_NO) && sWOFH_ST == "0010") {
			this.div_DetailInput.form.div_ComGrdBtn.set_visible(true);
		} else {
			this.div_DetailInput.form.div_ComGrdBtn.set_visible(false);
		}
		
		this.div_DetailInput.form.btn_Cancel.set_visible(true);
		
		//카드매출전표 출력 버튼 활성화(결재상태 상관없이20220707)				  
		this.div_DetailInput.form.btn_CdUsePrt.set_visible(false);		
		this.div_DetailInput.form.btn_AllCheck.set_visible(false);
		this.div_DetailInput.form.btn_AllUnCheck.set_visible(false);	
		
	//Tab-첨부파일
	} else if (iTabIndex == 3) {	
		this.fn_SetGridFileButton();//[공통] 그리드 파일 버튼 설정
		
		//카드매출전표 출력 버튼 활성화(결재상태 상관없이20220707)				  
		this.div_DetailInput.form.btn_CdUsePrt.set_visible(false);		
		this.div_DetailInput.form.btn_AllCheck.set_visible(false);
		this.div_DetailInput.form.btn_AllUnCheck.set_visible(false);	
	}	
};

