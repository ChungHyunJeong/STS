/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {	
		
		
		
		
		//trace("ds_Master10_onrowposchanged 1 CREH_NO = " + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CREH_NO") );
		//정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CREH_NO"))) {
			
			//[결재] 결재상태에 따른 컴포넌트 제어
			this.fn_SetWfCompEnable();
		
		
			//입력영역 버튼 비활성화
			//this.fn_SetGridInputButtonEnable();				
			if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST") == "0040") {
				//trace("ds_Master10_onrowposchanged WOFH_ST 1 = 0040" );
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(true);
				//결재상태가 등록인 경우만 이벤트 활성화 한다.
				//eval(this.fvAtobDiv).fn_enableEvent("0");
				//trace("ds_Master10_onrowposchanged WOFH_ST 2 = 0040 "  + this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.enable  );
			} else {
				//eval(this.fvAtobDiv).fn_enableEvent("1");
				//trace("ds_Master10_onrowposchanged WOFH_ST= " + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST")  );
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(false);
			}
			
			
			//등록한 상태만 
			if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST") == "0010") {
			    eval(this.fvAtobDiv).fn_ScrnEnable(true);
				this.div_DetailInput.form.btn_Cancel.set_enable(true);
			} else {
				eval(this.fvAtobDiv).fn_ScrnEnable(false);
				this.div_DetailInput.form.btn_Cancel.set_enable(false);
			}
		
			//[월마감제어-버튼/상세그리드]
			var sBUDG_MM = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CRDH_DD"), "");//마감년월
			var sACUM_CD = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ACUM_CD"), "");//회계단위
			
			if (!this.gfn_IsNull(sBUDG_MM)) {
			
				//마감여부 체크
				var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
				var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
				paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
				paramObj.MODM_CD = "FA";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
				paramObj.ACUM_CD = sACUM_CD;//[선택] 회계단위
				paramObj.BUSM_CD  = "";//[선택] 사업부문
				paramObj.SAUM_CD  = "";//[선택] 사업장코드
				paramObj.DEPM_CD  = "";//[선택] 부서코드
				paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
				paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
				
				var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크
				
				//월마감된 경우
				if (bCheck) {
					this.fvMmCloseYn = true;//월마감여부-Tab버튼 제어용
				} else {
					this.fvMmCloseYn = false;//월마감여부-Tab버튼 제어용
				}
			}
			
			//발의상세 카드사용인쇄 버튼 설정(20220707)
			var iTabIndex = this.div_DetailInput.form.tab_Detail.tabindex;	
		
			if (iTabIndex == 0) {						
				 //카드매출전표 출력 버튼 활성화(결재상태 상관없이20220707)				  
				 this.div_DetailInput.form.btn_CdUsePrt.set_visible(true);		
				 this.div_DetailInput.form.btn_AllCheck.set_visible(true);
				 this.div_DetailInput.form.btn_AllUnCheck.set_visible(true);
				 
				 this.div_DetailInput.form.btn_CdUsePrt.set_enable(true);		
				 this.div_DetailInput.form.btn_AllCheck.set_enable(true);
				 this.div_DetailInput.form.btn_AllUnCheck.set_enable(true);			
			}			 
			
			
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Detail20.clearData();
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "CREH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CREH_NO"));			
			this.ds_Input.setColumn(0, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));				
			
			//[결재]
			
			//var strWoftSt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST");
			
			//trace("ds_Master10_onrowposchanged 4 "  + this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.enable );
				
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "fa:M_FA_7130_D_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_FA;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조			
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Detail10=ds_Detail10";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//trace("ds_Master10_onrowposchanged 5 "  + this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.enable );
			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);	
			
		}
		this.ds_Detail20.clearData();
		
		//trace("ds_Master10_onrowposchanged 6 "  + this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.enable );
		//this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.set_enable(false);
		//trace("ds_Master10_onrowposchanged 7 "  + this.div_MasterInput.form.tab_Master.tpg_01.form.btn_AcchNo.enable );
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master10] Row 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무

	//데이터 변경여부 체크 
	if (this.gfn_DsIsUpdated(this.ds_Master10) || this.gfn_DsIsUpdated(this.ds_Detail10) || eval(this.fvAtobDiv).fn_CheckAtobUpdate()) {
		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	

	return true;
};  

/*---------------------------------------------------------------------------------------------
 *    03. [데이터셋-ds_Master10] 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//회계단위코드
	if (e.columnid == "ACUM_CD") {	 
		this.gfn_GetCummAcum("C", this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumCd, this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcumNm, "M_ACUM_CD_01");
	}
	
	
	if(e.columnid == "BGDP_NM" || e.columnid == "BGDP_MM") {	
		this.fn_SetAcchNm();
		
		//자동전표 예산부서 변경
		if (eval(this.fvAtobDiv).fn_AtobRowCount() > 0) {
		
			var bgdpCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BGDP_CD");
			var bgdpNm = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "BGDP_NM");
			
			eval(this.fvAtobDiv).fn_budpChange(bgdpCd, bgdpNm);
		}		
	}
	
}

/*---------------------------------------------------------------------------------------------
 *    04. [데이터셋-ds_Master10] 데이터변경 전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_cancolumnchange = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//발의일자
	//[월마감제어-등록/수정]
	if (e.columnid == "CRDH_DD") {	 
		var sBUDG_MM = "";//마감년월
		var sACUM_CD = "";//마감회계단위
		var iMasterRow = e.row;
		
		if (!this.gfn_IsNull(this.fvMmCloseDay)) {
			sBUDG_MM = this.gfn_Nvl(this.fvMmCloseDay, "");//마감년월
		} else {
			sBUDG_MM = this.gfn_Nvl(e.newvalue, "");//마감년월
		}
		
		if (!this.gfn_IsNull(this.fvMmCloseAcum)) {
			sACUM_CD = this.gfn_Nvl(this.fvMmCloseAcum, "");//마감회계단위
		} else {
			sACUM_CD = this.gfn_Nvl(this.ds_Master10.getColumn(iMasterRow, "ACUM_CD"),"");
		}
		
		this.fvMmCloseDay = "";
		this.fvMmCloseAcum = "";
		
		if (!this.gfn_IsNull(sBUDG_MM)) {
			//마감여부 체크
			var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
			var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
			paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
			paramObj.MODM_CD = "FA";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
			paramObj.ACUM_CD = sACUM_CD;//[선택] 회계단위
			paramObj.BUSM_CD  = "";//[선택] 사업부문
			paramObj.SAUM_CD  = "";//[선택] 사업장코드
			paramObj.DEPM_CD  = "";//[선택] 부서코드
			paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
			paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
			
			var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크
			
			//월마감된 경우
			if (bCheck) {
				this.gfn_Alert("CA025");//해당월의 정보가 마감이 되어서 등록/수정 할 수 없습니다.
				var sOrgDate = e.oldvalue;
				
				//변경전값이 없을경우
				if (this.gfn_IsNull(sOrgDate)) {				
					var sToDay = this.gfn_GetCommDate("toDay");//현재일자
					if (sBUDG_MM.substr(0,6) == sToDay.substr(0,6)) {
						this.ds_Master10.setColumn(iMasterRow, "CRDH_DD", "");
					} else {
						this.ds_Master10.setColumn(iMasterRow, "CRDH_DD", sToDay);//입력날짜가 마감된경우 오늘날짜로 세팅
					}
				} else {
					this.ds_Master10.setColumn(iMasterRow, "CRDH_DD", sOrgDate);//입력날짜가 마감된경우 변경전 값으로 세팅
				}
				
				return false;
			} else {
				return true;
			}
		}
	}

}

