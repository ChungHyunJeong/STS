 * 5. 저장
 ***********************************************************************************************/
/***********************************************************************************************
 * 05.01. [공통버튼] 저장 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnSave = function()
{ 



	//trace("fn_ComBtnSave 1");
	//변경여부 체크
	if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && !this.fvFileDiv.fn_CheckFileAddYn() && !eval(this.fvAtobDiv).fn_CheckAtobUpdate()) {		
		this.gfn_Alert("SM036");//변경된 내용이 없습니다.		
		return;
	}
	//trace("fn_ComBtnSave 2 = ");
	//첨부파일정보 변경시
	if (this.fvFileDiv.fn_CheckFileAddYn()) {
		//첨부파일정보만 변경시
		if (!this.gfn_DsIsUpdated(this.ds_Master10) && !this.gfn_DsIsUpdated(this.ds_Detail10) && this.ds_Master10.getRowCount() > 0 && !eval(this.fvAtobDiv).fn_CheckAtobUpdate()) {
			//첨부파일정보 저장
			fvFileInfo = new Object();//[공통] 첨부파일정보
			fvFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			fvFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, B-상세(그리드Row), E-기타
			fvFileInfo.CALLBACK_FUNC = "";//호출할 콜백함수
			fvFileInfo.PATH_NM = this.fvPathNm;//업무파일경로
			fvFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			fvFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CREH_NO");//PK번호2-매입번호
			fvFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
			//첨부파일Div 화면에 Key세팅하게 변경
			this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);
			return;	
		}
	}
	

	//trace("fn_ComBtnSave 3 = ");
	//저장전 유효성 체크
	//Master 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		if (!this.fn_VaildMasterSave()) return;
	}
		
	//trace("fn_ComBtnSave 4 = ");
	if(this.ds_Detail10.rowcount < 1){
		var arrMsgArg = [this.div_DetailInput.form.tab_Detail.tpg_01.text];
		this.gfn_Alert("SM045", arrMsgArg);//[법인카드전표발의상세]는 1건 이상 존재해야 합니다.
		return;
	}
	
	//trace("fn_ComBtnSave 5 = ");
	//Detail 데이터 유효성 체크
	if (this.gfn_DsIsUpdated(this.ds_Detail10)) {
		if (!this.fn_VaildDetailSave()) return;
	}	
	
	//[자동전표] 데이터 유효성 체크
	//trace(""[자동전표] 데이터 유효성 체크 1");

	
	var li_UpdateAcomCnt = 0;
	for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
		var sAcomCdOrg = this.ds_Detail10.getOrgColumn(iLoop, "ACOM_CD");
		var sAcomCd = this.ds_Detail10.getColumn(iLoop, "ACOM_CD");
		
		if (!this.gfn_IsNull(sAcomCdOrg) && sAcomCdOrg != sAcomCd) {
			li_UpdateAcomCnt++;
		}
	}

	//trace("=========" + eval(this.fvAtobDiv).fn_AtobRowCount());
	if(li_UpdateAcomCnt == 0 && eval(this.fvAtobDiv).fn_AtobRowCount() > 0){

		//자동전표 체크
		if (!eval(this.fvAtobDiv).fn_AtobVaildSave(true)){
			this.div_DetailInput.form.tab_Detail.set_tabindex(2);
			return;
		}

	}
	
	//trace("fn_ComBtnSave 6 = ");
	
	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	
	//trace("fn_ComBtnSave 7 = ");
	
	//Master 입력 상태이면
	if (sChkRowType == "I") {		
	
		//채번-매입번호
		var sTemhNo = this.gfn_GetTbnmNo(this.fvComPrgmId, this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CRDH_DD"));//매입번호
		if (this.gfn_IsNull(sTemhNo)) {
			//매입번호를 가져오는데 실패했습니다.
			return;
		}
		
		var sChkTemhNo = "";//매입번호-체크용		
		var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position
		
		//Master 데이터셋에 채번정보 입력-매입번호
		this.ds_Master10.setColumn(iMasterRowPos, "CREH_NO", sTemhNo);
		this.ds_Master10.setColumn(iMasterRowPos, "WOFH_SQ", "1");
		this.ds_Master10.setColumn(iMasterRowPos, "WOFH_YN", "1");	
		this.ds_Master10.setColumn(iMasterRowPos, "WOFH_ST", "0010");
		
		var sCommCd = this.ds_Master10.getColumn(iMasterRowPos, "COMM_CD");//회사코드
		var sTemhNo = this.ds_Master10.getColumn(iMasterRowPos, "CREH_NO");//매입번호		
		var sMophDd = this.ds_Master10.getColumn(iMasterRowPos, "CRDH_DD");//매입일자
		var sWofhSq = this.ds_Master10.getColumn(iMasterRowPos, "WOFH_SQ");//차수
				
		//Detail 데이터셋에 기본정보 입력-회사코드/매입번호/결재차수
