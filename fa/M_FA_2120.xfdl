this.fn_Sum = function(){

	var ds10ForesumAm = this.ds_Detail10.getSum("FORE_AM");
	var ds10WonesumAm = this.ds_Detail10.getSum("WONE_AM");
	var ds10Count     = this.ds_Detail10.getRowCount();
	
	var ds20ForesumAm = this.ds_Detail20.getSum("FORE_AM");
	var ds20WonesumAm = this.ds_Detail20.getSum("WONE_AM");
	var ds20Count     = this.ds_Detail20.getRowCount();
	
	this.ds_Master20.setColumn(0, "TOTAL_CNT", ds10Count + ds20Count);
	this.ds_Master20.setColumn(0, "TOTAL_FORE_AM", ds10ForesumAm + ds20ForesumAm);
	this.ds_Master20.setColumn(0, "TOTAL_WONE_AM", ds10WonesumAm + ds20WonesumAm);
	
	this.ds_Master20.setColumn(0, "C_CNT", ds10Count);
	this.ds_Master20.setColumn(0, "C_FORE_AM", ds10ForesumAm);
	this.ds_Master20.setColumn(0, "C_WONE_AM", ds10WonesumAm);
	
	this.ds_Master20.setColumn(0, "D_CNT", ds20Count);
	this.ds_Master20.setColumn(0, "D_FORE_AM", ds20ForesumAm);
	this.ds_Master20.setColumn(0, "D_WONE_AM", ds20WonesumAm);
}




