 * 02.DG1.05. [Detail그리드2-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
this.fn_GrdDetail20Choice = function()
{
	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {
			
			var arrCheckRow = [];//체크된 Row
			
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
			
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {

					//Detail10에 적용되어 있는지 체크
					var objSearchList = ["EMPM_CD|" + this.ds_Detail20.getColumn(iLoop, "EMPM_CD") //작업자
										];

					//[공통] Detail10 중복건 체크후 해당건 Skip
					if (!this.gfn_DsSchDupcheck(this.ds_Detail10, objSearchList)) {
						
						var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)
							
						this.ds_Detail10.copyRow(iAddRow, this.ds_Detail20, iLoop);
						this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
						this.ds_Detail10.setColumn(iAddRow, "WRGH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WRGH_NO"));
						var nBase = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.mae_BasiHr.value;
						var nExte = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.mae_ExteHr.value;
						var nNigh = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.mae_NighHr.value;
						var sHoli = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_HoliGu.value;
						//평일
						if(this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_BahoGu.value == "0010"){
							this.ds_Detail10.setColumn(iAddRow, "BASI_HR", nBase);
							this.ds_Detail10.setColumn(iAddRow, "EXTE_HR", nExte);
							this.ds_Detail10.setColumn(iAddRow, "NIGH_HR", nNigh);
							this.ds_Detail10.setColumn(iAddRow, "HOBA_HR", 0);
							this.ds_Detail10.setColumn(iAddRow, "HOEX_HR", 0);
							this.ds_Detail10.setColumn(iAddRow, "HONI_HR", 0);
							this.ds_Detail10.setColumn(iAddRow, "LATE_HR", 0);
							this.ds_Detail10.setColumn(iAddRow, "LEAV_HR", 0);
							this.ds_Detail10.setColumn(iAddRow, "ABSE_HR", 0);
							this.ds_Detail10.setColumn(iAddRow, "HOLI_GU", sHoli);
						}
						//휴일
						else if(this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_BahoGu.value == "0020"){
							this.ds_Detail10.setColumn(iAddRow, "BASI_HR", 0);
							this.ds_Detail10.setColumn(iAddRow, "EXTE_HR", 0);
							this.ds_Detail10.setColumn(iAddRow, "NIGH_HR", 0);
							this.ds_Detail10.setColumn(iAddRow, "HOBA_HR", nBase);
							this.ds_Detail10.setColumn(iAddRow, "HOEX_HR", nExte);
							this.ds_Detail10.setColumn(iAddRow, "HONI_HR", nNigh);
							this.ds_Detail10.setColumn(iAddRow, "LATE_HR", 0);
							this.ds_Detail10.setColumn(iAddRow, "LEAV_HR", 0);
							this.ds_Detail10.setColumn(iAddRow, "ABSE_HR", 0);
							this.ds_Detail10.setColumn(iAddRow, "HOLI_GU", sHoli);
						}
						
						arrCheckRow.push(iLoop );
						
						//선택항목 비활성화처리 (색상변경 및 체크박스 Lock)
						this.ds_Detail20.setColumn(iLoop, "s_conf", "1");
						this.ds_Detail20.setColumn(iLoop, "s_chk", "0");//Uncheck로 변경
					}
				}
			}

			this.ds_Detail20.set_rowposition(-1);
			//출고상세로 Tab이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);
		}
	}
};

