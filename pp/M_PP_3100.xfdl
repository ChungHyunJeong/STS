 ***********************************************************************************************/ 
this.fn_SetWfCompEnable = function()
{
	var sWofhSt = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_ST"),"0010");
	
	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);	
	
	if(sWofhSt != "0010"){
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
		//this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 입력
		this.div_DetailInput.form.set_enable(true);
		this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);		
		this.div_MasterInput.form.btn_WrghY.set_enable(false);
		this.div_MasterInput.form.btn_WrghN.set_enable(false);
		this.div_DetailInput.form.btn_Create.set_enable(false);
	}
	//입력상태
	else if(sChkRowType == "I"){
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
		this.div_DetailInput.form.set_enable(true);
		this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);		
		this.div_MasterInput.form.btn_WrghY.set_enable(false);
		this.div_MasterInput.form.btn_WrghN.set_enable(false);
		this.div_DetailInput.form.btn_Create.set_enable(true);
 		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SAUM_CD.set_enable(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WkgmCd.set_enable(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_WRGH_DD.set_enable(true);		
		
	}
	//확정상태.
	else if(this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WRGH_YN"),"0")=="1"){
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
		//this.gfn_SetCompGroupEnable(this.div_DetailInput.form, false);//Detail 입력
		this.div_DetailInput.form.set_enable(true);
		this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);
		this.div_MasterInput.form.btn_WrghY.set_enable(false);
		this.div_MasterInput.form.btn_WrghN.set_enable(true);
		this.div_DetailInput.form.btn_Create.set_enable(false);
	}
	//미확정상태
	else{
		this.gfn_SetCompGroupEnable(this.div_MasterInput.form, false);//Master 입력 프리폼
		this.div_DetailInput.form.set_enable(true);
		this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
 		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SAUM_CD.set_enable(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WkgmCd.set_enable(true);
		this.div_DetailInput.form.btn_Create.set_enable(true);
		this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(false);
		this.div_MasterInput.form.tab_Master.tpg_01.form.cal_WRGH_DD.set_enable(true);
		this.div_MasterInput.form.btn_WrghY.set_enable(true);
		this.div_MasterInput.form.btn_WrghN.set_enable(false);
		
	}
	
}
