			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SAUM_CD^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_METH_NO.text + "|NULL"
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WkgmCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_RPTH_MM.text + "|NULL"
			      ]; 

	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	
	//Master 입력 상태이면
	if (sChkRowType != "I") {
		arrArgs.push("this.div_MasterInput.form.tab_Master.tpg_01.form.mae_WRGH_NO^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_RPTH_NO.text + "|NULL");
	}
	var sCheckGroupTitle = this.div_MasterInput.form.tab_Master.tpg_01.text;//체크그룹 타이틀

	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
		return false;
	}

	if(this.ds_Detail10.getRowCount() <= 0){
		var arrMsgArg = [this.div_DetailInput.form.tab_Detail.tpg_01.text];
		this.gfn_Alert("SM008", arrMsgArg);//필수 입력 항목입니다.
		return false;	
	}
	
	//중복키 확인
	if (sChkRowType == "I" && !this.fn_DupValid()){
		this.ds_Master10.setColumn(this.ds_Master10.rowposition,"WRGH_NO","");
		return false;
	}
	
	return true;
}

