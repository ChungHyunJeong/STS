this.div_DetailInput_tab_Detail_onchanged = function(obj:nexacro.Tab,e:nexacro.TabIndexChangeEventInfo)
{
	var iTabIndex = e.postindex;
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	//근태내역상세
	if (iTabIndex == 0) {	
		this.fn_SetGridInputButton();//[공통] 그리드 파일 버튼 설정
		this.div_DetailInput.form.btn_Create.set_visible(true);
	}
	//작업자검색
	else if (iTabIndex == 1) {	
		this.fn_SetGridDetail20Button();//[공통] 그리드 파일 버튼 설정
		this.div_DetailInput.form.btn_Create.set_visible(false);
	}
	//Tab-첨부파일
	else if (iTabIndex == 2) {	
		this.fn_SetGridFileButton();//[공통] 그리드 파일 버튼 설정
		this.div_DetailInput.form.btn_Create.set_visible(false);
	}
	this.fn_SetWfCompEnable();
};

