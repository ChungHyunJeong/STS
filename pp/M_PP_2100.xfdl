 * 01.02. [Vaildation] 조회 필수값 체크
 ***********************************************************************************************/
this.fn_VaildSearch = function()
{
	//[결재] 결재내역 조회가 아니면
	if (!this.fvWfSearchMode) {
		if (!this.gfn_CheckFromToDate(this.fvComPrgmId, this.div_Search.form.cal_JobhDdFr, this.div_Search.form.cal_JobhDdTo)) return false;//조회기간 범위 체크
		//조회조건 필수입력값 있으면 체크
		var arrArgs = ["this.div_Search.form.cbo_SaumCd^" + this.div_Search.form.sta_SaumCd.text + "|NULL" //사업장
					  ,"this.div_Search.form.edt_DepmCd^" + this.div_Search.form.sta_DepmCd.text + "|NULL" //등록부서
					  ];
		
		var sCheckGroupTitle = "검색조건";//체크그룹 타이틀
		
		//컴포넌트 Vaild 체크
		if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
			return false;
		}
	}
	return true;
}

