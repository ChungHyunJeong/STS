 * 02.DG0.04. [Detail그리드-버튼] 행삭제 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd01DeleteRow = function()
{
	var s_chk = "";//체크
	
	//작업지시대상검색 내역 체크 가능하게 변경
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		s_chk = this.ds_Detail10.getColumn(iLoop, "s_chk");
		
		//체크
		if (s_chk == "1") {
		
			//검색값설정 - 컬럼명:검색값
			var objSearchList = ["CNTH_NO|" + this.ds_Detail10.getColumn(iLoop, "CNTH_NO") //계약번호
								,"CNTB_SE|" + this.ds_Detail10.getColumn(iLoop, "CNTB_SE") //계약순번
								,"WORB_NO|" + this.ds_Detail10.getColumn(iLoop, "WORB_NO") //W/O번호
				                ,"WIPM_CD|" + this.ds_Detail10.getColumn(iLoop, "WIPM_CD") //공정코드
								];

			//[공통] 행삭제시 검색Tab 선택Row 해제 처리
			this.gfn_DeleteRowSchTabUncheck(this.ds_Detail20, objSearchList);
		}
	}
	
	this.gfn_GridDelProcess(this, this.ds_Detail10);//crud_flag 변경-삭제(D)

	//작업지시상세 데이터 없으면, 마스터 입력값 초기화
	if (this.ds_Detail10.getRowCount() < 1) {
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SaumCd.set_value("");
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WkgmCd.set_value("");
		this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WkgmCd.set_enable(false);
	}
};

