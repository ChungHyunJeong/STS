 * 05.02. [Vaildation] Master 저장 필수값 체크 - 입력항목 
 ***********************************************************************************************/ 
this.fn_VaildMasterSave = function()
{
	//[저장] Validation 설정
	var arrArgs = ["this.div_MasterInput.form.tab_Master.tpg_01.form.cal_JobhDd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_JobhDd.text + "|NULL" //지시일자
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_DepmCd.text + "|NULL" //등록부서
			      ,"this.div_MasterInput.form.tab_Master.tpg_01.form.edt_SystId^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_EmpmCd.text + "|NULL" //등록자
				  ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SaumCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_SaumCd.text + "|NULL" //사업장
				  ,"this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WkgmCd^" + this.div_MasterInput.form.tab_Master.tpg_01.form.sta_WkgmCd.text + "|NULL" //작업조
			      ];
				  
	var sCheckGroupTitle = this.div_MasterInput.form.tab_Master.tpg_01.text;//체크그룹 타이틀

	//컴포넌트 Vaild 체크
	if (!this.gfn_CheckFormCompValid(this, arrArgs, sCheckGroupTitle)) {
		return false;
	}	
		
	//Master 입력/수정/삭제 상태 체크
	var sChkRowType = this.gfn_GetRowType(this.ds_Master10, this.ds_Master10.rowposition);
	var sJobhDdOri  = this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "JOBH_DD");
	var sJobhDd     = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JOBH_DD");
			
	//Master 입력 상태이면
	if (sChkRowType == "I" || sJobhDdOri != sJobhDd) {	
	
		//---같은일자에 같은작업조 존재유무 체크		
		this.ds_Input.clearData();
		this.ds_Input.addRow();
		
