this.fn_ComBtnAdd = function()
{
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	var yy = parseInt(sToDay.substr(0, 4), 10);
	var mm = parseInt(sToDay.substr(4, 2), 10);
	var dd = parseInt(sToDay.substr(6, 2), 10);
	var d = new Date(yy, mm-1, dd+1);

	yy = d.getFullYear();
	mm = d.getMonth() + 1; mm = (mm < 10) ? '0'+mm : mm;
	dd = d.getDate(); dd = (dd < 10) ? '0'+dd : dd;
	var nextDay = yy + mm + dd;

	var sToDay = this.gfn_GetCommDate("toDay");//현재일자

	var iAddRow = this.ds_Master10.addRow();
	
	this.fvMmCloseDay = sToDay;//[월마감] 마감일자
	
	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", "1");//결재차수
	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");//최종여부
	this.ds_Master10.setColumn(iAddRow, "WOFH_ST", "0010");//결재상태-등록(0010)
	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//등록부서코드	
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//등록부서명	
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//등록자사번	
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//등록자명		
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//등록자ID	
	//this.ds_Master10.setColumn(iAddRow, "JOBH_DD", nextDay);//지시일자-내일날자로 설정
	this.ds_Master10.setColumn(iAddRow, "JOBH_DD", sToDay);//오늘날자로 설정
	
	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);//grd_Detail10 버튼그룹
	this.div_DetailInput.form.tab_Detail.set_enable(true);
	
	//입력영역 비활성화
// 	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_enable(false);//등록부서
// 	this.div_MasterInput.form.tab_Master.tpg_01.form.btn_DepmCd.set_enable(false);//등록부서버튼
// 	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_enable(false);//등록자
// 	this.div_MasterInput.form.tab_Master.tpg_01.form.btn_SystId.set_enable(false);//등록자버튼

	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_readonly(true);//등록부서
	this.div_MasterInput.form.tab_Master.tpg_01.form.btn_DepmCd.set_enable(false);//등록부서버튼
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_readonly(true);//등록자
	this.div_MasterInput.form.tab_Master.tpg_01.form.btn_SystId.set_enable(false);//등록자버튼

	
//	//this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SaumCd.set_enable(false);//사업장
//	//this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WkgmCd.set_enable(false);//작업조
	
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_SaumCd.set_enable(true);
	
	//작업지시대상검색 탭 이동
	this.div_DetailInput.form.tab_Detail.set_tabindex(1);
	
	//콤보 디폴트 값
// 	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_FromHr.set_value("09");
// 	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_FromMi.set_value("00");
// 	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ClosHr.set_value("18");
// 	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ClosMi.set_value("00");
	
	
	//처음 입력 항목에 포커스
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SaumCd.setFocus();
	
	this.ds_Detail10.clearData();//상세정보 초기화
	this.ds_Detail20.clearData();//검색정보 초기화
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
	//첨부파일정보 권한설정
	var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
	oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
	this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
	
	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
	
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CnthNm.set_enable(false);
	
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SaumCd.set_index(0);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WkgmCd.set_index(0);
	
	this.fn_WorkTime();
}

