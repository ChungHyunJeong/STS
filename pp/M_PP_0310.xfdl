				if (iFvFindRow > -1 && sChkRowType != "I") {
					this.ds_Detail10.setColumn(iFvFindRow, "s_chk", "0");
					this.ds_Detail10.setColumn(iFvFindRow, "crudType", "");
					
// 					this.ds_Detail20.setColumn(iLoop, "s_chk", "0");
// 					this.ds_Detail20.deleteRow(iLoop);
				} else if (iFvFindRow < 0) {
					var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)
					//var iAddRow = this.ds_Detail10.addRow();	// 한줄추가
					var iFromRow = iLoop;
					
					this.ds_Detail10.copyRow(iAddRow, this.ds_Detail20, iFromRow);
					
					this.ds_Detail10.setColumn(iAddRow, "SAUM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SAUM_CD"));
					this.ds_Detail10.setColumn(iAddRow, "WKGM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WKGM_CD"));//작업조코드
					this.ds_Detail10.setColumn(iAddRow, "WKMM_CD", this.ds_Detail20.getColumn(iFromRow, "SYST_ID"));
					this.ds_Detail10.setColumn(iAddRow, "LEAD_YN", "0"); // 조장여부
					this.ds_Detail10.setColumn(iAddRow, "crudType", "I"); 
					this.ds_Detail10.setColumn(iAddRow, "s_chk", "0"); 

					// 데이터셋 Copy 후 Delete
					this.ds_Detail20.setColumn(iFromRow, "s_chk", "0");
					
				} else if (iFvFindRow > -1 && sChkRowType == "I") {
					this.ds_Detail20.setColumn(iLoop, "s_chk", "0");
				}
			}
		} //for문 종료
		this.gfn_GridDelProcess(this, this.ds_Detail20);//crud_flag 변경-삭제(D)	
	}
};

