/***********************************************************************************************
 * 13. [CallBack] 함수
 ***********************************************************************************************/
/***********************************************************************************************
 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
	
		case "COMBO_SEARCH" : // 공통코드 조회	  
			//결재함 화면에 결재번호가 있는 경우 조회 호출[결재]
		   if (!this.gfn_IsNull(this.parent.parent.fvNowWofhNo)) {
			  this.fn_ComBtnSearch(this.parent.parent.fvNowWofhNo); 
		   }
 			
			this.ds_FaSaumIn.clearData();
			this.ds_PpWkgmIn.clearData();
			this.ds_FaSaumIn.copyData(this.ds_FaSaum);
			this.ds_PpWkgmIn.copyData(this.ds_PpWkgm);
			
			break;
			
		case "MST_SEARCH": //조회
			
			if (this.ds_Master10.getRowCount() > 0) {
				
				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {				
					var iFvFindRow = this.ds_Master10.findRowExpr("WRKH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ=='" + this.fvSchKey02 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}			
				
				
				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
				
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
				
				this.fvInserMode = false; //입력상태여부 : 조회
				
			} else {
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
			
				//입력영역 비활성화
				this.div_MasterInput.set_enable(false);				
				
				//[공통]그리드NoData Text설정
				//this.gfn_SetGridNoDataText(this.div_MasterView.form.grd_Master10);
				
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = false;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);					
			}
			
			//저장후 재조회여부-true
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}
			
			break;
		case "SAVE":   //저장		

			//첨부파일 추가건이 없으면
// 			if (!this.fvFileDiv.fn_CheckFileAddYn()) {
// 				this.fvSchKeyYn = true;//저장후 재조회여부
// 				this.fn_ComBtnSearch();//저장후 재조회
// 			} else {
// 				this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);			
// 			}
// 			
// 			this.fn_MrpSearch();			
			
			break;	
		case "DTL_SEARCH": //조회
			this.ds_Detail40.clearData();
			this.ds_Detail50.clearData();
			
			/* 자재입출고 */
			this.ds_PDIO.filter("");
			this.ds_PDIO.filter("PROD_GU=='0010'");
			this.ds_Detail40.copyData(this.ds_PDIO, true);
			
			/* 제품입출고 */
			this.ds_PDIO.filter("");
			this.ds_PDIO.filter("PROD_GU=='0020'");
			this.ds_Detail50.copyData(this.ds_PDIO, true);
			//this.alert(this.ds_Detail10.rowcount);			
						
			//this.cbo_PdrhGu_onitemchanged(this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_PdrhGu);
			
			//첨부파일 정보 조회			
// 			var oFileInfo = new Object();//[공통] 첨부파일정보
// 			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID			
// 			oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타			
// 			oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
// 			oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "JOBH_NO");//PK번호2- 	
// 			oFileInfo.PK03_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");//PK번호3-결재차수
// 
// 			this.fvFileDiv.fn_AtchFileSearch(oFileInfo);			
			 
			break;			 
			
		case "DTLS_SEARCH": //미매입검색		

			break;			
			
		case "DELETE": //삭제		
			this.fn_ComBtnSearch();//조회
			break;		
			
		case "WF_RECIDE": //[결재]재기안처리		
			break;
	}
};

