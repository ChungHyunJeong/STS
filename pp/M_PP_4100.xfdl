			}
			
			//데이터셋 이벤트가 비활성시 - 활성화
			if (!this.ds_Detail20.enableevent) {
				this.ds_Detail20.set_enableevent(true);
			}
			
			break;
			
		case "SEQMAX_SEARCH": //입고순번 MAX				
