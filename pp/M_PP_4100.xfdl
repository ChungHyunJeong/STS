	/* maxlength, format 등 기본적인 부분 정의 시작 */
	var argComponents = ["div_Search.form.mae_CnthNo"
						 ,"div_Search.form.edt_CnthNm"
						 ,"div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.mae_CnthNo"
						 ,"div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CnthNm"
						 ,"div_Search.form.edt_DepmCd"
						 ,"div_Search.form.edt_DepmNm"
						 ,"div_Search.form.edt_EmpmCd"
						 ,"div_Search.form.edt_EmpmNm"
						];
	this.fn_setAttribute(argComponents);
	/* maxlength, format 등 기본적인 부분 정의 종료 */
	
}

this.fn_setAttribute = function(argComponents) {

	var nMaxLength = 0;
	var obj_id = "";
	var obj_pos = "";
	var obj = "";
	
	for(var i=0;i<argComponents.length;i++) {
	
		obj = eval("this."+argComponents[i]);
		if(obj === undefined) { alert(argComponents[i] + "해당 componet 없음\n프로그램 초기화 실패\n관리자에 문의하십시오."); return; }
		
		obj_id = obj.id.toUpperCase();
		
		if(obj instanceof MaskEdit) { // MaskEdit
			obj.set_limitbymask("BOTH");
			if(obj_id.indexOf("CNTHNO") > -1) {			obj.set_format("@@@@@@@@@");		} // 계약번호
		} else if(obj instanceof Edit) { // Edit
			if(obj_id.indexOf("DEPMCD") > -1) {			obj.set_maxlength(10);		} // 입고부서
			if(obj_id.indexOf("DEPMNM") > -1) {			obj.set_maxlength(30);		} // 입고부서명
			if(obj_id.indexOf("EMPMCD") > -1) {			obj.set_maxlength(10);		} // 입고자
			if(obj_id.indexOf("EMPMNM") > -1) {			obj.set_maxlength(30);		} // 입고자명
		}
		
	}
}

