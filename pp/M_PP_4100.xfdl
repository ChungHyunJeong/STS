/***********************************************************************************************
 * 02.DG0.04. [Detail그리드-버튼] 행삭제 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd01DeleteRow = function()
{
	var s_chk = "";//체크
	var isYes = true;
	//견적검색에서 체크 가능하게 변경
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		s_chk = this.ds_Detail10.getColumn(iLoop, "s_chk");
		
		if(nexacro.toNumber(this.ds_Detail10.getColumn(iLoop, "PDOB_QT")) >= nexacro.toNumber(this.ds_Detail10.getColumn(iLoop, "PDIB_QT"))) {
			alert("출고수량 초과되어\n\n삭제할 수 없습니다.");
			isYes = false;
			iLoop = 99999;
			continue;
		}
		
		//체크
		if (s_chk == "1") {
		
			//검색값설정 - 컬럼명:검색값
			var objSearchList = ["PJTH_NO|" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PJTH_NO") //프로젝트번호
								,"PJTB_SE|" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PJTB_SE") //프로젝트순번
								,"QUTH_NO|" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO") //견적번호
								,"QUTB_SE|" + this.ds_Detail10.getColumn(iLoop, "QUTB_SE") //견적순번
				                ,"ITEM_CD|" + this.ds_Detail10.getColumn(iLoop, "ITEM_CD") //품목번호
							    ,"ISPH_NO|" + this.ds_Detail10.getColumn(iLoop, "ISPH_NO") //제품사양번호
								];

			//[공통] 행삭제시 검색Tab 선택Row 해제 처리
			this.gfn_DeleteRowSchTabUncheck(this.ds_Detail20, objSearchList);

			//입고상세 데이터셋에 데이터 존재 확인
			if (this.ds_Detail10.rowcount > 0) {
				var sCntbSe = this.ds_Detail10.getColumn(iLoop, "CNTB_SE");
				var codeKey = this.ds_Detail10.getColumn(iLoop, "codeKey");
				var seqKey = this.ds_Detail10.getColumn(iLoop, "seqKey");
				
			}	
		}
	}
	
	if(!isYes) { return; }
	
	trace(this.gfn_GridDelProcess(this, this.ds_Detail10));//crud_flag 변경-삭제(D)	

	//입고번호가 없고 입고상세 내용이 미존재시 Master Clear
	if (this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition,"PDIH_NO")) && this.ds_Detail10.rowcount == 0) {
		this.fn_ClearMasterValue();
	}
	
};

/***********************************************************************************************
 * 02.DG0.05. [Detail그리드-버튼] 취소 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd01Cancel = function()
{

	//검색값설정 - 컬럼명:검색값
	var objSearchList = ["PJTH_NO|" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PJTH_NO") //프로젝트번호
	                    ,"PJTB_SE|" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PJTB_SE") //프로젝트순번
	                    ,"QUTH_NO|" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "QUTH_NO") //견적번호
