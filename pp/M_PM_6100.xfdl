this.fn_DetailGrd01AddRow = function()
{
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자	
	//if(!this.fn_SaumCdChk()) return;
		
	var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I) 

	this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));//회사코드
	this.ds_Detail10.setColumn(iAddRow, "EQUH_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EQUH_CD"));//관리번호
	this.ds_Detail10.setColumn(iAddRow, "REGI_ID", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SYST_ID"));//
	this.ds_Detail10.setColumn(iAddRow, "REGI_NM", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "USEM_NM"));//	
	
	this.ds_Detail10.setColumn(iAddRow, "FROM_DD", sToDay);	
	this.ds_Detail10.setColumn(iAddRow, "CLOS_DD", sToDay);		
	
	this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();
	this.ds_Detail10.set_rowposition(iAddRow);	
	this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getBindCellIndex("body","EQUB_CD"));
};  

