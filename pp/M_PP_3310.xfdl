this.fn_ComBtnSearch = function()
{ 
	//조회전 유효성 체크
	if (!this.fn_VaildSearch()) return;	

	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정	
	this.ds_Input.clearData();
	
	//[업무] 조회조건 입력
	this.ds_Input.addRow();
	this.ds_Input.setColumn(0, "FROM_DD", this.div_Search.form.cal_TemhDtFr.value);  // 작업일자 FROM
	this.ds_Input.setColumn(0, "CLOS_DD", this.div_Search.form.cal_TemhDtTo.value);  // 작업일자 TO
// 	this.ds_Input.setColumn(0, "DEPM_CD", this.div_Search.form.edt_DepmCd.value);    // 등록부서
// 	this.ds_Input.setColumn(0, "SYST_ID", this.div_Search.form.edt_SystId.value);    // 등록자
	this.ds_Input.setColumn(0, "SAUM_CD", this.div_Search.form.cbo_SaumCd.value);	 // 사업장
	this.ds_Input.setColumn(0, "CNTH_NO", this.div_Search.form.mae_CnthNo.value);	 // 계약번호
	
	this.ds_Input.setColumn(0, "PRGM_ID", "M_PP_3310");//프로그램ID
	
	
	var sFromDd = this.div_Search.form.cal_TemhDtFr.value + "";
	var sClosDd = this.div_Search.form.cal_TemhDtTo.value + "";
		
	var iTermDd = this.gfn_GetDiffDate(sFromDd,sClosDd) + 1	;
	if(iTermDd > 730){
		this.div_Search.form.cal_TemhDtTo.setFocus();
		this.gfn_Alert("PP009");//조회기간이 2년을 초과할 수 없습니다.
		return;
	}		

	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	
	var sOutDatset = "";
	
	var iTabIdx = this.div_MasterView.form.tab_Master.tabindex;
	
	//기간별작업내역
	if(iTabIdx == 0){	
		this.ds_Master10.clearData();		
		svcAddObj.sSelectSql = "pp:M_PP_3310_M_SL10";
		sOutDatset = "ds_Master10=ds_Master10";//출력 데이터셋
		
	//일자별작업내역
	} else if (iTabIdx == 1) {		
		this.ds_Master20.clearData();
		svcAddObj.sSelectSql = "pp:M_PP_3310_M_SL20";
		sOutDatset = "ds_Master20=ds_Master20";//출력 데이터셋
		
		this.fn_SetGridFormatItem();//일자별 그리드설정
		
	//업체별작업내역
	} else if (iTabIdx == 2) {		
		this.ds_Master30.clearData();
		svcAddObj.sSelectSql = "pp:M_PP_3310_M_SL30";
		sOutDatset = "ds_Master30=ds_Master30";//출력 데이터셋
		
	//업체별일자별작업내역
	} else if (iTabIdx == 3) {		
		this.ds_Master40.clearData();
		svcAddObj.sSelectSql = "pp:M_PP_3310_M_SL40";
		sOutDatset = "ds_Master40=ds_Master40";//출력 데이터셋	
		
		this.fn_SetGridFormatCusm();//업체별 그리드설정
		
	//계약별업체별작업집계
	} else if (iTabIdx == 4) {
		this.ds_Master50.clearData();
		this.ds_Detail50.clearData();
		svcAddObj.sSelectSql = "pp:M_PP_3310_M_SL50";
		sOutDatset = "ds_Master50=ds_Master50";//출력 데이터셋	
	}

	//[공통]서비스정보 추가
	//svcAddObj.sSelectSql = "pp:M_PP_3310_M_SL10";
	this.gfn_AddComSearchService(this, svcAddObj);

	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MST_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_PP;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
	//svcCallObj.sOutDatset = "ds_Master10=ds_Master10";//출력 데이터셋
	svcCallObj.sOutDatset = sOutDatset;//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
	svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
	svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);	
}

