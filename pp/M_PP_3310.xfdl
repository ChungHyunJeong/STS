		sBandBody        += '<Cell col="9"  displaytype="mask" maskeditformat="#,##0.0#" textAlign="right" text="bind:WRJB_HR_TOT"/>';
	
	var sBandBodyEnd      = '</Band>';//Band Body End
	
	var sBandSummary      = '<Band id="summary">';//Band Summary - 변동	
