		sBandBody        += '<Cell col="6" text="bind:WORB_QT" textAlign="right" suppress="1"/>';
		sBandBody        += '<Cell col="7" displaytype="normal" textAlign="left" text="bind:CUSM_NM" suppress="1"/>';	 	
