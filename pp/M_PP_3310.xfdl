	var sWIPM_CD 		= "";	
	var sWIPM_NM 		= "";
	var sWRJH_DD 		= "";
	var iWRJB_HR_TOT 	= 0;//합계시간
	var iWRJB_HR 		= 0;//작업시간
	var iFindRow 		= 0;
	var iAddRow 		= 0;
	var sFromDate 		= this.div_Search.form.cal_TemhDtFr.value;
	var sClosDate 		= this.div_Search.form.cal_TemhDtTo.value;
	var sDay 			= "";
	var iDiff 			= this.gfn_GetDiffDate(sFromDate, sClosDate);//일차이	
	var arrDay 			= new Array();
	
	//초기값 설정
	if (iDiff > 0) {
		for (var iLoop=0; iLoop <= iDiff; iLoop++) {
			sDay = this.gfn_AddDate(sFromDate, iLoop);
			sDay = sDay.toString();			
			arrDay.push(sDay.substr(0,8));
		}
	} else {
		sDay = sFromDate.substr(0,8);
		arrDay.push(sDay);
	}		
	
	this.ds_Master21.clearData();
	
	this.ds_Master21.set_enableevent(false);
	
 	for (var iLoop=0; iLoop<this.ds_Master20.getRowCount(); iLoop++) {
	    sCNTH_NO 	= this.ds_Master20.getColumn(iLoop, "CNTH_NO");
	    sCNTH_NM 	= this.ds_Master20.getColumn(iLoop, "CNTH_NM");
	    sCNTB_SE 	= this.ds_Master20.getColumn(iLoop, "CNTB_SE");
	    sWORB_NO 	= this.ds_Master20.getColumn(iLoop, "WORB_NO");
	    sWOITEM_NM 	= this.ds_Master20.getColumn(iLoop, "WOITEM_NM");
	    iWORB_QT 	= this.ds_Master20.getColumn(iLoop, "WORB_QT");	    
		sWIPM_CD 	= this.ds_Master20.getColumn(iLoop, "WIPM_CD");	    
		sWIPM_NM 	= this.ds_Master20.getColumn(iLoop, "WIPM_NM");
	    sWRJH_DD 	= this.ds_Master20.getColumn(iLoop, "WRJH_DD");
		iWRJB_HR_TOT = this.ds_Master20.getColumn(iLoop,"WRJB_HR_TOT");		
	    iWRJB_HR 	= this.ds_Master20.getColumn(iLoop, "WRJB_HR");

 		sWRJH_DD 	= sWRJH_DD + "";
		
		sWRJH_DD 	= sWRJH_DD.substr(0,8);
		
 		if (this.ds_Master21.getCaseCount("CNTH_NO=='" + sCNTH_NO + "' && CNTB_SE=='" + sCNTB_SE + "' && WORB_NO=='" + sWORB_NO + "' && WIPM_CD=='" + sWIPM_CD + "'") < 1) {
			iAddRow = this.ds_Master21.addRow();
			this.ds_Master21.setColumn(iAddRow, "CNTH_NO", sCNTH_NO);
			this.ds_Master21.setColumn(iAddRow, "CNTH_NM", sCNTH_NM);
			this.ds_Master21.setColumn(iAddRow, "CNTB_SE", sCNTB_SE);
			this.ds_Master21.setColumn(iAddRow, "WORB_NO", sWORB_NO);
			this.ds_Master21.setColumn(iAddRow, "WOITEM_NM", sWOITEM_NM);
			this.ds_Master21.setColumn(iAddRow, "WORB_QT", iWORB_QT);			
			this.ds_Master21.setColumn(iAddRow, "WIPM_CD", sWIPM_CD);
			this.ds_Master21.setColumn(iAddRow, "WIPM_NM", sWIPM_NM);			
			//this.ds_Master21.setColumn(iAddRow, "WRJH_DD", sWRJH_DD);
			this.ds_Master21.setColumn(iAddRow, "WRJB_HR_TOT", iWRJB_HR_TOT);
			this.ds_Master21.setColumn(iAddRow, "WRJB_HR", iWRJB_HR);			
			
			//초기값설정
			for (var jLoop=0; jLoop<arrDay.length; jLoop++) {
				this.ds_Master21.setColumn(iAddRow, "S" + arrDay[jLoop] + "_QT", 0);				
			}
			
			this.ds_Master21.setColumn(iAddRow, "S" + sWRJH_DD + "_QT", iWRJB_HR);			
		}		
		
		iFindRow = this.ds_Master21.findRowExpr("CNTH_NO=='" + sCNTH_NO + "' && CNTB_SE=='" + sCNTB_SE + "' && WORB_NO=='" + sWORB_NO + "' && WIPM_CD=='" + sWIPM_CD + "'");
				
		this.ds_Master21.setColumn(iFindRow, "S" + sWRJH_DD + "_QT", iWRJB_HR);
		
 	}
 	
 	this.ds_Master21.set_enableevent(true);
 	this.ds_Master21.set_rowposition(0);
}


