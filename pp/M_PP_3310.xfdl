/***********************************************************************************************
 * 15.01. 일자별 그리드 설정
 ***********************************************************************************************/ 
this.fn_SetGridFormatItem = function() {
	
	//데이터셋 설정
	var sFromDate = this.div_Search.form.cal_TemhDtFr.value;
	var sClosDate = this.div_Search.form.cal_TemhDtTo.value;
	var sDay = "";

	var iDiff = this.gfn_GetDiffDate(sFromDate, sClosDate);//일차이
	
	this.ds_Master21.clear();
	
	this.ds_Master21.addColumn("CNTH_NO", 		"string");
	this.ds_Master21.addColumn("CNTH_NM", 		"string");
	this.ds_Master21.addColumn("CNTB_SE", 		"string");
	this.ds_Master21.addColumn("WORB_NO", 		"string");
	this.ds_Master21.addColumn("WOITEM_NM", 	"string");
	this.ds_Master21.addColumn("WORB_QT", 		"string");
	this.ds_Master21.addColumn("WIPM_CD", 		"string");
	this.ds_Master21.addColumn("WIPM_NM", 		"string");	
	this.ds_Master21.addColumn("WRJB_HR_TOT", 	"BIGDECIMAL");
	this.ds_Master21.addColumn("WRJB_HR_SUM", 	"BIGDECIMAL");		
	
	var sSumQt = "";//시간합계컬럼	

	if (iDiff > 0) {
		for (var iLoop=0; iLoop <= iDiff; iLoop++) {
			sDay = this.gfn_AddDate(sFromDate, iLoop);
			sDay = sDay.toString();		
			
			this.ds_Master21.addColumn("S" + sDay.substr(0,8) + "_QT", "BIGDECIMAL");			
			
			if (this.gfn_IsNull(sSumQt)) {
				sSumQt = "S" + sDay.substr(0,8) + "_QT";//합계컬럼				
			} else {
				sSumQt = sSumQt + "+S" + sDay.substr(0,8) + "_QT";//시간합계컬럼					
			}			
		}
	} else {
		sDay = sFromDate.substr(0,8);
		this.ds_Master21.addColumn("S" + sDay + "_QT", "BIGDECIMAL");		
		sSumQt = "S" + sDay + "_QT";//시간합계컬럼
		
	}
	
	//그리드 설정
	//그리드 고정컬럼 설정
	var sTotFormats       = '';//전체 Foramts
	var sFormatHead       =  '<Formats>';//Formats - Head
	    sFormatHead      += '<Format id="default">';
	var sFormatColumns    = '<Columns>';//Formats - Colums - 변동
	    sFormatColumns   += '<Column size="35" band="left"/>';
		sFormatColumns   += '<Column size="80"/>';
		sFormatColumns   += '<Column size="200"/>';
		sFormatColumns   += '<Column size="40"/>';
		sFormatColumns   += '<Column size="120"/>';
		sFormatColumns   += '<Column size="140"/>';
		sFormatColumns   += '<Column size="54"/>';
		sFormatColumns   += '<Column size="100"/>';
		sFormatColumns   += '<Column size="60"/>';		
		
	var sFormatColumnsEnd = '</Columns>';//Formats - Colums End
	
	var sFormatRows       = '<Rows>';//Rows
        sFormatRows      += '<Row size="24" band="head"/>';
        sFormatRows      += '<Row size="26" band="head"/>';
        sFormatRows      += '<Row size="24"/>';
        sFormatRows      += '<Row size="24" band="summ"/>';
        sFormatRows      += '</Rows>';	
		
	var sBandHead         = '<Band id="head">';//Band Head
	var sBandHeadCol      = '<Cell rowspan="2" text="No."/>';//Band Head Col - 변동	    
		sBandHeadCol     += '<Cell col="1" colspan="3" text="영업계약정보"/>';
		sBandHeadCol     += '<Cell col="4" colspan="3" text="W/o정보"/>';
		sBandHeadCol     += '<Cell col="7" rowspan="2" text="공정"/>';
		sBandHeadCol     += '<Cell col="8" rowspan="2" text="합계"/>';
		
	var sBandHeadRow      = '<Cell row="1" col="1" text="계약번호"/>';//Band Head Row - 변동
	    sBandHeadRow     += '<Cell row="1" col="2" text="계약명"/>';
		sBandHeadRow     += '<Cell row="1" col="3" text="순번"/>';
		sBandHeadRow     += '<Cell row="1" col="4" text="W/O"/>';
		sBandHeadRow     += '<Cell row="1" col="5" text="구성장치명"/>';
		sBandHeadRow     += '<Cell row="1" col="6" text="수량" />';
		
	var sBandHeadEnd      = '</Band>';//Band Head End
	
	var sBandBody         = '<Band id="body">';//Band Body - 변동	
 	    sBandBody        += '<Cell expr="currow + 1" textAlign="center"/>';		
 		sBandBody        += '<Cell col="1" text="bind:CNTH_NO" displaytype="mask" textAlign="center" maskeditformat="expr:(CNTH_NO.length == 9)?@@@@-@-@@@@:@@@-@@@@" maskedittype="string"  suppress="1" />';
 		//sBandBody        += '<Cell col="1" text="bind:CNTH_NO" displaytype="mask" textAlign="center" maskedittype="string" suppress="1"/>';
 		sBandBody        += '<Cell col="2" text="bind:CNTH_NM" textAlign="left" suppress="1"/>';
 		sBandBody        += '<Cell col="3" displaytype="normal" textAlign="center" text="bind:CNTB_SE" suppress="1"/>';
 		sBandBody        += '<Cell col="4" displaytype="mask" textAlign="center" text="bind:WORB_NO"  maskedittype="string" maskeditformat="expr:comp.parent.gfn_WorbNoFormat(WORB_NO)" suppress="1"/>';				
		sBandBody        += '<Cell col="5" displaytype="normal" textAlign="left" text="bind:WOITEM_NM" suppress="1"/>';
		sBandBody        += '<Cell col="6" text="bind:WORB_QT" textAlign="right" suppress="1"/>';
		sBandBody        += '<Cell col="7" displaytype="normal" textAlign="left" text="bind:WIPM_NM"/>';	 	
		sBandBody        += '<Cell col="8"  displaytype="mask" maskeditformat="#,##0.0#" textAlign="right" text="bind:WRJB_HR_TOT"/>';
	
	var sBandBodyEnd      = '</Band>';//Band Body End
	
	var sBandSummary      = '<Band id="summary">';//Band Summary - 변동	
	    sBandSummary     += '<Cell/>';
		sBandSummary     += '<Cell col="1" text="합계" textAlign="center"/>';
		sBandSummary     += '<Cell col="2"/>';
		sBandSummary     += '<Cell col="3"/>';
		sBandSummary     += '<Cell col="4"/>';
		sBandSummary     += '<Cell col="5"/>';
		sBandSummary     += '<Cell col="6"/>';
		sBandSummary     += '<Cell col="7"/>';
		sBandSummary     += '<Cell col="8" expr="dataset.getSum(&quot;WRJB_HR_TOT&quot;)" displaytype="mask" textAlign="right" maskeditformat="###,###,###,##0.0#"/>';

		//sBandSummary     += '<Cell col="8" expr="dataset.getSum(&quot;' + sSumQt  + '&quot;)" displaytype="mask" textAlign="right" maskeditformat="###,###,###,##0.#0"/>';
			
	var sBandSummaryEnd   = '</Band>';//Band Body End
	
	var sFormatEnd        = '</Format>';//Formats - End
	    sFormatEnd       += '</Formats>';
				
		//동적그리드 컬럼설정
		//From~To 월 차이가 0보다 큰경우		
		
		if (iDiff > 0) {
			var sChkDate = "";
			var iMonth 	= 9;//일자컬럼번호
			var iQt 	= 9;//시간컬럼번호
			
			for (var iLoop=0; iLoop <= iDiff; iLoop++) {
				sDay = this.gfn_AddDate(sFromDate, iLoop);
				sDay = sDay.toString();
				sChkDate = sDay.substr(0,8);
				sChkDate = sChkDate.toString();
				
				sFormatColumns   += '<Column size="70"/>';
				//sFormatColumns   += '<Column size="100"/>';

				if (iLoop > 0) {
					iMonth = iMonth + 1;
					iQt = iQt + 1;					
				}
				
				sBandHeadCol     += '<Cell col="' + iMonth + '" rowspan="2" text="' + sChkDate.substr(0,4) + '-' + sChkDate.substr(4,2) + '-' + sChkDate.substr(6,2)+ '"/>';

				sBandBody        += '<Cell col="' + iQt + '" text="bind:S' + sChkDate + '_QT' + '" maskeditformat="###,##0.0#" textAlign="right" displaytype="mask"/>';				

				sBandSummary     += '<Cell col="' + iQt + '" expr="dataset.getSum(&quot;S' + sChkDate + '_QT' + '&quot;)" displaytype="mask" textAlign="right" maskeditformat="###,##0.0#"/>';				
			}
		} else {
			sDay = sFromDate.substr(0,8);
			sFormatColumns   += '<Column size="70"/>';
			//sFormatColumns   += '<Column size="110"/>';

			sBandHeadCol     += '<Cell col="9" rowspan="2" text="' + sDay.substr(0,4) + '-' + sDay.substr(4,2) + '-'+ sDay.substr(6,2) +'"/>';

			sBandBody        += '<Cell col="9" text="bind:S' + sDay + '_QT' + '" maskeditformat="###,##0.0#" textAlign="right" displaytype="mask"/>';			

			sBandSummary     += '<Cell col="9" expr="dataset.getSum(&quot;S' + sDay + '_QT' + '&quot;)" displaytype="mask" textAlign="right" maskeditformat="###,##0.0#"/>';			
		}		
		
	  sTotFormats = sFormatHead + sFormatColumns + sFormatColumnsEnd + sFormatRows + sBandHead + sBandHeadCol + sBandHeadRow + sBandHeadEnd + sBandBody + sBandBodyEnd + sBandSummary + sBandSummaryEnd + sFormatEnd;//전체 Foramts				
	
	//this.alert(sTotFormats);
	this.div_MasterView.form.tab_Master.tpg_02.form.grd_Master20.set_formats("")	
	this.div_MasterView.form.tab_Master.tpg_02.form.grd_Master20.set_formats(sTotFormats);

}
/***********************************************************************************************
 * 15.02. 업체별 그리드 설정
 ***********************************************************************************************/ 
this.fn_SetGridFormatCusm = function() {
	
	//데이터셋 설정
	var sFromDate = this.div_Search.form.cal_TemhDtFr.value;
	var sClosDate = this.div_Search.form.cal_TemhDtTo.value;
