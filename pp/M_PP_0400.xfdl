/***********************************************************************************************
 * 02.MG0.01. [Master그리드-버튼] 복사 클릭 이벤트
 ***********************************************************************************************/
this.fn_MasterGrd01Copy = function()
{
    //선택된 Row
	var arrRows = this.div_MasterInput.form.tab_Master.tpg_01.form.grd_Master10.getSelectedRows();
	var iAddRow = 0;
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	/*
   - 사업장 / 거래처 / 직위 / 성별 / 부서코드 / 급여구분 / 급여 정보 복사
     (생년월일 : 빈값 / 입사일자 : 금일로 행복사) 
	
	*/
	for (var iLoop=0; iLoop < arrRows.length; iLoop++) {
		iAddRow = this.ds_Master10.addRow();
		this.ds_Master10.copyRow(iAddRow, this.ds_Master10, arrRows[iLoop]);
			
		this.ds_Master10.setColumn(iAddRow, "crudType", "I");
		this.ds_Master10.setColumn(iAddRow, "DWKM_CD", ""); //일용직사번
		this.ds_Master10.setColumn(iAddRow, "DWKM_NM", ""); //사원명
		this.ds_Master10.setColumn(iAddRow, "BIRT_DD", ""); //생년월일
		this.ds_Master10.setColumn(iAddRow, "ENTR_DD", sToDay); //입사일자
		this.ds_Master10.setColumn(iAddRow, "RETI_YN", 0); //퇴사여부
		this.ds_Master10.setColumn(iAddRow, "RETI_DD", ""); //퇴사일자
		this.ds_Master10.setColumn(iAddRow, "REMK_NM", ""); //비고
		
		this.ds_Master10.setColumn(iAddRow, "DELE_ID", "");
		this.ds_Master10.setColumn(iAddRow, "DELE_NM", "");
		this.ds_Master10.setColumn(iAddRow, "DELE_DT", "");		
	} 

};
