/***********************************************************************************************
 * 02.MG0.07. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
 /*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{	
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	var iMasterRowPos = this.ds_Master10.rowposition;//Master 현재 Row Position

	//부서코드
	if (e.columnid == "DEPM_CD") {
		this.gfn_GetCommDepmGrid(obj, "DEPM_CD", "DEPM_NM");//부서명 검색

	
	//퇴사여부
	} else if (e.columnid == "RETI_YN") {
		if(this.ds_Master10.getColumn(iMasterRowPos, "RETI_YN") == "1"){
			var sToDay = this.gfn_GetCommDate("toDay");//현재일자
			var sToDayFull = this.gfn_GetCommDate("toDayFull");//현재일시
			
			this.ds_Master10.setColumn(iMasterRowPos, "RETI_DD", sToDay);
// 			this.ds_Master10.setColumn(iMasterRowPos, "DELE_ID", this.gfn_GetSessionValue("SYST_ID"));
// 			this.ds_Master10.setColumn(iMasterRowPos, "DELE_NM", this.gfn_GetSessionValue("USEM_NM"));
// 			this.ds_Master10.setColumn(iMasterRowPos, "DELE_DT", sToDayFull);
		} else {
			this.ds_Master10.setColumn(iMasterRowPos, "RETI_DD", null);
// 			this.ds_Master10.setColumn(iMasterRowPos, "DELE_ID", null);
// 			this.ds_Master10.setColumn(iMasterRowPos, "DELE_NM", null);
// 			this.ds_Master10.setColumn(iMasterRowPos, "DELE_DT", null);
		}
		
		this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U) => 종료일자 변경때문에 
	//거래처
	} else if (e.columnid == "SECO_CD") {
		this.gfn_GetCommCusmGrid(obj, "SECO_CD", "SECO_NM", "H");//거래처명 검색
	//금액
	} else if (e.columnid == "SALA_AM") {
		if (e.newvalue < 0) {
			this.ds_Master10.setColumn(iMasterRowPos, "SALA_AM", e.oldvalue);
		}
	}
	//입사일자, 퇴사일자 비교
	//퇴사일자
	if (e.columnid == "RETI_DD") {
		var sFromDd = this.ds_Master10.getColumn(iMasterRowPos, "ENTR_DD");
		var sRetiDd = e.newvalue;
		var sRetiYn = this.ds_Master10.getColumn(iMasterRowPos, "RETI_YN");
		if (sRetiDd < sFromDd && sRetiYn == 1) {
			this.gfn_Alert("SM018");//종료일자가 시작일자보다 빠릅니다
			this.ds_Master10.setColumn(iMasterRowPos, "RETI_DD", e.oldvalue);
		}
	//입사일자
	} else if (e.columnid == "ENTR_DD") {
		var sFromDd = e.newvalue;
		var sRetiDd = this.ds_Master10.getColumn(iMasterRowPos, "RETI_DD");
		var sRetiYn = this.ds_Master10.getColumn(iMasterRowPos, "RETI_YN");
		if (sRetiDd < sFromDd && sRetiYn == 1) {
			this.gfn_Alert("SM018");//종료일자가 시작일자보다 빠릅니다
			this.ds_Master10.setColumn(iMasterRowPos, "ENTR_DD", e.oldvalue);
		}
	}
};

