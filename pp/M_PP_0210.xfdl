/***********************************************************************************************
 * 05.03. [Vaildation] Detail 저장 필수값 체크 - 그리드항목
 ***********************************************************************************************/ 
this.fn_VaildDetailSave = function()
{
	if (this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.rowcount < 1) return true;	

	//[저장] Validation 설정
	var arrArgs = ["ROUH_NO^NULL"
				  ,"SORT_NO^NULL"
	              ,"WIPM_CD^NULL"			      
				  ,"STAD_HR^NULL"
				  ,"STAD_MA^NULL"
				  ,"STAD_RT^NULL"
			      ];

	var sCheckGroupTitle = this.div_DetailInput.form.tab_Detail.tpg_01.text;//체크그룹 타이틀
				  
	//Grid Vaild 체크
	if (!this.gfn_CheckGridValid(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10, arrArgs, sCheckGroupTitle) ) {
		return false;
	}
	
	// Grid 내에 중복 체크
	
	var nRow 	= this.ds_Detail10.findRowExpr("dataset.getRowType(rowidx) == 2 || dataset.getRowType(rowidx) == 4");
	var nRowCnt = this.ds_Detail10.getRowCount();	
	var arrMsgArg = "";
	
	while (nRow > -1)
	{
		var nRowType = this.ds_Detail10.getRowType(nRow);
		var nKey	 = this.ds_Detail10.getColumn(nRow, "WIPM_CD");		
		var nSortNo  = this.ds_Detail10.getColumn(nRow, "SORT_NO");
		
		if (String(nKey).trim() == '' || nKey == null || String(nKey).trim().length < 1 || nKey == undefined)
		{
			
			var arrMsgArg = ["공정코드"];
			this.gfn_Alert("SM008", arrMsgArg); //공정코드는 필수 입력 항목입니다.		
			
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();
			this.ds_Detail10.set_rowposition(nRow);
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getBindCellIndex("body","WIPM_CD"));
			return false;
		}
		if(nRowType == 2 || nRowType == 4)
		{	
			var nKeyChk    = 0;
			var nSortNoChk = 0;
			
			for (var i = 0 ; i < nRowCnt ; i++)
			{
				var nKeyNext   	= this.ds_Detail10.getColumn(i, "WIPM_CD");
				var nSortNoNext	= this.ds_Detail10.getColumn(i, "SORT_NO");
				
				if (nKey == nKeyNext) 		nKeyChk++;
				if (nSortNo == nSortNoNext) nSortNoChk++;
			}
			if (nKeyChk > 1)
			{				
				//this.gfn_Alert("SM041", arrMsgArg); //해당 공정코드(이)가 이미 존재합니다.				
				this.gfn_Alert("SM041"); //중복된 데이터가 존재합니다.		
				this.ds_Detail10.set_rowposition(nRow);				
				return false;
			}
			
			if (nSortNoChk > 1)
			{				
				this.gfn_Alert("SM041"); //중복된 데이터가 존재합니다.		
				this.ds_Detail10.set_rowposition(nRow);
				return false;			
			}			
		}		
		var nRow = this.ds_Detail10.findRowExpr("dataset.getRowType(rowidx) == 2 || dataset.getRowType(rowidx) == 4", nRow + 1);
	}	
 
	return true;
}

