/***********************************************************************************************
 * 15.05. [그리드내 ds_PpRouh 설정] 라우팅 콤보 세팅
 ***********************************************************************************************/ 
this.fn_ComboFilter = function(row, obj) {
	var itlmCd = this.ds_Detail10.getColumn(row, "ITLM_CD");
	var itmmCd = this.ds_Detail10.getColumn(row, "ITMM_CD");
	var saumCd = this.ds_Detail10.getColumn(row, "SAUM_CD");

	var filterStr = "";
	if (!this.gfn_IsNull(itlmCd)) {
		filterStr += "ITLM_CD == '"+ itlmCd +"'";
	}
	if (!this.gfn_IsNull(itmmCd)) {
		if (filterStr != "") filterStr += " && ";
		filterStr += "ITMM_CD == '"+ itmmCd +"'";
	}
	if (!this.gfn_IsNull(saumCd)) {
		if (filterStr != "") filterStr += " && ";
		filterStr += "SAUM_CD == '"+ saumCd +"'";
	}
//	this.ds_PpRouh.filter("");
	this.ds_PpRouh.filter(filterStr);
	
	if (obj.newvalue != obj.oldvalue) {
		this.ds_Detail10.setColumn(row, "ROUH_NO", "");
		this.ds_Detail10.setColumn(row, "ROUH_NM", "");
	}
//	var rouhNo = this.ds_Detail10.getColumn(row, "ROUH_NO");
//	var rouhNm = this.ds_Detail10.getColumn(row, "ROUH_NM");
};

