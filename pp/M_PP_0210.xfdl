		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PlphNm.set_value("");//연구개발명 초기화
		this.ds_Master10.set_updatecontrol(true);//ds_Master10 rowType 자동변경 활성화
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PlphNo.set_enable(false);//연구개발번호 비활성화
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_PlphNo.set_enable(false);//연구개발번호 검색버튼 비활성화
		
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 4, "edittype", "none");//품목코드 비활성화
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 7, "edittype", "none");//수량 비활성화
		
