 * 13.01. [서비스] CallBack 함수 처리
 ***********************************************************************************************/ 
this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		

			if (this.ds_Master10.getRowCount() > 0) {
				//중분류 조회
				this.ds_RdItmm.filter("");
				if (!this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItlmCd.value == ""){
					this.ds_RdItmm.filter("ITLM_CD=='"+this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_ItlmCd.value+"' || ITMM_NM=='ALL'" );	
				}
				
				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					//var iFvFindRow = this.ds_Master10.findRowExpr("ROUH_NO=='" + this.fvSchKey01 + "' && WORH_SQ=='" + this.fvSchKey02 + "'");//업무Key Row 찾기
					var iFvFindRow = this.ds_Master10.findRowExpr("ROUH_NO=='" + this.fvSchKey01 + "'");//업무Key Row 찾기					

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}				
				//공정 Filter
				var strSaumCd = this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SaumCd.value;
				this.ds_PpWipm.filter("");
				this.ds_PpWipm.filter("SAUM_CD == '" + strSaumCd + "'");
				
				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
				this.div_DetailInput.form.tab_Detail.set_enable(true);
					
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
				
				//[공통]그리드NoData Text 초기화 설정
				this.gfn_InitGridNoDataText(this.div_MasterView.form.grd_Master10);
				
			} else {
				
				//[공통]그리드NoData Text설정
				this.gfn_SetGridNoDataText(this.div_MasterView.form.grd_Master10);
				
				//마스터 데이터셋 이벤트가 비활성시 - 활성화
				if (!this.ds_Master10.enableevent) {
					this.ds_Master10.set_enableevent(true);
				}
				
				//입력영역 비활성화
				this.div_MasterInput.set_enable(false);
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);
				this.div_DetailInput.form.tab_Detail.set_enable(false);
				
				//첨부파일정보 권한설정
				var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
				oScrnBtnAuth.BTN_ENABLE = false;//권한 유무
				this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);
			}
			
			//저장후 재조회여부-true
			if (this.fvSchKeyYn) {
				this.fvSchKeyYn = false;//저장후 재조회여부
			}
			
			break;
			
		case "SAVE":   //저장		
			//첨부파일 추가건이 없으면
			if (!this.fvFileDiv.fn_CheckFileAddYn()) {
				this.fvSchKeyYn = true;//저장후 재조회여부
				this.fn_ComBtnSearch();//저장후 재조회
			} else {
				this.fvFileDiv.fn_AtchFileSave(this, fvFileInfo);			
			}
			break;	
			
		case "DTL_SEARCH": //조회		
			this.ds_Detail10.addColumn("s_chk", "string");
			this.ds_Detail10.addColumn("crudType", "string");
			
			//첨부파일 정보 조회
			var oFileInfo 	  = new Object();//[공통] 첨부파일정보
			oFileInfo.PRGM_ID = this.fvComPrgmId;//프로그램ID
			oFileInfo.FILH_GU = "H";//첨부파일위치구분 : H-Head, D-상세(그리드Row), E-기타
			oFileInfo.PK01_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD");//PK번호1-회사코드
			oFileInfo.PK02_NO = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ROUH_NO");//PK번호2-출고번호
						
			this.fvFileDiv.fn_AtchFileSearch(oFileInfo);
			
			break;
			
		case "DTLS_SEARCH": //미출고검색		
			this.ds_Detail20.addColumn("s_chk", "string");
			this.ds_Detail20.addColumn("s_conf", "string");
			break;
			
		case "DELETE": //삭제		
			this.fn_ComBtnSearch();//조회
			break;	
			
		case "COMBO_SEARCH_ITLM": //대분류
			/*
			1) ds_master에서 같은 ds 사용
			2) 
			*/			
			//this.gfn_Confirm("SM004", "", "CONF.DELETE", "fn_MsgCallback");
				
			//this.alert(this.ds_RdItlmSch.getRowCount() );			
			if (this.ds_RdItlmSch.getRowCount() > 0) {
				
				//ds_master10 copy
				this.ds_RdItlm.copyData(this.ds_RdItlmSch) ;				
				//ds_RdItlmSch add 전체추가
				this.ds_RdItlmSch.insertRow(0);
				this.ds_RdItlmSch.setColumn(0, "ITLM_NM", "ALL");			
			}
			
			if (this.ds_RdItmmSch.getRowCount() > 0) {
			
				//ds_master10 copy
				this.ds_RdItmm.copyData(this.ds_RdItmmSch);				
				
				//ds_RdItlmSch add 전체추가
				this.ds_RdItmmSch.insertRow(0);
				this.ds_RdItmmSch.setColumn(0, "ITMM_NM", "ALL");
			}
			
			break;					
			
		case "COMBO_SEARCH":	//일반콤보서치		
			this.fn_ComboSearchItlm(); // 대분류&중분류
			
			if(this.ds_FaSaum.getRowCount() > 0){
				this.ds_FaSaumSch.copyData(this.ds_FaSaum);
				this.ds_FaSaumSch.insertRow(0);
				this.ds_FaSaumSch.setColumn(0, "SAUM_CD", "");		
				this.ds_FaSaumSch.setColumn(0, "SAUM_NM", "ALL");		
				
				this.div_Search.form.cbo_SaumCd.set_index(0);
			}		
			
			if(this.ds_PpWipm.getRowCount() > 0)
			{
				this.ds_PpWipmSch.copyData(this.ds_PpWipm);
				this.ds_PpWipmSch.insertRow(0);
				this.ds_PpWipmSch.setColumn(0, "WIPM_NM", "");
			}			
		
			break;

	}
};

