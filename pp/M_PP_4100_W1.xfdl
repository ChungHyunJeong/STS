/***********************************************************************************************
 * 15.06. [데이터변경] 환율정보 변경시
 ***********************************************************************************************/ 
this.fn_RatmChange  = function()
{
	//Detail11 -> Detal10 -> Master10 순서로 원화단가/원화금액, 외화단가/외화금액 적용
	var iCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐코드
	var iExchRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "EXCH_RT");//환율정보

	var iCntbQt = 0; 	//계약세부상세 수량
	var iCntbPr = 0; 	//계약세부상세 계약단가
	var iWonePr = 0;	//계약세부상세 원화단가
	var iWoneAm = 0;	//계약세부상세 원화금액
	var iForePr = 0;	//계약세부상세 외화단가
	var iForeAm = 0;	//계약세부상세 외화금액
	
	var sCommCd = "";   //회사코드
	var sCnthNo = "";   //계약번호
	var iWofhSq = 0;    //결재차수
	var sCntbSeSe = "";   //계약순번
	
	//1. Detail11 데이터셋에 원화금액, 외화금액 변경
	for (var iLoop=0; iLoop<this.ds_Detail11.getRowCount(); iLoop++) {
	
		iCntbQt = this.ds_Detail11.getColumn(iLoop, "CNTC_QT");//수량
		iCntbPr = this.ds_Detail11.getColumn(iLoop, "CNTC_PR");//계약단가

		iWonePr = nexacro.floor(iExchRt * iCntbPr, 2);          	//원화단가 = 환율 * 계약단가
		iWoneAm = nexacro.floor(iExchRt * iCntbQt * iCntbPr, 2);	//원화금액 = 환율 * 수량 * 계약단가

		if (iCurrCd == '0090') {			//KRW(원화)
			iForePr = 0;					//외화단가 = 0원
			iForeAm = 0;					//외화금액 = 0원
		} else {
			iForePr = iCntbPr;          	//외화단가 = 계약단가
			iForeAm = iCntbQt * iCntbPr;	//외화금액 = 수량 * 계약단가
		}
		
		//Detail11 데이터셋에 적용
		this.ds_Detail11.setColumn(iLoop, "WONE_PR", iWonePr);	//원화단가
		this.ds_Detail11.setColumn(iLoop, "WONE_AM", iWoneAm);	//원화금액
		this.ds_Detail11.setColumn(iLoop, "FORE_PR", iForePr);	//외화단가
		this.ds_Detail11.setColumn(iLoop, "FORE_AM", iForeAm);	//외화금액
	}	

	//2. Detail10 데이터셋에 원화금액, 외화금액 변경
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		codeKey = this.ds_Detail10.getColumn(iLoop, "codeKey");//임시Key
		seqKey = this.ds_Detail10.getColumn(iLoop, "seqKey");//임시seqKey
		sCntbSe = this.ds_Detail10.getColumn(iLoop, "CNTB_SE");//계약순번
		
		//Detail11에서 조건 합계
		iWoneAm = this.ds_Detail11.getCaseSum("codeKey == '"+codeKey+"' && seqKey == '"+seqKey+"' && CNTB_SE == '"+sCntbSe+"'", "WONE_AM");//원화금액 (상세합계)
		iForeAm = this.ds_Detail11.getCaseSum("codeKey == '"+codeKey+"' && seqKey == '"+seqKey+"' && CNTB_SE == '"+sCntbSe+"'", "FORE_AM");//외화금액 (상세합계)

		//원화금액, 외화금액을 변경하면 "ds_Detail10_oncolumnchanged"에서 원화단가,외화단가가 변경됨
		//이중로직을 타지 않기 위해 단가 계산부분은 제외함
		this.ds_Detail10.setColumn(iLoop, "WONE_AM", iWoneAm);	//원화금액
		this.ds_Detail10.setColumn(iLoop, "FORE_AM", iForeAm);	//외화금액
		
		//Master10 적용의 경우 "ds_Detail10_oncolumnchanged"에서 처리함
	}	

}		


