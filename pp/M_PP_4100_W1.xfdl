/***********************************************************************************************
 * 02.MG0.07. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
			
	//  계약일자 입력(변경)시
	if (e.columnid == "CNTH_DD") {
		var iCurrCd  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐코드
		var iCnthDd  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_DD");//계약일자
		var nOrgRatm = this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "EXCH_RT");//이전 환율정보

		//화폐코드
		if (!this.gfn_IsNull(iCurrCd)) {	//화폐코드 존재할 경우
			var nRatm = this.gfn_GetCommRatm(iCnthDd, iCurrCd);//환율정보
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EXCH_RT", nRatm);	//환율정보
			
			//이전환율정보와 재조회된 환율정보가 서로 다를경우 Detail10, Detail11 원화, 외화 데이터 변경
			if (!this.gfn_IsNull(nOrgRatm) && nOrgRatm != nRatm) {
				this.fn_RatmChange();
			}
		} 
		
		
	// 화폐코드 변경시
	} else if (e.columnid == "CURR_CD") {
		var iCurrCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CURR_CD");//화폐코드
		var iCnthDd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_DD");//계약일자
		var nOrgRatm = this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "EXCH_RT");//이전 환율정보

		//계약일자
		if (!this.gfn_IsNull(iCnthDd)) {	//계약일자 존재할 경우
			var nRatm = this.gfn_GetCommRatm(iCnthDd, iCurrCd);//환율정보
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "EXCH_RT", nRatm);	//환율정보

			//이전환율정보와 재조회된 환율정보가 서로 다를경우 Detail10, Detail11 원화, 외화 데이터 변경
			if (!this.gfn_IsNull(nOrgRatm) && nOrgRatm != nRatm) {
				this.fn_RatmChange();
			}
		} 		
		
	// 이행보증보험유무 변경시
	} else if (e.columnid == "ASPE_YN") {
		var sAspeYn = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ASPE_YN");//이행보증보험유무
		
		//이행보증보험유무 발생시 이행보증발행일자(시작일자, 종료일자) 활성화
		if (sAspeYn == '1') {
			this.div_MasterInput.form.tab_Master.tpg_02.form.cal_AsfrDd.set_enable(true);
			this.div_MasterInput.form.tab_Master.tpg_02.form.cal_AstoDd.set_enable(true);
			
			
		} else if (sAspeYn == '0') {
			this.div_MasterInput.form.tab_Master.tpg_02.form.cal_AsfrDd.set_value("");
			this.div_MasterInput.form.tab_Master.tpg_02.form.cal_AstoDd.set_value("");
			this.div_MasterInput.form.tab_Master.tpg_02.form.cal_AsfrDd.set_enable(false);
			this.div_MasterInput.form.tab_Master.tpg_02.form.cal_AstoDd.set_enable(false);
		}

	// [1-1 지불방법] 합계 (계약금)
	} else if (e.columnid == "PTCT_RT") {
		this.fn_PayTermSum();
		
	// [1-2 지불방법] 합계 (중도금)
	} else if (e.columnid == "PTMD_RT") {
		this.fn_PayTermSum();
	
	// [1-3 지불방법] 합계 (시운전후)
	} else if (e.columnid == "PTTS_RT") {
		this.fn_PayTermSum();
	
	// [1-4 지불방법] 합계 (설치완료)
	} else if (e.columnid == "PTFI_RT") {
		this.fn_PayTermSum();
	
	// [1-5 지불방법] 합계 (선수금)
	} else if (e.columnid == "PTAD_RT") {
		this.fn_PayTermSum();
	
	// [1-6 지불방법] 합계 (기타)
	} else if (e.columnid == "PTET_RT") {
		this.fn_PayTermSum();


	// [2-1 지불조건] 합계(현금)
	} else if (e.columnid == "TECA_RT") {
		this.fn_PayMethSum();
	
	// [2-2 지불조건] 합계(어음)
	} else if (e.columnid == "TEBI_RT") {
		this.fn_PayMethSum();
		
	}
};

