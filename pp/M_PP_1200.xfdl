 * 02.DG1.05. [Detail그리드2-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Choice = function()
{
	if (this.ds_Master10.rowposition > -1) {
		this.ds_Detail20.filter("s_chk == '1'");

		if (this.ds_Detail20.getRowCount() > 0) {
			var mCnthNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO");//Head 계약번호
			var cnthNo = this.ds_Detail20.getColumn(0, "CNTH_NO");//Body 계약번호
			
			if (!this.gfn_IsNull(mCnthNo) && mCnthNo != cnthNo) {
				var arrMsgArg = ["계약번호"];
				this.gfn_Alert("SM057", arrMsgArg); //동일한 계약번호만 적용 가능합니다.
				for (var iLoop = 0; iLoop < this.ds_Detail20.getRowCount(); iLoop++) {
					this.ds_Detail20.setColumn(iLoop, "s_chk", "0");//Uncheck로 변경
				}
				this.ds_Detail20.filter("");
				return;
			}

