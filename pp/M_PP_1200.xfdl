 ***********************************************************************************************/
this.fn_DetailGrd01AddRow = function()
{
	var nRow = this.ds_Detail10.rowposition;
	
	var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)

	//W/O형태 선택 값 가져오기
	var worhCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WORH_CD");
	var cnthNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO");

	if (worhCd == "0040") {	//연구이면..
		this.ds_Detail10.set_enableevent(false);
		var sPlphNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PLPH_NO");//연구개발번호
		this.ds_Detail10.setColumn(iAddRow, "WORB_NO", sPlphNo + "***");//W/O번호
		this.ds_Detail10.setColumn(iAddRow, "CNTB_SE", "*");//계약순번		
		this.ds_Detail10.set_enableevent(true);
		
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 3, "edittype", "none");//계약순번 비활성화
	}
	
	if(nRow > -1){
// 		this.ds_Detail10.copyRow(iAddRow, this.ds_Detail10, nRow);
// 		this.ds_Detail10.setColumn(iAddRow, "crudType", "I");
	}	

	this.ds_Detail10.setColumn(iAddRow, "USED_YN", "1");//사용유무
	
	this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));//회사코드
	this.ds_Detail10.setColumn(iAddRow, "WORH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WORH_NO"));//관리번호
	this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));//결재차수
	this.ds_Detail10.setColumn(iAddRow, "WORH_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WORH_CD"));//W/O형태
	this.ds_Detail10.setColumn(iAddRow, "CNTH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO"));//계약번호
	this.ds_Detail10.setColumn(iAddRow, "PLPH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PLPH_NO"));//연구관리번호	
	this.ds_Detail10.setColumn(iAddRow, "ISPH_NO_CHK", null);
	
	this.ds_Detail10.setColumn(iAddRow, "WORB_QT", 1); //수량
	this.ds_Detail10.setColumn(iAddRow, "SAUM_CD", "0400"); //사업장
	this.ds_Detail10.setColumn(iAddRow, "IOMA_CD", "0010"); //내외작	
	
	//this.alert(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
	
	this.ds_Detail10.setColumn(iAddRow, "ISPH_NO", ""); //제품사양번호	
	this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setFocus();
	this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getBindCellIndex("body","CNTB_SE"));

};

