}

/***********************************************************************************************
 * 15.05. [그리드내 ds_PpRouh 설정] 라우팅 콤보 세팅
 ***********************************************************************************************/ 
this.fn_ComboFilter = function(row, obj) {
	var itlmCd = this.ds_Detail10.getColumn(row, "ITLM_CD");
	var itmmCd = this.ds_Detail10.getColumn(row, "ITMM_CD");
	var saumCd = this.ds_Detail10.getColumn(row, "SAUM_CD");

	var filterStr = "";
	if (!this.gfn_IsNull(itlmCd)) {
		filterStr += "ITLM_CD == '"+ itlmCd +"'";
	}
	if (!this.gfn_IsNull(itmmCd)) {
		if (filterStr != "") filterStr += " && ";
		filterStr += "ITMM_CD == '"+ itmmCd +"'";
	}
	if (!this.gfn_IsNull(saumCd)) {
		if (filterStr != "") filterStr += " && ";
		filterStr += "SAUM_CD == '"+ saumCd +"'";
	}
	this.ds_PpRouh.filter("");
	this.ds_PpRouh.filter(filterStr);
	
	if (obj.newvalue != obj.oldvalue) {
		//this.ds_Detail10.setColumn(row, "ROUH_NO", "");
		//this.ds_Detail10.setColumn(row, "ROUH_NM", "");
	}
};

/***********************************************************************************************
 * 15.06. [공통 그리드 버튼] 입력 버튼 설정(활성 / 비활성)
 ***********************************************************************************************/ 
this.fn_SetGridInputButtonEnable = function()
{
	//그리드 공통 버튼 권한이 있으면
	if (this.fvFormComBtnAuthYn)
	{
		var objBtnInfo01List = [];
		var sWorhCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WORH_CD");
		
		if (sWorhCd == "0040"){
			//행추가 버튼 활성화
			objBtnInfo01List = ["ADD|Y","DELETE|Y","CANCEL|Y"];
		} else {
			//행추가 버튼 비활성화
			var strCnthNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO");
			
			if(this.gfn_IsNull(strCnthNo) || strCnthNo == ""){			
				objBtnInfo01List = ["ADD|N","DELETE|Y","CANCEL|Y"];
			} else {
				objBtnInfo01List = ["ADD|Y","DELETE|Y","CANCEL|Y"];
			}
		}
		
		if (this.div_DetailInput.form.tab_Detail.tabindex == 0) {
			this.div_DetailInput.form.div_ComGrdBtn.form.fn_SetGrdButtonEnable(objBtnInfo01List);
		}
	}
};

/***********************************************************************************************
 * 15.07. [W/O내역 입력폼내 W/O형태] 변경 이벤트
 ***********************************************************************************************/
this.cbo_WorhCd_onitemchanged = function()
{
	if (this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WORH_CD") == "0040") {// 연구이면..
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PlphNo.set_cssclass("essential");
		//this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PlphNo.set_enable(true);//연구개발번호 활성화
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PlphNo.set_readonly(false);//연구개발번호 활성화
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_PlphNo.set_enable(true);//연구개발번호 검색버튼 활성화
		
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 5, "edittype", "text");//품목코드 활성화
		this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 8, "edittype", "text");//수량 활성화
		
		this.div_DetailInput.form.tab_Detail.set_tabindex(0);
		this.div_DetailInput.form.tab_Detail.tpg_02.set_enable(false);
	} else {//수주
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PlphNo.set_cssclass("");
		this.ds_Master10.set_updatecontrol(false);//ds_Master10 rowType 자동변경 비활성화
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PlphNo.set_value("");//연구개발번호 초기화
		this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PlphNm.set_value("");//연구개발명 초기화
		this.ds_Master10.set_updatecontrol(true);//ds_Master10 rowType 자동변경 활성화
		//this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PlphNo.set_enable(false);//연구개발번호 비활성화
		this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PlphNo.set_readonly(true);//연구개발번호 비활성화
		this.div_MasterInput.form.tab_Master.tpg_01.form.btn_PlphNo.set_enable(false);//연구개발번호 검색버튼 비활성화
		
		//this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 5, "edittype", "none");//품목코드 비활성화
		//this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellProperty("body", 8, "edittype", "none");//수량 비활성화
		
		this.div_DetailInput.form.tab_Detail.tpg_02.set_enable(true);
		
		var delRows = [];
		for (var iLoop = 0; iLoop < this.ds_Detail10.getRowCount(); iLoop++) {
			if (this.ds_Detail10.getColumn(iLoop, "crudType") == "I") {
				delRows.push(iLoop);
			}
		}

		this.ds_Detail10.deleteMultiRows(delRows);
	}
};

