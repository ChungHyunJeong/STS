 * 02.01. [공통버튼] 입력 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnAdd = function()
{
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10)) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자

	var iAddRow = this.ds_Master10.addRow();
	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", "1");//결재차수
	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");//최종여부
	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//등록자부서코드	
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//등록자부서명
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//등록자사번
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//등록자명
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//등록자ID
	this.ds_Master10.setColumn(iAddRow, "WORH_DD", sToDay);//등록일자-오늘날자로 설정
	this.ds_Master10.setColumn(iAddRow, "WORH_CD", "0020");//W/O형태 - 수주
	this.ds_Master10.setColumn(iAddRow, "WOFH_ST", "0010");//결재상태-등록(0010-WF110)으로 설정	


	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.div_DetailInput.form.tab_Detail.set_enable(true);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WorhCd.set_enable(true);//W/O형태 활성화

	//입력영역 비활성화
	this.cbo_WorhCd_onitemchanged();	
	this.fn_SetGridInputButtonEnable();	
	
//	this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PlphNo.set_enable(false);//연구개발번호 비활성화
//	this.div_MasterInput.form.tab_Master.tpg_01.form.btn_PlphNo.set_enable(false);//연구개발번호 검색버튼 비활성화
//	this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_enable(false);//계약번호 비활성화
	this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_readonly(true);//계약번호 비활성화
	this.div_MasterInput.form.tab_Master.tpg_01.form.btn_CnthNo.set_enable(false);//계약번호 검색버튼 비활성화
	//this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_enable(false);//거래처코드 비활성화
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_readonly(true);//거래처코드 비활성화
	this.div_MasterInput.form.tab_Master.tpg_01.form.btn_CusmCd.set_enable(false);//거래처 검색버튼 비활성화
	
	//처음 입력 항목에 포커스
	this.div_DetailInput.form.tab_Detail.set_tabindex(1);
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WorhCd.setFocus();

	this.ds_Detail10.clearData();//상세정보 초기화
	this.ds_Detail20.clearData();//검색정보 초기화
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화
	
	//첨부파일정보 권한설정
	var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
	oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
	this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	
	
	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
}

