 ***********************************************************************************************/ 
this.fn_VaildDetailSave = function()
{
	var sPaldNm = "";
	var sEmpmCd = "";
	var sItlmYn = "";
	
	//[저장] Validation 설정
	var arrArgs = ["CNTB_SE^NULL"
	              ,"WORB_NO^NULL"
			      ,"ITEM_CD^NULL"
			      ,"WORB_QT^NULL"
				  ,"SAUM_CD^NULL"
				  ,"IOMA_CD^NULL"
				  ,"HOGI_NM^NULL"
				  //,"PALD_NM^NULL|MIN_LENGTH=1"
				  //,"EMPM_CD^NULL|MIN_LENGTH=1"
				  //,"ROUH_NO^NULL"
			      ];

	var sCheckGroupTitle = this.div_DetailInput.form.tab_Detail.tpg_01.text;//체크그룹 타이틀
				  
	//Grid Vaild 체크
	if (!this.gfn_CheckGridValid(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10, arrArgs, sCheckGroupTitle) ) {
		return false;
	}
	
	var sCrudType = "";
		
	for(var i=0 ; i<this.ds_Detail10.getRowCount(); i++) {
		var sItlmYn = this.ds_Detail10.getColumn(i, "ITLM_YN");
		var sPaldNm = this.ds_Detail10.getColumn(i, "PALD_NM");
		var sEmpmCd = this.ds_Detail10.getColumn(i, "EMPM_CD");
		
		var sCrudType = this.ds_Detail10.getColumn(i, "crudType");
		
		sItlmYn = (this.gfn_IsNull(sItlmYn)  || sItlmYn.trim() == "") ? "0" : sItlmYn;			
			
		
		if(sCrudType != "D") {
			if(sItlmYn == "0" || sItlmYn ==""){
			
				if(this.gfn_IsNull(sPaldNm)) {
					this.ds_Detail10.set_rowposition(i);
					var arrMsgArg = ["Playload"];
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getBindCellIndex("body",'PALD_NM'));
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.showEditor(true);
					this.gfn_Alert("SM008", arrMsgArg); //필수 입력 항목입니다.			
					return false;
				}
				
				if(this.gfn_IsNull(sEmpmCd)) {
					this.ds_Detail10.set_rowposition(i);
					var arrMsgArg = ["대표설계자"];
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getBindCellIndex("body",'EMPM_CD'));
					this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.showEditor(true);
					this.gfn_Alert("SM008", arrMsgArg); //필수 입력 항목입니다.			
					return false;
				}
			}			
		}		
	}
	
	
	
	return true;
}

