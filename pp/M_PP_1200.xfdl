 ***********************************************************************************************/
this.fn_DetailGrd01DeleteRow = function()
{
	var s_chk = "";//체크
	var WofhSq  =  this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ");
	
	
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		s_chk = this.ds_Detail10.getColumn(iLoop, "s_chk");		
		
		if (s_chk == "1") {
			if(!this.gfn_IsNull(WofhSq) && WofhSq != "1"){
 				this.gfn_Alert("PP010");  //결재차수가 1차수만 삭제하실 수 있습니다.
 				return false;
 			}			
		}
	}
	
	
	//미출고검색 내역 체크 가능하게 변경
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		s_chk = this.ds_Detail10.getColumn(iLoop, "s_chk");
		
		//체크
		if (s_chk == "1") {
		
			//검색값설정 - 컬럼명:검색값
			var objSearchList = ["CNTH_NO|" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO") //계약번호
				                ,"CNTB_SE|" + this.ds_Detail10.getColumn(iLoop, "CNTB_SE") //계약순번
								,"CNTC_SE|" + this.ds_Detail10.getColumn(iLoop, "CNTC_SE") //계약상세순번
								];

			//[공통] 행삭제시 검색Tab 선택Row 해제 처리
			this.gfn_DeleteRowSchTabUncheck(this.ds_Detail20, objSearchList);
		}
	}
	this.gfn_GridDelProcess(this, this.ds_Detail10);//crud_flag 변경-삭제(D)	
};

