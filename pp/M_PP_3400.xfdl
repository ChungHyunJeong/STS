/***********************************************************************************************
 * 02.DG1.05. [Detail그리드2-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Choice = function()
{
	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {
		
			//1. 거래처 중복 체크
			var sIdntNoChk=""; //Key(거래처) 체크			
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					
					//1. Detail20 Tab과 Master Input 화면 항목 비교 : 거래처코드
					if(this.ds_Detail10.rowcount > 0 ){
						if(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD") != this.ds_Detail20.getColumn(iLoop,"CUSM_CD") ){
							var arrMsgArg = ["거래처"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 거래처만 적용 가능합니다.
							return;
						}
					}
					
					//2. Detail20 그리드내 체크된 대상 동일여부 비교 : 거래처코드
					if(!this.gfn_IsNull(sIdntNoChk) ){
						if(sIdntNoChk != this.ds_Detail20.getColumn(iLoop,"CUSM_CD") ){
							var arrMsgArg = ["거래처"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 거래처만 적용 가능합니다.
							return;
						}
					}
					sIdntNoChk = this.ds_Detail20.getColumn(iLoop,"CUSM_CD"); //for문 비교를 위한 이전값 저장
				}
			}
			
			//------------------------------------------------------------------------------------------------------------------------//
			
			var sBUDG_MM = "";//마감년월
		
			if (!this.gfn_IsNull(this.fvMmCloseDay)) {
				sBUDG_MM = this.gfn_Nvl(this.fvMmCloseDay, "");//마감년월
			} else {
				//sBUDG_MM = this.gfn_Nvl(e.newvalue, "");//마감년월
				sBUDG_MM = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CPAH_DD"), "");//마감년월				
			}			
			
			var nRow = 0 ;
			
			nRow = this.ds_Detail20.findRowExpr("s_chk == '1'");		
			
			//var sSAUM_CD = this.ds_Detail20.getColumn(nRow,"SAUM_CD");		
			var sSAUM_CD = this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_SaumCd.value;
			
			//this.fvMmCloseDay = "";
			
			//마감여부 체크
			var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
			var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
			paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
			paramObj.MODM_CD = "PP";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
			paramObj.ACUM_CD = "";//[선택] 회계단위
			paramObj.BUSM_CD  = "";//[선택] 사업부문
			paramObj.SAUM_CD  = sSAUM_CD;//[선택] 사업장코드
			paramObj.DEPM_CD  = "";//[선택] 부서코드
			paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
			paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
			
			var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		

			//월마감된 경우
			if (bCheck) {
				this.gfn_Alert("CA025");//해당월의 정보가 마감이 되어서 등록/수정 할 수 없습니다.
				return;
			}
			
			
			//------------------------------------------------------------------------------------------------------------------------//
			
			
			
			
			//2. Detail20 --> Detail10 복사
			var arrCheckRow = [];//체크된 Row
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
			
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					//Detail10에 적용되어 있는지 체크
					var objSearchList = ["SAUM_CD|" + this.ds_Detail20.getColumn(iLoop, "SAUM_CD") //사업장
										,"CUSM_CD|" + this.ds_Detail20.getColumn(iLoop, "CUSM_CD") //거래처
										,"SYST_ID|" + this.ds_Detail20.getColumn(iLoop, "SYST_ID") //작업자
										,"CPAB_DD|" + this.ds_Detail20.getColumn(iLoop, "CPAB_DD") //작업일자										
										,"CNTH_NO|" + this.ds_Detail20.getColumn(iLoop, "CNTH_NO") //계약번호
										,"CNTB_SE|" + this.ds_Detail20.getColumn(iLoop, "CNTB_SE") //계약순번
										,"WORB_NO|" + this.ds_Detail20.getColumn(iLoop, "WORB_NO") //W/O
										];		
										
					//[공통] Detail10 중복건 체크후 해당건 Skip
					if (!this.gfn_DsSchDupcheck(this.ds_Detail10, objSearchList)) {
						
						if (this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CPAH_NO")) && this.ds_Detail10.rowcount == 0 ) { 
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"CUSM_CD",this.ds_Detail20.getColumn(iLoop,"CUSM_CD"));	//거래처
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"CUSM_NM",this.ds_Detail20.getColumn(iLoop,"CUSM_NM"));	//거래처명
							this.ds_Master10.setColumn(this.ds_Master10.rowposition,"SAUM_CD",this.ds_Detail20.getColumn(iLoop,"SAUM_CD"));	//사업장
							
							//Master10 거래처 비활성화
// 							this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SaumCd.set_enable(false); //사업장
// 							this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_enable(false); //거래처코드
							this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SaumCd.set_readonly(true); //사업장
							this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_readonly(true); //거래처코드
							this.div_MasterInput.form.tab_Master.tpg_01.form.btn_CusmCd.set_enable(false); //거래처돋보기
						}
						
						var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)
						
						//Detail20 -> Detail10 복수
						this.ds_Detail10.copyRow(iAddRow, this.ds_Detail20, iLoop);

						//Detail10에 Key값 저장 : 부가세(VTAX_AM)는 저장시 Master10의 부가세코드(VATM_CD) 기준으로 계산
						this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD")); //회사코드
						this.ds_Detail10.setColumn(iAddRow, "CPAH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CPAH_NO")); //번호
						this.ds_Detail10.setColumn(iAddRow, "WORH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WORH_SQ")); //결재차수						
						
						arrCheckRow.push(iLoop );

						//선택항목 비활성화처리
						this.ds_Detail20.setColumn(iLoop, "s_conf", "1");
						this.ds_Detail20.setColumn(iLoop, "s_chk", "0");//Uncheck로 변경
					}
				}
			}
			
			//Detail20 -> Detail10 적용 후 Master10 공급가액, 부가세, 합계 계산
			this.fn_VtaxAmountSum(); //공급가액, 부가세, 합계 Function 호출
			
			//Detail10 부가세 계산 (전체)
			var iVatmRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "VATM_RT"); //Master10 부가세율 가져오기
			var iTrjbAm = 0; //Detail10 운송금액
			var iVtaxAm = 0; //Detail10 부가세
			
			if (!this.gfn_IsNull(iVatmRt)) {	//부가세율 존재시
				for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
					iTrjbAm = this.ds_Detail10.getColumn(iLoop, "CPAB_AM"); //운송금액
					
					iVtaxAm = nexacro.floor(iTrjbAm * iVatmRt, 0);  	   //부가세 = 운송금액 * 부가세율 (원단위)
					this.ds_Detail10.setColumn(iLoop, "VTAX_AM", iVtaxAm); //부가세
				}
			}
			
			
						
			//-----거래처 계좌정보 가져오기-----------------------------------------------------------------------------------------------------------------------//
			this.ds_Input.clearData();
			this.ds_Input.addRow();
			
			//var sCusmCd    = this.ds_Detail20.getColumn(0, "CUSM_CD");
			var sCusmCd    = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD");
			
			this.ds_Input.setColumn(0,	"CUSM_CD", sCusmCd);
			
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "sd:M_SD_CBAB_C_SL10";
			this.gfn_AddComSearchService(this, svcAddObj);
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH_GET";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_SD;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_SdCbab=ds_SdCbab";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명
			svcCallObj.sComBtnDst = "SEARCH";//공통버튼구분-조회버튼
			svcCallObj.sPrgmId = this.fvComPrgmId;//프로그램ID		
			svcCallObj.isSync = true;//동기식으로 설정할지 여부
			
			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);
			
			//trace(this.ds_Input.saveXML());
			//trace(this.ds_SdCbab.saveXML());
			
			//거래처계좌 filter
			this.ds_SdCbabFull.filter("");
			this.ds_SdCbabFull.filter("CUSM_CD == '" + sCusmCd + "'");	
			
			this.ds_SdCbabFull.set_keystring( "S:-CBAB_YN" );
			
			//this.alert(this.ds_SdCbab.rowcount);
			
			if(this.ds_SdCbab.rowcount > 0){
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "ACNO_GU", this.ds_SdCbab.getColumn(0, "ACNO_GU"));	//계좌구분
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "BANM_CD", this.ds_SdCbab.getColumn(0, "BANM_CD"));	//은행코드
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "ACNM_NO", this.ds_SdCbab.getColumn(0, "ACNM_NO"));	//계좌번호				
				
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "ACNM_CD", this.ds_SdCbab.getColumn(0, "BANM_CD") + this.ds_SdCbab.getColumn(0, "ACNO_GU") + this.ds_SdCbab.getColumn(0, "ACNM_NO"));	//계좌정보
			}			
			
	
			this.ds_Detail20.set_rowposition(-1);
			
			//운반정산상세로 Tab이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);
		}
	}
};

