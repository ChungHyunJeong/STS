/***********************************************************************************************
 * 02.DG0.04. [Detail그리드-버튼] 행삭제 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd01DeleteRow = function()
{
	var s_chk = "";//체크
	//대상검색에서 체크 가능하게 변경
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		s_chk = this.ds_Detail10.getColumn(iLoop, "s_chk");
		
		//체크
		if (s_chk == "1") {
		
			//검색값설정 - 컬럼명:검색값
			var objSearchList = ["SAUM_CD|" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SAUM_CD") //사업장
								,"CUSM_CD|" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD") //거래처
								,"SYST_ID|" + this.ds_Detail10.getColumn(iLoop, "SYST_ID") 
				                ,"CPAB_DD|" + this.ds_Detail10.getColumn(iLoop, "CPAB_DD") 
								,"CNTH_NO|" + this.ds_Detail10.getColumn(iLoop, "CNTH_NO") //계약번호
								,"CNTB_SE|" + this.ds_Detail10.getColumn(iLoop, "CNTB_SE") //계약순번
								,"WORB_NO|" + this.ds_Detail10.getColumn(iLoop, "WORB_NO") //W/O
								];

			//[공통] 행삭제시 검색Tab 선택Row 해제 처리
			this.gfn_DeleteRowSchTabUncheck(this.ds_Detail20, objSearchList);
		}
	}

	this.gfn_GridDelProcess(this, this.ds_Detail10);//crud_flag 변경-삭제(D)	

	//운반정산번호가 없고 운반정산상세 내용이 미존재시 Master Clear
	if (this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition,"CPAH_NO")) && this.ds_Detail10.rowcount == 0) {
		this.fn_ClearMasterValue();
	} else {
		this.fn_VtaxAmountSum(); //공급가액, 부가세, 합계 Function 호출 (재계산)
	}

};

