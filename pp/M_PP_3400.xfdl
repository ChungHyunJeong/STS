/***********************************************************************************************
 * 02.01. [공통버튼] 입력 버튼 클릭 이벤트
 ***********************************************************************************************/ 
this.fn_ComBtnAdd = function()
{
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10) || this.gfn_DsIsUpdated(this.ds_Detail10) || this.fvFileDiv.fn_CheckFileAddYn()) {
		this.gfn_Alert("SM002");//변경된 내용이 있습니다.\n저장 후 작업하시기 바랍니다.

		return;
	}

	this.fvDsRowPosChange = false;//데이터셋 Row 변경이벤트-실행유무
	var sToDay = this.gfn_GetCommDate("toDay");//현재일자
	
	this.ds_SdCbabFull.filter("@"); //없는자료를 Set
	
	var iAddRow = this.ds_Master10.addRow();
	
	this.fvMmCloseDay = sToDay;//[월마감] 마감일자
	
	this.ds_Master10.setColumn(iAddRow, "WOFH_ST", "0010");
	this.ds_Master10.setColumn(iAddRow, "WOFH_SQ", "1");//결재차수
	this.ds_Master10.setColumn(iAddRow, "WOFH_YN", "1");//최종여부	
	this.ds_Master10.setColumn(iAddRow, "DEPM_CD", this.gfn_GetSessionValue("DEPM_CD"));//출고부서코드	
	this.ds_Master10.setColumn(iAddRow, "DEPM_NM", this.gfn_GetSessionValue("DEPM_NM"));//출고부서명	
	this.ds_Master10.setColumn(iAddRow, "EMPM_CD", this.gfn_GetSessionValue("USEM_ID"));//출고자사번	
	this.ds_Master10.setColumn(iAddRow, "EMPM_NM", this.gfn_GetSessionValue("USEM_NM"));//출고자명		
	this.ds_Master10.setColumn(iAddRow, "SYST_ID", this.gfn_GetSessionValue("SYST_ID"));//출고자ID	
	this.ds_Master10.setColumn(iAddRow, "CPAH_DD", sToDay);//계산서일자-오늘날자로 설정
	this.ds_Master10.setColumn(iAddRow, "VATM_CD", "A101");//부가세코드 - 매입공제(일반) A101
	this.ds_Master10.setColumn(iAddRow, "ACUM_CD", "1000");//회계단위 - 시너스텍 1000
	this.ds_Master10.setColumn(iAddRow, "CASH_GU", "0010");//지급구분 - 현금 0010
	
	this.ds_Master10.setColumn(iAddRow, "CPAH_AM", 0);
	this.ds_Master10.setColumn(iAddRow, "VTAX_AM", 0);
	this.ds_Master10.setColumn(iAddRow, "TOTL_AM", 0);
	

	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SaumCd.set_index(0); //사업장 Default

	//운반정산상세로 Tab이동
	this.div_DetailInput.form.tab_Detail.set_tabindex(0);

	//입력영역 활성화
	this.div_MasterInput.set_enable(true);
	this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
	this.div_DetailInput.form.tab_Detail.set_enable(true);
	
	this.ds_PpWkgmIn.filter("");
	if (!this.gfn_IsNull(this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_SaumCd.value)){
		this.ds_PpWkgmIn.filter("SAUM_CD == '"+ this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_SaumCd.value +"'" );		
	}		
	
	//처음 입력 항목에 포커스
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SaumCd.setFocus();

	//Master10 사업장/거래처 활성화
//	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SaumCd.set_enable(true); //사업장
//	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_enable(true); //거래처코드
	this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SaumCd.set_readonly(false); //사업장
	this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CusmCd.set_readonly(false); //거래처코드

	this.div_MasterInput.form.tab_Master.tpg_01.form.btn_CusmCd.set_enable(true); //거래처돋보기

	this.ds_Detail10.clearData();//상세정보 초기화
	this.ds_Detail20.clearData();//검색정보 초기화
	this.fvFileDiv.fn_ClearAtchFile();//첨부파일 정보 초기화

	//첨부파일정보 권한설정
	var oScrnBtnAuth = new Object();//[공통] 첨부파일 권한정보
	oScrnBtnAuth.BTN_ENABLE = true;//권한 유무	
	this.fvFileDiv.fn_SetCommFileBtnAuth(oScrnBtnAuth);	

	//운반비정산대상검색 초기화/활성화
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_SaumCd.set_index(0);  //사업장	
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmCd.set_value(""); //거래처코드
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmNm.set_value(""); //거래처명

	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_SaumCd.set_enable(true);  //사업장
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmCd.set_enable(true); //거래처코드
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmNm.set_enable(true); //거래처명
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.btn_CusmCd.set_enable(true); //거래처버튼

	this.fvDsRowPosChange = true;//데이터셋 Row 변경이벤트-실행유무
	
	this.div_DetailInput.form.tab_Detail.set_tabindex(1);
	this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_WkgmCd.set_index(0);
	
	eval(this.fvAtobDiv).fn_ClearAtobData();//자동전표 정보 초기화

}

