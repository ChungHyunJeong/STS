 * 02.DG5.03. [자동전표상세 그리드-버튼] 자동전표 미리보기 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd01Atob = function() {
	var iCurRow = this.ds_Master10.rowposition;//현재 선택된 행
	var comm_cd = this.ds_Master10.getColumn(iCurRow, "COMM_CD")	

	var autm_cd	= "PP_3400_10"; // 자동전표호출할 프로그램 ID
	var kye_01	= this.ds_Master10.getColumn(iCurRow, "CPAH_NO");
	var kye_02	= this.ds_Master10.getColumn(iCurRow, "WOFH_SQ");
	
	if (iCurRow > -1) {
		var atobObj = new Object();//[공통] 자동전표 파라메타 정보 Object
		atobObj.COMM_CD = comm_cd;//회사코드
		atobObj.AUTM_CD = autm_cd;//분류코드-T_FA_AUTM(자동전표환경설정) 테이블 내용 참조
		atobObj.PRGM_ID = this.fvComPrgmId;//프로그램ID
		atobObj.MANA_NO = kye_01;//관리번호-업무Key
		atobObj.WOFH_SQ = kye_02;//결재차수
		atobObj.REGI_ID = this.gfn_GetSessionValue("SYST_ID");//등록자
		atobObj.REGI_NM = this.gfn_GetSessionValue("USEM_NM");//등록자명
		atobObj.KEY_01 = "@COMM_CD=" + comm_cd;//T_FA_AUTD-자동전표환경설정-D 테이블의 쿼리에 사용하는 파라메타 Key-Value정보
		atobObj.KEY_02 = "@CPAH_NO=" + kye_01;
		atobObj.KEY_03 = "@WOFH_SQ=" + kye_02;
		atobObj.KEY_04 = "";
		atobObj.KEY_05 = "";
		atobObj.KEY_06 = "";
		atobObj.KEY_07 = "";
		atobObj.KEY_08 = "";
		atobObj.KEY_09 = "";
		atobObj.KEY_10 = "";
		
		eval(this.fvAtobDiv).fn_DetailGrd01Atob(atobObj);//자동전표 미리보기
	}
}



