/***********************************************************************************************
 * 02.DG0.07. [Detail 데이터셋] 변경 이벤트
 ***********************************************************************************************/
/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	//부가세코드 선택시 : 공급가액, 부가세, 합계금액 구하는 Function 호출
	if (e.columnid == "CPAB_AM") {
		if (!this.gfn_IsNull(this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "CPAB_AM"))) {
			this.fn_VtaxAmountSum(); //공급가액, 부가세, 합계 Function 호출
			
			//Detail10 부가세 계산 (단건)
			var iVatmRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "VATM_RT"); //부가세율
			if (!this.gfn_IsNull(iVatmRt)) {	//부가세율 존재시
				var iTrjbAm = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "CPAB_AM"); //운송금액
				
				var iVtaxAm = nexacro.floor(iTrjbAm * iVatmRt, 0); //부가세 = 운송금액 * 부가세율			
				this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "VTAX_AM", iVtaxAm); //부가세
			}
		}
		
	// 체크박스 체크해제시 (curdType D 해제)
	} else if (e.columnid == "s_chk") {
		var s_chk = this.ds_Detail10.getColumn(this.ds_Detail10.rowposition, "s_chk"); 
	
		//s_chk가 변경 대상인 경우
		if (s_chk == '0') {		
			this.fn_VtaxAmountSum(); //공급가액, 부가세, 합계 Function 호출
		}
	}
	
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Detail20] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.alert("FD");
	//체크시 해당 프로그램 전체선택
	if (e.columnid == "s_chk") {			
		if (this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "s_chk") == '1') {				
			var objSearchList = ["SAUM_CD|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "SAUM_CD") //사업장
								,"CUSM_CD|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "CUSM_CD") //거래처
								,"SYST_ID|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "SYST_ID") //
								,"CPAB_DD|" + this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "CPAB_DD") //
								];		
								
			//[공통] Detail10 중복건 체크후 해당건 Skip
			if (this.gfn_DsSchDupcheck(this.ds_Detail10, objSearchList)) {
				this.gfn_Alert("SM041", arrMsgArg); //중복된 데이터가 존재합니다.
				this.ds_Detail20.setColumn(this.ds_Detail20.rowposition, "s_chk", '0');// Check 해제
				return;
			}			

			//동일 운반거래처 일괄 선택
			var sCusmCd = this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "CUSM_CD");//운송거래처
			
			//운송거래처가 최초 1건만 체크됐을 경우에만 처리함(2건일 경우 return)
			var iChkCnt = this.ds_Detail20.getCaseCount("CUSM_CD == '"+sCusmCd+"' && s_chk == '1'"); //체크건수
			if (iChkCnt > 1) return;
			
			//최초 1건만 체크시 로직 처리
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
			
				//동일 운송거래처 대상 체크박스 체크
				if (this.ds_Detail20.getColumn(iLoop, "CUSM_CD") == sCusmCd 
				 && (this.gfn_IsNull(this.ds_Detail20.getColumn(iLoop, "s_conf")) || this.ds_Detail20.getColumn(iLoop, "s_conf") == '0')) {
								
					//체크박스 Check
					this.ds_Detail20.setColumn(iLoop, "s_chk", "1");//Check로 변경
					
				}
			}

		}
		
	}
	
};


