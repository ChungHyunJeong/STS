/***********************************************************************************************
 * 15.06. [Sum] Mater 공급가액, 부가세, 합계
 ***********************************************************************************************/ 
this.fn_VtaxAmountSum = function()
{
	var iVatmRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition,"VATM_RT");	//부가세율

	if (!this.gfn_IsNull(iVatmRt)) {	//부가세율이 존재할 경우
		var iVtaxAm = 0; //부가세		
		var iTrjhAm = this.ds_Detail10.getCaseSum("crudType!='D'","CPAB_AM"); 	 	//공급가액 : Detail10 운송금액 Sum (crudType=D 제외)
		
		if (!this.gfn_IsNull(iVatmRt) && iVatmRt > 0) {
			iVtaxAm = nexacro.floor(iTrjhAm * iVatmRt, 0);  	//부가세 : 공급가액 * 부가세율 (원단위)
		}
		var iTotlAm = iTrjhAm + iVtaxAm; 					 	//합계 : 공급가액 + 부가세 (원단위)

		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CPAH_AM",iTrjhAm);	//공급가액
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "VTAX_AM",iVtaxAm);	//부가세
		this.ds_Master10.setColumn(this.ds_Master10.rowposition, "TOTL_AM",iTotlAm);	//합계
		
	}
	
};

