/***********************************************************************************************
 * 02.MG0.07. [Master 데이터셋] 변경 이벤트
 ***********************************************************************************************/
this.ds_Master10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//부가세코드 선택시 : 공급가액, 부가세, 합계금액 구하는 Function 호출
	if (e.columnid == "VATM_CD") {
		var sVatmCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition,"VATM_CD");	//부가세코드
		if (!this.gfn_IsNull(sVatmCd)) {			

			var iFindRow = this.ds_FaVatm.findRowExpr("VATM_CD=='" + sVatmCd + "'");
			var iVatmRt = this.ds_FaVatm.getColumn(iFindRow, "VATM_RT");	//부가세율
			this.ds_Master10.setColumn(this.ds_Master10.rowposition, "VATM_RT",iVatmRt);	//부가세율
			
			this.fn_VtaxAmountSum(); //공급가액, 부가세, 합계 Function 호출
			
			//Detail10 부가세 계산 (전체)
			var iVatmRt = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "VATM_RT"); //부가세율
			var iTrjbAm = 0; //운송금액
			var iVtaxAm = 0; //부가세
			for (var iLoop=0;iLoop<this.ds_Detail10.getRowCount();iLoop++) {
				iTrjbAm = this.ds_Detail10.getColumn(iLoop, "CPAB_AM"); //운송금액
				
				iVtaxAm = nexacro.floor(iTrjbAm * iVatmRt, 0);  	   //부가세 = 운송금액 * 부가세율 (원단위)
 				this.ds_Detail10.setColumn(iLoop, "VTAX_AM", iVtaxAm); //부가세
			}
		}
	//사업장 선택시	
	} else if (e.columnid == "SAUM_CD") {
		//운반정산대상검색 - 셋팅/비활성화 (사업장)
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SAUM_CD"))) {
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.cbo_SaumCd.set_value(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SAUM_CD")); //사업장	
		}

	//거래처 선택시
	} else if (e.columnid == "CUSM_CD") {
		//운반정산대상검색 - 셋팅/비활성화 (거래처)
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD"))) {
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmCd.set_value(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_CD")); //거래처코드
		}
	//거래처명 선택시
	} else if (e.columnid == "CUSM_NM") {
		//운반정산대상검색 - 셋팅/비활성화 (거래처명)
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_NM"))) {
			this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_CusmNm.set_value(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CUSM_NM")); //거래처명			
		}
		
	} else if (e.columnid == "ACNM_CD") {
		var nRow = this.ds_SdCbabFull.findRowExpr("ACNM_CD =='" + e.newvalue + "'");
		
		this.ds_Master10.setColumn(this.ds_Master10.rowposition,  "ACNO_GU",  this.ds_SdCbabFull.getColumn(nRow, "ACNO_GU"));
		this.ds_Master10.setColumn(this.ds_Master10.rowposition,  "BANM_CD",  this.ds_SdCbabFull.getColumn(nRow, "BANM_CD"));
		this.ds_Master10.setColumn(this.ds_Master10.rowposition,  "ACNM_NO",  this.ds_SdCbabFull.getColumn(nRow, "ACNM_NO"));
	}	
	
};

