 ***********************************************************************************************/
this.fn_GetYoil = function(data)
{
	var sYoil = this.gfn_GetDay(data);
	
	//0 = 일요일 ~ 6 = 토요일
	if (sYoil == 0) {
		sYoil = "일";
	} else if (sYoil == 1) {
		sYoil = "월";
	} else if (sYoil == 2) {
		sYoil = "화";
	} else if (sYoil == 3) {
		sYoil = "수";
	} else if (sYoil == 4) {
		sYoil = "목";
	} else if (sYoil == 5) {
		sYoil = "금";
	} else if (sYoil == 6) {
		sYoil = "토";
	}
	
	return sYoil;

