	var objCondList = ["PP045|ds_Pp045"
					  ,"SM034|ds_Sm034"
					  ,"SM036|ds_Sm036"
					  ,"SM046|ds_Sm046"
					  ,"PP040|ds_Pp040"];
					  
	//공통코드 설정함수 호출				   
	this.gfn_GetCommCode(this, objCondList, "");
	
