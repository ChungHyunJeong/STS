this.fn_AllCreate = function() 
{
	if (!this.fn_VaildSearch()) return;	
	if (!this.fn_VaildMasterCreate()) return;

	//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
	this.ds_InputSch.clearData();
	this.ds_Master10.clearData();

 	//[업무] 조회조건 입력
 	this.ds_InputSch.addRow();
 	this.ds_InputSch.setColumn(0, "FROM_DD", this.div_Search.form.cal_WkpmDdFr.value); //일괄 - 시작일자
 	this.ds_InputSch.setColumn(0, "CLOS_DD", this.div_Search.form.cal_WkpmDdTo.value); //일괄 - 종료일자
 	this.ds_InputSch.setColumn(0, "SAUM_CD", this.div_Search.form.cbo_SaumCd.value); //사업장
 	this.ds_InputSch.setColumn(0, "WKGM_CD", this.div_MasterInput.form.tab_Master.tpg_01.form.div_Search.form.cbo_WkgmCd.value); //작업조
 	this.ds_InputSch.setColumn(0, "DANI_CD", this.div_MasterInput.form.tab_Master.tpg_01.form.div_Search.form.cbo_Sm046.value);  //주야구분
 	this.ds_InputSch.setColumn(0, "FROM_HR", this.div_MasterInput.form.tab_Master.tpg_01.form.div_Search.form.cbo_Sm034F.value); //일괄 - 시작(시간)
 	this.ds_InputSch.setColumn(0, "FROM_MI", this.div_MasterInput.form.tab_Master.tpg_01.form.div_Search.form.cbo_Sm036F.value); //일괄 - 시작(분)
 	this.ds_InputSch.setColumn(0, "CLOS_HR", this.div_MasterInput.form.tab_Master.tpg_01.form.div_Search.form.cbo_Sm034C.value); //일괄 - 종료(시간)
 	this.ds_InputSch.setColumn(0, "CLOS_MI", this.div_MasterInput.form.tab_Master.tpg_01.form.div_Search.form.cbo_Sm036C.value); //일괄 - 종료(분)
 	this.ds_InputSch.setColumn(0, "CALM_GU", this.div_MasterInput.form.tab_Master.tpg_01.form.div_Search.form.cbo_Pp045.value);  //생성구분(휴일제외)
	
	//[공통]서비스정보 초기화
	this.gfn_InitComService(this);
	
	//[공통]서비스정보 추가
	var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
	svcAddObj.sSelectSql = "pp:M_PP_1100_M_SL20";
	this.gfn_AddComSearchService(this, svcAddObj);
	
	//[공통]서비스 호출 변수
	var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
	svcCallObj.sTrid = "MSTS_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
	svcCallObj.sActionName = this.G_SV_PP;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
	svcCallObj.sInputDaset = "ds_Input=ds_InputSch" //입력 데이터셋
	svcCallObj.sOutDatset = "ds_Master10=ds_Master10";//출력 데이터셋
	svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
	svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

	//[공통]서비스 호출
	this.gfn_CallComService(this, svcCallObj);
} 

