	var sOp01Nm = this.ds_Pp040.getColumn(0, "OP01_NM");//근무시작시간
	var sOp02Nm = this.ds_Pp040.getColumn(0, "OP02_NM");//근무시작분
	var sOp03Nm = this.ds_Pp040.getColumn(0, "OP03_NM");//근무종료시간
	var sOp04Nm = this.ds_Pp040.getColumn(0, "OP04_NM");//근무종료분
	
	//근무시작시간
	if (sOp01Nm < 10) {
		sOp01Nm = sOp01Nm.substr(1,1);
	}
	//근무종료시간
	if (sOp03Nm < 10) {
		sOp03Nm = sOp03Nm.substr(1,1);
	}
	
	this.div_MasterInput.form.tab_Master.tpg_01.form.div_Search.form.cbo_Sm034F.set_index(sOp01Nm); //근무시작시간(시간)
	this.div_MasterInput.form.tab_Master.tpg_01.form.div_Search.form.cbo_Sm036F.set_index(sOp02Nm.substr(0,1)); //근무시작시간(분)
	this.div_MasterInput.form.tab_Master.tpg_01.form.div_Search.form.cbo_Sm034C.set_index(sOp03Nm); //근무종료시간(시간)
	this.div_MasterInput.form.tab_Master.tpg_01.form.div_Search.form.cbo_Sm036C.set_index(sOp04Nm.substr(0,1)); //근무종료시간(분)
	this.div_MasterInput.form.tab_Master.tpg_01.form.div_Search.form.cbo_Pp045.set_index(0);  //생성구분
	this.div_MasterInput.form.tab_Master.tpg_01.form.div_Search.form.cbo_Sm046.set_index(0);  //주야구분
	
	//[업무] 일반콤보조회
	this.fn_ComboSearch();
}

