this.fn_ComBtnDecide = function()
{ 
	//[공통] 결재요청 Vaildation 체크
	if (!this.fn_VaildWfDecide()) return;
	
	var iRow = this.ds_Master10.rowposition;
	var sPks = this.ds_Master10.getColumn(iRow, "COMM_CD");//회사코드
	sPks += "ⓐ" + this.ds_Master10.getColumn(iRow, "WRKH_NO");//관리번호
	sPks += "ⓐ" + this.ds_Master10.getColumn(iRow, "WOFH_SQ");//결재차수
	
	var nRow 	   	= this.ds_FaSaum.findRowExpr("SAUM_CD == '" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SAUM_CD") + "'");
	var strSaumNm  	= this.ds_FaSaum.getColumn(nRow, "SAUM_NM");
	
	nRow 	   		= this.ds_PpWkgm.findRowExpr("SAUM_CD == '" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SAUM_CD") + "' && WKGM_CD == '" + this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WKGM_CD") + "'");
	strWkgmNm  		= this.ds_PpWkgm.getColumn(nRow, "WKGM_NM");		
	
	var sPopupId = "C_WF_REQUEST";//팝업ID - 결재요청
	var oArg = { CommCd:this.gfn_GetSessionValue("COMM_CD")//회사코드
	           , ModuCd:"PP"//모듈구분
			   , WctmId:"WF_M_PP_3300_W1"//결재업무ID
			   , SystId:this.gfn_GetSessionValue("SYST_ID")//결재자_시스템사번
			   //, WofhNm:"공정실적관리"//결재명
			   , WofhNm:"[공정실적:" + strSaumNm + "] " +  this.gfn_StringFormat(this.ds_Master10.getColumn(iRow, "WRKH_DD"), "@@@@-@@-@@")  + " " + strWkgmNm   //결재명
			   , WofhNo:this.ds_Master10.getColumn(iRow, "WOFH_NO")//결재PID
			   , InnrGb:"1"//내부결재구분(1:신규, 2:내부)
			   , CharSystId:""//담당자_시스템사번
