this.fn_WorkTime = function()
{	
	
	var strWkgmCd = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WKGM_CD");
	var nRow      = this.ds_PpWkgmIn.findRowExpr("WKGM_CD=='" + strWkgmCd + "'");
	
	var strFhr	  = this.ds_PpWkgmIn.getColumn(nRow, "FHR");
	var strFmi	  = this.ds_PpWkgmIn.getColumn(nRow, "FMI");
	var strThr	  = this.ds_PpWkgmIn.getColumn(nRow, "THR");
	var strTmi	  = this.ds_PpWkgmIn.getColumn(nRow, "TMI");
	
	this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_HR", strFhr);
	this.ds_Master10.setColumn(this.ds_Master10.rowposition, "FROM_MI", strFmi);
	this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_HR", strThr);
	this.ds_Master10.setColumn(this.ds_Master10.rowposition, "CLOS_MI", strTmi);
	
}


this.fn_CountEmpm = function()
{
   
   var arrEmpm = new Array();
   var sEMPM_CD = "";//작업자
   for (var iLoop=0; iLoop<this.ds_Detail30.getRowCount(); iLoop++) {
      sEMPM_CD = this.ds_Detail30.getColumn(iLoop, "EMPM_CD");//작업자
      
      //해당배열에 정보가 없으면 추가
      if (arrEmpm.indexOf(sEMPM_CD) < 0) {
         arrEmpm.push(sEMPM_CD);
      }
   }
   
   return arrEmpm.length;
}]]></Script>
