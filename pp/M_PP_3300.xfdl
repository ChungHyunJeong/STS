 * 15.07. [작업내역그리드] 소요시간계산
 ***********************************************************************************************/ 
this.fn_getCalTime = function(nRow) 
{
	var obj = this.div_DetailInput.form.tab_Detail.tpg_03.form.grd_Detail30;
	var dsObj = this.ds_Detail30;
	var sRtn = "";
		
	if(this.gfn_IsNull(dsObj.getColumn(obj.getDatasetRow(nRow), "FROM_HR")) 
		|| this.gfn_IsNull(dsObj.getColumn(obj.getDatasetRow(nRow), "CLOS_HR"))){
		return sRtn;
	}
		
	var objFrom 	= new Object();//시작시간
	var objTo 		= new Object();//시작시간
	var sFhr,sFmi,sThr,sTmi = "";

	sFhr = dsObj.getColumn(obj.getDatasetRow(nRow),"FROM_HR").substr(0,2);
	sFmi = this.gfn_Nvl(dsObj.getColumn(obj.getDatasetRow(nRow),"FROM_HR").substr(2,2),"00");
	sThr = dsObj.getColumn(obj.getDatasetRow(nRow),"CLOS_HR").substr(0,2);
	sTmi = this.gfn_Nvl(dsObj.getColumn(obj.getDatasetRow(nRow),"CLOS_HR").substr(2,2),"00");
	
	objFrom.Hours 	= sFhr;//시작-시간
	objFrom.Minutes = sFmi ;//시작-분   
	objTo.Hours 	= sThr;//종료-시간
	objTo.Minutes 	= sTmi;//종료-분   

	var rtnValue = this.gfn_CalTimeDiffMi(objFrom, objTo);
	
	sRtn = (""+rtnValue.Hours).padLeft(2, "0") + "" + (""+rtnValue.Minutes).padLeft(2, "0");// 소요시간
	
	return sRtn;
}

