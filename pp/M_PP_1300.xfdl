/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail10] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail10_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
	if (e.columnid == "JAEG_QT") {	
		var sSoyoQt = nexacro.toNumber(this.ds_Detail10.getColumn(e.row, "MRPB_QT"));
 		var sMrpbQt = nexacro.toNumber(this.ds_Detail10.getColumn(e.row, "STOC_QT"));		
		var sPurcQt = nexacro.toNumber(this.ds_Detail10.getColumn(e.row, "PURC_QT"));	
		var sPurcQtOri = nexacro.toNumber(this.ds_Detail10.getOrgColumn(e.row, "PURC_QT"));	
 		var sJaegQt = nexacro.toNumber(e.newvalue);		
		var sJaegQtOri = nexacro.toNumber(this.ds_Detail10.getOrgColumn(e.row, "JAEG_QT"));	
		var sJaegQtOrg = e.oldvalue; //this.ds_Detail10.getOrgColumn(e.row, "JAEG_QT") 		
		if(this.gfn_IsNull(sJaegQt)){sJaegQt = 0;}
		
		if(sMrpbQt < sJaegQt)
		{				
			this.ds_Detail10.setColumn(e.row, "JAEG_QT", sJaegQtOrg)
			
			var arrText1 = this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getCellProperty("head", 8, "text");
			var arrText2 = this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getCellProperty("head", 10, "text");
			//var arrMsgArg = [arrText1,arrText2];
			var arrMsgArg = ["재고사용수량","가용재고수량"];			
			
			this.gfn_Alert("SM010", arrMsgArg); //재고사용수량은(는) 소요량 이하의 숫자만 입력 가능합니다.
			return;
		 } 
		 
		if(sSoyoQt < sJaegQt)
		{				
			this.ds_Detail10.setColumn(e.row, "JAEG_QT", sJaegQtOrg)
			
			var arrText1 = this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getCellProperty("head", 8, "text");
			var arrText2 = this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getCellProperty("head", 10, "text");
			//var arrMsgArg = [arrText1,arrText2];
			var arrMsgArg = ["재고사용수량","소요량"];			
			
			this.gfn_Alert("SM010", arrMsgArg); //재고사용수량은(는) 소요량 이하의 숫자만 입력 가능합니다.
			return;
		 } 
		 
		
		//if(!this.gfn_IsNull(sJaegQt) && sJaegQt != "0" && !this.gfn_IsNull(sMrpbQt) && sMrpbQt != "0"){
		if(!this.gfn_IsNull(sMrpbQt) && sMrpbQt != "0"){
			var sAplyQt  = 0 ;
			
			sAplyQt  = sPurcQtOri - sJaegQt;
			
						
			if(sJaegQtOri != 0){
				sAplyQt  = sSoyoQt - sJaegQt;					
			}			
			
			if(sJaegQt == 0 || this.gfn_IsNull(sJaegQt)){
				sAplyQt  = sSoyoQt;
			}
			
			this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "PURC_QT", nexacro.toNumber(sAplyQt));		
		}
		
		//if(e.newvalue=="0" || this.gfn_IsNull(e.newvalue)){
			//this.ds_Detail10.setColumn(this.ds_Detail10.rowposition, "PURC_QT", this.ds_Detail10.getOrgColumn(e.row, "PURC_QT"));		
		//}
		
	}else if (e.columnid == "MRPB_QT") {
		var sSoyoQt = nexacro.toNumber(e.newvalue);
		var sJaegQt = nexacro.toNumber(this.ds_Detail10.getColumn(e.row, "JAEG_QT"));
		
		if(sSoyoQt < sJaegQt){
			var arrMsgArg = ["소요량","재고사용수량"];						
			this.gfn_Alert("SM104", arrMsgArg); //소요량]은(는) 재고사용수량 같거나 이상의 숫자만 입력 가능합니다.
			return false;
		}	
		
		if(isNaN(sJaegQt)) { sJaegQt = 0;}
		
		this.ds_Detail10.setColumn(e.row, "PURC_QT", sSoyoQt - sJaegQt);
	
	
	
	//청구수량
