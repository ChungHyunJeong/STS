this.ds_Detail20_cancolumnchange = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	if(e.columnid =="APLY_QT"){
		var nAplyQtMi = obj.getColumn(e.row, "APLY_QT_MI");
		
		if(e.newvalue <= 0){
			var arrMsgArg = ["수립계획수량"];
			this.gfn_Alert("SM008",arrMsgArg);
			return false;
		}
		
		if(nAplyQtMi < e.newvalue){
			var arrMsgArg = ["수립계획수량","미수립계획수량"];
			this.gfn_Alert("SM010",arrMsgArg);
			return false;
		}
	}
};
