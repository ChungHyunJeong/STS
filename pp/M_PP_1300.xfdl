this.ds_Detail10_cancolumnchange = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	if (e.columnid == "JAEG_QT") {
		var sMrpbQt = nexacro.toNumber(this.ds_Detail10.getColumn(e.row, "STOC_QT"));
		var sJaegQt = nexacro.toNumber(e.newvalue);			
		var sJaegQtOrg =  this.ds_Detail10.getOrgColumn(e.row, "JAEG_QT");  //nexacro.toNumber(e.newvalue);	
		
		if(sJaegQt < 0){return false;}
		
		//this.alert(sJaegQt + "     " + sMrpbQt 	);
				
		if(this.gfn_IsNull(sJaegQtOrg)) {sJaegQtOrg = 0;}

		if(sMrpbQt < sJaegQt)
		{	
			var nCell = this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getBindCellIndex("body",'JAEG_QT');
			
			this.ds_Detail10.setColumn(e.row, "JAEG_QT", sJaegQtOrg);
			
			this.ds_Detail10.set_rowposition(e.row);
			this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.setCellPos(nCell);
			
			var arrText1 = this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getCellProperty("head", 7, "text");
			var arrText2 = this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.getCellProperty("head", 9, "text");
			var arrMsgArg = [arrText1,arrText2];
			this.gfn_Alert("SM010", arrMsgArg); //재고사용수량은(는) 소요량 이하의 숫자만 입력 가능합니다.
			
			return false;
		}
	}else if (e.columnid == "MRPB_QT") {
		var nMrpbQtOri = this.ds_Detail10.getOrgColumn(this.ds_Detail10.rowposition,"MRPB_QT");
		var nMrpbQt    = e.newvalue;		
		
		
		if(nMrpbQt > nMrpbQtOri){
			return false;
		}
			
		var sSoyoQt = nexacro.toNumber(e.newvalue);
		var sJaegQt = nexacro.toNumber(this.ds_Detail10.getColumn(e.row, "JAEG_QT"));
		
		if(sSoyoQt < sJaegQt){
			var arrMsgArg = ["소요량","재고사용수량"];						
			this.gfn_Alert("SM104", arrMsgArg); //소요량]은(는) 재고사용수량 같거나 이상의 숫자만 입력 가능합니다.
			return false;
		}	
		
		if(isNaN(sJaegQt)){sJaegQt = 0;}
		
		//this.alert(sJaegQt);
		
		this.ds_Detail10.setColumn(e.row, "PURC_QT", sSoyoQt - sJaegQt);
		
		
		
		
		
		
// 	} else if (e.columnid == "s_chk") {	
// 		var sChkYn = this.ds_Detail10.getColumn(e.row, "s_chk");	//체크박스
// 		var sParbQt = this.ds_Detail10.getColumn(e.row, "MORB_QT");	//자재출고의뢰
