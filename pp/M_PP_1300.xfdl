/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Detail20] 데이터변경 이벤트
  ---------------------------------------------------------------------------------------------*/  
this.ds_Detail20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//선택
	if (e.columnid == "s_chk") {
		var sWorhNo = this.ds_Detail20.getColumn(e.row, "WORH_NO").trim();
		
		var mWorbNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WORB_NO");
		
		if(!this.gfn_IsNull(mWorbNo)){
			mWorbNo = mWorbNo.trim();
		}
		
				
		if (!this.gfn_IsNull(mWorbNo) && mWorbNo != sWorhNo) {
			var arrMsgArg = ["W/O번호"];
			this.gfn_Alert("SM057", arrMsgArg); //동일한 W/O번호만 적용 가능합니다.
			for (var iLoop = 0; iLoop < this.ds_Detail20.getRowCount(); iLoop++) {
				this.ds_Detail20.setColumn(iLoop, "s_chk", "0");//Uncheck로 변경
			}
			this.ds_Detail20.filter("");
			return;
		}
		
		var value = "0";
// 		for (var iLoop = 0; iLoop < this.ds_Detail20.getRowCount(); iLoop++) {
// 			if (this.ds_Detail20.getColumn(iLoop, "s_conf") != "1" && this.ds_Detail20.getColumn(iLoop, "PARB_QT") > 0) {
// 				value = (this.ds_Detail20.getColumn(iLoop, "WORH_NO") == sWorhNo) ? e.newvalue : "0";
// 				this.ds_Detail20.setColumn(iLoop, "s_chk", value);
// 			}
// 		}
		
		
		if (this.ds_Detail20.getColumn(this.ds_Detail20.rowposition, "s_chk") == '1') {				
			
						
			var iChkCnt = this.ds_Detail20.getCaseCount("WORB_NO == '"+sWorhNo+"' && s_chk == '1'"); //체크건수
			if (iChkCnt > 1) return;
			
			//최초 1건만 체크시 로직 처리
			this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.set_enableevent(false);
			
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				if (this.ds_Detail20.getColumn(iLoop, "s_conf") != "1" && this.ds_Detail20.getColumn(iLoop, "PARB_QT") > 0) {
					if (this.ds_Detail20.getColumn(iLoop, "WORH_NO") == sWorhNo ){						
						this.ds_Detail20.setColumn(iLoop, "s_chk", "1");//Check로 변경						
					}
				}
			}
			this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.set_enableevent(true);
		}
		
		
		
	}
};

