/***********************************************************************************************
 * 02.DG1.05. [Detail그리드2-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Choice = function()
{
	if (this.ds_Master10.rowposition > -1) {
		//if (this.ds_Detail20.getCaseCount("s_chk=='1' && APLY_QT > 0") > 0) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {
			
			var arrCheckRow = [];//체크된 Row
			var mWorbNo = "";
			var sWorbNo = "";
			/*
			var mWorbNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WORB_NO");
			var sWorbNo = this.ds_Detail20.getColumn(0, "WORH_NO");	

		
			if (!this.gfn_IsNull(mWorbNo) && mWorbNo != sWorbNo) {
				var arrMsgArg = ["W/O번호"];
				this.gfn_Alert("SM057", arrMsgArg); //동일한 W/O번호만 적용 가능합니다.
				for (var iLoop = 0; iLoop < this.ds_Detail20.getRowCount(); iLoop++) {
					this.ds_Detail20.setColumn(iLoop, "s_chk", "0");//Uncheck로 변경
				}
				this.ds_Detail20.filter("");
				return; 
			}
			*/	
			//---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
			var sCnthNoChk;
			//계약번호 추가 체크(단일 계약번호만 추가 가능)
			if (this.ds_Detail10.rowcount > 0) {
				var sWorbNo = this.ds_Detail10.getColumn(0, "WORH_NO");
			}
			
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					
				 
					if (!this.gfn_IsNull(sWorbNo)){
						if (sWorbNo != this.ds_Detail20.getColumn(iLoop, "WORH_NO")) {
							var arrMsgArg = ["W/O번호" + strMirhNo + ")"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 [W/O번호]만 적용 가능합니다.
							return;						
						}
					}					
					
					//그리드내 출고번호 체크
					if(!this.gfn_IsNull(sCnthNoChk) ){
						if(sCnthNoChk != this.ds_Detail20.getColumn(iLoop,"WORH_NO") ){
							var arrMsgArg = ["W/O번호(" + sCnthNoChk + ")"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 [W/O번호]만 적용 가능합니다.
							return;
						}
					}					
					sCnthNoChk = this.ds_Detail20.getColumn(iLoop,"WORH_NO");					
				 
				}
			}
			
			
			//---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
			
			var mWorbNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WORB_NO");
			
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {				
					if (!this.gfn_IsNull(mWorbNo)){
						if (mWorbNo != this.ds_Detail20.getColumn(iLoop, "WORH_NO")) {
							var arrMsgArg = ["W/O번호"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 W/O번호만 적용 가능합니다.
							return;
						}
					}				
				}
			}						

	
			this.ds_Detail20.set_enableevent(false);
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {
					var iAddRow = this.gfn_GridInsertProcess(this, this.ds_Detail10);//crud_flag 변경-입력(I)

					this.ds_Detail10.copyRow(iAddRow, this.ds_Detail20, iLoop);
					
					this.ds_Detail10.setColumn(iAddRow, "COMM_CD", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "COMM_CD"));
					this.ds_Detail10.setColumn(iAddRow, "MRPH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "MRPH_NO"));
					this.ds_Detail10.setColumn(iAddRow, "WOFH_SQ", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "WOFH_SQ"));
					
					this.ds_Detail10.setColumn(iAddRow, "MRPB_SE", "*");
					this.ds_Detail10.setColumn(iAddRow, "MRPB_QT", this.ds_Detail20.getColumn(iLoop, "APLY_QT"));//PDM소요량
					this.ds_Detail10.setColumn(iAddRow, "PURC_QT", this.ds_Detail20.getColumn(iLoop, "APLY_QT"));//청구수량
					this.ds_Detail10.setColumn(iAddRow, "BFMR_QT", this.ds_Detail20.getColumn(iLoop, "BFPA_QT"));//이전소요량
					
					this.ds_Detail10.setColumn(iAddRow, "STOC_QT", this.ds_Detail20.getColumn(iLoop, "STOC_QT"));//재고수량
					
					this.ds_Detail10.setColumn(iAddRow, "PARH_NO", this.ds_Detail20.getColumn(iLoop, "PARH_NO"));//PARH관리번호
					this.ds_Detail10.setColumn(iAddRow, "PARB_SE", this.ds_Detail20.getColumn(iLoop, "PARB_SE"));//PARH 순번
					
					this.ds_Detail10.setColumn(iAddRow, "WOFH_ST", "0010");		//승인이 됐더라도 제외여부, 제외사유를 항상 수정상태로 체크하기위해(Grid의 소요량, 재고사용수량 edittype 체크함)
					
					//첫번째일 경우 마스터 업데이트 
					if(this.ds_Detail10.rowcount == 1 ){ 
						this.ds_Master10.set_enableevent(false);
						var nRow = this.ds_Master10.rowposition;						
						this.ds_Master10.setColumn(nRow, "WORB_NO", this.ds_Detail20.getColumn(iLoop, "WORH_NO"));
						this.ds_Master10.setColumn(nRow, "WORH_CD", this.ds_Detail20.getColumn(iLoop, "WORH_CD"));
						this.ds_Master10.setColumn(nRow, "SAUM_CD", this.ds_Detail20.getColumn(iLoop, "SAUM_CD"));
						
						this.ds_Master10.setColumn(nRow, "ITEM_CD", this.ds_Detail20.getColumn(iLoop, "ITEM_CD_WORB"));
						this.ds_Master10.setColumn(nRow, "ITEM_NM", this.ds_Detail20.getColumn(iLoop, "ITEM_NM_WORB"));
						
						this.ds_Master10.setColumn(nRow, "WORB_QT", this.ds_Detail20.getColumn(iLoop, "WORB_QT"));
						this.ds_Master10.setColumn(nRow, "CNTH_NO", this.ds_Detail20.getColumn(iLoop, "CNTH_NO"));
						this.ds_Master10.setColumn(nRow, "CNTH_NM", this.ds_Detail20.getColumn(iLoop, "CNTH_NM"));
						this.ds_Master10.setColumn(nRow, "PLPH_NO", this.ds_Detail20.getColumn(iLoop, "PLPH_NO"));
						this.ds_Master10.setColumn(nRow, "PLPH_NM", this.ds_Detail20.getColumn(iLoop, "PLPH_NM"));
						this.ds_Master10.setColumn(nRow, "HOGI_NM", this.ds_Detail20.getColumn(iLoop, "HOGI_NM"));
						
						
						var strWorbNo = this.ds_Detail20.getColumn(iLoop,"WORH_NO");
						var iWorbLen  = 0
						if(!this.gfn_IsNull(strWorbNo))
						{
							iWorbLen = this.ds_Detail20.getColumn(iLoop,"WORH_NO").length;
						}
					
						//W/O관리 format 변경 (14자리, 13자리, 10, 기타)
						if (iWorbLen == 14) 
							this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WorbNo.set_format("@@@@-@-@@@@-@@-@@@");								     
						else if (iWorbLen == 13) 
							this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WorbNo.set_format("@@@-@@@@-@@@-@@@");		
						else if (iWorbLen == 12) 
							this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WorbNo.set_format("@@@-@@@@-@@@-@@");
						else if (iWorbLen == 11) 
							this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WorbNo.set_format("@@@@-@-@@@@-@@");
						else if (iWorbLen == 10) 
							this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WorbNo.set_format("@@@-@@@@-@@@");		
						else if (iWorbLen == 9) 
							this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WorbNo.set_format("@@@@@@-@@@");			
						else if (iWorbLen == 1)
							this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WorbNo.set_format("@");	
						else 
							this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WorbNo.set_format("@@@@@@@@@@@@@@");	
						
						
						
						
						this.ds_Master10.set_enableevent(true);
					
					}					
					
					this.ds_Detail10.setColumn(iAddRow, "s_chk", "0");

					arrCheckRow.push(iLoop );
					
					//선택항목 비활성화처리
					//if (this.ds_Detail20.getColumn(iLoop, "APLY_QT") > 0) {
						this.ds_Detail20.setColumn(iLoop, "s_conf", "1");
					//}
					this.ds_Detail20.setColumn(iLoop, "s_chk", "0");//Uncheck로 변경
				}
			}
			this.ds_Detail20.set_enableevent(true);

			this.ds_Detail20.set_rowposition(-1);
			//출고상세로 Tab이동
			this.div_DetailInput.form.tab_Detail.set_tabindex(0);
		}
	}
};

