this.fn_Callback = function(sSvcId, nErrorCode, sErrorMsg)
{
	if(nErrorCode != 0)
	{
		return;
	}	
	switch(sSvcId)
	{
		case "MST_SEARCH": //조회		
	
			if (this.ds_Master10.getRowCount() > 0) {
			
				//저장후 재조회여부-true
				if (this.fvSchKeyYn) {
					var iFvFindRow = this.ds_Master10.findRowExpr("MRPH_NO=='" + this.fvSchKey01 + "' && WOFH_SQ=='" + this.fvSchKey02 + "'");//업무Key Row 찾기

					//마스터 데이터셋 이벤트가 비활성시 - 활성화
					if (!this.ds_Master10.enableevent) {
						this.ds_Master10.set_enableevent(true);
					}
				
					if (iFvFindRow > -1) {
						if (iFvFindRow == 0) {
							this.ds_Master10.set_rowposition(-1);
						} 
						this.ds_Master10.set_rowposition(iFvFindRow);
					} else {
						this.ds_Master10.set_rowposition(0);
					}
				}				
				
				//입력영역 활성화
				this.div_MasterInput.set_enable(true);
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
				this.div_DetailInput.form.tab_Detail.set_enable(true);
				
				//입력영역 컨트롤 비활성화
// 				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_MrphDd.set_enable(false);
// 				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_enable(false);
// 				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_DepmCd.set_enable(false);
// 				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_enable(false);//등록자사번
// 				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_SystId.set_enable(false);//등록자검색
// 				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WorhCd.set_enable(false);
// 				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SaumCd.set_enable(false);
// 				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WorbNo.set_enable(false);
// 				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ItemCd.set_enable(false);
// 				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_ItemCd.set_enable(false);
// 				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WorbQt.set_enable(false);
// 				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_enable(false);
// 				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CntbSe.set_enable(false);
// 				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_CnthNo.set_enable(false);
// 				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PlphNo.set_enable(false);
// 				//this.div_MasterInput.form.tab_Master.tpg_01.form.btn_PlphNo.set_enable(false);

				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_MrphDd.set_readonly(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_DepmCd.set_readonly(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_DepmCd.set_enable(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_EmpmCd.set_readonly(true);//등록자사번
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_SystId.set_enable(false);//등록자검색
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_WorhCd.set_readonly(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_SaumCd.set_readonly(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WorbNo.set_readonly(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_ItemCd.set_readonly(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_ItemCd.set_enable(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_WorbQt.set_readonly(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_readonly(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_CntbSe.set_readonly(true);
				this.div_MasterInput.form.tab_Master.tpg_01.form.btn_CnthNo.set_enable(false);
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PlphNo.set_readonly(true);
