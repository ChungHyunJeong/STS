/***********************************************************************************************
 * 02.DG0.04. [Detail그리드-버튼] 행삭제 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd01DeleteRow = function()
{
	var s_chk = "";//체크
	
	//자재소요량검색 내역 체크 가능하게 변경
	for (var iLoop=0; iLoop<this.ds_Detail10.getRowCount(); iLoop++) {
		s_chk = this.ds_Detail10.getColumn(iLoop, "s_chk");
		
		//체크
		if (s_chk == "1") {
		
			var MorbQt = this.ds_Detail10.getColumn(iLoop, "MORB_QT");
			var PcrbQt = this.ds_Detail10.getColumn(iLoop, "PCRB_QT");
			
			if(this.gfn_IsNull(MorbQt)) {MorbQt = 0;}
			if(this.gfn_IsNull(PcrbQt)) {PcrbQt = 0;}
			
			MorbQt = nexacro.toNumber(MorbQt);
			PcrbQt = nexacro.toNumber(PcrbQt);
			
			if(MorbQt > 0){
				this.ds_Detail10.set_rowposition(iLoop);
				var arrMsgArg = ["자재 출고요청이 있는 자료는 삭제할 수"];
				this.gfn_Alert("MM046",arrMsgArg);
				return false;
			}
			if(PcrbQt > 0){
				this.ds_Detail10.set_rowposition(iLoop);
				var arrMsgArg = ["구매청구건이 있는 자료는 삭제할 수"];
				this.gfn_Alert("MM046",arrMsgArg);
				return false;
			}
		
		
			//검색값설정 - 컬럼명:검색값
			var objSearchList = ["PARH_NO|" + this.ds_Detail10.getColumn(iLoop, "PARH_NO") //PART 관리번호
								,"PARB_SE|" + this.ds_Detail10.getColumn(iLoop, "PARB_SE") //PART 순번
								];

			//[공통] 행삭제시 검색Tab 선택Row 해제 처리
			this.gfn_DeleteRowSchTabUncheck(this.ds_Detail20, objSearchList); 
			
		}
	}
	
	this.gfn_GridDelProcess(this, this.ds_Detail10);//crud_flag 변경-삭제(D)
	
	//신규등록 && 모든데이터 삭제일 경우, 기존 입력된 내역 초기화
	if (this.ds_Master10.getRowType(this.ds_Master10.rowposition) == Dataset.ROWTYPE_INSERT && this.ds_Detail10.getRowCount() == 0) {
		this.fn_SetMasterInputInit();
	}
	
	
	
};

