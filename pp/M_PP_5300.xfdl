				this.div_DetailInput.form.tab_Detail.tpg_02.form.div_Search.form.edt_WorbNm.set_value("");//호기명
		}		
	
	//[Master영역]
	//운반거래처	
	} else if (strId == "M_CUSM_CD_01") {
		if (!this.gfn_IsNull(rtnValue)) {
			var arrRtnVal = eval(rtnValue);
			
