/***********************************************************************************************
 * 02.DG1.05. [Detail그리드2-버튼] 적용 클릭 이벤트
 ***********************************************************************************************/
this.fn_DetailGrd02Choice = function()
{
	if (this.ds_Master10.rowposition > -1) {
		if (this.ds_Detail20.getCaseCount("s_chk=='1'") > 0) {
			
			var sCnthNoChk;
			//계약번호 추가 체크(단일 계약번호만 추가 가능)			
	
			
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {				
					
					//출고요청 상세 탭 계약번호 체크
					if(this.ds_Detail10.rowcount > 0 ){
						if(this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.value != this.ds_Detail20.getColumn(iLoop,"CNTH_NO") ){
							var arrMsgArg = ["계약번호"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 [계약번호]만 적용 가능합니다.
							return;
						}
					}
					
					//그리드내 계약번호 체크
					if(!this.gfn_IsNull(sCnthNoChk) ){
						if(sCnthNoChk != this.ds_Detail20.getColumn(iLoop,"CNTH_NO") ){
							var arrMsgArg = ["계약번호"];
							this.gfn_Alert("SM057", arrMsgArg); //동일한 [계약번호]만 적용 가능합니다.
							return;
						}
					}
					sCnthNoChk = this.ds_Detail20.getColumn(iLoop,"CNTH_NO");
					
					
					//출고요청 수량 체크 
					if(this.ds_Detail20.getColumn(iLoop, "PDOB_QT") <= 0){
						var arrMsgArg = ["출고수량"];
						
						this.ds_Detail20.set_rowposition(iLoop);
						this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.setCellPos(this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.getBindCellIndex("body","PDOB_QT"));
						this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.showEditor(true);
						
						this.gfn_Alert("SM055", arrMsgArg); //[출고요청 수량]을(를) 1 이상 입력해 주세요.
						return;
					}
					
					if(this.ds_Detail20.getColumn(iLoop, "PDOB_QT") > this.ds_Detail20.getColumn(iLoop, "STOC_QT") ){
						var arrMsgArg = ["출고수량", "재고수량"];
						this.gfn_Alert("SM056", arrMsgArg); //[출고수량 수량]이(가) [재고수량]을(를) 초과할 수 없습니다.
						return;
					}
					
				}
			}
			
			var arrCheckRow = [];//체크된 Row
			var sCOMM_CD = "";//회사코드
			var sCNTH_NO = "";//계약번호
			var sCNTB_SE = "";//계약순번
			var sCNTC_SE = "";//계약상세순번				
				
			this.ds_Detail20.set_enableevent(false);
			
			for (var iLoop=0; iLoop<this.ds_Detail20.getRowCount(); iLoop++) {				
				
				
				if (this.ds_Detail20.getColumn(iLoop, "s_chk") == "1") {

					//검색값설정 - 컬럼명:검색값
					var objSearchList = ["COMM_CD|" + this.ds_Detail20.getColumn(iLoop, "COMM_CD") //회사코드
										,"PDRH_NO|" + this.ds_Detail20.getColumn(iLoop, "PDRH_NO") //제품출고요청번호
										,"CNTH_NO|" + this.ds_Detail20.getColumn(iLoop, "CNTH_NO") //계약번호
