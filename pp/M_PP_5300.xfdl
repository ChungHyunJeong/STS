/*---------------------------------------------------------------------------------------------
 *    01. [데이터셋-ds_Master10] 데이터 Row 변경 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_onrowposchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (e.newrow > -1) {
		
		//[결재] 결재상태에 따른 컴포넌트 제어
		//this.fn_SetWfCompEnable();		
		
// 		//선택한 Row BackgroundColor 설정 
// 		this.gfn_SetGridSelectRowColor(this.div_MasterView.form.grd_Master10, e.newrow);//선택한 Row BackgroundColor 설정 
		
		
				
		//출고No정보가 있으면 - 상세조회
		if (!this.gfn_IsNull(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PDOH_DD"))) {
					
			//[월마감제어-버튼/상세그리드]
			var sBUDG_MM = this.gfn_Nvl(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PDOH_DD"), "");//마감년월
			
			//마감여부 체크
			var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
			var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
			paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
			paramObj.MODM_CD = "PP";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
			paramObj.ACUM_CD = "";//[선택] 회계단위
			paramObj.BUSM_CD  = "";//[선택] 사업부문
			paramObj.SAUM_CD  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SAUM_CD");//[선택] 사업장코드 
			paramObj.DEPM_CD  = "";//[선택] 부서코드
			paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
			paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
			
			var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		

			//월마감된 경우
			if (bCheck) {
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(false);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.set_readonly(true);
				this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.set_readonly(true);
				this.fvFileDiv.fn_SetCopmGridAuth(false);			
				this.fvMmCloseYn = true;//월마감여부-Tab버튼 제어용
				
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PdohDd.set_readonly(true);	//출고일자
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcsuNm.set_readonly(true);	//인수자
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PlacNm.set_readonly(true);	//납품장소
				this.div_MasterInput.form.tab_Master.tpg_01.form.mse_AtelNo.set_readonly(true);	//인수자전화
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_StopNm.set_readonly(true);	//경유지
				this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(true);	//비고
			} else {
				this.div_DetailInput.form.div_ComGrdBtn.set_enable(true);
				this.div_DetailInput.form.tab_Detail.tpg_01.form.grd_Detail10.set_readonly(false);
				this.div_DetailInput.form.tab_Detail.tpg_02.form.grd_Detail20.set_readonly(false);
				this.fvFileDiv.fn_SetCopmGridAuth(true);
				this.fvMmCloseYn = false;//월마감여부-Tab버튼 제어용
				
				this.div_MasterInput.form.tab_Master.tpg_01.form.cal_PdohDd.set_readonly(false);	//출고일자
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_AcsuNm.set_readonly(false);	//인수자
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_PlacNm.set_readonly(false);	//납품장소
				this.div_MasterInput.form.tab_Master.tpg_01.form.mse_AtelNo.set_readonly(false);	//인수자전화
				this.div_MasterInput.form.tab_Master.tpg_01.form.edt_StopNm.set_readonly(false);	//경유지
				this.div_MasterInput.form.tab_Master.tpg_01.form.txt_RemkNm.set_readonly(false);	//비고
			
			}
			
			
			//출고No Mask
			var strPdohNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PDOH_NO")
			if (strPdohNo.length == 12)
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PdohNo.set_format("@@@@@@@@-@@@@");
			else
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_PdohNo.set_format("@######-####");
			
			//계약번호 format 변경 (9자리, 7자리)
			var strCnthNo = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "CNTH_NO");
			if (strCnthNo.length == 9) 
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_format("####-@-####");		
			else 
				this.div_MasterInput.form.tab_Master.tpg_01.form.mae_CnthNo.set_format("@##-####");
				
			//전화번호 포멧설정
			this.gfn_TelFormat(this.ds_Master10.getColumn(this.ds_Master10.rowposition, "ATEL_NO"),this.div_MasterInput.form.tab_Master.tpg_01.form.mse_AtelNo);
		
			//수불구분 비활성화
			//this.div_MasterInput.form.tab_Master.tpg_01.form.cbo_PdrhGu.set_enable(true);
			
			this.ds_PpRadd.filter("RADM_CD == '"+ this.ds_Master10.getColumn(this.ds_Master10.rowposition,"RADM_CD")  +"'");	//수불사유필터
			//입력영역 버튼 비활성화
			//this.fn_SetGridInputButtonEnable();
			
			
			//[업무] 데이터셋 설정 - 데이터셋 초기화, 조회조건 설정
			this.ds_Detail10.clearData();
			this.ds_Input.clearData();
			
			this.ds_Input.addRow();
			this.ds_Input.setColumn(0, "PDOH_NO", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PDOH_NO"));
			//this.ds_Input.setColumn(0, "PDOB_SE", this.ds_Master10.getColumn(this.ds_Master10.rowposition, "PDOB_SE"));
			//this.ds_Input.setColumn(0, "PRGM_ID", this.fvComPrgmId);
			
			//[공통]서비스정보 초기화
			this.gfn_InitComService(this);
			
			//[공통]서비스정보 추가
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "pp:M_PP_5300_M_SL20";	// 운반상세
			this.gfn_AddComSearchService(this, svcAddObj);
			
			
			var svcAddObj = new Object();//[공통] 서비스 추가 파라메타 정보 Object
			svcAddObj.sSelectSql = "pp:M_PP_5300_D_SL10";	//상세	
			this.gfn_AddComSearchService(this, svcAddObj);
			
			//[공통]서비스 호출 변수
			var svcCallObj = new Object();//[공통] 서비스 파라메타 정보 Object
			svcCallObj.sTrid = "DTL_SEARCH";//Transaction ID 등 callback에서 넘겨 받을 String을 지정
			svcCallObj.sActionName = this.G_SV_PP;//서버에서 호출할 Action - 모듈별로 호출되는 기본 Actio이 다름 : 공통 서비스 Action 업무구분  변수 참조
			svcCallObj.sInputDaset = "ds_Input=ds_Input" //입력 데이터셋
			svcCallObj.sOutDatset = "ds_Master20=ds_Master20 ds_Detail10=ds_Detail10";//출력 데이터셋
			svcCallObj.sOtherArg = "";//서버로 전송할 기타 Argument 정보 문자열, 예) key=value key2=value2
			svcCallObj.sCallBackFnc = "fn_Callback";//서버에서 처리가 완료된 후 Callback 받을 Function 명

			//[공통]서비스 호출
			this.gfn_CallComService(this, svcCallObj);		
		}
	}
};

/*---------------------------------------------------------------------------------------------
 *    02. [데이터셋-ds_Master10] Row 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_canrowposchange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if (!this.fvDsRowPosChange) return;//Row변경이벤트 수행유무
	
	//데이터 변경여부 체크
	if (this.gfn_DsIsUpdated(this.ds_Master10) || this.gfn_DsIsUpdated(this.ds_Detail10)) {
		this.gfn_Confirm("SM003", "", "CONF.MAST_CHANGE", "fn_MsgCallback");//변경된 내역을 저장 하시겠습니까?
		return false;
	}	

	return true;
};  

/*---------------------------------------------------------------------------------------------
 *    03. [데이터셋-ds_Master10] 컬럼 변경전 이벤트
  ---------------------------------------------------------------------------------------------*/
this.ds_Master10_cancolumnchange = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//[월마감제어-등록/수정]
	if (e.columnid == "PDOH_DD") {
		var sBUDG_MM = "";//마감년월
		
		if (!this.gfn_IsNull(this.fvMmCloseDay)) {
			sBUDG_MM = this.gfn_Nvl(this.fvMmCloseDay, "");//마감년월
		} else {
			sBUDG_MM = this.gfn_Nvl(e.newvalue, "");//마감년월
		}
		
		this.fvMmCloseDay = "";

		//마감여부 체크
		var paramObj= new Object();//[공통] 예산통제체크 파라메타 정보 Object
		var mgasDst  = "C";//[필수] 마감구분 - C: 마감여부, A: 회계마감여부  
		paramObj.MGAS_MM = sBUDG_MM.substr(0,6);//마감년월
		paramObj.MODM_CD = "PP";//[필수] 모듈구분(공통코드-CA910(마감구분) 참조) : CC - 예산편성확정 마감여부, CB - 예산마감 여부
		paramObj.ACUM_CD = "";//[선택] 회계단위
		paramObj.BUSM_CD  = "";//[선택] 사업부문
		paramObj.SAUM_CD  = this.ds_Master10.getColumn(this.ds_Master10.rowposition, "SAUM_CD");//[선택] 사업장코드
		paramObj.DEPM_CD  = "";//[선택] 부서코드
		paramObj.MODD_CD  = "";//[선택] 모듈상세구분(CA900)
		paramObj.BFPR_YN  = "Y";//[선택] 마감전 프로세스여부
		
		var bCheck = this.gfn_CheckMgas(paramObj, mgasDst);//마감여부 체크		

		//월마감된 경우
		if (bCheck) {
			this.gfn_Alert("CA025");//해당월의 정보가 마감이 되어서 등록/수정 할 수 없습니다.
			
			var sOrgDate = this.ds_Master10.getOrgColumn(this.ds_Master10.rowposition, "PDOH_DD");//변경전값
			
			//변경전값이 없을경우
			if (this.gfn_IsNull(sOrgDate)) {				
				var sToDay = this.gfn_GetCommDate("toDay");//현재일자
				
				if (sBUDG_MM.substr(0,6) == sToDay.substr(0,6)) {
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PDOH_DD", "");//입력날짜가 마감된경우 오늘날짜로 세팅				
				} else {
					this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PDOH_DD", sToDay);//입력날짜가 마감된경우 오늘날짜로 세팅
				}
			} else {
				this.ds_Master10.setColumn(this.ds_Master10.rowposition, "PDOH_DD", sOrgDate);//입력날짜가 마감된경우 변경전 값으로 세팅
			}
			
			return false;
		} else {
			return true;
		}

	}	
};

/***********************************************************************************************
 *    03. [데이터셋-ds_Master20] 데이터 변경 이벤트
 ***********************************************************************************************/
this.ds_Master20_oncolumnchanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	this.gfn_GridUpdateProcess(this, obj, e);//crudType 변경-수정(U)
	
		
	//품목코드
	if (e.columnid == "CUSM_CD") {
		this.gfn_GetCommCusmGrid(obj, "CUSM_CD", "CUSM_NM");//거래처 검색			
	
	// 기본료 / 대기료 / 기타추가금
	}else if (e.columnid == "BASI_AM" || e.columnid == "WAIT_AM" || e.columnid == "ETAD_AM" ){
	//var sBasiAm = this.gfn_Nvl(obj.getColumn(e.row,"BASI_AM"), 0);
		
		var sBasiAm = obj.getColumn(e.row,"BASI_AM");
		var sWaitAm = obj.getColumn(e.row,"WAIT_AM");
		var sEtadAm = obj.getColumn(e.row,"ETAD_AM");
		obj.setColumn(e.row,"TRAN_AM", sBasiAm + sWaitAm + sEtadAm);
	
	//차량. 거리별 기본료 계산 / 대기시간별 대기요금 계산
	}else if (e.columnid =="VEHI_CD" || e.columnid =="KIMT_CD" || e.columnid =="WAIT_CD"){
		
		var i520FindRow = this.ds_Pp520.findRowExpr("CODM_CD=='" + obj.getColumn(e.row,"VEHI_CD") + "'");//차량톤수 키 찾기
		var itonCode = this.ds_Pp520.getColumn(i520FindRow,"CODM_CD")
		//trace("톤수키-->"+i520FindRow);
		//trace("톤수-->"+this.ds_Pp520.getColumn(i520FindRow,"OP01_NM"));
		
		//차량. 거리별 기본료 계산
		if(!this.gfn_IsNull(obj.getColumn(e.row,"VEHI_CD")) && !this.gfn_IsNull(obj.getColumn(e.row,"KIMT_CD"))){
			var iKimtAm = 0;
			var i525FindRow = this.ds_Pp525.findRowExpr("CODM_CD=='" + obj.getColumn(e.row,"KIMT_CD") + "'");
			var itonCode = this.ds_Pp520.getColumn(i520FindRow,"CODM_CD")
			switch(itonCode)
			{
				case "0010" : //1톤
					iKimtAm = this.ds_Pp525.getColumn(i525FindRow,"OP01_NM");
				break;				
				case "0020" : //2.5톤
					iKimtAm = this.ds_Pp525.getColumn(i525FindRow,"OP02_NM");
				break;				
				case "0030" : 	//5톤(윙)
					iKimtAm = this.ds_Pp525.getColumn(i525FindRow,"OP03_NM");
				break;				
				case "0040" : 	//5톤(카고)
					iKimtAm = this.ds_Pp525.getColumn(i525FindRow,"OP04_NM");
				break;				
				case "0050" : 	//11톤(단축윙)
					iKimtAm = this.ds_Pp525.getColumn(i525FindRow,"OP05_NM");
				break;				
				case "0060" : 	//11톤(장축윙)
					iKimtAm = this.ds_Pp525.getColumn(i525FindRow,"OP06_NM");
				break;	
				case "0070" : 	//11톤카고
					iKimtAm = this.ds_Pp525.getColumn(i525FindRow,"OP07_NM");
				break;	
				case "0080" : 	//18톤
					iKimtAm = this.ds_Pp525.getColumn(i525FindRow,"OP08_NM");
				break;	
				case "0090" : 	//25톤
					iKimtAm = this.ds_Pp525.getColumn(i525FindRow,"OP09_NM");
				break;	
				
			}
			//trace("기본료-->"+iKimtAm);
			obj.setColumn(e.row,"BASI_AM",iKimtAm);
		}
		
		
		//차량. 대기시간별 대기요금 계산
		if(!this.gfn_IsNull(obj.getColumn(e.row,"VEHI_CD")) && !this.gfn_IsNull(obj.getColumn(e.row,"WAIT_CD"))){
			//trace("차량종류->"+obj.getColumn(e.row,"VEHI_CD"));
			var iWaitAm = 0;
			
			var i530FindRow = this.ds_Pp530.findRowExpr("CODM_CD=='" + obj.getColumn(e.row,"WAIT_CD") + "'");
			var itonCnt = this.ds_Pp520.getColumn(i520FindRow,"OP01_NM")
			switch(itonCnt)
			{
				case "1" : //1톤
					iWaitAm = this.ds_Pp530.getColumn(i530FindRow,"OP01_NM");
				break;				
				case "2.5" : //2.5톤
					iWaitAm = this.ds_Pp530.getColumn(i530FindRow,"OP02_NM");
				break;				
				case "5" : 	//5톤
					iWaitAm = this.ds_Pp530.getColumn(i530FindRow,"OP03_NM");
				break;				
				case "11" : //11톤
					iWaitAm = this.ds_Pp530.getColumn(i530FindRow,"OP04_NM");
				break;				
				case "18" : //18톤
					iWaitAm = this.ds_Pp530.getColumn(i530FindRow,"OP05_NM");
				break;				
				case "25" : //25톤
					iWaitAm = this.ds_Pp530.getColumn(i530FindRow,"OP06_NM");
				break;	
			}
			//trace("대기요금-->"+iWaitAm);
			obj.setColumn(e.row,"WAIT_AM",iWaitAm);
			
		}
		
		var sBasiAm = obj.getColumn(e.row,"BASI_AM");
		var sWaitAm = obj.getColumn(e.row,"WAIT_AM");
		var sEtadAm = obj.getColumn(e.row,"ETAD_AM");
		obj.setColumn(e.row,"TRAN_AM", sBasiAm + sWaitAm + sEtadAm);
		
		
	}
};
